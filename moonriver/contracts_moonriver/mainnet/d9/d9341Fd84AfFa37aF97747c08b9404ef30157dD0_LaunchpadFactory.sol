pragma solidity =0.8.4;














contract LaunchpadIDO is
    Adminable,
    ReentrancyGuard,
    Timed,
    GeneralIDO,
    Withdrawable,
    WithLimits,
    WithWhitelist,
    WithLevelsSale,
    WithCancelable
{
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;

    string public id;
    uint256 public tokensSold;
    uint256 public raised;
    EnumerableSet.AddressSet internal participants;
    uint256 public firstPurchaseBlockN;
    uint256 public lastPurchaseBlockN;

    event TokensPurchased(
        address indexed beneficiary,
        uint256 value,
        uint256 amount
    );

    constructor(
        string memory _id,
        uint256 _startTime,
        uint256 _duration,
        uint256 _rate,
        uint256 _tokensForSale,
        address _fundToken,
        address _priceFeed,
        bool[2] memory _saleTypes
    )
        GeneralIDO(_rate, _tokensForSale, _priceFeed)
        Timed(_startTime, _duration)
        Withdrawable(_fundToken)
    {
        id = _id;
        levelsEnabled = _saleTypes[0];
        whitelistEnabled = _saleTypes[1];
    }

    receive() external payable {
        revert("Sale: Cannot buy, use the 'buyTokens' function");
    }

    fallback() external {
        revert("Sale: Cannot buy, use the 'buyTokens' function");
    }

    function buyTokens(uint256 amount,uint256 userWeight, bytes32[] memory merkleProof)
        public
        payable
        ongoingSale
        nonReentrant
        notCanceled
    {
        require(
            !fundByTokens,
            "Sale: presale is funded by tokens but value is missing"
        );

        internalBuyTokens(msg.value,amount, userWeight, merkleProof);
    }

    /**
     * The fund token must be first approved to be transferred by presale contract for the given "value".
     */
    function buyTokens(
        uint256 value,
        uint256 amount,
        uint256 userWeight,
        bytes32[] memory merkleProof
    ) public ongoingSale nonReentrant notCanceled {
        require(fundByTokens, "Sale: funding by tokens is not allowed");
        require(
            fundToken.allowance(msg.sender, address(this)) >= value,
            "Sale: fund token not approved"
        );

        internalBuyTokens(value,amount, userWeight, merkleProof);

        fundToken.safeTransferFrom(msg.sender, address(this), value);
    }

    function internalBuyTokens(
        uint256 value,
        uint256 amount,
        uint256 userWeight,
        bytes32[] memory merkleProof
    ) private {
        address account = _msgSender();
        // Verify the merkle proof.
        bytes32 node = keccak256(abi.encodePacked(account, userWeight));
        userWeight = MerkleProof.verify(merkleProof, merkleRoot, node)
            ? userWeight
            : 0;
        uint256 maxAllocation = checkAccountAllowedToBuy(userWeight);
        require(value > 0, "Sale: value is 0");

        uint256 requiredValue = calculateNativeRequiredAmount(amount);
        require(amount > 0, "Sale: amount is 0");
        require(msg.value >= requiredValue , "Sale: low balance.");
        tokensSold += amount;
        balances[account] += amount;
        console.log(
            maxAllocation,
            balances[account] <= maxAllocation,
            amount,
            "maxAllocation"
        );
        require(value >= minSell, "Sale: amount is too small");
        require(
            maxAllocation == 0 || balances[account] <= maxAllocation,
            "Sale: amount exceeds max allocation"
        );
        require(tokensSold <= tokensForSale, "Sale: cap reached");

        raised = raised + value;
        participants.add(msg.sender);

        // Store the first and last block numbers to simplify data collection later
        if (firstPurchaseBlockN == 0) {
            firstPurchaseBlockN = block.number;
        }
        lastPurchaseBlockN = block.number;

       uint256 extra = msg.value.sub( requiredValue);
        if(extra > 0)
        payable(msg.sender).transfer(extra);

        emit TokensPurchased(account, value, amount);
    }

    function checkAccountAllowedToBuy(uint256 userWeight)
        private
        view
        returns (uint256)
    {
        // address account = _msgSender();
        uint256 levelAllocation = userWeight;
        uint256 maxAllocation = calculatePurchaseAmountUSD(maxSell);

        // Public sale with no whitelist or levels
        if (!whitelistEnabled && !levelsEnabled) {
            return maxAllocation;
        }

        // User whitelisted, consider his level allocation too
        if (whitelistEnabled && whitelisted[msg.sender]) {
            if (levelAllocation > 0 && levelsOpenAll()) {
                (, uint256 fcfsAllocation) = getUserLevelState(userWeight);
                require(
                    fcfsAllocation > 0,
                    "Sale: user does not have FCFS allocation"
                );
                levelAllocation = fcfsAllocation;
            }

            return maxAllocation + levelAllocation;
        }
        if (whitelistEnabled && !levelsEnabled) {
            revert("Sale: not in the whitelist");
        }

        // Check user level if levels enabled and user was not whitelisted
        if (levelsEnabled) {
            // If opened for all levels, just return the level allocation without checking user weight
            if (levelsOpenAll()) {
                (, uint256 fcfsAllocation) = getUserLevelState(userWeight);
                require(
                    fcfsAllocation > 0,
                    "Sale: user does not have FCFS allocation"
                );

                return fcfsAllocation;
            }

            // bytes memory levelBytes = bytes(userLevel[account]);
            // require(
            //     levelBytes.length > 0,
            //     "Sale: user level is not registered"
            // );
            require(
                levelAllocation > 0,
                "Sale: user has no level allocation or level is too low"
            );

            return levelAllocation;
        }

        revert("Sale: unreachable state");
    }

    function getParticipants () public view returns (address [] memory,uint256 [] memory) {
        address [] memory users  = participants.values();
        uint256[] memory amounts = new uint256[](participants.length());
        for(uint256 i =0;i<participants.length();i++){
            amounts[i] = balances[users[i]];
        }
        return (users,amounts);
    }

    function max(uint256 a, uint256 b) private pure returns (uint256) {
        return a > b ? a : b;
    }
}


// File contracts/Factory/factory.sol


pragma solidity =0.8.4;


contract LaunchpadFactory is Adminable {
    struct IdoInfo {
        LaunchpadIDO ido;
        string dataUri;
    }

    IdoInfo[] public idos;

    function deployIDO(
        string memory _id,
        uint256 _startTime,
        uint256 _duration,
        uint256 _rate,
        uint256 _tokensForSale,
        address _fundToken,
        address _priceFeed,
        bool[2] memory _saleTypes,
        string memory dataUri
    ) external onlyOwnerOrAdmin {
        LaunchpadIDO ido = new LaunchpadIDO(
            _id,
            _startTime,
            _duration,
            _rate,
            _tokensForSale,
            _fundToken,
            _priceFeed,
            _saleTypes
        );
        idos.push(IdoInfo(ido, dataUri));
    }

    function updateDataUri(uint256 index, string memory _dataUri)
        external
        onlyOwnerOrAdmin
    {
        require(index < idos.length, "Index out of bounds");
        idos[index].dataUri = _dataUri;
    }

    function getAllIdos() public view returns (IdoInfo[] memory) {
        return idos;
    }
}