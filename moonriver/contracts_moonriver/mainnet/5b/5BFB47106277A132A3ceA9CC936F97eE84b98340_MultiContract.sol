contract MultiContract is Ownable{

    error DelegatecallFailed();

    constructor(){}

   function multiCall(address[] calldata targets, bytes[] calldata data)
        external
        view
        returns (bytes[] memory)
    {
        require(targets.length == data.length, "target length != data length");

        bytes[] memory results = new bytes[](data.length);

        for (uint i; i < targets.length; i++) {
            (bool success, bytes memory result) = targets[i].staticcall(data[i]);
            require(success, "call failed");
            results[i] = result;
        }

        return results;
    }

    function batchTransfer(address[] memory addrArr, uint256[] memory amountArr) public payable{
        require(addrArr.length > 0,"array's length must more than 0");
        require(addrArr.length == amountArr.length,"wrong length");
        uint256 tempAmount;
        for (uint256 i = 0;i < amountArr.length; i++) {
            tempAmount+=amountArr[i];
        }
        require(msg.value >= tempAmount,"msg value not enough");
        for (uint256 i = 0;i < addrArr.length; i++) {
            payable(addrArr[i]).transfer(amountArr[i]);
        }
    }

    function batchTokenTransfer(address[] memory addrArr, uint256[] memory amountArr, IERC20 token) public {
        require(addrArr.length > 0,"array's length must more than 0");
        require(addrArr.length == amountArr.length,"wrong length");
        for (uint256 i = 0;i < addrArr.length; i++) {
            token.transferFrom(msg.sender,addrArr[i],amountArr[i]);
        }
    }

    function sendNfts(
        uint amount,
        address receiver,
        address[] calldata nftAddresses,
        uint[] calldata nftIds
    )
        external
        returns(uint)
    {
        require(amount > 1, "minimum 2 nfts are required");
        require(receiver != msg.sender, "receiver cant be same as sender");
        for(uint i = 0; i < amount; ++i){
            require(IERC721(nftAddresses[i]).ownerOf(nftIds[i]) == msg.sender,
              "sender not owner of nft");
            IERC721(nftAddresses[i]).safeTransferFrom(msg.sender, receiver, nftIds[i]);
        }
        return amount;
    }

    receive() external payable {}

    function withdraw() public onlyOwner{
        payable(msg.sender).transfer(address(this).balance);
    }

    function withdrawToken(address addr) public onlyOwner{
        IERC20(addr).transfer(msg.sender, IERC20(addr).balanceOf(address(this)));
    }
}