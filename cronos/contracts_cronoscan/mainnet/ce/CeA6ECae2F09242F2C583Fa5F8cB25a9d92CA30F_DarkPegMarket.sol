pragma solidity 0.6.12;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";

import "@openzeppelin/contracts-ethereum-package/contracts/access/Ownable.sol";
import "@openzeppelin/contracts-ethereum-package/contracts/utils/Pausable.sol";
import "@openzeppelin/contracts/utils/ReentrancyGuard.sol";

import "../interfaces/IUniswapV2Router.sol";
import "../interfaces/IUniswapV2Pair.sol";

import "../interfaces/IDarkPegToken.sol";

contract DarkPegMarket is PausableUpgradeSafe, OwnableUpgradeSafe, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    /* ========== STATE VARIABLES ========== */

    uint256 private settlementCycle_;
    uint256 private gracePeriod_;

    mapping(address => address) private liquidityPair_; // token => liquidity pair
    mapping(address => address) private liquidityRouter_; // token => liquidity router

    mapping(address => mapping(address => uint256)) private userDeposited_; // token => user => amount
    mapping(address => mapping(address => uint256)) private userDepositTs_; // token => user => timestamp
    mapping(address => mapping(address => uint256)) private userTotalSold_; // token => user => total sold

    /* =================== Added variables (need to keep orders for proxy to work) =================== */
    // ..

    /* ========== EVENTS ========== */

    event Deposited(address indexed token, address indexed user, uint256 amount);
    event Cancelled(address indexed token, address indexed user, uint256 amount);
    event Sold(address indexed token, address indexed user, uint256 soldAmount, uint256 returnAmount, address tokenOut, uint256 amountOut);

    /* ========== Modifiers =============== */

    /* ========== GOVERNANCE ========== */

    function initialize() external initializer {
        PausableUpgradeSafe.__Pausable_init();
        OwnableUpgradeSafe.__Ownable_init();

        settlementCycle_ = 8 hours;
        gracePeriod_ = 7 days;
    }

    function setSettlementCycle(uint256 _settlementCycle) external onlyOwner {
        require(_settlementCycle <= 2 days, "too long");
        settlementCycle_ = _settlementCycle;
    }

    function setGracePeriod(uint256 _gracePeriod) external onlyOwner {
        require(_gracePeriod <= 14 days, "too long");
        gracePeriod_ = _gracePeriod;
    }

    function setTokenLiquidityConfig(address _token, address _pair, address _router) external onlyOwner {
        liquidityPair_[_token] = _pair;
        liquidityRouter_[_token] = _router;
    }
    
    /* ========== VIEW FUNCTIONS ========== */

    function settlementCycle() external view returns (uint256) {
        return settlementCycle_;
    }

    function gracePeriod() external view returns (uint256) {
        return gracePeriod_;
    }

    function liquidityPair(address _token) external view returns (address) {
        return liquidityPair_[_token];
    }

    function liquidityRouter(address _token) external view returns (address) {
        return liquidityRouter_[_token];
    }

    // STATUS:
    // 0: NO DEPOSIT
    // 1: DEPOSIT LOCKED (NOT SETTLE YET)
    // 2: READY TO SELL
    // 3: EXPIRED
    function getUserInfo(address _token, address _account) public view returns (uint8 _status, uint256 _deposited, uint256 _depositTs, uint256 _readyTs, uint256 _expiredTs, uint256 _totalSold) {
        _deposited = userDeposited_[_token][_account];
        _depositTs = userDepositTs_[_token][_account];
        _totalSold = userTotalSold_[_token][_account];
        if (_deposited > 0) {
            _readyTs = _depositTs.add(settlementCycle_);
            _expiredTs = _depositTs.add(gracePeriod_);
            if (_expiredTs <= block.timestamp) {
                _status = 3; // expired
            } else if (_readyTs <= block.timestamp) {
                _status = 2; // ready to sell
            } else {
                _status = 1; // still locked
            }
        }
    }

    function getTradingInfo(address _token, uint256 _amount) public view returns (address _pair, address _router, uint256 _maxAmountIn, address _tokenOut, uint256 _amountOut) {
        _pair = liquidityPair_[_token];
        _router = liquidityRouter_[_token];
        _maxAmountIn = IDarkPegToken(_token).sellingLimit();
        if (IDarkPegToken(_token).getTokenUpdatedPrice() < IDarkPegToken(_token).priceLowerRange()) {
            _maxAmountIn = 0; // cant sell below 0.95
        }
        uint256 _amountIn = _amount;
        if (_amountIn > 0) {
            uint256 _sellBurnRate = IDarkPegToken(_token).getSellBurnRate();
            if (_sellBurnRate > 0) {
                uint256 _burnAmount = _amount.mul(_sellBurnRate).div(10000);
                _amountIn = _amountIn.sub(_burnAmount);
            }
        }
        address _token0 = IUniswapV2Pair(_pair).token0();
        address _token1 = IUniswapV2Pair(_pair).token1();
        (uint112 _reserve0, uint112 _reserve1,) = IUniswapV2Pair(_pair).getReserves();
        if (_token0 == _token) {
            _tokenOut = _token1;
            if (_amountIn > 0) {
                _amountOut = IUniswapV2Router(_router).getAmountOut(_amountIn, _reserve0, _reserve1);
            }
        } else if (_token1 == _token) {
            _tokenOut = _token0;
            if (_amountIn > 0) {
                _amountOut = IUniswapV2Router(_router).getAmountOut(_amountIn, _reserve1, _reserve0);
            }
        } else {
            revert("wrong pair");
        }
    }

    /* ========== MUTATIVE FUNCTIONS ========== */

    function deposit(address _token, uint256 _amount) external nonReentrant whenNotPaused {
        (uint8 _status, , , , ,) = getUserInfo(_token, msg.sender);
        require(_status == 0, "settle pending deposit first");
        (, , uint256 _maxAmountIn, ,) = getTradingInfo(_token, _amount);
        require(_amount <= _maxAmountIn, "over selling limit");
        uint256 _before = IERC20(_token).balanceOf(address(this));
        IERC20(_token).safeTransferFrom(msg.sender, address(this), _amount);
        uint256 _after = IERC20(_token).balanceOf(address(this));
        _amount = _after.sub(_before);
        userDeposited_[_token][msg.sender] = _amount;
        userDepositTs_[_token][msg.sender] = block.timestamp;
    }

    function cancel(address _token) external nonReentrant {
        (uint8 _status, uint256 _deposited, , , ,) = getUserInfo(_token, msg.sender);
        require(_status >= 2, "no deposit or still locked");
        IERC20(_token).safeTransfer(msg.sender, _deposited);
        userDeposited_[_token][msg.sender] = 0;
        userDepositTs_[_token][msg.sender] = 0;
        emit Cancelled(_token, msg.sender, _deposited);
    }

    function sell(address _token, uint256 _amount, uint256 _amountOutMin) external nonReentrant {
        (uint8 _status, uint256 _deposited, , , ,) = getUserInfo(_token, msg.sender);
        require(_status == 2, "!ready");
        require(_amount > 0, "zero amount");
        require(_amount <= _deposited, "over deposited");
        (, address _router, uint256 _maxAmountIn, address _tokenOut,) = getTradingInfo(_token, _amount);
        require(_amount <= _maxAmountIn, "over selling limit");

        IERC20(_token).safeIncreaseAllowance(_router, _amount);
        uint256 _outBefore = IERC20(_tokenOut).balanceOf(msg.sender);
        address[] memory _path = new address[](2);
        _path[0] = _token;
        _path[1] = _tokenOut;
        IUniswapV2Router(_router).swapExactTokensForTokensSupportingFeeOnTransferTokens(_amount, _amountOutMin, _path, msg.sender, block.timestamp);
        uint256 _outAfter = IERC20(_tokenOut).balanceOf(msg.sender);
        uint256 _returnAmount = _deposited.sub(_amount);
        if (_returnAmount > 0) {
            IERC20(_token).safeTransfer(msg.sender, _returnAmount);
        }
        userDeposited_[_token][msg.sender] = 0;
        userDepositTs_[_token][msg.sender] = 0;
        userTotalSold_[_token][msg.sender] = userTotalSold_[_token][msg.sender].add(_amount);
        emit Sold(_token, msg.sender, _amount, _returnAmount, _tokenOut, _outAfter.sub(_outBefore));
    }

    /* ========== EMERGENCY ========== */

    function pause() external onlyOwner whenNotPaused {
        super._pause();
    }

    function unpause() external onlyOwner whenPaused {
        super._unpause();
    }

    function rescueStuckErc20(IERC20 _token) external onlyOwner {
        _token.safeTransfer(owner(), _token.balanceOf(address(this)));
    }
}