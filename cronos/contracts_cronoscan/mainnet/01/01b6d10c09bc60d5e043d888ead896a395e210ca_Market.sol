contract Market is ReentrancyGuard, Ownable {
    using Counters for Counters.Counter;
    Counters.Counter private _listingId;
    Counters.Counter private _offerId;

    mapping(uint256 => Listing) private _listings;
    mapping(uint256 => Offer) private _offers;

    uint256 private fee = 0;
    uint256 private artistFee = 5;

    enum ListingStatus {
        Active,
        Sold,
        Canceled
    }

    enum OfferStatus {
        Accepted,
        Sold,
        Canceled
    }

    struct Listing {
        ListingStatus status;
        address seller;
        address token;
        uint256 tokenId;
        uint256 price;
    }

    struct Offer {
        OfferStatus status;
        address seller;
        address buyer;
        address token;
        uint256 tokenId;
        uint256 price;
    }

    event Listed(
        address seller,
        address token,
        uint256 tokenId,
        uint256 price,
        uint256 listingId
    );

    event Sale(
        address seller,
        address buyer,
        address token,
        uint256 tokenId,
        uint256 price,
        uint256 listingId
    );

    event CancelListing(
        address seller,
        address token,
        uint256 tokenId,
        uint256 listingId
    );

    event OfferAccepted(
        address seller,
        address buyer,
        address token,
        uint256 tokenId,
        uint256 price,
        uint256 offerId
    );

    event tradeFinalized(
        address seller,
        address buyer,
        address token,
        uint256 tokenId,
        uint256 price,
        uint256 offerId
    );

    function getListing(uint256 listingId)
        public
        view
        returns (Listing memory)
    {
        return _listings[listingId];
    }

    function getOffer(uint256 offerId) public view returns (Offer memory) {
        return _offers[offerId];
    }

    function getAllListings() public view returns (Listing[] memory) {
        uint256 listingCount = _listingId.current();
        uint256 currentIndex = 0;
        Listing[] memory allListings = new Listing[](listingCount);
        for (uint256 i = 0; i < listingCount; i++) {
            uint256 currentId = i + 1;
            Listing memory currentListing = _listings[currentId];
            allListings[currentIndex] = currentListing;
            currentIndex++;
        }
        return allListings;
    }

    function getAllOffers() public view returns (Offer[] memory) {
        uint256 offerCount = _offerId.current();
        uint256 currentIndex = 0;
        Offer[] memory allOffers = new Offer[](offerCount);
        for (uint256 i = 0; i < offerCount; i++) {
            uint256 currentId = i + 1;
            Offer memory currentOffer = _offers[currentId];
            allOffers[currentIndex] = currentOffer;
            currentIndex++;
        }
        return allOffers;
    }

    function listToken(
        address token,
        uint256 tokenId,
        uint256 price
    ) external nonReentrant {
        require(price > 0, "price can't be 0");
        IERC721(token).transferFrom(msg.sender, address(this), tokenId);
        Listing memory listing = Listing(
            ListingStatus.Active,
            msg.sender,
            token,
            tokenId,
            price
        );
        _listingId.increment();
        uint256 listingId = _listingId.current();
        _listings[listingId] = listing;

        emit Listed(msg.sender, token, tokenId, price, listingId);
    }

    function buyToken(uint256 listingId) external payable nonReentrant {
        Listing storage listing = _listings[listingId];
        require(msg.sender != listing.seller, "seller can't be buyer");
        require(
            listing.status == ListingStatus.Active,
            "listing is not active"
        );
        require(
            msg.value >= listing.price,
            "payment sent is less than the price"
        );
        IERC721(listing.token).transferFrom(
            address(this),
            msg.sender,
            listing.tokenId
        );
        payable(listing.seller).transfer(
            (listing.price * (100 - fee - artistFee)) / 100
        );
        listing.status = ListingStatus.Sold;

        emit Sale(
            listing.seller,
            msg.sender,
            listing.token,
            listing.tokenId,
            listing.price,
            listingId
        );
    }

    function cancelListing(uint256 listingId) public {
        Listing storage listing = _listings[listingId];
        require(
            listing.status == ListingStatus.Active,
            "listing is not active"
        );
        require(
            msg.sender == listing.seller,
            "only seller can cancel the listing"
        );
        listing.status = ListingStatus.Canceled;
        IERC721(listing.token).transferFrom(
            address(this),
            msg.sender,
            listing.tokenId
        );

        emit CancelListing(
            msg.sender,
            listing.token,
            listing.tokenId,
            listingId
        );
    }

    function acceptOffer(
        address token,
        address buyer,
        uint256 tokenId,
        uint256 price
    ) external nonReentrant {
        require(price > 0, "price can't be 0");
        IERC721(token).transferFrom(msg.sender, address(this), tokenId);
        Offer memory offer = Offer(
            OfferStatus.Accepted,
            msg.sender,
            buyer,
            token,
            tokenId,
            price
        );
        _offerId.increment();
        uint256 offerId = _offerId.current();
        _offers[offerId] = offer;

        emit OfferAccepted(msg.sender, buyer, token, tokenId, price, offerId);
    }

    function cancelOffer(uint256 offerId) public {
        Offer storage offer = _offers[offerId];
        require(offer.status == OfferStatus.Accepted, "offer is not active");
        require(
            msg.sender == offer.seller,
            "only seller can cancel the active offer"
        );

        offer.status = OfferStatus.Canceled;
        IERC721(offer.token).transferFrom(
            address(this),
            msg.sender,
            offer.tokenId
        );
    }

    function finalizeTrade(uint256 offerId) external payable nonReentrant {
        Offer storage offer = _offers[offerId];
        require(offer.seller != msg.sender, "seller can't finalize own trades");
        require(
            offer.buyer == msg.sender,
            "only sellected buyer can finalize the trade"
        );
        require(offer.status == OfferStatus.Accepted, "offer must be accepted");
        require(
            msg.value >= offer.price,
            "payment is less than an offer price"
        );
        IERC721(offer.token).transferFrom(
            address(this),
            offer.buyer,
            offer.tokenId
        );
        payable(offer.seller).transfer(
            (offer.price * (100 - fee - artistFee)) / 100
        );
        offer.status = OfferStatus.Sold;

        emit tradeFinalized(
            offer.seller,
            msg.sender,
            offer.token,
            offer.tokenId,
            msg.value,
            offerId
        );
    }

    function setFee(uint256 _fee) public onlyOwner {
        fee = _fee;
    }

    function setArtistFee(uint256 _artistFee) public onlyOwner {
        artistFee = _artistFee;
    }

    function withdraw() public payable onlyOwner {
        (bool os, ) = payable(owner()).call{value: address(this).balance}("");
        require(os);
    }
}