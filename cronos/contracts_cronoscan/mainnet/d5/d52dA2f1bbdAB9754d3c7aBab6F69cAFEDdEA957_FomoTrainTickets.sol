import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721RoyaltyUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract FomoTrainTickets is Initializable, ERC721Upgradeable, ERC721EnumerableUpgradeable, ERC721RoyaltyUpgradeable, PausableUpgradeable, OwnableUpgradeable, UUPSUpgradeable {
    using SafeMath for uint256;
    using CountersUpgradeable for CountersUpgradeable.Counter;

    CountersUpgradeable.Counter private _tokenIdCounter;

    uint public constant MAX_SUPPLY = 150;
    uint public constant PRICE = 220 ether;

    mapping(address => bool) private _bannedAddresses;

    string private _baseUri;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(
        string memory name,
        string memory symbol,
        string memory baseUri
    ) initializer public {
        __ERC721_init(name, symbol);
        __ERC721Enumerable_init();
        __ERC721Royalty_init();
        __Pausable_init();
        __Ownable_init();
        __UUPSUpgradeable_init();

        _baseUri = baseUri;
        _tokenIdCounter.increment(); // start with 1
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

//    function safeMintBatch(uint count) public payable {
//        uint256 tokenId = _tokenIdCounter.current();
//
//        require(
//            tokenId.add(count) <= MAX_SUPPLY,
//            string(abi.encodePacked("Maximum supply of NFTs is capped at ", MAX_SUPPLY))
//        );
//
//        require(
//            count > 0 && count <= MAX_PER_MINT,
//            string(abi.encodePacked("Maximum allowed number of NFTs to mint is ", MAX_PER_MINT))
//        );
//
//        require(
//            msg.value >= PRICE.mul(count),
//            string(abi.encodePacked("Not enough CRO to purchase NFTs: ", Strings.toString(msg.value)))
//        );
//
//        for (uint i = 0; i < count; i++) {
//            _tokenIdCounter.increment();
//            uint256 newTokenId = _tokenIdCounter.current();
//            _safeMint(msg.sender, newTokenId);
//        }
//    }

    function safeMint() public payable {
        uint256 tokenId = _tokenIdCounter.current();

        require(
            _bannedAddresses[msg.sender] != true,
            "Sender address banned from minting FTTs."
        );

        require(
            tokenId.add(1) <= MAX_SUPPLY,
            string(abi.encodePacked("Maximum supply of NFTs is capped at ", MAX_SUPPLY))
        );

        require(
            msg.value >= PRICE,
            string(abi.encodePacked("Not enough CRO to purchase NFTs: ",  Strings.toString(msg.value)))
        );

        uint tokenCount = balanceOf(msg.sender);
        require(
            tokenCount == 0,
            "There can only be one token per address at most."
        );

        _tokenIdCounter.increment();
        _safeMint(msg.sender, tokenId);
    }

    function ban(address bannedAddress) public onlyOwner {
        _bannedAddresses[bannedAddress] = true;
        emit AddressBanned(bannedAddress);
    }

    function unban(address unbannedAddress) public onlyOwner {
        _bannedAddresses[unbannedAddress] = false;
        emit AddressUnbanned(unbannedAddress);
    }

    function _baseURI() internal view virtual
    override(ERC721Upgradeable)
    returns (string memory) {
        return _baseUri;
    }

    function tokensOfOwner(address owner)
    external view
    returns (uint[] memory) {
        uint tokenCount = balanceOf(owner);
        uint[] memory tokensId = new uint256[](tokenCount);

        for (uint i = 0; i < tokenCount; i++) {
            tokensId[i] = tokenOfOwnerByIndex(owner, i);
        }

        return tokensId;
    }

    function withdraw()
    public payable onlyOwner {
        uint balance = address(this).balance;
        require(balance > 0, "No CRO left to withdraw");


        (bool success,) = (msg.sender).call{value : balance}("");
        require(success, "Transfer failed.");

        emit Transfer(address(this), msg.sender, balance);
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
    internal
    whenNotPaused
    override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _authorizeUpgrade(address newImplementation)
    internal
    onlyOwner
    override
    {}

    // The following functions are overrides required by Solidity.

    function _burn(uint256 tokenId)
    internal
    override(ERC721Upgradeable, ERC721RoyaltyUpgradeable)
    {
        super._burn(tokenId);
    }

    function setDefaultRoyalty(address receiver, uint96 feeNumerator) public onlyOwner {
        _setDefaultRoyalty(receiver, feeNumerator);
    }

    function currentTokenId() public view virtual returns (uint256) {
        return _tokenIdCounter.current();
    }

    function tokenURI(uint256 tokenId)
    public
    view
    override(ERC721Upgradeable)
    returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
    public
    view
    override(ERC721Upgradeable, ERC721EnumerableUpgradeable, ERC721RoyaltyUpgradeable)
    returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    /**
     * @dev Emitted when `banned` address is not allowed to mint an NFT anymore.
    */
    event AddressBanned(address indexed banned);

    /**
     * @dev Emitted when `unbanned` address gets permission to mint an NFT again.
    */
    event AddressUnbanned(address indexed unbanned);
}

