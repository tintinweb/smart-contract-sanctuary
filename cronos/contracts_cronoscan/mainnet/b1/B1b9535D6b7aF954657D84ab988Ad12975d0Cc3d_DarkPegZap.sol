pragma solidity 0.6.12;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";

import "@openzeppelin/contracts-ethereum-package/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/ReentrancyGuard.sol";

import "../interfaces/IDarkPegToken.sol";
import "../interfaces/IUniswapV2Router.sol";
import "../interfaces/IUniswapV2Pair.sol";
import '../lib/TransferHelper.sol';
import "../lib/Babylonian.sol";

contract DarkPegZap is OwnableUpgradeSafe, ReentrancyGuard {
    using SafeMath for uint;
    using SafeERC20 for IERC20;

    mapping(address => bool) private isDarkPegToken_;

    event ZapIn(address indexed sender, address tokenA, address tokenB, address pool, uint256 amtLp);
    event SwapToken(address inputToken, address outputToken, uint256 amount);

    modifier notContract() {
        uint256 size;
        address addr = msg.sender;
        assembly {
            size := extcodesize(addr)
        }
        require(size == 0, "contract not allowed");
        require(tx.origin == msg.sender, "contract not allowed");
        _;
    }

    /* ========== GOVERNANCE ========== */

    function initialize() external initializer {
        OwnableUpgradeSafe.__Ownable_init();
    }

    function setDarkPegToken(address _token, bool _isDarkPegToken) external onlyOwner {
        isDarkPegToken_[_token] = _isDarkPegToken;
    }

    /* ========== VIEW FUNCTIONS ========== */

    function isDarkPegToken(address _token) external view returns (bool) {
        return isDarkPegToken_[_token];
    }

    /* ========== External Functions ========== */

    // _amounts: _amountADesired, _amountBDesired
    function zapInToken(address _tokenA, address _pair, uint256[] calldata _amounts, address _router) external
        nonReentrant
        returns (uint256 lpAmt)
    {
        address _tokenB;
        {
            address _token0 = IUniswapV2Pair(_pair).token0();
            if (_token0 == _tokenA) _tokenB = IUniswapV2Pair(_pair).token1();
            else if (IUniswapV2Pair(_pair).token1() == _tokenA) _tokenB = _token0;
            else revert("invalid _pair");
        }

        uint256 _tkABal = IERC20(_tokenA).balanceOf(address(this));
        uint256 _tkBBal = IERC20(_tokenB).balanceOf(address(this));
        lpAmt = IERC20(_pair).balanceOf(address(this));

        IERC20(_tokenA).safeTransferFrom(msg.sender, address(this), _amounts[0]);
        IERC20(_tokenB).safeTransferFrom(msg.sender, address(this), _amounts[1]);

        _approveTokenIfNeeded(_tokenA, _router);
        _approveTokenIfNeeded(_tokenB, _router);

        if (isDarkPegToken_[_tokenA]) { // allow to sell B -> A
            uint256 _neededAmountB = getOtherAmountToZapIn(_pair, _tokenA, _amounts[0]);
            if (_neededAmountB < _amounts[1]) {
                {
                    uint256 _sellingAmt = _amounts[1].sub(_neededAmountB).div(2);
                    _swapToken(_tokenB, _tokenA, _router, _sellingAmt);
                }
                uint256 _balA = IERC20(_tokenA).balanceOf(address(this)).sub(_tkABal);
                uint256 _balB = IERC20(_tokenB).balanceOf(address(this)).sub(_tkBBal);
                _addLiquidity(_pair, _tokenA, _tokenB, _router, _balA, _balB);
            } else {
                _addLiquidity(_pair, _tokenA, _tokenB, _router, _amounts[0], _amounts[1]);
            }
        } else if (isDarkPegToken_[_tokenB]) { // allow to sell A -> B
            uint256 _neededAmountA = getOtherAmountToZapIn(_pair, _tokenB, _amounts[1]);
            if (_neededAmountA < _amounts[0]) {
                {
                    uint256 _sellingAmt = _amounts[0].sub(_neededAmountA).div(2);
                    _swapToken(_tokenA, _tokenB, _router, _sellingAmt);
                }
                uint256 _balA = IERC20(_tokenA).balanceOf(address(this)).sub(_tkABal);
                uint256 _balB = IERC20(_tokenB).balanceOf(address(this)).sub(_tkBBal);
                _addLiquidity(_pair, _tokenA, _tokenB, _router, _balA, _balB);
            } else {
                _addLiquidity(_pair, _tokenA, _tokenB, _router, _amounts[0], _amounts[1]);
            }
        } else revert("At least one token must be Dark peg");

        _tkABal = IERC20(_tokenA).balanceOf(address(this)).sub(_tkABal);
        _tkBBal = IERC20(_tokenB).balanceOf(address(this)).sub(_tkBBal);
        lpAmt = IERC20(_pair).balanceOf(address(this)).sub(lpAmt);

        if (_tkABal > 0) IERC20(_tokenA).safeTransfer(msg.sender, _tkABal);
        if (_tkBBal > 0) IERC20(_tokenB).safeTransfer(msg.sender, _tkBBal);
        if (lpAmt > 0) IERC20(_pair).safeTransfer(msg.sender, lpAmt);

        emit ZapIn(msg.sender, _tokenA, _tokenB, _pair, lpAmt);
    }

    /* ========== View Functions ===========*/

    function getOtherAmountToZapIn(address _tokenA, address _pair, uint256 _amountA) public view returns (uint256 _neededAmountB) {
        address _token0 = IUniswapV2Pair(_pair).token0();
        address _token1 = IUniswapV2Pair(_pair).token1();
        (uint112 _reserve0, uint112 _reserve1, ) = IUniswapV2Pair(_pair).getReserves();
        if (_token0 == _tokenA) {
            _neededAmountB = _amountA.mul(uint256(_reserve1)).div(uint256(_reserve0));
        } else if (_token1 == _tokenA) {
            _neededAmountB = _amountA.mul(uint256(_reserve0)).div(uint256(_reserve1));
        } else revert("invalid _pair");
    }

    /* ========== Private Functions ========== */

    function _approveTokenIfNeeded(address token, address _router) private {
        if (IERC20(token).allowance(address(this), address(_router)) == 0) {
            IERC20(token).safeApprove(address(_router), type(uint256).max);
        }
    }

    function _calculateSwapInAmount(address pair, address tokenIn, uint256 userIn, address pairToken0) internal view returns (uint256) {
        (uint256 res0, uint256 res1,) = IUniswapV2Pair(pair).getReserves();
        uint reserveIn = (tokenIn == pairToken0) ? res0 : res1;
        // uint rMul = uint256(10000).sub(uint256(swapFee));
        return _getExactSwapInAmount(reserveIn, userIn, 9970);
    }

    function _getExactSwapInAmount(uint256 reserveIn, uint256 userIn, uint256 rMul) internal pure returns (uint256) {
        return Babylonian
            .sqrt(reserveIn.mul(userIn.mul(40000).mul(rMul) + reserveIn.mul(rMul.add(10000)).mul(rMul.add(10000))))
            .sub(reserveIn.mul(rMul.add(10000))) / (rMul.mul(2));
    }

    function _swapToken(address _inputToken, address _outputToken, address _router, uint256 _amount) internal {
        address[] memory _path = new address[](2);
        _path[0] = _inputToken;
        _path[1] = _outputToken;
        IUniswapV2Router(_router).swapExactTokensForTokens(_amount, 1, _path, address(this), block.timestamp);
        emit SwapToken(_inputToken, _outputToken, _amount);
    }

    function _addLiquidity(address _pair, address _tokenA, address _tokenB, address _router, uint256 _amountADesired, uint256 _amountBDesired) internal {
        IUniswapV2Router(_router).addLiquidity(_tokenA, _tokenB, _amountADesired, _amountBDesired, 1, 1, address(this), block.timestamp);
    }
}