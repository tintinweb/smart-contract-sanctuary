pragma solidity ^0.8.4;
// import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
// import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
struct Auction {
    address nft;
    address seller;
    address highestBidder;
    bool started;
    bool ended;
    bool isValue;
    uint nftId;
    uint highestBid;
    uint endAt;
    uint hashId;
    uint minimumBid;
    uint auctionIndex;
}
struct BidInfo{
  uint value;
  uint created;
  address bidder;
  bool hasWithdrawn;
}

contract NFTAuction is Ownable, ReentrancyGuard {
    event Start(bytes32 indexed auctionHash, uint256 indexed auctionIndex);
    event Bid(bytes32 indexed auctionHash, uint256 indexed auctionIndex, uint256 bidIndex, address indexed sender, uint amount);
    event CreateAuction(bytes32 indexed auctionHash, uint256 indexed auctionIndex);
    event UpdateAuction(bytes32 indexed auctionHash, uint256 indexed auctionIndex);
    event Withdraw(bytes32 indexed auctionHash, uint256 auctionIndex, uint256 bidIndex, address indexed bidder, uint amount);
    event Accept(bytes32 indexed auctionHash, uint256 auctionindex, address winner, uint amount);
    event Cancel(bytes32 indexed auctionHash, uint256 auctionIndex, address seller);
    event TimeIncreased(bytes32 indexed auctionHash, uint256 auctionIndex, address sender, uint increasedMinutes);

    using SafeMath for uint;
    using ERC165Checker for address;
    //AuctionId to Auction Data
    mapping(bytes32 => Auction[]) public auctions;
    mapping(bytes32 => mapping(uint256 => BidInfo[])) bids;
    bytes32[] public auctionHashes;
    uint runTime = 2 days;

    address immutable MADTOKEN_ADDRESS;

    // bytes4 public constant IID_IERC1155 = type(IERC1155).interfaceId;
    bytes4 public constant IID_IERC721 = type(IERC721).interfaceId;

    modifier onlyAuctionOwner(bytes32 _auctionHash, uint256 _auctionIndex) {
        require(auctions[_auctionHash][_auctionIndex].seller == msg.sender, "not seller");
        _;
    }

    modifier restricted(bytes32 _auctionHash, uint256 _auctionIndex){
        require(auctions[_auctionHash][_auctionIndex].seller == msg.sender || owner() == msg.sender , "not owner");
        _;
    }

    modifier onlyAuctionStarted(bytes32 _auctionHash, uint256 _auctionIndex) {
        require(auctions[_auctionHash][_auctionIndex].started, "not started");
        _;
    }

    function count() public view returns(uint){
        return auctionHashes.length;
    }

    function getAuctionsByHash(bytes32 hash) public view returns (Auction[] memory) {
        return auctions[hash];
    }

    function countByHash(bytes32 hash) public view returns(uint256) {
        return auctions[hash].length;
    }

    function hashes() public view returns(bytes32[] memory){
        return auctionHashes;
    }

    function updateRuntime(uint _time) public onlyOwner{
        runTime = _time;
    }

    constructor(address _madTokenAddress) {
        MADTOKEN_ADDRESS = _madTokenAddress;
    }
    
    function _createAuction(bytes32 _auctionHash, address _nft, uint _nftId, uint _startingBid, address _seller) internal {
        auctionHashes.push(_auctionHash);

        Auction memory auction;
        auction.nft = _nft;
        auction.nftId = _nftId;
        auction.seller = _seller;
        auction.isValue = true;
        auction.hashId = count() - 1;
        auction.minimumBid = _startingBid;
        auction.auctionIndex = auctions[_auctionHash].length;
        auctions[_auctionHash].push(auction);

        emit CreateAuction(_auctionHash, auction.auctionIndex);
    }
    
    function updateAuction(bytes32 _auctionHash, uint256 _auctionIndex, uint _startingBid) external {
        Auction memory _auction = auctions[_auctionHash][_auctionIndex];
        require(_auction.seller == msg.sender, "not owner of auction");
        if (_auction.isValue) {
            if (_auction.started) {
                revert("already started");
            }
            auctions[_auctionHash][_auctionIndex].highestBid = _startingBid;
            auctions[_auctionHash][_auctionIndex].minimumBid = minimumBid(_auctionHash, _auctionIndex);
        } else {
            revert("no exist auction");
        }
        
        emit UpdateAuction(_auctionHash, _auctionIndex);
    }

    function is721(address _nft) public view returns(bool){
        return _nft.supportsInterface(IID_IERC721);
    }
   
    // function is1155(address _nft) public view returns(bool){
    //     return _nft.supportsInterface(IID_IERC1155);
    // }

    // create or update auction
    function createAuction(address _nft, uint _nftId, uint _startingBid) external {
        require(is721(_nft), "unsupported type");
        // if (is721(_nft)) {
            require(IERC721(_nft).ownerOf(_nftId) == msg.sender, "not owner of token");
        // } else {
        //     require(IERC1155(_nft).balanceOf(msg.sender, _nftId) >= _quantity, "insufficient balance");
        // }
         _transferNFT(_nft, msg.sender, address(this), _nftId);
        // require(isApproved(_nft, msg.sender), "seller revoked approval");
        
        bytes32 auctionHash = keccak256(abi.encodePacked( _nft, _nftId));

        _createAuction(auctionHash, _nft, _nftId, _startingBid, msg.sender);
    }

    // get the seller of the auction
    function getSeller(bytes32 _auctionHash) public onlyOwner view returns(address) {
        return auctions[_auctionHash][0].seller;
    }

    function getAuction(bytes32 _auctionHash, uint256 _auctionIndex) public view returns(Auction memory) {
        return auctions[_auctionHash][_auctionIndex];
    }

    function start(bytes32 _auctionHash, uint256 _auctionIndex, uint _runTime) public restricted(_auctionHash, _auctionIndex) {
        require(!auctions[_auctionHash][_auctionIndex].started, "started");
        
        auctions[_auctionHash][_auctionIndex].started = true;
        auctions[_auctionHash][_auctionIndex].endAt = block.timestamp + _runTime;

        emit Start(_auctionHash, _auctionIndex);
    }
    
    function startAll() external onlyOwner {
        uint256 len = auctionHashes.length;
        
        for(uint256 i = 0; i < len; i ++) {
            uint256 auctionLen = countByHash(auctionHashes[i]);
            for(uint256 j = 0; j < auctionLen; j ++) {
                if(!auctions[auctionHashes[i]][j].started) {
                    start(auctionHashes[i], j, runTime);
                }
            }
        }
    }

    function getBidIndex(bytes32 _auctionHash,uint256 _auctionIndex, address _address) internal view returns (uint256) {
        uint256 len = bids[_auctionHash][_auctionIndex].length;
        
        for(uint256 i = 0; i < len; i ++) {
            if (bids[_auctionHash][_auctionIndex][i].bidder == _address && !bids[_auctionHash][_auctionIndex][i].hasWithdrawn) {
                return i;
            }
        }
        
        return type(uint256).max;
    }

    function bid(bytes32 _auctionHash, uint256 _auctionIndex, uint256 _amount) external onlyAuctionStarted(_auctionHash, _auctionIndex) {
        Auction memory auction = auctions[_auctionHash][_auctionIndex];
        require(auction.seller != msg.sender, "not available for token seller");
        require(!auction.ended, "auction ended");
        require(block.timestamp < auction.endAt, "ended");
        require(IERC20(MADTOKEN_ADDRESS).balanceOf(msg.sender) >= _amount, "insufficient balance");

        IERC20(MADTOKEN_ADDRESS).transferFrom(msg.sender, address(this), _amount);

        uint bidIndex = getBidIndex(_auctionHash, _auctionIndex, msg.sender);
        uint newBidAmount = _amount;
        if (type(uint256).max != bidIndex) {
            newBidAmount += bids[_auctionHash][_auctionIndex][bidIndex].value;
            
            require(newBidAmount >= minimumBid(_auctionHash, _auctionIndex), "not miniumbid");
            bids[_auctionHash][_auctionIndex][bidIndex].value = newBidAmount;
            bids[_auctionHash][_auctionIndex][bidIndex].created = block.timestamp;
         } else {
            require(_amount >= minimumBid(_auctionHash, _auctionIndex), "not miniumbid");

            BidInfo memory newBid;
            newBid.value = _amount;
            newBid.bidder = msg.sender;
            newBid.created = block.timestamp;
            
            bidIndex = bids[_auctionHash][_auctionIndex].length;

            bids[_auctionHash][_auctionIndex].push(newBid);
        }

        auction.highestBidder = msg.sender;
        auction.highestBid = newBidAmount;

        if (block.timestamp > auction.endAt - 15 minutes) {
            auction.endAt += 15 minutes;
            emit TimeIncreased(_auctionHash, _auctionIndex, msg.sender, 15);    
        }
        auctions[_auctionHash][_auctionIndex] = auction;
        auctions[_auctionHash][_auctionIndex].minimumBid = minimumBid(_auctionHash, _auctionIndex);
        emit Bid(_auctionHash, _auctionIndex, bidIndex, msg.sender, newBidAmount);
    }

    function minimumBid(bytes32 _auctionHash, uint256 _auctionIndex) public view returns(uint) {
        // if this is the first bid, it will return the starting bid
        if (bids[_auctionHash][_auctionIndex].length == 0) {
            return auctions[_auctionHash][_auctionIndex].minimumBid;
        }
        uint highestBid = auctions[_auctionHash][_auctionIndex].highestBid;
        uint increaseBid;

        if (highestBid < 100 ether) {
            increaseBid = 5 ether;
        } else if (highestBid < 500 ether) {
            increaseBid = 10 ether;
        } else if (highestBid < 1000 ether) {
            increaseBid = 50 ether;
        } else {
            increaseBid = 100 ether;
        }

        return highestBid.add(increaseBid);
    }

    function isApproved(address _nft, address _seller) public view returns(bool) {
        if (IERC721(_nft).isApprovedForAll(_seller, address(this))) {
            return true;
        } else {
            return false;
        }
    }

    function withdraw(bytes32 _auctionHash, uint256 _auctionIndex) external {
        Auction memory auction = auctions[_auctionHash][_auctionIndex];
        // if (auction.isValue && isApproved(auction.nft, auction.seller)) {
        if (auction.isValue) {
            require(auction.highestBidder != msg.sender, "not available for highest bidder");
        }
        
        uint bidIndex = getBidIndex(_auctionHash, _auctionIndex, msg.sender);
        require(type(uint256).max != bidIndex, "no bidder exist");

        uint balance = bids[_auctionHash][_auctionIndex][bidIndex].value;   

        bids[_auctionHash][_auctionIndex][bidIndex].hasWithdrawn = true;
        // payable(msg.sender).transfer(balance);
        IERC20(MADTOKEN_ADDRESS).transfer(msg.sender, balance);

        emit Withdraw(_auctionHash, _auctionIndex,  bidIndex, msg.sender, balance);
    }

    function getAllBids(bytes32 _auctionHash, uint256 _auctionIndex) external view returns(BidInfo[] memory) {
        return bids[_auctionHash][_auctionIndex];
    }
    
    // return all bids that are not high bids and not already withdrawn
    function returnBidsToWallets(bytes32 _auctionHash, uint256 _auctionIndex) external onlyOwner () {
        uint len = bids[_auctionHash][_auctionIndex].length;
        for (uint i = 0; i < len; i ++) {
            if ( 
                !bids[_auctionHash][_auctionIndex][i].hasWithdrawn &&
                (bids[_auctionHash][_auctionIndex][i].bidder != auctions[_auctionHash][_auctionIndex].highestBidder)
            ) {
                bids[_auctionHash][_auctionIndex][i].hasWithdrawn = true;
                // (bool sent, ) = bids[_auctionHash][i].bidder.call{value: bids[_auctionHash][i].value}("");
                // require(sent, "transfer failed");
                IERC20(MADTOKEN_ADDRESS).transfer(bids[_auctionHash][_auctionIndex][i].bidder, bids[_auctionHash][_auctionIndex][i].value);
            }
        }
    }

    function _transferNFT(address _nft, address _from, address _to, uint256 _nftId) private {
        // if (is721(_nft)) {
            IERC721(_nft).transferFrom(_from, _to, _nftId);
        // } else {
            // IERC1155(_nft).safeTransferFrom(_from, _to, _nftId, _quantity, "");
        // }
    }

    function accept(bytes32 _auctionHash, uint256 _auctionIndex) external 
        onlyAuctionStarted(_auctionHash, _auctionIndex) 
    {
        Auction memory auction = auctions[_auctionHash][_auctionIndex];
        require(!auction.ended, "ended");

        if (auction.highestBidder != address(0)) {
            auctions[_auctionHash][_auctionIndex].ended = true;
            _transferNFT(auction.nft, address(this), auction.highestBidder, auction.nftId);
            IERC20(MADTOKEN_ADDRESS).transfer(auction.seller, auction.highestBid);

            emit Accept(_auctionHash, _auctionIndex, auction.highestBidder, auction.highestBid);
        }
    }

    function cancel(bytes32 _auctionHash, uint256 _auctionIndex) external 
        onlyAuctionOwner(_auctionHash, _auctionIndex)
        onlyAuctionStarted(_auctionHash, _auctionIndex)
    {
        Auction memory auction = auctions[_auctionHash][_auctionIndex];
        require(!auction.ended, "ended");

        auction.isValue = false;
        auction.ended = true;
        auctions[_auctionHash][_auctionIndex] = auction;
        _transferNFT(auction.nft, address(this), auction.seller, auction.nftId);

        emit Cancel(_auctionHash, _auctionIndex, msg.sender);
    }

