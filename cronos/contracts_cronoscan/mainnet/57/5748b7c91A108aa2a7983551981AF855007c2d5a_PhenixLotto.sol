pragma solidity ^0.8.0;

contract WitnessRandomness {
    uint256 public globalNonce;

    constructor() {
        globalNonce = 0;
    }

    function random(
        uint32 _range,
        uint256 _nonce,
        uint256 _block
    ) external view returns (uint32) {
        return
            uint32(
                uint256(
                    keccak256(
                        abi.encodePacked(
                            block.number,
                            block.timestamp,
                            block.difficulty,
                            _block,
                            _nonce,
                            globalNonce
                        )
                    )
                )
            ) % _range;
    }
}

