pragma solidity ^0.8.4;

interface IFatherAuction {
    struct Auction {
        uint256 fatherId;
        uint256 amount;
        uint256 startTime;
        uint256 endTime;
        address payable bidder;
        bool settled;
    }
    event AuctionCreated(uint256 indexed fatherId, uint256 startTime, uint256 endTime);
    event AuctionBid(uint256 indexed fatherId, address sender, uint256 value, bool extended);
    event AuctionExtended(uint256 indexed fatherId, uint256 endTime);
    event AuctionSettled(uint256 indexed fatherId, address winner, uint256 amount);
    event AuctionEnded(uint256 indexed fatherId, uint256 startTime, uint256 endTime);
    event PausedDate(uint256 blocktime);
    event UnpausedDate(uint256 blocktime);

    function settleAuction() external;
    function settleCurrentAndCreateNewAuction() external;
    function bid(uint256 fatherId) external payable;
    function pause() external;
    function unpause() external;
}

