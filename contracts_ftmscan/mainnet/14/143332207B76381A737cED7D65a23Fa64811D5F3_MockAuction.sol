pragma solidity ^0.8.4;

import "../SevenDeadlySins.sol";
import "../Interfaces/IFatherAuction.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "hardhat/console.sol"; 

contract MockAuction is IFatherAuction, Ownable, Pausable, ReentrancyGuard, ERC1155Holder {

    SevenDeadlySins private sins;
    IFatherAuction.Auction public auction;

    address private merkleContract = 0xC370b50eC6101781ed1f1690A00BF91cd27D77c4;

    uint256 private immutable timeBuffer = 300;
    uint256 private immutable reservePrice = 0.2 ether;
    uint256 private immutable minBidIncrementPercentage = 5;
    uint256 private immutable duration = 3600;

    constructor(address _sins) {
        sins = SevenDeadlySins(_sins);
        _pause();
        console.log("Contract -> Deployed and paused");

    }

    modifier fatherMint {
        require(auction.fatherId < 666,"minting ended");
        _;
    }

    function setMerkleContract(address _merkleAddress) external onlyOwner {
        merkleContract = _merkleAddress;
    }

    function settleCurrentAndCreateNewAuction() external override nonReentrant whenNotPaused {
        _settleAuction();
        _createAuction();
    }

    function settleAuction() external override whenPaused nonReentrant {
        _settleAuction();
    }

    function endAuction() external whenNotPaused {
        require(auction.fatherId == 666);
        require(auction.endTime < block.timestamp);
        
        _settleAuction();
        
        emit AuctionEnded(auction.fatherId, auction.startTime, auction.endTime );
    }

    function pause() external override onlyOwner {
        super._pause();
        emit PausedDate(block.timestamp);
    }

    function unpause() external override onlyOwner {
        super._unpause();
        emit UnpausedDate(block.timestamp);

        if (auction.startTime == 0 || auction.settled) {
            _createAuction();
        }
    }

    function _createAuction() internal fatherMint {
        uint256 nextFatherId;
        if (auction.fatherId == 0) {
            nextFatherId = 1;
        } else {
            nextFatherId = auction.fatherId + 1;
        }

        try sins.mint(address(this), nextFatherId, 1) {
            uint256 startTime = block.timestamp;
            uint256 endTime = startTime + duration;
            auction = Auction({
                fatherId: nextFatherId,
                amount: reservePrice,
                startTime: startTime,
                endTime: endTime,
                bidder: payable(0),
                settled: false
            });
        emit AuctionCreated(nextFatherId, startTime, endTime);
        } catch Error(string memory result) {
            _pause();
            console.log("Try failed: ", result);
        }
    }

    function _settleAuction() internal {
        IFatherAuction.Auction memory _auction = auction;

        require(_auction.startTime != 0, "Auction hasn't begun");
        require(!_auction.settled, 'Auction has already been settled');
        require(block.timestamp >= _auction.endTime, "Auction hasn't completed");

        auction.settled = true;

        if (_auction.bidder == address(0)) {
            sins.safeTransferFrom(address(this), merkleContract, _auction.fatherId, 1, "");
        } else {
            sins.safeTransferFrom(address(this), _auction.bidder, _auction.fatherId,1,"");
        }

        if (_auction.amount > 0) {
            _safeTransferETH(owner(), _auction.amount);
        }

        emit AuctionSettled(_auction.fatherId, _auction.bidder, _auction.amount);
    }

    function bid(uint256 fatherId) external payable override nonReentrant {
        IFatherAuction.Auction memory _auction = auction; 

        require(_auction.fatherId == fatherId, "Father not up for auction");
        require(block.timestamp < _auction.endTime, "Auction has ended");
        require(msg.value >= reservePrice, "Must send at least reservePrice");
        require(
            msg.value >= _auction.amount + ((_auction.amount * minBidIncrementPercentage) / 100),
            'Need more to bid.'
        );

        address payable lastBidder = _auction.bidder;
        if (lastBidder != address(0)) {
            (bool sent, ) = payable(lastBidder).call{value: _auction.amount}("");
        }

        auction.amount = msg.value;
        auction.bidder = payable(msg.sender);

        bool extended = _auction.endTime - block.timestamp < timeBuffer;
        if (extended) {
            auction.endTime = _auction.endTime = block.timestamp + timeBuffer;
        }
        emit AuctionBid(_auction.fatherId, msg.sender, msg.value, extended);

        if (extended) {
            emit AuctionExtended(_auction.fatherId, _auction.endTime);
        }
    } 

    function _safeTransferETH(address to, uint256 value) internal returns (bool) {
        (bool success, ) = to.call{ value: value, gas: 30_000 }(new bytes(0));
        return success;
    }
}

