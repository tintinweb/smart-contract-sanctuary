// File: contracts/HoneyToken.sol
pragma solidity ^0.8.0;

/// @title Honey token smart contract
/// @author Aric Kuter
contract HoneyToken is ERC777 {
    /// @notice equals 20 token per day
    uint256 public immutable DEGENZ_PER_SECOND_PER_DEGEN = 231481 gwei;
    /// @notice To be updated with Genesis deploy time
    uint256 public immutable GENESIS_TIME = 1642177981;
    IERC721Enumerable public immutable GENESIS;

    mapping(uint256 => uint256) public last;

    constructor(address _genesisAddr, address[] memory defaultOperators)
        ERC777("Honey", "HONEY", defaultOperators)
    {
        GENESIS = IERC721Enumerable(_genesisAddr);
    }

    function claimable(address user) public view returns (uint256) {
        uint256 total = GENESIS.balanceOf(user);
        uint256 owed = 0;
        for (uint256 i = 0; i < total; i++) {
            uint256 id = GENESIS.tokenOfOwnerByIndex(user, i);
            uint256 claimed = lastClaim(id);
            owed += ((block.timestamp - claimed) * DEGENZ_PER_SECOND_PER_DEGEN);
        }
        return owed;
    }

    function mintForUser(address user) external {
        uint256 total = GENESIS.balanceOf(user);
        uint256 owed = 0;
        for (uint256 i = 0; i < total; i++) {
            uint256 id = GENESIS.tokenOfOwnerByIndex(user, i);
            uint256 claimed = lastClaim(id);
            owed += ((block.timestamp - claimed) * DEGENZ_PER_SECOND_PER_DEGEN);
            last[id] = block.timestamp;
        }
        _mint(user, owed, "", "");
    }

    function mintForIds(uint256[] calldata ids) external {
        for (uint256 i = 0; i < ids.length; i++) {
            uint256 id = ids[i];
            address owner = GENESIS.ownerOf(id);
            uint256 claimed = lastClaim(id);
            uint256 owed = (block.timestamp - claimed) *
                DEGENZ_PER_SECOND_PER_DEGEN;
            _mint(owner, owed, "", "");
            last[id] = block.timestamp;
        }
    }

    function lastClaim(uint256 id) public view returns (uint256) {
        return max(last[id], GENESIS_TIME);
    }

    function max(uint256 a, uint256 b) internal pure returns (uint256) {
        return a >= b ? a : b;
    }
}