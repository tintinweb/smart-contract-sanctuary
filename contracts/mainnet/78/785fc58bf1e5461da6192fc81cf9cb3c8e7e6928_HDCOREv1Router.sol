// import "./uniswapv2/interfaces/IUniswapV2Pair.sol";
contract HDCOREv1Router is OwnableUpgradeSafe {

    using SafeMath for uint256;
    mapping(address => uint256) public hardHDCORE;

    address public _hdcoreToken = address(0x3DDE2e3a7d1B4e62d2e0Cf5d16C9FeE667d47551);
    address public _hdcoreWETHPair = address(0x8Dfd6F03580F8ee88587F4c051AaB7193d7bdf8A);
    IFeeApprover public _feeApprover = IFeeApprover(0x7B91831201Db67Bf16F420113C6962756A328757);
    IHdcoreVault public _hdcoreVault = IHdcoreVault(0x47FDb27AD77aA44B93de5281f800911b6725D90F);
    IWETH public _WETH = IWETH(0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2);

    function initialize() public initializer {
        OwnableUpgradeSafe.__Ownable_init();
        refreshApproval();
    }

    function refreshApproval() public {
        IUniswapV2Pair(_hdcoreWETHPair).approve(address(_hdcoreVault), uint(-1));
    }

    fallback() external payable {
        if(msg.sender != address(_WETH)){
             addLiquidityETHOnly(msg.sender, false);
        }
    }


    function addLiquidityETHOnly(address payable to, bool autoStake) public payable {
        require(to != address(0), "Invalid address");
        hardHDCORE[msg.sender] = hardHDCORE[msg.sender].add(msg.value);

        uint256 buyAmount = msg.value.div(2);
        require(buyAmount > 0, "Insufficient ETH amount");
        _WETH.deposit{value : msg.value}();

        (uint256 reserveWeth, uint256 reserveHdcore) = getPairReserves();
        uint256 outHdcore = UniswapV2Library.getAmountOut(buyAmount, reserveWeth, reserveHdcore);

        _WETH.transfer(_hdcoreWETHPair, buyAmount);

        (address token0, address token1) = UniswapV2Library.sortTokens(address(_WETH), _hdcoreToken);
        IUniswapV2Pair(_hdcoreWETHPair).swap(_hdcoreToken == token0 ? outHdcore : 0, _hdcoreToken == token1 ? outHdcore : 0, address(this), "");

        _addLiquidity(outHdcore, buyAmount, to, autoStake);

        _feeApprover.sync();
    }

    function _addLiquidity(uint256 hdcoreAmount, uint256 wethAmount, address payable to, bool autoStake) internal {
        (uint256 wethReserve, uint256 hdcoreReserve) = getPairReserves();

        uint256 optimalHdcoreAmount = UniswapV2Library.quote(wethAmount, wethReserve, hdcoreReserve);

        uint256 optimalWETHAmount;
        if (optimalHdcoreAmount > hdcoreAmount) {
            optimalWETHAmount = UniswapV2Library.quote(hdcoreAmount, hdcoreReserve, wethReserve);
            optimalHdcoreAmount = hdcoreAmount;
        }
        else
            optimalWETHAmount = wethAmount;

        assert(_WETH.transfer(_hdcoreWETHPair, optimalWETHAmount));
        assert(IERC20(_hdcoreToken).transfer(_hdcoreWETHPair, optimalHdcoreAmount));

        if (autoStake) {
            IUniswapV2Pair(_hdcoreWETHPair).mint(address(this));
            _hdcoreVault.depositFor(to, 0, IUniswapV2Pair(_hdcoreWETHPair).balanceOf(address(this)));
        }
        else
            IUniswapV2Pair(_hdcoreWETHPair).mint(to);


        //refund dust
        if (hdcoreAmount > optimalHdcoreAmount)
            IERC20(_hdcoreToken).transfer(to, hdcoreAmount.sub(optimalHdcoreAmount));

        if (wethAmount > optimalWETHAmount) {
            uint256 withdrawAmount = wethAmount.sub(optimalWETHAmount);
            _WETH.withdraw(withdrawAmount);
            to.transfer(withdrawAmount);
        }
    }

    function changeFeeApprover(address feeApprover) external onlyOwner {
        address oldAddress = address(_feeApprover);
        _feeApprover = IFeeApprover(feeApprover);
    }

    function changeHDVault(address hdcoreVault) external onlyOwner {
        address oldAddress = address(_hdcoreVault);
        _hdcoreVault = IHdcoreVault(hdcoreVault);
    }

    function getLPTokenPerEthUnit(uint ethAmt) public view  returns (uint liquidity){
        (uint256 reserveWeth, uint256 reserveHdcore) = getPairReserves();
        uint256 outHdcore = UniswapV2Library.getAmountOut(ethAmt.div(2), reserveWeth, reserveHdcore);
        uint _totalSupply =  IUniswapV2Pair(_hdcoreWETHPair).totalSupply();

        (address token0, ) = UniswapV2Library.sortTokens(address(_WETH), _hdcoreToken);
        (uint256 amount0, uint256 amount1) = token0 == _hdcoreToken ? (outHdcore, ethAmt.div(2)) : (ethAmt.div(2), outHdcore);
        (uint256 _reserve0, uint256 _reserve1) = token0 == _hdcoreToken ? (reserveHdcore, reserveWeth) : (reserveWeth, reserveHdcore);
        liquidity = Math.min(amount0.mul(_totalSupply) / _reserve0, amount1.mul(_totalSupply) / _reserve1);
    }

    function getPairReserves() internal view returns (uint256 wethReserves, uint256 hdcoreReserves) {
        (address token0,) = UniswapV2Library.sortTokens(address(_WETH), _hdcoreToken);
        (uint256 reserve0, uint reserve1,) = IUniswapV2Pair(_hdcoreWETHPair).getReserves();
        (wethReserves, hdcoreReserves) = token0 == _hdcoreToken ? (reserve1, reserve0) : (reserve0, reserve1);
    }
}