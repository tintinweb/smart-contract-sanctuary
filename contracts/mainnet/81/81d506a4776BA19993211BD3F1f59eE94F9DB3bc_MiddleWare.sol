// File: contracts/MiddleWare.sol

pragma solidity 0.5.17;
pragma experimental ABIEncoderV2;


contract MiddleWare{

    struct UserInfo{
        uint borrowBalance; //  borrow balance
        uint gtokenBalance; //  gtoken balance = accountBalance
        uint gtokenInUnderlyingBalance;  //  gtokenBalance convert to underlying balance
        uint underlyingBalance;    //  underlying balance
    }

    struct GtokenInfo{
        address gtokenAddress;
        address underlyingAddress;
        bool underlyingIsNativeCoin;
        string gtokenName;
        uint totalSupply;
        uint supplyRate;
        uint totalBorrows;
        uint borrowRate;
        uint exchangeRate;
        uint cash;
        uint collateralFactor;
        uint totalReserves;
        uint reserveFactor;
        uint price;
        UserInfo userInfo;
    }

    function getGtokenInfoList(address comptrollerAddress, address userAddress) public returns(GtokenInfo[] memory, GToken[] memory){
        Comptroller comptroller = Comptroller(comptrollerAddress);
        PriceOracle priceOracle = comptroller.oracle();

        GToken[] memory getAllMarkets = comptroller.getAllMarkets();
        GtokenInfo[] memory gtokenInfo = new GtokenInfo[](getAllMarkets.length);  
        for(uint i=0; i<getAllMarkets.length; i++){
            GToken gToken = getAllMarkets[i];
            
            UserInfo memory userInfo = getUserInfo(gToken, userAddress);
            uint price_ = priceOracle.getUnderlyingPrice(gToken);
            (bool isListed, uint collateralFactorMantissa, bool isComped) = comptroller.markets(address(gToken));
            uint collateralFactor_ = collateralFactorMantissa;

            GtokenInfo memory gtokenInfo_ = getGTokenInfo(gToken);
            gtokenInfo_.collateralFactor = collateralFactor_;
            gtokenInfo_.price = price_;
            gtokenInfo_.underlyingIsNativeCoin = gToken.underlyingIsNativeCoin();
            gtokenInfo_.underlyingAddress = gToken.underlying();

            gtokenInfo_.userInfo = userInfo;

            gtokenInfo[i] = gtokenInfo_;
        }
        GToken[] memory addrArray;
        if(userAddress != address(0)){
            addrArray = comptroller.getAssetsIn(userAddress);
        }
        return (gtokenInfo, addrArray);
    }

    function getGTokenInfo(GToken gToken) public returns(GtokenInfo memory){
        string memory gtokenName = gToken.name();
        uint totalSupply = gToken.totalSupply();
        uint supplyRate = gToken.supplyRatePerBlock();
        uint totalBorrows = gToken.totalBorrowsCurrent();
        uint borrowRate = gToken.borrowRatePerBlock();
        uint exchangeRate = gToken.exchangeRateCurrent();
        uint cash = gToken.getCash();
        uint totalReserves = gToken.totalReserves();
        uint reserveFactor = gToken.reserveFactorMantissa();
        
        GtokenInfo memory gtokenInfo;

        gtokenInfo.gtokenAddress = address(gToken);
        gtokenInfo.gtokenName = gtokenName;
        gtokenInfo.totalSupply = totalSupply;
        gtokenInfo.supplyRate = supplyRate;
        gtokenInfo.totalBorrows = totalBorrows;
        gtokenInfo.borrowRate = borrowRate;
        gtokenInfo.exchangeRate = exchangeRate;
        gtokenInfo.cash = cash;
        gtokenInfo.totalReserves = totalReserves;
        gtokenInfo.reserveFactor = reserveFactor;

        return gtokenInfo;
    }

    function getUserInfo(GToken gToken, address userAddress) public returns(UserInfo memory){
        UserInfo memory userInfo;
        if(userAddress != address(0)){
            uint borrowBalance = gToken.borrowBalanceCurrent(userAddress);
            uint underlyingBalance = gToken.balanceOfUnderlying(userAddress);
            uint accountBalance = gToken.balanceOf(userAddress);
    
            userInfo.borrowBalance = borrowBalance;
            userInfo.gtokenInUnderlyingBalance = underlyingBalance;
            userInfo.gtokenBalance = accountBalance;
            if(gToken.underlyingIsNativeCoin()){
                userInfo.underlyingBalance = userAddress.balance;
            }else{
                userInfo.underlyingBalance = EIP20Interface(gToken.underlying()).balanceOf(userAddress);
            }
        }
        return userInfo;
    }

    function getBalanceList(address walletAddress, address[] memory erc20AddressArray) public view returns(uint[] memory){
        uint[] memory results = new uint[](erc20AddressArray.length); 
        for(uint i=0; i<erc20AddressArray.length; i++){
            if(erc20AddressArray[i] == address(0)){
                results[i] = walletAddress.balance;
            }else{
                EIP20Interface eIP20Interface = EIP20Interface(erc20AddressArray[i]);
                results[i] = eIP20Interface.balanceOf(walletAddress);
            }
        }
        return results;
    }

    /**
     * uniswapPairAddrArray = gtoken_array.uniswap_market
     */
    function getOracleAndMarketPrice(address uniswapPriceOracleAddr, string[] memory underlyingNameArray) public view returns(uint[] memory marketPrice, uint[] memory oraclePrice){
        require(uniswapPriceOracleAddr != address(0), "address cannot be 0x0");
        PriceOracle priceOracle = PriceOracle(uniswapPriceOracleAddr);
        marketPrice = new uint[](underlyingNameArray.length);
        oraclePrice = new uint[](underlyingNameArray.length);
        for(uint i=0; i<underlyingNameArray.length; i++){
            marketPrice[i] = priceOracle.getMarketPrice(underlyingNameArray[i]);
            oraclePrice[i] = priceOracle.price(underlyingNameArray[i]);
        }
        return (marketPrice, oraclePrice);
    }
}