tc).approve(ren_pool, liquidity[1]);\n        ICurveFi_2(ren_pool).add_liquidity(liquidity, 0);\n    }\n\n    // **** Tests **** //\n\n    function test_rencrv_v1_withdraw() public {\n        _test_withdraw();\n    }\n\n    function test_rencrv_v1_earn_harvest_rewards() public {\n        _test_get_earn_harvest_rewards();\n    }\n}\n"
