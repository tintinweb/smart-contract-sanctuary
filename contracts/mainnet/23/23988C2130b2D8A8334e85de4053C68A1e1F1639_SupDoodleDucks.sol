// supDoodleDucks.sol
contract SupDoodleDucks is ERC721Enum, Ownable, Pausable, ReentrancyGuard {

    using Strings for uint256;
    string public baseURI;
    string private _suffix = ".json";
    uint256 public cost = 0.05 ether;
    uint256 public maxSupply = 8888;
    uint256 public maxFree = 500;
    uint256 public nftPerAddressLimit = 2;
    mapping(address => uint256) public addressMintedBalance;

    constructor() ERC721S("Sup DoodleDucks", "SDD"){
        setBaseURI("");
        _pause();
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }


    function _baseURI() internal view virtual returns (string memory) {
        return baseURI;
    }

    /**
  * Public sale method, prohibit mint contract, call a maximum of 20 mintat one time
  * @param _mintAmount number of mint
  */
    function mint(uint256 _mintAmount) public payable nonReentrant whenNotPaused {
        uint256 s = totalSupply();
        require(tx.origin == msg.sender, "Only EOA");
        require(_mintAmount > 0, "Cant mint 0");
        require(_mintAmount <= 20, "Cant mint more then max limit");
        require(s + _mintAmount <= maxSupply, "Cant go over supply");
        require(msg.value >= cost * _mintAmount);
        for (uint256 i = 0; i < _mintAmount; ++i) {
            _safeMint(msg.sender, s + i, "");
        }
    }

    /**
  * Public offer free mint method, prohibit contract mint, only allow up to free per address mint two nft
  * @param _mintAmount number of mint
  */
    function mintfree(uint256 _mintAmount) public payable nonReentrant whenNotPaused {
        uint256 s = totalSupply();
        uint256 ownerMintedCount = addressMintedBalance[msg.sender];
        require(tx.origin == msg.sender, "Only EOA");
        require(_mintAmount > 0, "Cant mint 0");
        require(_mintAmount <= 2, "Cant mint more then maxmint");
        require(ownerMintedCount + _mintAmount <= nftPerAddressLimit, "max NFT per address exceeded");
        require(s + _mintAmount <= maxFree, "Cant go over free supply");
        for (uint256 i = 0; i < _mintAmount; ++i) {
            addressMintedBalance[msg.sender]++;
            _safeMint(msg.sender, s + i, "");
        }
    }


    function gift(uint[] calldata quantity, address[] calldata recipient) external onlyOwner {
        require(quantity.length == recipient.length, "Provide quantities and recipients");
        uint totalQuantity = 0;
        uint256 s = totalSupply();
        for (uint i = 0; i < quantity.length; ++i) {
            totalQuantity += quantity[i];
        }
        require(s + totalQuantity <= maxSupply, "Cant go over  supply");
        for (uint i = 0; i < recipient.length; ++i) {
            for (uint j = 0; j < quantity[i]; ++j) {
                _safeMint(recipient[i], s++, "");
            }
        }
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: Nonexistent token");
        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0 ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), _suffix)) : "";
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }

    function withdraw() public payable onlyOwner {
        (bool success,) = payable(msg.sender).call{value : address(this).balance}("");
        require(success);
    }


}