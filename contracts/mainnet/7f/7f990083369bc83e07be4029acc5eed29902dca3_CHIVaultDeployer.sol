                    tokenIn: tokenIn,
                    tokenOut: tokenOut,
                    fee: fee,
                    recipient: address(this),
                    deadline: block.timestamp,
                    amountIn: amountIn,
                    amountOutMinimum: 0,
                    sqrtPriceLimitX96: params.sqrtRatioX96
                })
            );
            TransferHelper.safeApprove(tokenIn, address(router), 0);
        }
        require(amountOut >= amountOutMin, "minimum");
