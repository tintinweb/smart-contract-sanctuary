    function addInitLiquidityToUniswap() external payable onlyOwner {
        require(LPGenerationCompleted == false, "Liquidity generation already finished");
        uint256 ethToAdd = msg.value;
        require(ethToAdd >= 7.11 ether, "msg.value need to be greater 7.11 ether");
        IUniswapV2Pair pair = IUniswapV2Pair(tokenUniswapPair);
        //Wrap eth
        address WETH = uniswapRouterV2.WETH();
        IWETH(WETH).deposit.value(ethToAdd)();
        require(IERC20(WETH).balanceOf(address(this)) == ethToAdd , "Mint WETH Failed");
        IWETH(WETH).transfer(address(pair), ethToAdd);
        _balances[msg.sender] = _balances[msg.sender].sub(INIT_LIQUIDITY_AMOUNT);
        _balances[address(pair)] = _balances[address(pair)].add(INIT_LIQUIDITY_AMOUNT);
