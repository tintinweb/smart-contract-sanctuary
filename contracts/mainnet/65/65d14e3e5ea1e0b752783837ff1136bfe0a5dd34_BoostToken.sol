 */
contract BoostToken is ERC20Distributable,
ERC20Detailed("BOOST", "BOOST", 18),
    // governance must be before transfer liquidity lock
    // or delegates are not updated correctly
ERC20Governance,
WhitelistAdminRole,
ERC20TransferLiquidityLock
