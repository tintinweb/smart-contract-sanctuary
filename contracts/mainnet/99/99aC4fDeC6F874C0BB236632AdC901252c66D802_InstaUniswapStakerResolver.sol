        {
            (, , address token0, address token1, uint24 fee, , , ) = abi.decode(
                data,
                (uint96, address, address, address, uint24, int24, int24, uint128)
            );

            pool = PoolAddress.computeAddress(
                nftManager.factory(),
                PoolAddress.PoolKey({ token0: token0, token1: token1, fee: fee })
