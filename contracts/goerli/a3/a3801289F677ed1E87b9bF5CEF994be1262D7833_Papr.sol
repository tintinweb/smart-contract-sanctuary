/**
 * This contract is the one of &PAPR ERC20 token, adapted to send fees when his price is uder the paprTransferFeesExclusiveCeiling variable.
 * by PaprPrintr
 *
 * _____                     _____        _         _         
 *|  __ \                   |  __ \      (_)       | |        
 *| |__) |__ _  _ __   _ __ | |__) |_ __  _  _ __  | |_  _ __ 
 *|  ___// _  ||  _ \ |  __||  ___/|  __|| ||  _ \ | __||  __|
 *| |   | (_| || |_) || |   | |    | |   | || | | || |_ | |   
 *|_|    \__,_|| .__/ |_|   |_|    |_|   |_||_| |_| \__||_|   
 *             |_|                                         
 */

import '@openzeppelin/contracts/token/ERC20/ERC20Burnable.sol';
import './owner/DoubleOperator.sol';
import './interfaces/IOracle.sol';

/**
 * @title PAPR base contract
 * @notice ..
 * @author PaprPrintr's team
 */

contract Papr is ERC20Burnable {

    /* ========== STATE VARIABLES ========== */

    // fees
    uint256 public paprTransferFeesExclusiveCeiling;
    uint256 public feesInDepletionPeriod;
    address public _primaryOperator;
    address public _secondaryOperator;

    // oracle
    address public paprOracle = 0x151d82d4bd64f9674F2C34350823117145Caf31c;
    address public papr = 0x3D522f18B1707F6f5b9cfa5528104fE8F1cA4F3b;

    // list of address which take fees
    mapping(address => bool) public addressesWithFees;

    /* ========== VIEW FUNCTIONS ========== */

    // flags
    function isAddressesWithFees(address adress) public view returns(bool isIndeed) {
        return addressesWithFees[adress];
    }



    /* ========== CONSTRUCTOR ========== */

    constructor() public ERC20('PAPR', 'PAPR') {
        // Mints 1 Basis Papr to contract creator for initial Uniswap oracle deployment.
        // Will be burned after oracle deployment
        _mint(msg.sender, 300000 * 10**18);
        _primaryOperator = msg.sender;
        feesInDepletionPeriod = 10; // %
        paprTransferFeesExclusiveCeiling = 10**18;
    }

    /* ========== GOVERNANCE ========== */

    modifier onlyDoubleOperator() {
        require(
            (_primaryOperator == msg.sender || _secondaryOperator == msg.sender),
            'operator: caller is not the operator'
        );
        _;
    }

    function setSecondaryOperator(address newOperator_) external onlyDoubleOperator {
        require(
            newOperator_ != address(0),
            'operator: zero address given for new operator'
        );
    }

    function addAddress(address newAddress) external onlyDoubleOperator returns(bool success) {
        require(!isAddressesWithFees(newAddress));
        addressesWithFees[newAddress] = true;
        return true;
    }

    function RemoveAddress(address oldAddress) external onlyDoubleOperator returns(bool success) {
        require(isAddressesWithFees(oldAddress));
        addressesWithFees[oldAddress] = false;
        return true;
    }

    function setFeesInDepletionPeriod(uint256 newFees) external onlyDoubleOperator {
        feesInDepletionPeriod = newFees;
    }

    /* ========== MAIN FUNCTIONS ========== */

    function mint(address recipient_, uint256 amount_) public onlyDoubleOperator returns (bool) {
        uint256 balanceBefore = balanceOf(recipient_);
        _mint(recipient_, amount_);
        uint256 balanceAfter = balanceOf(recipient_);

        return balanceAfter > balanceBefore;
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        uint256 paprPrice = getPaprPrice();
        bool checkAddress = isAddressesWithFees(recipient);
        
        // If recipient address requires fees (we are sending PAPR to an exchange, ie. selling our PAPR) :
      
        if (paprPrice < paprTransferFeesExclusiveCeiling && checkAddress == true) {
            _transferWithFees(_msgSender(), recipient, amount);
        } else {
            _transfer(_msgSender(), recipient, amount);
        }

        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        uint256 paprPrice = getPaprPrice();
        bool checkAddress = isAddressesWithFees(recipient);
        
        if (paprPrice < paprTransferFeesExclusiveCeiling && checkAddress == true) {
            _transferWithFees(sender, recipient, amount);
        } else {
            _transfer(sender, recipient, amount);
        }
        _approve(sender, _msgSender(), allowance(sender, _msgSender()).sub(amount, "ERC20: transfer amount exceeds allowance"));
        return true;
    }    

    function _transferWithFees(address sender, address recipient, uint256 amount) internal {
        uint256 feesAmount = amount.mul(feesInDepletionPeriod).div(100);
        uint256 amountAfterFees = amount.sub(feesAmount);

        burnFrom(sender, feesAmount);
        _transfer(sender, recipient, amountAfterFees);
    }

    /* ========== BURN FUNCTIONS ========== */

    // From ERC20Burnable :

    function burn(uint256 amount) public override (ERC20Burnable) {
        super.burn(amount);
    }

    function burnFrom(address account, uint256 amount) public override (ERC20Burnable) {
       super.burnFrom(account, amount);
    }

    /* ========== ORACLE FUNCTIONS ========== */

    function getpaprOraclePrice() public view returns (uint256) {
        return getPaprPrice();
    }

    function getPaprTransferFeeExclusiveCeiling() external view returns (uint256) {
        return paprTransferFeesExclusiveCeiling;
    }

    function setPaprTransferFeeExclusiveCeiling(uint256 newFee) external onlyDoubleOperator {
        paprTransferFeesExclusiveCeiling = newFee;
    }

    function setpaprOracleAddress(address newOracle) external onlyDoubleOperator {
        paprOracle = newOracle;
    }
    function setpaprAddress(address newPaprAddress) external onlyDoubleOperator {
        papr = newPaprAddress;
    }
    

    function getPaprPrice() public view returns (uint256 paprPrice) {
        try IOracle(paprOracle).consult(papr, 1e18) returns (uint256 price) {
            return price;
        } catch {
            revert("Failed to consult PAPR price from the oracle");
        }
    }



    function _updatePaprPrice() internal {
        try IOracle(paprOracle).update() {} catch {}
    }    
}

pragma solidity ^0.6.0;

interface IOracle {
    function update() external;

    function consult(address token, uint256 amountIn)
        external
        view
        returns (uint256 amountOut);
    // function getReserves() external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestamp);
}

