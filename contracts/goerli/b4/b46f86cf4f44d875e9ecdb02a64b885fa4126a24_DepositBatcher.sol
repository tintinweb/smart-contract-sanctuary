pragma solidity ^0.8.7;

import {Batch} from "../type/Batch.sol";

library DepositBatcherLib {
    function insert(
        Batch storage self,
        address user,
        uint256 amount,
        bytes memory detf
    ) internal returns (bool) {
        if (self.detfs.length == 0) {
            self.detfs.push(detf);
            self.total[detf] += amount;
            self.user[detf][user] += amount;
            self.created[detf] = true;
            self.users[detf].push(user);
        } else {
            if (self.created[detf]) {
                self.total[detf] += amount;
                if (self.user[detf][user] == 0) {
                    self.user[detf][user] += amount;
                    self.users[detf].push(user);
                } else {
                    self.user[detf][user] += amount;
                }
            } else {
                self.detfs.push(detf);
                self.created[detf] = true;
                self.total[detf] += amount;
                self.user[detf][user] += amount;
                self.users[detf].push(user);
            }
        }
        return true;
    }
}

