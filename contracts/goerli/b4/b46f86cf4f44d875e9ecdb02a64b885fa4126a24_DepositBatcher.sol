import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
// import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./interfaces/factory/IL2Factory.sol";
import {DepositData, MintedData} from "./type/Tunnel.sol";
import {FxBaseChildTunnel} from "./tunnel/FxBaseChildTunnel.sol";
import {Batch, BatchStatus} from "./type/Batch.sol";
import {DepositBatcherLib} from "./lib/DepositBatcherLib.sol";

interface IERC20L2 is IERC20 {
    function withdraw(uint256 amount) external;
}

contract DepositBatcher is FxBaseChildTunnel, Context {
    using DepositBatcherLib for Batch;
    // L2 factory address
    address public factory;

    // Instance of USDC Smart Contract Address
    IERC20L2 private usdc;

    // total batch count. Used as batch identifier.
    uint256 private totalBatch;

    // Mapping batch Id to batch structure
    mapping(uint256 => Batch) private batchInfo;

    // Mapping batch Id & detfName to minted info
    mapping(uint256 => mapping(bytes => MintedData)) private mintedInfo;

    // Proxy Deployment
    function initialize(address _fxChild) public virtual {
        FxBaseChildTunnel.initalize(_fxChild);
        totalBatch = 1;
    }

    // minting a detf
    function mintDetf(string memory _detfName, uint256 _usdcAmount)
        public
        virtual
        returns (bool)
    {
        // balance check
        require(
            usdc.balanceOf(_msgSender()) >= _usdcAmount,
            "batcher error: insufficient balance"
        );
        // allowance check
        require(
            usdc.allowance(_msgSender(), address(this)) >= _usdcAmount,
            "batcher error: insufficient allowance"
        );
        // transferring usdc from user wallet to contract
        usdc.transferFrom(_msgSender(), address(this), _usdcAmount);
        // Updating Batch Info
        bytes memory detfName = bytes(_detfName);
        return
            batchInfo[totalBatch].insert(_msgSender(), _usdcAmount, detfName);
    }

    // Sending a batch to L1
    function completeBatch() public virtual returns (bool) {
        usdc.withdraw(usdc.balanceOf(address(this)));

        Batch storage b = batchInfo[totalBatch];
        DepositData memory tunneldata;

        // creating a fixed size array to save gas cost.
        uint256[] memory amounts = new uint256[](b.detfs.length);

        for (uint256 i = 0; i < b.detfs.length; i++) {
            amounts[i] = b.total[b.detfs[i]];
        }

        // creating tunnel data.
        tunneldata.id = totalBatch;
        tunneldata.detfs = b.detfs;
        tunneldata.amounts = amounts;

        // sending data via state tunnel.
        sendMessageToRoot(abi.encode(tunneldata));
        // updation of state variables.
        b.status = BatchStatus.BATCHED;
        totalBatch += 1;
        return true;
    }

    // distribute a batch after completed.
    function distributeBatch(uint256 _batchId, string memory _detfName)
        public
        virtual
        returns (bool)
    {
        // casting
        bytes memory detfName = bytes(_detfName);

        // Distributing minted tokens on L1.
        Batch storage b = batchInfo[_batchId];

        // Minted Info
        MintedData storage m = mintedInfo[_batchId][detfName];

        // sanitary checks.
        require(
            b.status == BatchStatus.RECIEVED,
            "batcher error: invalid distribution status"
        );

        // distribution of rewards.
        for (uint256 i = 0; i < b.users[detfName].length; i++) {
            address user = b.users[detfName][i];
            uint256 userDeposit = b.user[detfName][user];

            uint256 percent = (userDeposit * 10**6) / b.total[detfName];
            for (uint256 j = 0; j < m.tokens.length; j++) {
                uint256 tokenAmount = (m.amounts[j] * percent) / 10**6;
                IERC20L2(m.tokens[j]).transfer(user, tokenAmount);
            }
        }

        return true;
    }

    // setting the l2 factory contract.
    function setFactory(address _factory) public virtual {
        factory = _factory;
    }

    // returns the current batchId
    function currentBatch() public view returns (uint256) {
        return totalBatch;
    }

    // returns the batch status
    function batchStatus(uint256 _batchId)
        public
        view
        virtual
        returns (BatchStatus)
    {
        return batchInfo[_batchId].status;
    }

    // returns the different types of detfs in a batch
    function detfsInBatch(uint256 _batchId)
        public
        view
        returns (bytes[] memory)
    {
        return batchInfo[_batchId].detfs;
    }

    // returns if a detf is present in a batch
    function isCreated(uint256 _batchId, string memory _detfName)
        public
        view
        returns (bool)
    {
        return batchInfo[_batchId].created[bytes(_detfName)];
    }

    // returns the user deposits per detf in a batch
    function userInfo(uint256 _batchId, string memory _detfName)
        public
        view
        returns (uint256)
    {
        return batchInfo[_batchId].user[bytes(_detfName)][_msgSender()];
    }

    // returns total deposits of a detfs in a batch
    function totalDeposits(uint256 _batchId, string memory _detfName)
        public
        view
        returns (uint256)
    {
        return batchInfo[_batchId].total[bytes(_detfName)];
    }

    // returns the all the users of a detf in a batch.
    function batchUsers(uint256 _batchId, string memory _detfName)
        public
        view
        returns (address[] memory)
    {
        return batchInfo[_batchId].users[bytes(_detfName)];
    }

    // State Tunnel Functions.
    function _processMessageFromRoot(
        uint256 _stateId,
        address _sender,
        bytes memory _data
    ) internal override validateSender(_sender) {
        // Logic to process message from root goes in here
        MintedData memory data = abi.decode(_data, (MintedData));
        mintedInfo[data.id][data.detfName] = data;
        batchInfo[data.id].status = BatchStatus.RECIEVED;
        distributeBatch(data.id, string(data.detfName));
    }

    function sendMessageToRoot(bytes memory _message) internal {
        _sendMessageToRoot(_message);
    }

    // Mock Tunnel
    function mockTunnelData(
        uint256 _id,
        string memory _detfName,
        address[] memory _tokens,
        uint256[] memory _amounts
    ) public virtual {
        MintedData memory tunneldata;
        tunneldata.id = _id;
        tunneldata.detfName = bytes(_detfName);
        tunneldata.tokens = _tokens;
        tunneldata.amounts = _amounts;

        _processMessageFromRoot(_id, address(this), abi.encode(tunneldata));
    }
}

