pragma solidity 0.8.6;

import "../external/DepositContract.sol";
import "./SmartStake.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

/**
    @title  Withdrawal Handler logic contract
    @author Dylan Kerler
    @notice This contract receives ETH from withdrawalCredentials address and forwards it
            to the owner of the NFT associated with those withdrawalCredentials
 */
contract WithdrawalHandler is Initializable {
    // solhint-disable var-name-mixedcase
    SmartStake public SMART_STAKE_CONTRACT;
    mapping(bytes => bool) public usedValidatorPubkeys;

    event ForwardEth(address withdrawalCredentials, address to, uint256 amount);

    function initialize(SmartStake smartStakeContract) public initializer {
        SMART_STAKE_CONTRACT = smartStakeContract;
    }

    /**
        @notice Forwards incoming ETH to it's associated owner.
        @dev    After receiving ETH it will look up what NFT the `msg.sender`
                is associated with in the `SMART_STAKE_CONTRACT` and then forward
                ETH to the owner of that NFT. Should only be called by `fallback()`.
     */
    function forwardEthToOwner() internal {
        address payable nftOwner = payable(
            SMART_STAKE_CONTRACT.withdrawalCredentialsToNftOwner(msg.sender)
        );

        nftOwner.transfer(msg.value);
        emit ForwardEth(msg.sender, nftOwner, msg.value);
    }

    fallback() external payable {
        forwardEthToOwner();
    }
}

