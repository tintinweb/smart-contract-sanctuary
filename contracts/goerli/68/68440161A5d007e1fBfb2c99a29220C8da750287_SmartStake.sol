pragma solidity 0.8.6;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

/**
    @title  Manages the NFT's for Topol
    @author Dylan Kerler
    @notice This contract contains all of the NFT related
            functionality for Topol.
 */
contract NFT is ERC721Upgradeable, OwnableUpgradeable {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    string public baseURI;
    mapping(uint256 => bytes) public tokenIdToValidatorPubkey;

    function initialize(
        string memory name_,
        string memory symbol_,
        string memory baseURI_
    ) internal initializer {
        __Ownable_init();
        __ERC721_init(name_, symbol_);
        setBaseURI(baseURI_);
    }

    /**
        @notice Updates the `baseURI`
        @param  newBaseURI The new base URI to use
     */
    function setBaseURI(string memory newBaseURI) public onlyOwner {
        baseURI = newBaseURI;
    }

    function ownerOf(uint256 tokenId) public view override returns (address) {
        return super.ownerOf(tokenId);
    }

    /**
        @notice Mints a new NFT and assigns it an associated validator public key.
        
        @param  to Who the newly minted NFT should belong to.
        @return The id of the NFT that was just minted.
     */
    function mint(address to) internal returns (uint256) {
        _tokenIds.increment();

        uint256 newTokenId = _tokenIds.current();
        _mint(to, newTokenId);

        return newTokenId;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }
}

