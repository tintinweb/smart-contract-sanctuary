pragma solidity 0.8.6;

import "../external/DepositContract.sol";
import "./NFT.sol";
import "./WithdrawalHandler.sol";
import "../proxy/ClonesCallForwarder.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "hardhat/console.sol";

/**
    @title  The hub for Topol contracts
    @author Dylan Kerler
    @notice This contract is the entrypoint for Topol and orchestrates
            all of the logic between the contracts.
 */
contract SmartStake is NFT {
    /* solhint-disable var-name-mixedcase */
    // These should not change after `initialize()` is called
    DepositContract public DEPOSIT_CONTRACT;
    WithdrawalHandler public WITHDRAWAL_HANDLER;
    /* solhint-disable var-name-mixedcase */

    bytes32 public validatorPubkeyMerkleRoot;
    mapping(bytes => uint256) public validatorPubKeyToTokenId;
    mapping(address => uint256) public withdrawalCredentialsToTokenId;

    uint256 public constant DEPOSIT_AMOUNT = 32 ether;

    struct DepositArgs {
        bytes pubkey;
        bytes withdrawalCredentials;
        bytes signature;
        bytes32 depositDataRoot;
        bytes32[] proof;
    }

    event WithdrawRequest(uint256 tokenId, address sender);
    event NewMint(
        address withdrawalCredentials,
        bytes pubkey,
        uint256 tokenId,
        uint256 amount
    );

    /**
        @notice Initializes the contract.
        @dev    Should only call this function once after contract creation.

        @param  depositContract The address of the ETH2 deposit smart contract.
        @param  withdrawalHandler The address of the withdrawal handler proxy contract.
        @param  validatorPubkeyMerkleRoot_ Merkle root of all valid pub keys that 
                can be used in this contract.
     */
    function initialize(
        DepositContract depositContract,
        WithdrawalHandler withdrawalHandler,
        bytes32 validatorPubkeyMerkleRoot_
    ) public initializer {
        DEPOSIT_CONTRACT = depositContract;
        WITHDRAWAL_HANDLER = withdrawalHandler;
        validatorPubkeyMerkleRoot = validatorPubkeyMerkleRoot_;
        NFT.initialize("ETH2Validator", "VETH", "http://nft.codefi.staking/");
    }

    /**
        @notice Maps a withdrawal credentials address to the owner of its associated NFT
        
        @param  withdrawalCredentials The withdrawal credentials address
        @return Address of the owner of the NFT
     */
    function withdrawalCredentialsToNftOwner(address withdrawalCredentials)
        public
        view
        returns (address)
    {
        uint256 tokenId = withdrawalCredentialsToTokenId[withdrawalCredentials];
        address nftOwner = NFT.ownerOf(tokenId);
        return nftOwner;
    }

    /**
        @notice Predicts what the withdrawal credentials will be using a given `salt`.
        @dev    Uses the `CREATE2` opcode to get a deterministic prediction of 
                the withdrawal credentials.

        @param  pubkey The pubkey to use as a salt for `CREATE2`
        @return The predicted address of the withdrawalHandler
    */
    function predictWithdrawalHandlerAddress(bytes calldata pubkey)
        public
        view
        returns (address)
    {
        return
            ClonesCallForwarder.predictDeterministicAddress(
                address(WITHDRAWAL_HANDLER),
                keccak256(pubkey)
            );
    }

    /**
        @notice Deploys a withdrawal handler smart contract.
        @dev    Deploys a minimal contract that forwards all calls and ETH 
                to `WITHDRAWAL_HANDLER`.

        @param  pubkey The pubkey to use as a salt for `CREATE2`.
        @return The address of the contract that was just deployed.
     */
    function cloneWithdrawalHandler(bytes calldata pubkey)
        internal
        returns (address)
    {
        address withdrawalCredentials = ClonesCallForwarder.cloneDeterministic(
            address(WITHDRAWAL_HANDLER),
            keccak256(pubkey)
        );

        return withdrawalCredentials;
    }

    /**
        @notice Updates the merkle root for the valid pub keys.
        @param  merkleRoot The new merkle root
     */
    function updateMerkleRoot(bytes32 merkleRoot) public onlyOwner {
        validatorPubkeyMerkleRoot = merkleRoot;
    }

    /**
        @notice Checks whether or not a validator pub key exists in the merkle root
                of all valid pub keys.
        
        @param  proof The merkle proof of the key to be checked.
        @param  pubkey The key that is going to be checked.
        @return Whether or not the pubkey is exists in the merkle root.
     */
    function isAuthorisedPubkey(bytes32[] memory proof, bytes memory pubkey)
        public
        view
        returns (bool)
    {
        return
            validatorPubkeyMerkleRoot == 0 ||
            MerkleProof.verify(
                proof,
                validatorPubkeyMerkleRoot,
                keccak256(pubkey)
            );
    }

    /**
        @notice Deposits ether to the ETH2 contract and mints an NFT 
                representing the validator that was created.
        @dev    This is the key entrypoint of Topol. Before minting it 
                will do several security checks.

        @param  pubkey The validator public key
        @param  withdrawalCredentials The address of the withdrawalHandler which will be deployed
        @param  signature The signature to send to the ETH2 deposit contract
        @param  depositDataRoot The depositDataRoot to send to ETH2 deposit contract
        @param  proof The merkle proof showing that the pubkey exists in the `validatorPubkeyMerkleRoot`
     */
    function depositAndMintNFT(
        bytes calldata pubkey,
        bytes calldata withdrawalCredentials,
        bytes calldata signature,
        bytes32 depositDataRoot,
        bytes32[] memory proof
    ) public payable {
        require(
            isAuthorisedPubkey(proof, pubkey),
            "Invalid `pubkey` merkle proof"
        );

        require(
            validatorPubKeyToTokenId[pubkey] == 0,
            "`pubkey` has already been used"
        );

        require(
            msg.value > 0 && msg.value % DEPOSIT_AMOUNT == 0,
            "`msg.value` must be multiple of 32 Ether"
        );

        // Create an ETH2 deposit
        uint256 balanceBeforeTransfer = address(this).balance;
        DEPOSIT_CONTRACT.deposit{value: DEPOSIT_AMOUNT}(
            pubkey,
            withdrawalCredentials,
            signature,
            depositDataRoot
        );

        // Check that correct amount of ETH was sent to DEPOSIT_CONTRACT
        assert(address(this).balance == balanceBeforeTransfer - DEPOSIT_AMOUNT);

        // Mint the NFT
        uint256 newTokenId = NFT.mint(msg.sender);

        // Create withdrawalHandler contract for NFT
        address deployedWithdrawalCredentials = cloneWithdrawalHandler(pubkey);
        withdrawalCredentialsToTokenId[
            deployedWithdrawalCredentials
        ] = newTokenId;

        // Assign the tokenId to the pubkey: pubkey -> tokenId
        validatorPubKeyToTokenId[pubkey] = newTokenId;

        emit NewMint(
            deployedWithdrawalCredentials,
            pubkey,
            newTokenId,
            DEPOSIT_AMOUNT
        );
    }

    /**
        @notice Deposits ether to the ETH2 contract and mints multiple NFTs
                representing each validator that was created.
        @dev    This is the key entrypoint of Topol. Before minting it 
                will do several security checks.

        @param  deposits A list of all the deposits to be taken in.
     */
    function batchDepositAndMintNFTs(DepositArgs[] calldata deposits)
        external
        payable
    {
        require(
            msg.value == deposits.length * 32 ether,
            "`msg.value` must be equal to deposits * 32 Ether"
        );

        for (uint256 i = 0; i < deposits.length; i++) {
            DepositArgs calldata deposit = deposits[i];

            depositAndMintNFT(
                deposit.pubkey,
                deposit.withdrawalCredentials,
                deposit.signature,
                deposit.depositDataRoot,
                deposit.proof
            );
        }
    }

    function initiateNftValidatorWithdrawal(
        uint256 tokenId,
        bytes calldata pubkey
    ) external {
        require(NFT.ownerOf(tokenId) == msg.sender, "Not owner of NFT");
        require(
            validatorPubKeyToTokenId[pubkey] == tokenId,
            "tokenId or pubkey not valid"
        );

        emit WithdrawRequest(tokenId, msg.sender);
    }
}

