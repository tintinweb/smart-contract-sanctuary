contract coatard is ERC721Enumerable {
    uint256 private seed;
    string constant name = "Shan Hai Jin";
    uint256 next_person;
    string constant symbol = "SHJ";
    uint8 constant male = 1;
    uint8 constant female = 2;
    mapping(uint256 => person) public personMap;
    mapping(uint256 => Lucky) public luckyMap;
    event created(address indexed owner, uint256 sex, uint256 person);
    struct person {
        uint256 sex;
        uint256 character;
        uint256 appearance;
    }
    //先天气运
    struct Lucky {
        uint256 first;
        uint256 second;
        uint256 third;
    }

    function create(uint256 _sex) external {
        require(male == _sex || female == _sex, "no exits sex");
        require(balanceOf(msg.sender) == 0, "exits one token");
        uint256 _next_person = next_person;
        personMap[_next_person] = person({
            sex: _sex,
            character: getCharacter(),
            appearance: getAppearance()
        });
        luckyMap[_next_person] = getLucky();
        _safeMint(msg.sender, _next_person);
        emit created(msg.sender, _sex, _next_person);
        next_person++;
    }

    function getCharacter() internal returns (uint256) {
        uint256 randomNum = random(7) + 1;
        return randomNum;
    }

    /**
      *仙姿=1，概率5%，
      *超凡=2，概率10%，
      *英俊=3，概率15%，
      *平庸=4，概率40%，
      *怪异=5，概率15%，
      *丑陋=6，概率10%，
      *憎呕=7，概率5%，
     */
    function getAppearance() internal returns (uint256) {
        uint256 randomNum = random(100);
        if (randomNum < 5) {
            return 1;
        } else if (randomNum < 15) {
            return 2;
        } else if (randomNum < 30) {
            return 3;
        } else if (randomNum < 70) {
            return 4;
        } else if (randomNum < 85) {
            return 5;
        } else if (randomNum < 95) {
            return 6;
        } else {
            return 7;
        }
    }

    function getLucky() internal returns (Lucky memory) {
        uint256 first = getLucky(0, 0);
        uint256 second = getLucky(first, 0);
        uint256 third = getLucky(first, second);
        Lucky memory lucky = Lucky({
            first: first,
            second: second,
            third: third
        });
        return lucky;
    }

    function getLucky(uint256 _lucky1, uint256 _lucky2)
        internal
        returns (uint256)
    {
        uint256 _color = getColor();
        uint256 lucky = 0;
        bool flag = false;
        while (!flag) {
            if (1 == _color) {
                lucky = random(22);
            } else if (2 == _color) {
                lucky = random(22) + 22;
            } else if (3 == _color) {
                lucky = random(24) + 44;
            } else if (4 == _color) {
                lucky = random(45) + 68;
            } else if (5 == _color) {
                lucky = random(34) + 113;
            } else if (6 == _color) {
                lucky = random(39) + 147;
            }
            lucky = lucky + 1;
            if (lucky != _lucky1 && lucky != _lucky2 && lucky != 0) {
                flag = true;
            }
        }
        return lucky;
    }

    /**
     * 灰色 30
       绿色 25
       蓝色 20
       紫色 15
       橙色 10
       红色 5
     */
    function getColor() internal returns (uint256) {
        uint256 num = random(105);
        if (num < 30) {
            return 1;
        } else if (num < 55) {
            return 2;
        } else if (num < 75) {
            return 3;
        } else if (num < 90) {
            return 4;
        } else if (num < 100) {
            return 5;
        } else {
            return 6;
        }
    }

    function random(uint256 length) internal returns (uint256) {
        uint256 randomNum = uint256(
            keccak256(
                abi.encodePacked(block.difficulty, block.timestamp + seed)
            )
        );
        seed = randomNum;
        return randomNum % length;
    }

    function personString(uint256 _personId)
        public
        view
        returns (Lucky memory, person memory)
    {
        require(_exists(_personId), "tokenId no exist");
        require(ownerOf(_personId) == msg.sender, "SHJ: not owner");
        Lucky memory lucky = luckyMap[_personId];
        person memory _person = personMap[_personId];
        return (lucky, _person);
    }

    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT license
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
}

