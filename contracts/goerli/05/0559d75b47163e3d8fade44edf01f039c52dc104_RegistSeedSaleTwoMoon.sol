pragma solidity >=0.6.6;

contract RegistSeedSaleTwoMoon is Ownable, Pausable{
    using Address for address;
    using SafeMath for uint256;
    address public receiveAddr;
    uint256 public minRegistAmount;
    uint256 public maxRegistAmount;
    uint256 public totalRegistEth;
    uint256 public limitRegistEth;
    bool public isStartRegister;
    address[] public listRegistAddress;

    mapping(address => uint256) private registList;

    constructor(
        uint256 _minRegistAmount,
        uint256 _maxRegistAmount,
        uint256 _limitRegistEth,
        address _receiveAddr
    )public{
        minRegistAmount = _minRegistAmount;
        maxRegistAmount = _maxRegistAmount;
        receiveAddr = _receiveAddr;
        limitRegistEth = _limitRegistEth;
        totalRegistEth = 0;
        isStartRegister = false;
    }

    receive() external payable {}
    
    

    function registSeedSale() public payable {
        require(isStartRegister,'event is not start yet');
        require(msg.value >= minRegistAmount, 'msg value too low');
        require(msg.value <= maxRegistAmount, 'msg value too high');
        require(totalRegistEth.add(msg.value) >= limitRegistEth, 'limit regist');
        uint256 maxRegistOnTransaction = limitRegistEth.sub(totalRegistEth.sub(registAmountOf(msg.sender)));
        uint256 registEthCurrentTransfer = 0;
        if(msg.value>maxRegistOnTransaction){
            TransferHelper.safeTransferETH(receiveAddr, maxRegistOnTransaction);
            TransferHelper.safeTransferETH(msg.sender, msg.value.sub(maxRegistOnTransaction));
            registEthCurrentTransfer = msg.value.sub(maxRegistOnTransaction);
        }else{
            TransferHelper.safeTransferETH(receiveAddr, msg.value);   
            registEthCurrentTransfer = msg.value;
        }
        totalRegistEth = totalRegistEth.add(registEthCurrentTransfer);
        listRegistAddress.push(msg.sender);
        registList[msg.sender] = msg.value;
    }
    
    function changeRegisterStatus() public onlyOwner {
        isStartRegister = !isStartRegister;
    }
    
    function changeMinRegistAmount( uint256 _amount) public onlyOwner {
        minRegistAmount = _amount;
    }
    
    function changeMaxRegistAmount( uint256 _amount) public onlyOwner {
        maxRegistAmount = _amount;
    }
    
    function changeLimitRegistEth( uint256 _amount) public onlyOwner {
        limitRegistEth = _amount;
    }
    
    function registAmountOf(address account) public view returns (uint256) {
        return registList[account];
    }

    function getRegistList() public view returns (address[] memory) {
        return listRegistAddress;
    }
    
    function pause() public onlyOwner whenNotPaused {
        _pause();
    }

    function unpause() public onlyOwner whenPaused {
        _unpause();
    }
}