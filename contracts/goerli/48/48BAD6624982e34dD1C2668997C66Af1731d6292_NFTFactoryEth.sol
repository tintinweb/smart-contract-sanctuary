contract FactoryStorage {
  address public last_upgrade;

  address public storage_admin;

  address public contract_admin;

  address public royaltyRecipient;

  uint256 public NFT_ID;

  uint256 public URIChanged;

  struct ModelNFT {
    string name;
    uint256 id;
    uint24 fee;
  }

  ModelNFT [] public NFTmodels;

  //map to get the artist with the token ID (unique identifier)
  mapping (uint256 => address) public NFTToArtist;
  //map to get all the NFTs of an artist
  mapping(address => ModelNFT [])  public artistToNFTS;

  mapping(uint256 => uint256) public tokenAmount;

  constructor() {
    storage_admin = _msg_Sender();
    contract_admin = _msg_Sender();
    royaltyRecipient = _msg_Sender();
    NFT_ID = 0;
    URIChanged = 0;
  }

  function setNewAdmin(address _newAdmin) virtual public onlyAdmin {
    storage_admin = _newAdmin;
  }

  function setNewContractAdmin(address _newAdmin) virtual public onlyAdmin {
    contract_admin = _newAdmin;
  }

  function setRoyaltyRecipient(address _royaltyRecipient) virtual public onlyAdmin {
    royaltyRecipient = _royaltyRecipient;
  }

  function addNFT(address _artist, string memory _name, uint256 _amount, uint24 _fee) virtual public onlyAdmin {
      ModelNFT memory model = ModelNFT(_name, NFT_ID , _fee);
      NFTmodels.push(model);
      artistToNFTS[_artist].push(model);
      NFTToArtist[NFT_ID] = _artist;
      tokenAmount[NFT_ID] = _amount;
      NFT_ID++;
  }

  function incrementURIChanged() public onlyAdmin {
    URIChanged++;
  }

  function getAdmin() public view returns (address, address) {
    return (storage_admin, contract_admin);
  }

  function getRoyaltyRecipient() public view returns (address) {
    return royaltyRecipient;
  }

  function getNFT_ID() public view returns (uint256) {
    return NFT_ID;
  }

  function getUriChanged() public view returns (uint256) {
    return URIChanged;
  }

  function getNFTmodels(uint256 _tokenId) public view returns (ModelNFT memory) {
    return NFTmodels[_tokenId];
  }

  function getNFTToArtist(uint256 _tokenId) public view returns (address) {
    return NFTToArtist[_tokenId];
  }

  function getTokenAmount(uint256 _tokenId) public view returns (uint256) {
    return tokenAmount[_tokenId];
  }
  

  function getTokenInfo(uint256 _tokenId) virtual public view returns (string memory name, uint24 fee,uint256 amount, address artist) {
    return(
      NFTmodels[_tokenId].name,
      NFTmodels[_tokenId].fee,
      tokenAmount[_tokenId],
      NFTToArtist[_tokenId]
    );
  }

  function _exists(uint256 tokenId) public view virtual returns (bool) {
        return NFTToArtist[tokenId] != address(0);
    }

  function _msg_Sender() internal view virtual returns (address) {
      return msg.sender;
    }

  modifier onlyAdmin() {
    require(storage_admin == _msg_Sender() || contract_admin == _msg_Sender());
    _;
  }

  function setLastUpgrade(address _c) public onlyAdmin returns (bool success) {
        last_upgrade = _c;
        return true;
  }

  fallback() external {
    assembly {
      let _target := sload(last_upgrade.slot)
      calldatacopy(0x0, 0x0, calldatasize())
      let result := delegatecall(gas(), _target, 0x0, calldatasize(), 0x0, 0)
      returndatacopy(0x0, 0x0, returndatasize())
      switch result case 0 {revert(0, 0)} default {return (0, returndatasize())}
        }   
    }

}

