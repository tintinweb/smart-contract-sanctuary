contract Timelock is Ownable, Pausable {
    using EnumerableSet for EnumerableSet.UintSet;
    
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    
    uint256 private internalTicket;

    struct userStore {
        uint256 lockAmount;
        uint256 deadline;
        uint256 claimTime;
    }

    mapping(address => mapping(bytes32 => userStore)) private userData;
    mapping (address => EnumerableSet.UintSet) private userLockTicketInfo;    
    event lockEvent(address indexed user, uint256 amount, uint256 time);
    event unlockEvent(address indexed user, uint256 amount,uint256 lockid, uint256 time);

    constructor() {}

    receive() external payable {}

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function adminEmergency(
        address addr,
        address account,
        uint256 amount
    ) public onlyOwner {
        if (addr == address(0)) {
            payable(account).transfer(amount);
        } else {
            IERC20(addr).transfer(account, amount);
        }
    }
    
    function hashView(address token,uint256 id) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(token,id));
    }

    function lock(
        address _token,
        uint256 _tokenAmount,
        uint256 _deadline
    ) public whenNotPaused {
        address user = _msgSender();

        IERC20(_token).safeTransferFrom(user, address(this), _tokenAmount);
        internalTicket++;
        bytes32 hash_ = hashView(_token,internalTicket);
        userData[user][hash_] = userStore(
            userData[user][hash_].lockAmount + _tokenAmount,
            _deadline,
            0
        );
        userLockTicketInfo[user].insert(internalTicket);
        emit lockEvent(user, _tokenAmount, block.timestamp);
    }

    function unlock(address _token,uint256 lockId) public whenNotPaused {
        address user = _msgSender();
        bytes32 hash_ = hashView(_token,lockId);
        require(userLockTicketInfo[user].contains(lockId), "Invalid ticket id");
        userStore storage store = userData[user][hash_];
        require(store.deadline < block.timestamp, "unable to claim now");
        IERC20(_token).safeTransfer(user, store.lockAmount);
        store.lockAmount = 0;
        userLockTicketInfo[user].remove(internalTicket);
    }
    
    function multiUnlock(address token) public whenNotPaused {
        address user = _msgSender();
        uint256[] memory tickets = userLockTicketInfo[user].values();
        
        uint256 len = tickets.length;
        require(len < 128, "unable to process too much of loop");
        for(uint256 i; i<len; i++){
            bytes32 hash_ = hashView(token,tickets[i]);
            if((userData[user][hash_].deadline < block.timestamp) && userLockTicketInfo[user].contains(tickets[i])) {
                IERC20(token).safeTransfer(user,userData[user][hash_].lockAmount);
                userLockTicketInfo[user].remove(tickets[i]);
                
                userData[user][hash_].claimTime = block.timestamp;
                
                emit unlockEvent(user,userData[user][hash_].lockAmount,tickets[i],block.timestamp);
            }
        }
    }
    

    function extendReleasetime(address token,uint256 lockId, uint256 newdeadline) public whenNotPaused  {
        address user = _msgSender();
        bytes32 hash_ = hashView(token,lockId);
        userStore storage store = userData[user][hash_];
        require(userLockTicketInfo[user].contains(lockId), "Invalid ticket id");
        require(newdeadline > store.deadline,"Releasetime must be greater than old releasetime");
        store.deadline = newdeadline;
    }
    
    function userTicketLength(address user) public view returns (uint256) {
         return userLockTicketInfo[user].length();
    }
    
    function userTicketAt(address user,uint256 index) public view returns (uint256) {
        return userLockTicketInfo[user].at(index);
    }

    function userTickets(address user) public view returns (uint256[] memory) {
        return userLockTicketInfo[user].values();
    }
    
    function userTicketContains(address user,uint256 ticket) public view returns (bool) {
        return userLockTicketInfo[user].contains(ticket);
    }
    
    function userInfo(address user,address token,uint256 lockId) public view returns (userStore memory result){
        bytes32 hash_ = hashView(token,lockId);
        return userData[user][hash_];
    }
}