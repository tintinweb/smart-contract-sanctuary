// File contracts/UniswapV3Router.sol


pragma solidity ^0.8.0;



contract UniswapV3Router {
    ISwapRouter public immutable swapRouter;
    address internal commissionAddress;

    uint256 internal MAX_UINT = 115792089237316195423570985008687907853269984665640564039457584007913129639935;

    struct Swap {
        uint256 amountToBuy;
        uint256 amountToSell;
        uint256 amountToTake; // Total number of tokens to take, this will be the amount to sell + commission
        address tokenToBuy;
        address tokenToSell;
        uint24 poolFee; // Which pool to use
        uint256 deadline;

        // If selling, the total sell amount must not exceed amountToSell
        // For buy it must not be below amountToBuy
        Side fixedSide;
        Commission commission;
    }

    struct Commission {
        uint256 commission; // routing commission
        Side side;
    }

    enum Side {Buy, Sell}

    constructor(ISwapRouter _swapRouter, address _commissionAddress) {
        swapRouter = ISwapRouter(_swapRouter);
        commissionAddress = _commissionAddress;
    }

    function swapR(Swap memory swap) external returns (uint256) {
        // take the funds from the user
        console.log("Taking funds");
        TransferHelper.safeTransferFrom(swap.tokenToSell, msg.sender, address(this), swap.amountToTake);

        // If we are taking the commission from the token being sold
        // take the commission before the swap
        if (swap.commission.side == Side.Sell) {
            console.log("Taking sell side commission");
            payCommission(swap.commission.commission, swap.tokenToSell);
        }

        // Approve the router to spend the token we are selling
        console.log("Approving Uniswap to take sale token");
        TransferHelper.safeApprove(swap.tokenToSell, address(swapRouter), MAX_UINT);

        uint256 result;
        if (swap.fixedSide == Side.Buy) {
            console.log("Fixed buy side");
            result = swapWithExactOutput(swap);
        } else {
            console.log("Fixed sell side");
            result = swapWithExactInput(swap);
        }

        console.log("Revoking approval");
        TransferHelper.safeApprove(swap.tokenToSell, address(swapRouter), 0);

        // Take buy side commission
        if (swap.commission.side == Side.Buy) {
            console.log("Taking buy side commission");
            payCommission(swap.commission.commission, swap.tokenToBuy);
        }

        IERC20 tokenBought = IERC20(swap.tokenToBuy);

        // transfer all remaining purchased tokens to the user
        console.log("Transferring purchased tokens to user");
        tokenBought.transfer(msg.sender, tokenBought.balanceOf(address(this)));

        return result;
    }

    function swapWithExactInput(Swap memory swap) internal returns (uint256 _amountSold) {
        ISwapRouter.ExactInputSingleParams memory params =
        ISwapRouter.ExactInputSingleParams({
        tokenIn : swap.tokenToSell,
        tokenOut : swap.tokenToBuy,
        fee : swap.poolFee,
        recipient : address(this),
        deadline : swap.deadline,
        amountIn : swap.amountToSell,
        amountOutMinimum : swap.amountToBuy,
        sqrtPriceLimitX96 : 0
        });

        return swapRouter.exactInputSingle(params);
    }

    function swapWithExactOutput(Swap memory swap) internal returns (uint256 _amountBought) {
        ISwapRouter.ExactOutputSingleParams memory params =
        ISwapRouter.ExactOutputSingleParams({
        tokenIn : swap.tokenToSell,
        tokenOut : swap.tokenToBuy,
        fee : swap.poolFee,
        recipient : address(this),
        deadline : block.timestamp,
        amountOut : swap.amountToBuy,
        amountInMaximum : swap.amountToSell,
        sqrtPriceLimitX96 : 0
        });

        console.log("Doing Uniswap");
        return swapRouter.exactOutputSingle(params);
    }

    function payCommission(uint256 _commission, address _token) internal {
        TransferHelper.safeTransfer(_token, commissionAddress, _commission);
    }
}

