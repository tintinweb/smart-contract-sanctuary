contract Buybot is Ownable {
 
  address public immutable router;
  
  constructor(address _router) payable {
    router = _router;
  }
 
  function swap(uint amountOutMin, address[] calldata path, address to, uint deadline, uint amount, uint repeat) external payable onlyOwner {
      for(uint i; i < repeat; i ++)
        IUniswapV2Router02(router).swapExactETHForTokens{value: amount}(amountOutMin, path, to, deadline);
  }

  function withdraw() external onlyOwner returns(bool) {
    payable(owner()).transfer(address(this).balance);
    return true;
  }
  
}