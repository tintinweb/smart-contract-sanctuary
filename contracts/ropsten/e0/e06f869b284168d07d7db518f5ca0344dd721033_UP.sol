contract UP is ERC20, ERC20Detailed {
    using SafeERC20 for IERC20;
    using Address for address;
    using SafeMath for uint;
    
    event BurnedAmount(uint256 amount);
    
    address public governance;
    address public uniswapV2Pair;
    
    uint256 public BurnInterval = 2 minutes;
    uint256 public LastBurnStamp = block.timestamp;
    uint256 public BurnAmount = 50e18;
    bool public BurnEnabled;
    
    mapping (address => bool) public burners;
    
    constructor () public ERC20Detailed("UP", "UP", 18) {
        governance = msg.sender;
        _mint(msg.sender, 10000e18);
        IUniswapV2Router02 uniswapRouterV2 = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        uniswapV2Pair = IUniswapV2Factory(uniswapRouterV2.factory())
        .createPair(address(this), uniswapRouterV2.WETH());
    }
    
    function setGov(address _governance) public {
        require(msg.sender == governance, "Governance");
        governance = _governance;
    }

    function BurnPoolTokens() public {
        require(burners[msg.sender], "Burner");
        _BurnPoolTokens;
    }
    function addBurner(address _burner) public {
        require(msg.sender == governance, "Governance");
        burners[_burner] = true;
    }
    function removeBurner(address _burner) public {
        require(msg.sender == governance, "Governance");
        burners[_burner] = false;
    }
    function setBurnEnabled(bool value) public {
        require(burners[msg.sender], "Burner");
        BurnEnabled = value;
    }
}