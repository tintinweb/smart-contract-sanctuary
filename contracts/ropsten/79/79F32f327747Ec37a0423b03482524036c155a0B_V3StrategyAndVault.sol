import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
pragma solidity ^0.8.10;

interface IVaultRegistry {
    /**
     * @dev all necessary data for vault. Name and symbol are stored in vault's ERC20. Owner is stored with tokenID in StrategyRegistry.
     * TODO: store owner here? More expensive initialization, slightly less expensive access
     * tokenID: NFT identifier number
     * vaultAddress: address of vault this describes
     * state: state of the vault. TODO: can this be upgradeable? If not is that an issue? We may want to add more states.
     * vaultType: type of vault. Currently there is only one type of vault: the v3Strategy vault. TODO: make this an integer?
            We'll probably eventually be making all sorts of vaults, so it would be nice to not have to update the enum every time.
     */
    struct VaultData {
        VaultState state;
        VaultType vaultType;
        uint256 tokenID; //NFT ownership of this vault and all others that use vault's exec bundle
        uint256 vaultID; //unique identifier for this vault and strategy token id
    }

    /**
     * @dev returns NFT owner of given vault
     * @param _vault address of vault in question
     * @return address of owner of given vault
     */
    function getStrategyCreatorForVault(address _vault)
        external
        view
        returns (address);

    function updateVaultState(address _vault, VaultState _newState) external;

    function getVaultState(address _vault) external view returns (VaultState);

    function getVaultType(address _vault) external view returns (VaultType);

    /**
     * @dev Registers an execution bundle, printing an NFT and mapping to execution bundle and name.
     * @param _name The name of the strategy. Auto-generate this using exec bundle and tokens?
     * @param _symbol the symbol of the strategy.
     * @param _params is extra parameters in vault. Currently abi-encoded (address,address,string,string,string,string,int24,uint24)) TODO: document this better
     * @param _tokenID is the NFT of the execution bundle this vault will be using.
     * @dev owner is set as msg.sender.
     */
    function createVault(
        string memory _name,
        string memory _symbol,
        address _baseRelay,
        bytes memory _params,
        uint256 _tokenID
    ) external returns (uint256);

    /**
     * @return symbols of ERC20 tokens used by vault
     */
    function getAssetSymbols(address _vault)
        external
        view
        returns (string[2] memory);

    /**
     * PendingApproval: strategy is submitted but has not yet been approved by the owner
     * PendingThreshold: strategy is approved but has not yet reached the threshold of TVL required
     * Paused: strategy was active but something went wrong, so now it's paused
     * Active: strategy is active and can be used
     * Retired: strategy is retired and can no longer be used
     */
    enum VaultState {
        PendingApproval,
        PendingThreshold,
        Paused,
        Active,
        Retired
    }

    enum VaultType {
        SINGLE,
        PAIR,
        MULTI
    } //TODO: make this into another registry?

    //TODO: what needs to be indexed?
    //TODO: replace with just deployer and tokenID?
    event VaultCreated(
        address deployer,
        address vault,
        uint256 tokenID,
        string name
    );

    event VaultStateChanged(address vault, VaultState newState);

    function getVaultDetails(address vaultAddress) external view returns (IVaultRegistry.VaultData memory);

    function getVaultCountByStrategyId(uint256 _tokenId) external view returns (uint256);

    function getVaultByStrategyAndIndex(uint256 _tokenId, uint256 _vaultId) external view returns (IVaultRegistry.VaultData memory);
}

