        //Send out tokens
        token0.safeTransfer(strategist, value0);
        token1.safeTransfer(strategist, value1);
    }

    /*
     * Function for steer to withdraw the funds they have accrued
     */
    function steerPullFees() external returns(uint256 value0, uint256 value1) {
        //Make sure funds are going to Steer
        _checkRole(STEER_ROLE, msg.sender); 

        //Get values owed to steer
        value0 = steerPromisedT0;
        value1 = steerPromisedT1;

        //Update values due to steer to 0, since those values will be sent out        
        steerPromisedT0 = 0;
        steerPromisedT1 = 0;

        //Report this withdrawal
        emit SteerPulled(value0, value1);

        //Send out tokens
        token0.safeTransfer(msg.sender, value0);
        token1.safeTransfer(msg.sender, value1);
    }
}

