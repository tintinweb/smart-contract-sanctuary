    bytes32 public constant ORCHESTRATOR_ROLE = keccak256("ORCHESTRATOR_ROLE");     //Can call update function
    //address strategist can collect strategist fees, but is not stored here.

    address public vaultRegistry;

    IERC20 public token0;                                  
    IERC20 public token1;

    //Promised tokens are held by this vault, but not counted towards LP value or anything. They're just held here until strategist/steer withdraws their allocation.
    uint256 public strategistPromisedT0;
    uint256 public strategistPromisedT1;
    uint256 public steerPromisedT0;
    uint256 public steerPromisedT1;

    //promisedTokens represent total tokens held (i.e. waiting for withdrawal) by withdrawers whose transactions have gone through.
    uint256 public promisedToken0;
    uint256 public promisedToken1;

    //LPTqueuedForWithdrawal is how many LP tokens are queued for withdrawal (i.e. queued to be burned, their value in t0 and t1 made available to withdrawers).
    uint256 public LPTqueuedForWithdrawal;

    //depositedTokens are total tokens deposited into this contract, and queued to enter the system and be exchanged for LP tokens
    uint256 public depositedToken0;
    uint256 public depositedToken1;

    //currentTransactionIndex tracks the index of the next queue handling, so that deposits/withdrawals know where to look for info when their queue is processed
    uint256 public currentTransactionIndex;

    //LP tokens set aside, but not claimed, from queued deposits which have gone through. We need it so that withdrawn LP tokens are still counted towards total LP supply,
        //thus meaning we can calculate LP value correctly.
    uint256 public claimableLPT;
    
    //Structs

    /*
        Each user who has queued a deposit has one of these. It tracks how much of each token they deposited. The funds are not made available to the vault until the next tend, 
        at which point the user is instead credited with LP tokens corresponding to the value they deposited. That value is tracked by recording t0 value and t1 value
        at the transaction pointed to by the transactionIndex in this queuedDeposit. note: each address can have only 1 queuedDeposit at a time, and only 1 queuedWithdrawal at a time.
        If the address already has a queuedDeposit and tries to queue another deposit, 
    */
    struct queuedDeposit   {
        uint256 token0In;
        uint256 token1In;
        uint256 transactionIndex;
    }

    /*
        Same deal as queuedDeposit. LPTs refers to how many LP tokens withdrawer is queueing for withdrawal. transactionIndex points to transaction where the value of those LP tokens
        is recorded. Withdrawals will be returned in equal values of token0 and token1 by default, but this may be adjusted one way or another depending on vault rebalance().
    */
    struct queuedWithdrawal {
        uint256 LPTs;
        uint256 transactionIndex;
    }
    
    /*
        Tracks value of LP tokens at time of tend. These 2 values must be separated because deposits take place BEFORE vault rebalancing, whereas withdrawals take place afterwards.
        This means that between deposit and withdraw, token1PerToken0 and tokens / LP token could change, so we need separate LP valuations for each one.
        When withdrawing, an LP token earns you both withdrawT0 and withdrawT1. When depositing, depositing depositT0PerLPT t0 will get you 1 LPT, as will depositing depositT1PerLPT t1.
        This is to minimize math.
    */
    struct transaction   {
        uint256 depositT0ValuePerLPT;
        uint256 depositT1ValuePerLPT;
        uint256 withdrawT0EarnedPerLPT;
        uint256 withdrawT1EarnedPerLPT;
    }

    //Struct used to avoid stack too deep issues
    struct BinInfo   {
        uint256 token0WeightSum;
        uint256 token1WeightSum;
        uint24 currentTickBinIndex;
        uint256 contestedT0Weight;
    }

    //Another struct to avoid stack issues
    struct VaultState   {
        uint256 finalToken0Balance;             //How many token 0 were in uniswap at the end of the latest epoch.
        uint256 finalToken1Balance;             //How many token 1 were in uniswap at the end of the latest epoch. 
        uint256 newToken0Balance;               //How much token0 the vault controls, i.e. how much token0 liquidity should be put into uniswap
        uint256 newToken1Balance;               //How much token1 the vault controls, i.e. how much token1 liquidity should be put into uniswap
        uint256 token1PerToken0X128;            //How much token1 per token0 in uniswap pool, * 2**128
        uint256 totalLPTs;                      //LPTs with a claim to vault holdings. Includes minted LPTs and unminted LPTs queued to be minted.
        uint256 TVLinT0;                
        uint256 desiredToken0Amount;            //How much token0 the vault wants to have in uniswap, if the holdings ratio between t0 and t1 is perfect.
        uint256 t0ValuePerLPT;          
        uint256 LPTqueuedForWithdrawal;
        uint256 valueInT0ToWithdraw;
    }

    //Mappings
    mapping(address => queuedDeposit) public depositors;
    mapping(address => queuedWithdrawal) public withdrawers;
    mapping(uint256 => transaction) public transactions;   //uint256 refers to transaction index. Increment after each successful transaction.

    //--------END Vault slots, used: 26
    uint256[24] __vault_gap;    //TODO: update this

    //--------Start Strategy slots

    //Constants
    IUniswapV3Factory constant FACTORY = 
        IUniswapV3Factory(0x1F98431c8aD98523631AE4a59f267346ea31F984);
        
    IUniswapV3Pool public POOL;
    uint256 constant MAX_SLIPPAGE = 97;
    uint256 constant SLIPPAGE_DIVISOR = 100;
    uint256 constant deadline = 30;     //30 seconds @todo should this be 0?

    //Uniswap Bins 
    //TODO: re-add packing once other things are sorted out
    //TODO: add a way to change ticksPerBin?
    uint24 public ticksPerBin;                                      //NOTE: this MUST be a multiple of uniswap initializable tick spacing.
    //0.05% pool is tick spacing of 10. 0.3% pool is tick spacing of 60. 1% is tick spacing of 200.
    int24 maxTickChange = 500;  //Roughly corresponds to a 5% diff between current price and twap price

    uint24 public FEE;                          //Pool fee TODO gas efficiency: make this immutable
    int24 public activeStartTick;              //tick where our first bin begins
    uint32[] public activeBins;               //Bins. Each represents a proportion of total vault liquidity.

    mapping(int24 => uint256) public binRegistry;  //tickLower -> tokenID

    //---------END Strat slots, used: 15
    uint256[35] __strat_gap;

    //------------------------------------------------Events------------------------------------------------\\
    //oldHoldings are total TVL (in token0) of vault after a week. newHoldings are totalTVL later on, as part of the same transaction, after deposits and withdrawals go through.
    event VaultReport(uint256 lastT0, uint256 lastT1, uint256 newT0, uint256 newT1);
