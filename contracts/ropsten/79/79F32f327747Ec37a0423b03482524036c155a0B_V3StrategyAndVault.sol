    //TODO TODO TODO: remove this when we're actually deploying
    function setFactoryAndPool(address factory) external   {
        POOL = IUniswapV3Pool(IUniswapV3Factory(factory).getPool(address(token0), address(token1), FEE));
    }

    /**
     * @dev allows a user to queue a deposit. Their token0 and token1 will be pulled from their wallet to here. The LP value will be calculated when someone calls tend.
     * @param token0Amount is amount of token0 they want to deposit
     * @param token1Amount is amount of token1 they want to deposit
     * TODO: Add "to" so that function can credit shares to an address other than msg.sender. Do the same with withdrawal maybe.
     */
    function queueDeposit(uint256 token0Amount, uint256 token1Amount) external  {
        //Transfer funds in
        if (token0Amount > 0)   {
            token0.safeTransferFrom(msg.sender, address(this), token0Amount);
        }
        if (token1Amount > 0)   {
            token1.safeTransferFrom(msg.sender, address(this), token1Amount);
        }

        //Load user's queuedDeposit info
        queuedDeposit memory thisDeposit = depositors[msg.sender];

        //Make sure we're interfacing with correct deposit index. If we're not, that means we're working with a past deposit index with 
        //unaccessed funds, so pull those funds out and then create a new deposit.
        if (thisDeposit.transactionIndex < currentTransactionIndex && (thisDeposit.token0In > 0 || thisDeposit.token1In > 0))   {
            //Claim the last queuedDeposit, since it's already gone through
            claimLPTokensFromDeposit();

            //Update user deposit with new values just provided
            queuedDeposit memory newdepositinfo = queuedDeposit(token0Amount, token1Amount, currentTransactionIndex);
            depositors[msg.sender] = newdepositinfo;
        } else   {
            //Add new deposit amounts to current user queuedDeposit info
            queuedDeposit memory newDepositInfo = queuedDeposit(token0Amount + thisDeposit.token0In, token1Amount + thisDeposit.token1In, currentTransactionIndex);
            depositors[msg.sender] = newDepositInfo;
        }

        //Set aside the tokens that were just deposited, so that they don't count towards LP value (the vault doesn't really own these tokens yet)
        depositedToken0 += token0Amount;
        depositedToken1 += token1Amount;
    }

    /**
     * @return uint256 amount of LPT claimed
     * @dev claims LP tokens earned from queued deposit
     * Claims tokens earned from queued deposits which have been processed.
     * TODO: add "to" field in order to make it possible for people to send their claimed LP tokens anywhere. 
     */
    function claimLPTokensFromDeposit() public returns(uint256)   {
        uint256 _currentTransactionIndex = currentTransactionIndex; //SLOAD for gas efficiency

        //If transactionIndex == currentTransactionIndex, that means that this transaction has not yet gone through, so you can't claim your LP tokens yet (your funds aren't staked yet).
        require(depositors[msg.sender].transactionIndex < _currentTransactionIndex, "!Deposited");

        //SLOAD for efficiency. thisTransaction contains price values at the time of the transaction.
        queuedDeposit memory thisDeposit = depositors[msg.sender];
        transaction memory thisTransaction = transactions[thisDeposit.transactionIndex];

        //Now that deposit info has been copied (and will be processed), Reset queuedDeposit. 
        depositors[msg.sender] = queuedDeposit(0, 0, _currentTransactionIndex);

        //LP earned for this deposit = LP/token0 * token0 + LP/token1 * token1
        uint256 LPTearned = (thisDeposit.token0In * PRECISION / thisTransaction.depositT0ValuePerLPT) + (thisDeposit.token1In * PRECISION / thisTransaction.depositT1ValuePerLPT);

        //Now that we're sending out these LP tokens, they no longer are earmarked
        claimableLPT -= LPTearned;

        //Send them out
        _mint(msg.sender, LPTearned);

        //Return amount sent out
        return LPTearned;
    }

    /**
     * Allows a user to cancel a queued deposit. Their funds are returned to them.
     */
    function cancelQueuedDeposit() external   {
        //Load user's queuedDeposit info
        queuedDeposit memory thisDeposit = depositors[msg.sender];

        //If deposit hasn't gone through yet, set it back to zero and send deposited tokens back to user
        if (thisDeposit.token0In > 0 || thisDeposit.token1In > 0)   {
            //Send back deposited, unprocessed tokens
            if (thisDeposit.token0In > 0)   {
                depositedToken0 -= thisDeposit.token0In;
                token0.safeTransfer(msg.sender, thisDeposit.token0In);
            }
            if (thisDeposit.token1In > 0)   {
                depositedToken1 -= thisDeposit.token1In;
                token1.safeTransfer(msg.sender, thisDeposit.token1In);
            }

            //Reset user's queuedDeposit to 0
            depositors[msg.sender] = queuedDeposit(0, 0, thisDeposit.transactionIndex);
        }
    }

    /**
     * @param _LPTs is amount of LP tokens to be queued for withdrawal
     * TODO gas efficiency: possibly use something other than burn--maybe write new ERC20 to be slightly more efficient, since we're kind of double-updating LPTqueuedForWithdrawal's.
     */
    function queueWithdrawal(uint256 _LPTs) external {
        //SLOAD for gas efficiency
        queuedWithdrawal memory thisWithdrawal = withdrawers[msg.sender];

        //Take LP tokens from msg.sender 
        _burn(msg.sender, (_LPTs));

        //If there's a prior queuedWithdrawal, it will have gone through by now, so claim it and then update records
        if (thisWithdrawal.transactionIndex < currentTransactionIndex && thisWithdrawal.LPTs > 0)   {
            claimWithdrawal();
        }

        //Update msg.sender's withdrawal info
        uint256 totalLPTs = _LPTs + thisWithdrawal.LPTs;
        withdrawers[msg.sender] = queuedWithdrawal(totalLPTs, currentTransactionIndex);

        //Set aside LP tokens for withdrawal
        LPTqueuedForWithdrawal += _LPTs;
    }

    /**
     * @dev claims token0 and token1 once LP tokens queued for withdrawal have been processed. Gives equal amounts of token0 and token1.
     */
    function claimWithdrawal() public {
        //Make sure withdrawal has gone through
        //SLOAD for efficiency
        queuedWithdrawal memory thisWithdrawal = withdrawers[msg.sender];
        require(thisWithdrawal.transactionIndex < currentTransactionIndex, "!Processed");

        //Now that withdrawal has been copied to memory, reset stored withdrawal info
        transaction memory thisTransaction = transactions[thisWithdrawal.transactionIndex];

        //SLOAD for efficiency
        withdrawers[msg.sender] = queuedWithdrawal(0, currentTransactionIndex);

        //Calculate funds earned by withdrawal. 
        uint256 token0ToWithdraw = thisWithdrawal.LPTs * thisTransaction.withdrawT0EarnedPerLPT / (PRECISION);
        uint256 token1ToWithdraw = thisWithdrawal.LPTs * thisTransaction.withdrawT1EarnedPerLPT / (PRECISION);

        //Send funds
        token0.safeTransfer(msg.sender, token0ToWithdraw);
        promisedToken0 -= token0ToWithdraw;
        token1.safeTransfer(msg.sender, token1ToWithdraw);
        promisedToken1 -= token1ToWithdraw;
    }

    /**
     * Cancels a queued withdrawal that hasn't yet been processed, refunding the user's LP tokens.
     */
    function cancelQueuedWithdrawal() external   {
        //Load user's queuedDeposit info
        queuedWithdrawal memory thisWithdrawal = withdrawers[msg.sender];

        //If deposit hasn't gone through yet, set it back to zero and send deposited tokens back to user
        if (thisWithdrawal.LPTs > 0)   {
            //Send back unprocessed LP tokens
            LPTqueuedForWithdrawal -= thisWithdrawal.LPTs;
            _mint(msg.sender, thisWithdrawal.LPTs);
        }
        withdrawers[msg.sender] = queuedWithdrawal(0, thisWithdrawal.transactionIndex);
    }

    /**
     * External function which users call when they want to tend
     */
    function tendVaults() external   {
        tend(activeStartTick, activeBins, false);
    }

    /**
     * @dev calls tend and also updates bins. So handles queued deposits and queuedwithdrawals, rebalances to correct token ratio, and adds liquidity to new bins. Only Orchestrator can call.
     * @notice orchestrator should just call tendVaults() if bins are not being changed.
     * @param startTick is lower tick of first bin in newBins
     * @param newBins is array of new bins. Each value represents the weight of (token value that should be devoted to) that bin
     */
    function update(int24 startTick,  uint32[] calldata newBins) external {
        _checkRole(ORCHESTRATOR_ROLE,msg.sender);
        require(newBins.length > 0, "Too few bins");
        tend(startTick, newBins, true);
    }

     //TODO: add slippage for bin setting, rebalance, etc.
     //TODO: add tick spacing somewhere--maybe in periphery contract
     /**
      * @param startTick is lower tick of first bin, either of current bins (if bins are not changing) or new bins (if they are)
      * @param bins is array of weights of each bin. 
      * @param moveBins is whether the bins are changing--this is true if called from update, false if called from tend.
      * 1. Make sure price is close enough to TWAP price (we're not getting flashloan attacked)
      * 2. Drain all liquidity from uniswap
      * 3. Send out fees to steer/strategist
      * 4. Record vault holdings--at this point vault holdings represent original holders' principal and growth
      * 5. Bring in deposits
      * 6. If necessary, rebalance vault to desired ratio of token0 to token1 (if bins are being moved, or deposits are imbalanced)
      * 7. Send out withdrawals
      * 8. Record vault holdings--at this point they represent new holders' principal
      * 9. Redeposit liquidity into uniswap
      */
    function tend(int24 startTick, uint32[] memory bins, bool moveBins) internal  {
        //This variable will track the state of the vault, so that we can conserve stack slots.
        VaultState memory vaultState;

        //Get our pool currentTick. This value is crucial for knowing where to put liquidity, and what the current exchange rate of token1/token0 is.
        (uint256 sqrtPriceX96, int24 currentTick,,,,,) = POOL.slot0();

        //Compare current tick token1PerToken0X128 to twap token1PerToken0X128. Revert if they are too different.
        require(checkVolatility(currentTick, getTWAPTick()), "TWAP");

        //Pull fees and liquidity from uniswap into this contract, sending appropriate fees to steer/strategist.
        drainAll();

        //TODO gas efficiency: possibly return to getting these values from drainAll
        vaultState.finalToken0Balance = looseToken0Balance();
        vaultState.finalToken1Balance = looseToken1Balance();

        //Calculate token1PerToken0X128 given current tick
        uint256 token1PerToken0X128 = getToken1PerToken0X128(sqrtPriceX96);

         //If orchestrator is calling this (from update) we will update the contract's bins to new positions before putting liquidity back into the bins.
        if (moveBins) {
            activeBins = bins;
            activeStartTick = startTick;
        }

        //Get bin info.
        //token0WeightSum and token1WeightSum represent amount of value we want allocated to each token.
        //currentTickBinIndex is which bin contains the currentTick. contestedT0Weight is fraction of bin containing currentTick which will be allocated to token0, going by value.
        BinInfo memory binInfo;
        (binInfo.token0WeightSum, binInfo.token1WeightSum, binInfo.currentTickBinIndex, binInfo.contestedT0Weight) = 
            getWeightSums(bins, startTick, currentTick, sqrtPriceX96, token1PerToken0X128);
    
        //Now that fees are handled, token balances at this point represent principal + fees, i.e. tokens previous LP owners own.

        //Calculate total number of LPTs--all LPTs which have a claim to the vault.
        vaultState.totalLPTs = totalSupply() + claimableLPT + LPTqueuedForWithdrawal;
        vaultState.t0ValuePerLPT = _getT0PerLPT(token1PerToken0X128, vaultState.totalLPTs, vaultState.finalToken0Balance, vaultState.finalToken1Balance);   //Total worth of an LPT

        //Bring in new deposits.
        (vaultState.newToken0Balance, vaultState.newToken1Balance, vaultState.totalLPTs) = 
            handleDeposits(vaultState.finalToken0Balance, vaultState.finalToken1Balance, vaultState.totalLPTs, vaultState.t0ValuePerLPT, token1PerToken0X128);
        
        //Set necessary values for the following functions
        vaultState.TVLinT0 = vaultState.newToken0Balance + vaultState.newToken1Balance * X128 / token1PerToken0X128;
        vaultState.desiredToken0Amount = vaultState.TVLinT0 * binInfo.token0WeightSum / (binInfo.token0WeightSum + binInfo.token1WeightSum);
        vaultState.LPTqueuedForWithdrawal = LPTqueuedForWithdrawal;
        vaultState.valueInT0ToWithdraw = vaultState.LPTqueuedForWithdrawal * vaultState.t0ValuePerLPT / PRECISION;            //Total worth of LPTs being withdrawn
        bool swapOccurred;                                                                                                      //Whether or not balancePositions swapped

        //TODO important: add in uniswap fees to make sure we're swapping correct amount, rather than slightly too little (because fees eat some)
        //Calculate current token ratio and desired token ratio. Calculate amount that can be sent to withdrawals (rather than rebalanced). Swap anything above that, so vault has the right
        //distribution of tokens.
        (vaultState.newToken0Balance, vaultState.newToken1Balance, swapOccurred) = 
            balancePositions(vaultState.newToken0Balance, vaultState.newToken1Balance, vaultState.valueInT0ToWithdraw, vaultState.desiredToken0Amount, token1PerToken0X128, sqrtPriceX96); 

        //Recalculate these figures since some value will be lost if positions are balanced.
        if (swapOccurred)   {
            //Since we've rebalanced, the pool currentTick may have been altered. If it has, recalculate price ratio and bin weights.

            (sqrtPriceX96, currentTick,,,,,) = POOL.slot0();
            token1PerToken0X128 = getToken1PerToken0X128(sqrtPriceX96);

            (binInfo.token0WeightSum, binInfo.token1WeightSum, binInfo.currentTickBinIndex, binInfo.contestedT0Weight) = 
                getWeightSums(bins, startTick, currentTick, sqrtPriceX96, token1PerToken0X128);

            //TVL and other values must be recalculated, since the swap will lose some TVL to fees and possibly change valuation due to price change.
            //TODO gas effiency: not all of these are needed.
            //TODO gas efficiency: replace desiredToken0Amount (here and especially in balancePositions) with desiredRatio.
            vaultState.TVLinT0 = vaultState.newToken0Balance + vaultState.newToken1Balance * X128 / token1PerToken0X128;
            vaultState.t0ValuePerLPT = _getT0PerLPT(token1PerToken0X128, vaultState.totalLPTs, vaultState.newToken0Balance, vaultState.newToken1Balance);  
            vaultState.valueInT0ToWithdraw = vaultState.LPTqueuedForWithdrawal * vaultState.t0ValuePerLPT / PRECISION;
        }

        //Take withdrawals from vault, rebalancing in the process
        (vaultState.newToken0Balance, vaultState.newToken1Balance) = handleWithdrawals(
            vaultState.valueInT0ToWithdraw, vaultState.newToken0Balance, vaultState.newToken1Balance, binInfo.token0WeightSum, binInfo.token1WeightSum,
            token1PerToken0X128, vaultState.LPTqueuedForWithdrawal);

        //Now that deposits, withdrawals, etc. have happened, report previous final balance and new starting balance. Used to track vault performance.
        emit VaultReport(vaultState.finalToken0Balance, vaultState.finalToken1Balance, vaultState.newToken0Balance, vaultState.newToken1Balance);

        //Put resulting liquidity back into bins
        //TODO gas efficiency: pass in vaultState.newToken0Balance and vaultState.newToken1Balance
        //TODO: track the amount actually deposited into each bin. Possibly put that amount into a float somewhere, but definitely use it for the vaultReport.
        //TODO: make sure that amount actually deposited is always very close to what it should be, or add a way to access funds that aren't deposited.
        setBins(bins, binInfo, uint160(sqrtPriceX96));
    }

    //For each bin: pulls fees and liquidity into tokensOwed, then withdraws tokensOwed to this contract.
    // TODO: if not all bins have liquidity, make sure to only call this for bins with liquidity. We're assuming for now that all bins passed have some liquidity, though.
    function drainAll() internal returns(uint256 t0Amount, uint256 t1Amount)  { //should make external when governance modifier is created and available
        //SLOAD relevant variables
        int numBins = int(activeBins.length);
        int24 _startTick = activeStartTick;
        int24 _ticksPerBin = int24(ticksPerBin);

        uint256 t0FeesEarned;
        uint256 t1FeesEarned;

        for (int24 i = 0; i < numBins; i++)   {
            //Get relevant parameters
            int24 tickLower = _startTick + (i * _ticksPerBin);
            int24 tickUpper = tickLower + _ticksPerBin;

            (uint128 binLiquidity, uint256 t0FeeGrowthPerLiquidity, uint256 t1FeeGrowthPerLiquidity, , ) = POOL.positions(keccak256(abi.encodePacked(address(this), tickLower, tickUpper)));
            t0FeesEarned += t0FeeGrowthPerLiquidity * binLiquidity;
            t1FeesEarned += t1FeeGrowthPerLiquidity * binLiquidity;

            //Burn, pulling fees and liquidity into tokensOwed
            //TODO gas efficiency: I'm almost positive liquidity has to be tracked here, but try passing in uint128.max just to make sure
            POOL.burn(tickLower, tickUpper, binLiquidity);

            //Collect, pulling tokensOwed into this contract
            (uint256 _t0Amount, uint256 _t1Amount) = POOL.collect(address(this), tickLower, tickUpper, type(uint128).max, type(uint128).max);
            t0Amount += _t0Amount;
            t1Amount += _t1Amount;
        }

        //Send percentage of fees to steer and strategist
        //TODO gas efficiency: this is a good candidate for some golfing--also possibly rework how fees are distributed, and make it just 1 function to claim them.
        if (t0FeesEarned > 0)   {
            strategistPromisedT0 += t0FeesEarned * strategistFee / PRECISION;
            steerPromisedT0 += t0FeesEarned * steerFee / PRECISION;
            t0Amount *= (PRECISION - (strategistFee + steerFee) / PRECISION);
        }
        if (t1FeesEarned > 0)   {
            strategistPromisedT1 += t1FeesEarned * strategistFee / PRECISION;
            steerPromisedT1 += t1FeesEarned * steerFee / PRECISION;
            t1Amount *= (PRECISION - (strategistFee + steerFee) / PRECISION);
        }
    }

    /**
     * @param bins is uniswap v3 relevant positions. This will be activeBins if bins is not changing, and new bins if bins are changing.
     * @param startTick is lower tick of first bin in bin array.
     * @param currentTick is slot0 current tick.
     * @param sqrtPriceRatioX96 is the sqrtPriceRatio at the currentTick.
     * returns token0Weight: weight of vault which we want in token0, based on bins and where the currentTick is. Is multiplied by precision.
     * returns token1Weight: weight of vault which we want in token1. If token0Weight is 20 and token1Weight is 80, we want 20% of the VALUE of the vault liquidity to be in token0,
        and 80% to be in token1. Is multiplied by precision.
     * returns currentTickBinIndex: which bin contains the pool currentTick, if any.
     * returns contestedT0Weight: Inside the bin containing currentTick, this is the fraction of value desired in token0, multiplied by precision.
     */
    function getWeightSums(uint32[] memory bins, int24 startTick, int24 currentTick, uint256 sqrtPriceRatioX96, uint256 token1PerToken0X128)
    internal 
    view
    returns(uint256 token0Weight, uint256 token1Weight, uint24 currentTickBinIndex, uint256 contestedT0Weight)   {
        uint256 arrayLength = bins.length;
        //If the currentTick is lower than the startTick, all bins are above it, and thus all weight should be in token0
        if (startTick > currentTick)   {
            token0Weight = PRECISION;
        } else   {
            //This casting is safe because currentTick is guaranteed >= startTick
            //Calculate currentTickBinIndex, which represents which bin contains the currentTick.
            currentTickBinIndex = uint24(currentTick - startTick) / ticksPerBin;

            //If the currentTick is above the last bin's upper tick, all bins are below it, and thus all weight should be in token1
            if (currentTickBinIndex >= arrayLength)   {
                token1Weight = PRECISION;

            } else   {  //We know that the bin containing the current tick is inside the provided tick range
                
                //If we're adding liquidity to the bin containing the current tick, add the fraction of liquidity inside that bin devoted to each token to the weights of each token
                if (bins[currentTickBinIndex] > 0)   {
                    //Sqrt ratio at lower tick of bin which contains currentTick
                    uint160 sqrtPriceLowerX96 = TickMath.getSqrtRatioAtTick(startTick + int24((currentTickBinIndex * ticksPerBin)));

                    //Sqrt ratio at upper tick of bin which contains currentTick
                    uint160 sqrtPriceUpperX96 = TickMath.getSqrtRatioAtTick(startTick + int24((currentTickBinIndex + 1) * ticksPerBin));

                    //TODO gas efficiency: possibly create a good proxy, since this is pretty expensive gas-wise and the results aren't usually going to be precise anyways.
                    uint256 amt0Needed = sqrtPriceUpperX96 - sqrtPriceRatioX96;
                    uint256 amt1Needed = sqrtPriceRatioX96 - sqrtPriceLowerX96;
                    contestedT0Weight = (PRECISION * amt0Needed) / (amt0Needed + (amt1Needed * X128 / token1PerToken0X128));
                    token0Weight += contestedT0Weight;
                    token1Weight += PRECISION - contestedT0Weight;  

                    //Library method
                    // uint256 amount0Needed = SqrtPriceMath.getAmount0Delta(uint160(sqrtPriceRatioX96), sqrtPriceUpperX96, 1e21, true);
                    // uint256 amount1Needed = SqrtPriceMath.getAmount1Delta(sqrtPriceLowerX96, uint160(sqrtPriceRatioX96), 1e21, true);
                    // contestedT0Weight = PRECISION * amount0Needed / (amount0Needed + (amount1Needed * X128 / token1PerToken0X128));
                    // console.log('Library t0Weight: %d', contestedT0Weight);
                    //token0Weight += contestedT0Weight;
                    //token1Weight += PRECISION - contestedT0Weight;  
                    //contestedT0Weight represents fraction of bin value which should be allocated to token0, multiplied by precision. The rest of the bin's value should go to token 1.
                }

                //Add weights for bins below current tick to token1Weight
                for (uint256 i = 0; i < currentTickBinIndex; i++){
                    token1Weight += uint256(bins[i]) * ticksPerBin * PRECISION;
                }

                //Add weights for bins above current tick to token0Weight
                for (uint256 j = currentTickBinIndex + 1; j < arrayLength; j++)   {
                    token0Weight += uint256(bins[j]) * ticksPerBin * PRECISION; 
                }
            }
        }
    }

    /**
     * @dev Updates vault to bring deposits into free token0 and token1, setting aside LP tokens equivalent to the deposit at the current price of token1 / token0.
     * @param token0Balance is how much free token0 (token0 the vault has not set aside for withdrawals etc.) the vault has.
     * @param token1Balance is how much free token1 the vault has.
     * @param totalLPTs is totalLPTs that have a claim on vault balance. Includes LPTs not yet claimed.
     * @param t0PerLPT is total value of an LPT in terms of t0.
     * @param token1PerToken0X128 is how much token 1 is worth 1 token0, multiplied by 2^128.
     * @return (uint256, uint256, uint256) token0Balance, token1Balance, newLPT. 
     *      These represent the new amounts of token0 and token1 to be deposited into uniswap, and the total LPT's with a claim on them.
     */
    function handleDeposits(uint256 token0Balance, uint256 token1Balance, uint256 totalLPTs, uint256 t0PerLPT, uint256 token1PerToken0X128) 
    internal 
    returns (uint256, uint256, uint256)   {
        //Handle deposits
        //SLOADS for gas efficiency
        uint256 _depositedToken0 = depositedToken0;
        uint256 _depositedToken1 = depositedToken1;

        //Updating LPTNum, and token0Balance and token1Balance, to add funds and LP tokens from new deposits. 
        uint256 LPTadded = (_depositedToken0 + (_depositedToken1 * X128 / token1PerToken0X128)) * PRECISION / t0PerLPT;
        totalLPTs += LPTadded;
        claimableLPT += LPTadded;

        //TODO gas efficiency: possibly do the entire update at the same time in tend. Also pass in currentTransactionIndex, and just make this more efficient
        transactions[currentTransactionIndex].depositT0ValuePerLPT = t0PerLPT;
        transactions[currentTransactionIndex].depositT1ValuePerLPT = t0PerLPT * token1PerToken0X128 / X128;

        //User-deposited funds are now available for depositing into uniswap
        token0Balance += _depositedToken0;
        token1Balance += _depositedToken1;

        //Reset tokens set aside for deposits to 0, since they're now in the system. Updating queueDeposit index etc. will come later.
        depositedToken0 = 0;
        depositedToken1 = 0;

        return (token0Balance, token1Balance, totalLPTs);
    }

     //TODO: make it check and see whether a rebalance is worth it
     /**
      * @dev rebalances held token ratio to match target ratio
      * @param token0Amount is loose token0, i.e. amount we want put back into the pool
      * @param token1Amount is loose token1, amouunt we want to put back into the pool
      * @param token1PerToken0X128 is current ratio of t0 to t1 inside the pool
      * @return (uint256, uint256, bool) token0Amount, token1Amount, swapOccurred
      */
    function balancePositions(uint256 token0Amount, uint256 token1Amount, uint256 valueInT0ToWithdraw, 
    uint256 desiredToken0Amount, uint256 token1PerToken0X128, uint256 sqrtPriceX96) 
    internal 
    returns (uint256, uint256, bool)   {
        bool swapOccurred;

        //First, use weightings and currentPrice to figure out which token needs to be swapped into which to get the optimal ratio. 
        //tokenIn is token to be swapped for tokenOut.
        //TODO: handle case where both weights are zero--maybe that could be the emergency out.
        
        //TODO: add buffer here. If amount to be swapped is less than buffer, don't swap it.
        //Handle swaps. If amount to swap > what withdrawals can handle, execute a swap for the excess. Withdrawals can handle valueInT0ToWithdraw / 2.
        if (desiredToken0Amount > token0Amount)   {
            //Missing t0 is the amount of t0 that needs to be swapped for t1.
            uint256 missingT0 = desiredToken0Amount - token0Amount;

            //Since t0 is to be swapped into t1, each swapped t0 will have the effect of BOTH adding 1 to t1 and subtracting 1 from t0. Withdrawing that t0 instead of swapping it
            //will thus have half the effect on rebalancing. Down below we check and see whether total withdrawals can, on their own, rebalance the vault. If not, we execute a swap
            //with the excess that withdrawals can't handle.
            if (missingT0 > valueInT0ToWithdraw / 2)   {
                //We know that withdrawals will handle valueInT0ToWithdraw / 2, so we only need to swap the excess (total imbalance minus balance withdrawals can take)
                missingT0 -= valueInT0ToWithdraw / 2;

                //Missing VALUE of t0 in terms of t1--we swap this to get missingT0
                uint256 t1ToSwap = (token1PerToken0X128 / X128) * missingT0;

                //Add HALF of swap fees and allowable slippage to this value, so that we're actually swapping enough
                //We add half because, while the other half of those fees will still be taken out of the input token, we're not swapping an equal amount of it into the output token,
                    //so really half the value of the fees and slippage is coming from the output token, which keeps the ratio correct.
                t1ToSwap *= (1 + (FEE / (2 * 1000000) + (SWAP_SLIPPAGE / 2) / PRECISION));
                if (t1ToSwap > token1Amount)   {
                    t1ToSwap = token1Amount;
                }

                //Execute a swap swapping exactOutput missingT0 from token1 to token0.
                //TODO: check that these casts always work
                //TODO: make sure multiply current sqrt price by (1 + swapSlippage / 2) is a good proxy for sqrt of total allowable slippage
                uint256 sqrtPriceSlippageCheck = sqrtPriceX96 * (PRECISION + (SWAP_SLIPPAGE / 2)) / PRECISION;

                //Set some aside, since this swap will move sqrtPriceX96 and mean that the bin containing currentTick wants more t1 than we are currently computing.
                //t1ToSwap *= (PRECISION - SWAP_SLIPPAGE * 10) / PRECISION;
                t1ToSwap = 98 * t1ToSwap / 100;
                token1Amount -= t1ToSwap;
                token0Amount += executeSwap(int256(t1ToSwap), false, sqrtPriceSlippageCheck);
                swapOccurred = true;
            }

        //If we have more token0 than we want, swap some token0 into token1 (through either swaps or withdrawals)
        } else if (token0Amount > desiredToken0Amount)   {
            //Calc surplusT0 
            uint256 surplusT0 = token0Amount - desiredToken0Amount;

            //If withdrawals can't handle surplus T0, send excess (above amount withdrawals they can handle) to be swapped
            if (surplusT0 * 2 > valueInT0ToWithdraw)   {
                //Only swap the amount withdrawals aren't handling
                surplusT0 -= valueInT0ToWithdraw / 2;

                uint256 t0ToSwap = surplusT0 * (1 + (FEE / (2 * 1000000) + (SWAP_SLIPPAGE / 2) / PRECISION));
                if (t0ToSwap > token0Amount)   {
                    t0ToSwap = token0Amount;
                }

                uint256 sqrtPriceSlippageCheck = sqrtPriceX96 * (PRECISION - (SWAP_SLIPPAGE / 2)) / PRECISION;

                //Set some aside, since this swap will move sqrtPriceX96 and mean that the bin containing currentTick wants more t0 than we are currently computing.
                //t0ToSwap *= (PRECISION - SWAP_SLIPPAGE * 10) / PRECISION;
                //Execute a swap swapping exactOutput surplusT0 from token0 to token1.
                //TODO: make sure this casting, and all others, is safe
                t0ToSwap = 98 * t0ToSwap / 100;
                token0Amount -= t0ToSwap;
                token1Amount += executeSwap(int256(t0ToSwap), true, sqrtPriceSlippageCheck);
                swapOccurred = true;
            }
        }
        return (token0Amount, token1Amount, swapOccurred);
    }

    /**
     * @param _amountIn is amount we're inputting to the swap.
     * @param _zeroForOne whether we're swapping token0 for token1 or the other way around.
     * @param _sqrtPriceLimit is the sqrtPriceX96 LIMIT. We use it as a slippage check--if, during the swap, the current tick PASSES this limit, it reverts.
        Must be less than current sqrtPriceX96 if we're swapping zero for one, greater if we're swapping one for zero.
     * @return amountIn amount of amountIn token used to get that amountOut.
     */
    function executeSwap(int256 _amountIn, bool _zeroForOne, uint256 _sqrtPriceLimit) 
    internal 
    returns(uint256)   {
        //Keep SWAP_SLIPPAGE of what's being swapped, since after we swap we'll want more tokens in what we swapped from due to the pool moving.
        //TODO: possibly find a better way to do this--we won't need 5% if there's no liquidity position covering the current tick.
        //TODO: make sure this is always sufficient.
        (int256 amount0Change, int256 amount1Change) = POOL.swap(
            address(this),
            _zeroForOne,
            _amountIn,
            uint160(_sqrtPriceLimit),
            ""
        );

        //Return the amount received back from the swap, after verifying that it is enough/within allowable slippage levels.
        if (_zeroForOne)   {    //Whichever was inputted, the delta of the other in the pool will always be negative, so it's safe to uint the negation of it.
            return uint(-amount1Change);
        } else {
            return uint(-amount0Change);
        }
    }


    //NEW
    /**
     * @dev updates current transaction with LP valuation info, including correct amount of tokens to withdraw given a queued withdrawal.
     * @param valueInT0ToWithdraw is total amount being withdrawn from the vault
     * @param token0Amount is amount of free token0 in the vault
     * @param token1Amount is amount of free token1 in the vault
     * @param token1PerToken0X128 is current ratio of t1 to t0 based on uniswap pool
     * @param _LPTqueuedForWithdrawal is the amount of LP tokens that have been queued for withdrawal, and will be handled here
     * @return (uint256, uint256) token0Amount and token1Amount, representing what's left to the contract to reinvest after withdrawals are handled.
     */
    function handleWithdrawals(uint256 valueInT0ToWithdraw, uint256 token0Amount, uint256 token1Amount, uint256 t0Weight, uint256 t1Weight, 
    uint256 token1PerToken0X128, uint256 _LPTqueuedForWithdrawal)
    internal
    returns (uint256, uint256)   {
        // Withdraw tokens if there are any to withdraw, helping to rebalance the pool in the process.
        if (valueInT0ToWithdraw > 0)   {
            uint256 t0ToWithdraw;
            uint256 t1ToWithdraw;

            //Brackets to deal with stack
            {
                // TODO gas efficiency: since this is only used once, we're just putting it inside TVLinT0AfterWithdrawals to save stack spots. 
                    // Might be slightly more efficient to pass it in as an argument though.
                // uint256 TVLinT0 = token0Amount + token1Amount * X128 / token1PerToken0X128;
                uint256 TVLinT0AfterWithdrawals = (token0Amount + token1Amount * X128 / token1PerToken0X128) - valueInT0ToWithdraw;
                uint256 amount0AfterWithdrawals = TVLinT0AfterWithdrawals * t0Weight / (t0Weight + t1Weight);
                uint256 amount1AfterWithdrawals = (TVLinT0AfterWithdrawals - amount0AfterWithdrawals) * token1PerToken0X128 / X128;
                if (amount0AfterWithdrawals >= token0Amount)   {    //If token surplus >= withdrawal amount, we'll withdraw everything from the token with the surplus.
                    t1ToWithdraw = valueInT0ToWithdraw * token1PerToken0X128 / X128;
                } else if (amount1AfterWithdrawals >= token1Amount)   {
                    t0ToWithdraw = valueInT0ToWithdraw;
                } else {                                            //Otherwise it will come from both tokens evenly
                    t0ToWithdraw = token0Amount - amount0AfterWithdrawals;
                    t1ToWithdraw = token1Amount - amount1AfterWithdrawals;
                }
            }

            //Credit withdrawers their due
            transactions[currentTransactionIndex].withdrawT0EarnedPerLPT = t0ToWithdraw * PRECISION / _LPTqueuedForWithdrawal;
            transactions[currentTransactionIndex].withdrawT1EarnedPerLPT = t1ToWithdraw * PRECISION / _LPTqueuedForWithdrawal;

            console.log('token0Amount: %d. t0ToWithdraw: %d', token0Amount, t0ToWithdraw);
            console.log('token1Amount: %d. t1ToWithdraw: %d', token1Amount, t1ToWithdraw);

            //Subtract this withdrawn amount from total discretionary tokens in vault
            token0Amount -= t0ToWithdraw;
            token1Amount -= t1ToWithdraw;

            //Add withdrawn amount to promised tokens
            promisedToken0 += t0ToWithdraw;
            promisedToken1 += t1ToWithdraw;

            //Now that LPTs queued for withdrawal have been withdrawn, set it to 0
            LPTqueuedForWithdrawal = 0;
        } //No else--if there are no withdrawals, there's no need to record how much each withdrawal is worth.

        //Update currentTransactionIndex now that withdrawals and deposits for this transaction index have been handled.
        //TODO gas efficiency: possibly make this not happpen if there are also no deposits happening.
        currentTransactionIndex++;
        return (token0Amount, token1Amount);
    }

    /**
     * @dev handles both setting new bins and redepositing into existing ones.  
     * @param bins is current vault liquidity positions
     * @param binInfo is following info:
     * param token0WeightSum is proportion of value we want in token0
     * param token1WeightSum is proportion of value we want in token1
     * param currentTickBinIndex is the bin containing the current tick
     * param contestedT0Weight is fraction of value of bin containing currentTick, multiplied by precision, which should be devoted to token0
     * TODO: fix this so that it tracks how much actually makes it into the pool. Use that to inform actual finalBalances.
     */
    function setBins (uint32[] memory bins, BinInfo memory binInfo, uint160 sqrtPriceX96)
    internal  {
        //TODO gas efficiency: pass in token0 and 1 available.
        uint256 initToken0Available = looseToken0Balance();
        uint256 initToken1Available = looseToken1Balance();

        //TODO gas efficiency: SLOAD this and other things if possible.
        int24 startTick = activeStartTick;
        int24 _ticksPerBin = int24(ticksPerBin);  //SLOAD for gas efficiency. TODO gas efficiency: probably make this an input parameter to avoid even 1 SLOAD. Same with startTick, maybe.

        //TODO gas efficiency: find a better way to calc sqrtPriceRatios for each tick.

        //For each bin, if it has liquidity, calculate how much liquidity it needs in each token and then deposit that liquidity into uniswap in the bin's corresponding liquidity position.
        //TODO: if liquidity amounts are variable for currentBin or other bins, make remaining bins accept liquidity from remaining total, not from their allotted total. This means more
        //liquidity ends up being deposited. Only do this if the amounts can possibly be significant at all.
        //TODO gas efficiency: make i and bins.length ints, if there is space in the stack.
        for (uint24 i = 0; i < bins.length; i++) {
            if (bins[i] > 0)   { //Check if we want any liquidity in this bin. If not, skip to the next one.
                //Set amounts based on bin weightings
                uint128 liquidity;

                int24 lowerTick = startTick + int24(i) * _ticksPerBin;
                int24 upperTick = lowerTick + _ticksPerBin;

                //Allocate liquidity based on bin weights

                //For bins below currentTick, all liquidity should be in token1
                if (i < binInfo.currentTickBinIndex)   {
                    uint256 token1ToDeposit = bins[i] * PRECISION * initToken1Available / binInfo.token1WeightSum;
                    liquidity = LiquidityAmounts.getLiquidityForAmount1(TickMath.getSqrtRatioAtTick(lowerTick), TickMath.getSqrtRatioAtTick(upperTick), token1ToDeposit);

                //For bins above currentTick, all liquidity should be in token0. The OR takes care of case when binInfo.currentTickBinIndex would be negative.
                } else if (i > binInfo.currentTickBinIndex || binInfo.token1WeightSum == 0)   { 
                    uint256 token0ToDeposit = bins[i] * PRECISION * initToken0Available / binInfo.token0WeightSum;
                    liquidity = LiquidityAmounts.getLiquidityForAmount0(TickMath.getSqrtRatioAtTick(lowerTick), TickMath.getSqrtRatioAtTick(upperTick), token0ToDeposit);
                } else   {//For the bin which contains currentTick, liquidity should be split between token0 for ticks above currentTick and token1 for ticks below currentTick.
                    uint256 token0ToDeposit = bins[i] * initToken0Available * binInfo.contestedT0Weight / (binInfo.token0WeightSum);
                    uint256 token1ToDeposit = bins[i] * initToken1Available * (PRECISION - binInfo.contestedT0Weight) / (binInfo.token1WeightSum);
                    //TODO gas efficiency: possibly store sqrtRatios at ticks so that we don't have to call this so many times--we're calling it 2n times where n is # of bins, and we could call it (n+1) times only.
                    liquidity = LiquidityAmounts.getLiquidityForAmounts(sqrtPriceX96, TickMath.getSqrtRatioAtTick(lowerTick), TickMath.getSqrtRatioAtTick(upperTick), token0ToDeposit, token1ToDeposit);
                }

                //TODO: add escape valve. If for some reason uniswap is offering horrible slippage--maybe the pool is mostly dead--there should be a way to go through with it anyways to get funds back
                
                console.log('Liquidity: %d', liquidity);

                POOL.mint(
                    address(this),
                    lowerTick,
                    upperTick,
                    liquidity / 1e18,
                    ""                  //empty bytes array
                );
            }
        }
    }

