    /**
     * @dev internal function which returns amount of t0 one LPT is worth, multiplied by precision.
     * @param token1PerToken0X128 is price of token1 / token0, multiplied by 2^128
     * @param totalLPTs is total LPTs with a claim to vault holdings
     * @param token0Amount is vault holdings of token0
     * @param token1Amount is vault holdings of token1
     * @return t0PerLPT amount of t0 1 LPT is worth, multiplied by precision.
     */
    function _getT0PerLPT(uint256 token1PerToken0X128, uint256 totalLPTs, uint256 token0Amount, uint256 token1Amount) 
    internal pure returns(uint256)   {
        if (totalLPTs == 0)   {
            return PRECISION;
        }
        return (token0Amount + (token1Amount * X128 / token1PerToken0X128)) * PRECISION / totalLPTs;
    }

    /**
     * @dev internal function used solely for code readability; converts sqrtPriceX96 into the actual ratio of token1/token0 * 2^128
     * @return token1PerToken0X128 is price of token1 / token0, multiplied by 2^128
     */
    function getToken1PerToken0X128(uint256 sqrtPriceX96) internal pure returns(uint256 token1PerToken0X128)   {
        token1PerToken0X128 = (sqrtPriceX96 >> 32)**2;
    }

    /**
     * @dev gets TWAP over last 45 seconds. Used to compare TWAP tick to current tick and make sure price hasn't changed too much within that timeframe.
     * @return TWAP tick
     */
    function getTWAPTick() public view returns (int24) {
        int32 twapInterval = 45; //@todo move to state var
        uint32[] memory secondsAgos = new uint32[](2);
        secondsAgos[0] = uint32(twapInterval);//From 45 seconds ago
        secondsAgos[1] = 0;
        
        //tickCumulatives is basically where the tick was 45 seconds ago
        (int56[] memory tickCumulatives, ) = POOL.observe(secondsAgos);

        //Convert tickCumulative to average tick over that timeframe
        return int24( (tickCumulatives[1] - tickCumulatives[0]) / twapInterval );  
    }

