import "./interfaces/external/IUniswapV2Pair.sol";
import "./interfaces/external/IUniswapV2Router02.sol";
import "./interfaces/external/IUniswapV2Factory.sol";

/*
    MetisEarthToken

    - Max supply: 1000
    - Initial supply: 200 ( 20% )
    - Team allocation: 150 ( 15% locked 3 months )
    - Airdrop 5%
    - 5% dev tax on each sell tx
    - 5% auto lp tax on each sell tx
*/

contract MetisEarthToken is ERC20, Ownable {
    using EnumerableSet for EnumerableSet.AddressSet;
    using SafeMath for uint;

    uint256 public constant MAX_SUPPLY = 1000 * 10 ** 9;

    IUniswapV2Pair public immutable pair;
    IUniswapV2Router02 public immutable router;
    uint256 public swapTokensAtAmount = 0.5 * 10 ** 9;

    address public rewarder;

    uint256 public liquidityFees = 5;
    uint256 public devFee = 5;
    uint256 public totalFees = liquidityFees.add(devFee);
    
    bool public swapping = false;

    EnumerableSet.AddressSet private excludedFromFees;
    EnumerableSet.AddressSet private automatedMarketMakerPairs;

    // EVENTS
    event ExcludedFromFees(address indexed addr);
    event IncludedToFees(address indexed addr);
    event RewarderChange(address indexed newRewarder);
    // ------

    constructor(address _router) ERC20("MetisEarth Token", "MET") {
        _mint(msg.sender, _decimalFrac(150)); // Team allocation
        _mint(msg.sender, _decimalFrac(50)); // Airdrop
        _mint(msg.sender, _decimalFrac(200)); // Initial supply

        IUniswapV2Router02 routerUni = IUniswapV2Router02(_router);
        router = IUniswapV2Router02(routerUni);
        IUniswapV2Pair _pair = IUniswapV2Pair(IUniswapV2Factory(routerUni.factory()).createPair(address(this), routerUni.WETH()));

        automatedMarketMakerPairs.add(address(_pair));

        pair = _pair;
    }

    function _decimalFrac(uint256 a) private view returns (uint256) {
        return a * 10 ** decimals();
    }

    function decimals() public view virtual override returns (uint8) {
        return 9;
    }

    function mint(uint256 tokens) public {
        require(msg.sender == rewarder, "only rewarder is authorized to mint");

        if (totalSupply().add(tokens) > MAX_SUPPLY) {
            tokens = MAX_SUPPLY.sub(totalSupply());
        }

        _mint(rewarder, tokens);
    }

    function setRewarder(address newRewarder) public onlyOwner {
        require(rewarder == address(0), "rewarder already set");

        emit RewarderChange(newRewarder);
        rewarder = newRewarder;
    }

    function excludeFromFees(address addr) public onlyOwner {
        emit ExcludedFromFees(addr);
        excludedFromFees.add(addr);
    }

    function includeToFees(address addr) public onlyOwner {
        emit IncludedToFees(addr);
        excludedFromFees.remove(addr);
    }

    function isExcludedFromFees(address addr) public view returns (bool) {
        return excludedFromFees.contains(addr);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = router.WETH();

        _approve(address(this), address(router), tokenAmount);

        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }


    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        _approve(address(this), address(router), tokenAmount);
        router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0,
            0,
            address(0),
            block.timestamp
        );
    }

    function swapAndLiquify(uint256 tokens) private {
        uint256 half = tokens.div(2);
        uint256 otherHalf = tokens.sub(half);

        uint256 initialBalance = address(this).balance;

        swapTokensForEth(half);

        uint256 newBalance = address(this).balance.sub(initialBalance);

        addLiquidity(otherHalf, newBalance);
    }

    function setSwapTokensAt(uint256 value) public onlyOwner {
        swapTokensAtAmount = value;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        if(amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        uint256 contractTokenBalance = balanceOf(address(this));
        bool canSwap = contractTokenBalance >= swapTokensAtAmount;

        if(canSwap &&
            !swapping &&
            automatedMarketMakerPairs.contains(to) &&
            from != owner() &&
            to != owner()
        ) {
            swapping = true;

            uint256 liquidityTokens = contractTokenBalance.mul(liquidityFees).div(totalFees);
            swapAndLiquify(liquidityTokens);

            uint256 devTokens = balanceOf(address(this));
            transfer(owner(), devTokens);

            swapping = false;
        }

        bool takeFee = !swapping;
        if(excludedFromFees.contains(from) || excludedFromFees.contains(to)) {
            takeFee = false;
        }

        if (takeFee) {
            uint256 fees = amount.mul(totalFees).div(100);
            amount = amount.sub(fees);

            super._transfer(from, address(this), fees);
        }

        super._transfer(from, to, amount);
    }
}

