// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "./interfaces/ITileUrlBuilder.sol";
import "./libraries/Utils.sol";

contract GoogleTileUrlBuilder is ITileUrlBuilder {
    string[] public tileServerUriParts;
    string[] public subdomains;

    constructor() {
        tileServerUriParts = ["https://", ".google.com/kh/v=908?x=", "&y=", "&z="];
        subdomains = ["khms0", "khms1", "khms2", "khms3"];
    }

    function subdomain(uint256 i) public override view returns (string memory) {
        return subdomains[i];
    }

    function subdomainsLength() public override view returns (uint256) {
        return subdomains.length;
    }

    function getTileUrl(
        uint256 x,
        uint256 y,
        uint256 z
    ) public override view returns (string memory) {
        return string(abi.encodePacked(
            tileServerUriParts[0],
            subdomains[0],
            tileServerUriParts[1],
            Utils.toString(x),
            tileServerUriParts[2],
            Utils.toString(y),
            tileServerUriParts[3],
            Utils.toString(z)
        ));
    }

    function getTileServerTemplate() public override pure returns (string memory) {
        return "https://{s}.google.com/kh/v=908?x={x}&y={y}&z={z}";
    }
}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Multicall.sol";

import "./interfaces/ITileUrlBuilder.sol";
import "./GoogleTileBuilder.sol";
import "./libraries/Base64.sol";
import "./libraries/Utils.sol";

contract MetisEarth is ERC721, Ownable, Multicall {
    uint256 public immutable zoomLevel;
    uint256 public immutable mapWidth;
    uint256 public immutable maxSupply;

    ITileUrlBuilder public tileBuilder;

    uint256 public mintPrice;
    uint256 public totalSupply;

    // EVENTS
    event MintPriceChange(uint256 newPrice);
    event Mint(uint256 tokensId);
    event TileBuilderChange(address newTileBuilder);
    // ------

    constructor(uint256 _zoomLevel) ERC721("MetisEarth", "ME") {
        maxSupply = 4 ** _zoomLevel;
        tileBuilder = new GoogleTileUrlBuilder();
        
        // NOTE: should be changed with zoom level by Sqrt(maxSupply)
        mapWidth = 131072;

        zoomLevel = _zoomLevel;
        mintPrice = 0.1 * 10 ** 18;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "no such token");

        (uint256 x, uint256 y) = (tokenId % mapWidth, tokenId / mapWidth);

        string memory json = string(abi.encodePacked(
            '{"name": "MetisEarth Land at (',
            Utils.toString(x),
            '; ',
            Utils.toString(y),
            ')',
            '", "description": "Second Earth Metaverse", "image": "',
            tileBuilder.getTileUrl(x, y, zoomLevel),
            '"}'
        ));

        return string(abi.encodePacked('data:application/json;base64,', Base64.encode(bytes(json))));
    }

    function withdraw(address payable to) public payable onlyOwner {
        to.transfer(address(this).balance);
    }

    function mint(uint256[] memory tokens) public payable {
        require(msg.value == tokens.length * mintPrice, "insufficient funds");

        for (uint i = 0; i < tokens.length; i++) {
            require(tokens[i] <= maxSupply, "maxSupply overflow");

            _safeMint(msg.sender, tokens[i]);
            totalSupply++;

            emit Mint(tokens[i]);
        }
    }

    function setMintPrice(uint256 newPrice) public onlyOwner {
        emit MintPriceChange(newPrice);
        mintPrice = newPrice;
    }

    function setTileBuilder(address newTileBuilder) public onlyOwner {
        emit TileBuilderChange(newTileBuilder);
        tileBuilder = ITileUrlBuilder(newTileBuilder);
    }
}

