        uint256 checkpoint;
    }

    mapping(address => User) public users;

    uint256 public rewardPerSecond;

    uint256 public lastUpdate;
    uint256 public lastCheckpoint;

    MetisEarthToken public immutable token;
    MetisEarth public immutable nft;

    // EVENTS
    event Claim(address indexed claimer, uint256 tokens);
    event RewardPerSecondChange(uint256 newReward);
    // ------

    constructor(address _token, address _erc721) {
        token = MetisEarthToken(_token);
        nft = MetisEarth(_erc721);
        lastUpdate = block.timestamp;
    }

    function _update() private {
        User storage user = users[msg.sender];
        if (user.checkpoint == 0) {
            user.checkpoint = lastCheckpoint;
        }

        if (nft.totalSupply() == 0) {
            return;
        }

        uint256 mint = block.timestamp.sub(lastUpdate).mul(rewardPerSecond);
        
        lastCheckpoint = lastCheckpoint.add(mint).mul(1e18).div(nft.totalSupply());
        lastUpdate = block.timestamp;

        user.earned = user.earned.add(pendingRewardOnCheckpoint(msg.sender));
        user.checkpoint = lastCheckpoint;

        token.mint(mint);
    }

    function pending(address _user) public view returns (uint256) {
        User memory user = users[_user];
        if (user.checkpoint == 0) {
            user.checkpoint = lastCheckpoint;
        }

        uint256 mint = block.timestamp.sub(lastUpdate).mul(rewardPerSecond);
        uint256 _lastCheckpoint = lastCheckpoint.add(mint).mul(1e18).div(nft.totalSupply());

        user.earned = user.earned.add(pendingRewardOnCheckpoint(msg.sender));
        user.checkpoint = _lastCheckpoint;

        return user.earned.sub(user.claimed);
    }

    function setReward(uint256 newReward) public onlyOwner {
        emit RewardPerSecondChange(newReward);
        rewardPerSecond = newReward;
    }

    function pendingRewardOnCheckpoint(address user) private view returns (uint256 reward) {
        User storage _user = users[user];
        uint preReward = lastCheckpoint.sub(_user.checkpoint);
        reward = preReward.mul(nft.balanceOf(user)).div(1e18);
    }

    function claim() public nonReentrant {
        require(nft.balanceOf(msg.sender) > 0, "you don't hold any lands");
        _update();

        User storage user = users[msg.sender];
        uint256 reward = user.earned.sub(user.claimed);

        user.claimed = user.claimed.add(reward);

        token.safeTransfer(msg.sender, reward);
        emit Claim(msg.sender, reward);
    }
}

