import '../../@openzeppelin/contracts/utils/cryptography/ECDSA.sol';
import '../../@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol';
import '../../hardhat/console.sol';
import '../../@openzeppelin/contracts/utils/Address.sol';

/**
 * @dev Simple minimal forwarder to be used together with an ERC2771 compatible contract. See {ERC2771Context}.
 */
contract MinimalForwarder is EIP712 {
  using ECDSA for bytes32;

  struct ForwardRequest {
    address from;
    address to;
    uint256 value;
    uint256 gas;
    uint256 nonce;
    bytes data;
  }

  bytes32 private constant _TYPEHASH =
    keccak256(
      'ForwardRequest(address from,address to,uint256 value,uint256 gas,uint256 nonce,bytes data)'
    );

  mapping(address => uint256) private _nonces;

  constructor() EIP712('MinimalForwarder', '0.0.1') {
    uint256 a = block.chainid;
    console.log('CHain id: ');
    console.log(a);
  }

  function getNonce(address from) public view returns (uint256) {
    return _nonces[from];
  }

  function verify(ForwardRequest calldata req, bytes calldata signature)
    public
    view
    returns (bool)
  {
    bytes32 b =
      keccak256(
        abi.encode(
          _TYPEHASH,
          req.from,
          req.to,
          req.value,
          req.gas,
          req.nonce,
          keccak256(req.data)
        )
      );
    console.log('mess hash on chain. ');
    console.logBytes32(b);
    address signer =
      _hashTypedDataV4(
        keccak256(
          abi.encode(
            _TYPEHASH,
            req.from,
            req.to,
            req.value,
            req.gas,
            req.nonce,
            keccak256(req.data)
          )
        )
      )
        .recover(signature);
    return _nonces[req.from] == req.nonce && signer == req.from;
  }

  function execute(ForwardRequest calldata req, bytes calldata signature)
    public
    payable
    returns (bool, bytes memory)
  {
    console.log('DOM SEP ON CHAIN');
    console.logBytes32(_domainSeparatorV4());
    console.log('Typehash on chain: ');
    console.logBytes32(_TYPEHASH);
    require(
      verify(req, signature),
      'MinimalForwarder: signature does not match request'
    );
    _nonces[req.from] = req.nonce + 1;
    console.log('data on chain');
    console.logBytes(req.data);
    (bool success, bytes memory returndata) =
      req.to.call{gas: req.gas, value: req.value}(
        abi.encodePacked(req.data, req.from)
      );
    // Validate that the relayer has sent enough gas for the call.
    // See https://ronan.eth.link/blog/ethereum-gas-dangers/
    assert(gasleft() > req.gas / 63);
    Address.verifyCallResult(success, returndata, 'BAD CALL');
    return (success, returndata);
  }
}

