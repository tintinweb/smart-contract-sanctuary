pragma solidity 0.8.4;

import "./IRhoStrategy.sol";
import "./IVault.sol";
interface IHorizonConfig {
    struct Underlying {
        int248 amount;
        uint8 decimal;
    }
    struct Asset {
        IVault vault;
        IRhoToken rhoToken;
        IERC20MetadataUpgradeable underlying;
        IVaultConfig vaultConfig;
        Underlying lastSwitchOutAmount;
        Underlying lastSwitchOutCost;
    }
    struct Distribution {
        UnderlyingDistribution[] underlying;
        bytes32[] underlyingIds;
    }
    struct ChainInfo {
        InvestmentInfo invst;
        RhoTokenInfo rho;
        uint256 chainId;
    }
    struct UnderlyingDistribution {
        ChainInfo[] chains;
    }
    struct InvestmentInfo {
        Underlying underlyingUninvested; // in underlying
        Underlying underlyingInvested; // in underlying
        Underlying redeployable;
    }

    struct RhoTokenInfo {
        uint256 adjustedRebasingSupply;
        uint256 totalSupply;
    }

    struct Allocation {
        UnderlyingStrategy[] underlying;
        bytes32[] underlyingIds;
    }
    struct UnderlyingStrategy{
        OptimalStrategy[] strategies;
    }
    struct OptimalStrategy {
        uint256 chainId;
        uint8 optimalIndex;
        uint248 effectiveSupplyRate;
        bytes12 checkByte; // first 12 byte of keccak256(abi.encodePacked(optimalIndex,optimalStrategyAddr))
        address vaultAddr;
    }
    function identifier(uint256) external view returns(bytes32);
    function listIdentifier() external view returns(bytes32[] memory);
    function underlying(bytes32 underlyingId) external view returns(string memory);
    function assets(bytes32) external view returns(Asset memory);
    function isPeerChainRegistered(uint256) external view returns(bool);
    function peerChains(uint256) external view returns(uint256);
    function registerPeerChains(uint256[] calldata chainIds) external;
    function unregisterPeerChains(uint256[] calldata chainIds) external;
    function listPeerChains() external view returns(uint256[] memory);
}
interface IHorizon {

    event ReportDistribution (uint256 indexed flareId, IHorizonConfig.Distribution dist);
    event DividendReceived(uint256 flareId, int256 profit, bytes32 underlyingId);
    // event ReportLocalOptimalAllocation (Allocation);
    // event Rebalanced(Allocation alloc, uint256 flareId);


    // emit an ReportDistribution event per vault
    function config() external view returns(IHorizonConfig);
    function reportDistribution(uint256 flareId_) external returns(IHorizonConfig.Distribution memory);

    // emit an ReportOptimalStrategy event per vault
    function reportLocalOptimalAllocation(uint256[] calldata globalRedeployable) external returns(IHorizonConfig.Allocation memory);

    function receiveDividend(uint256 flareId_, int256 profit, bytes32 vaultId) external;
    function rebalance(uint256 flareId_, IHorizonConfig.Allocation calldata alloc, IHorizonConfig.Underlying calldata upfrontCost) external;
    function optimalStrategy(bytes32 identifier_) external view returns(uint256 chainId, uint8 optimalIndex, uint248 effectiveSupplyRate, bytes12 checkByte, address vaultAddr);

    // function jointAtFlareId() external view returns(uint256);
    function flareId() external view returns(uint256);
}

// dominant chain receives report and calculate optimal strategy, and emit event
interface IHorizonDominant is IHorizon {
    struct Stats {
        int256 globalSupply;
        int256 rhoRebasing;
        int256 profitInRho;
    }
    event Flare(uint256 flareId, uint256[] peerChains);
    event DistributeDividend(uint256 indexed flareId, uint256 indexed toChainId, bytes32 indexed underlyingId, int256 profit);
    event Rebalance(IHorizonConfig.Allocation alloc, uint256 flareId);
    event DistributeDividedFinished(uint256 flareId, Stats[] stats);
    function flare() external;
    function distributeDividend(uint256 flareId, IHorizonConfig.Distribution calldata dist) external;
}

