    );
    
    function safeMint(string memory amendmentText,
                      string memory authorAvatarURI,
                      string memory authorDisplayName,
                      string memory textColor,
                      string memory backgroundColor,
                      string memory typeface
    ) public returns (bool) {
        require(bytes(amendmentText).length > 0, "Amendment text cannot be empty");
        
        uint cost = amendmentCost(amendments.length);
        
        require(currency.balanceOf(msg.sender) >= cost, "Not enough PFP tokens");
        
        Amendment memory newAmendment = Amendment({
            author: msg.sender,
            amendmentText: amendmentText,
            authorAvatarURI: authorAvatarURI,
            authorDisplayName: authorDisplayName,
            textColor: textColor,
            backgroundColor: backgroundColor,
            typeface: typeface
        });
        
        amendments.push(newAmendment);
        
        _tokenIdCounter.increment();
        uint tokenId = _tokenIdCounter.current();
        
        _safeMint(msg.sender, tokenId);
        
        currency.burnFrom(msg.sender, cost);
        
        emit AmendmentMinted(tokenId, msg.sender, amendmentText);
        
        return true;
