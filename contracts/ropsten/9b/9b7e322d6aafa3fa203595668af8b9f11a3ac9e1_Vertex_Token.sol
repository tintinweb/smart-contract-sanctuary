        // update state
        weiRaised = SafeMath.add(weiRaised, msg.value);

        // token.mint(beneficiary, tokens);
        emit TokenPurchase(msg.sender, beneficiary, msg.value, tokens);
        updatePrice();
        // forwardFunds();
    }


