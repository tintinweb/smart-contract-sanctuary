contract CallerAuthenticator {
    using SafeMath for uint256;
    // keccak256 hash of the string "foo"
    bytes32 hash = 0x41b1a0649752af1b28b3dc29a1556eee781e4a4c3a1f7f53f90fa834de098c4d;

    function process(uint256 _preprocessed_id, string memory _password) public view returns (uint256) {
        bool is_authenticated = authenticate(_password);
        require(is_authenticated == true, "Not authenticated.");
        return getTokenID(_preprocessed_id);
    }
    
    function authenticate(string memory _password) public view returns (bool) {
        return keccak256(abi.encodePacked(_password)) == hash;
    }

    function testEncryption(string memory _tester) public pure returns (bytes32) {
        return keccak256(abi.encodePacked(_tester));
    }

    function getTokenID(uint256 _preprocessed_id) public pure returns (uint256) {
        return _preprocessed_id.mod(8888);
    }
}