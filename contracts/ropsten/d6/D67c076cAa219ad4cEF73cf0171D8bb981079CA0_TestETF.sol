import "./IMintOverTimeERC20.sol";

abstract contract TimeMintERC20 is IMintOverTimeERC20 {

    uint public contractCreatedAt;
    uint public lastMintAt;
    uint256 public secondsBetweenMint;
    address public mintToAddress;
    uint256 public mintAmount;

    constructor(string memory name, string memory symbol, address _mintToAddress, uint256 _mintAmount, uint256 _secondsBetweenMint) ERC20(name, symbol) {
        require(_mintToAddress != address(0), "Mint address is 0");
        require(_mintAmount > 0, "Mint amount <= 0");
        require(_secondsBetweenMint > 0, "Seconds between mint <= 0");
        contractCreatedAt = block.timestamp;
        secondsBetweenMint = _secondsBetweenMint;
        mintToAddress = _mintToAddress;
        mintAmount = _mintAmount;
        lastMintAt = block.timestamp;
    }

    function mintsAvailable() public override view returns(uint256) {
        uint calledAt = block.timestamp;
        uint timeSinceLastMint = calledAt - lastMintAt;
        return timeSinceLastMint / secondsBetweenMint;
    }

    function readyToMint() public override view returns(bool) {
        return mintsAvailable() > 0;
    }

    function mint() internal {
        uint256 toMint = mintAmount * mintsAvailable();
        uint calledAt = block.timestamp;
        require(readyToMint(), "Not ready to mint.");
        require(toMint > 0, "Mint is <= 0.");
        lastMintAt = calledAt;
        super._mint(mintToAddress, toMint);
    }

    function _setMintToAddress(address newMintToAddress) internal {
        mintToAddress = newMintToAddress;
    }
}

