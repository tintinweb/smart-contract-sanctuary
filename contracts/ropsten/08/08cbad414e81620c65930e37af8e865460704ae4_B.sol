// File contracts/test/B.sol

pragma solidity ^0.8.0;
interface IEmbody {
    function validTraits(string memory trait) external returns (bool);
}

contract B is Ownable {

    address public immutable nftAddress;

    mapping(uint256 => mapping(string => uint256)) public nftRemintCounter;
    mapping(address => uint256) public userRemintCounter;
    mapping(string => uint256) public traitFees;

    event Reminted(address indexed user, uint256 userRemintCount, uint256 indexed tokenId, string indexed trait, uint256 nftRemintCount);
    event TraitFeeAdded(string trait, uint256 fee);
    event FeeClaimed(uint256 amount, address feeClaimer);

    constructor(address _nftAddress) public Ownable() {
        nftAddress = _nftAddress;
    }

    modifier isNftOwner(uint256 tokenId) {
        require(IERC721(nftAddress).ownerOf(tokenId) == msg.sender);
        _;
    }

    modifier isValidNftTrait(string memory trait) {
        require(IEmbody(nftAddress).validTraits(trait));
        _;
    }

    function remint(uint256 tokenId, string memory trait) external payable isNftOwner(tokenId) isValidNftTrait(trait) {
        require(msg.value >= traitFees[trait]);
        userRemintCounter[msg.sender] = userRemintCounter[msg.sender] + 1;
        nftRemintCounter[tokenId][trait] = nftRemintCounter[tokenId][trait] + 1;
        emit Reminted(msg.sender, userRemintCounter[msg.sender], tokenId, trait, nftRemintCounter[tokenId][trait]);
    }

    function addTraitFee(string memory trait, uint256 fee) external onlyOwner isValidNftTrait(trait) {
        traitFees[trait] = fee;
        emit TraitFeeAdded(trait, fee);
    }

    function claimFee() external onlyOwner { 
        (bool success, ) = owner().call{value : address(this).balance}("");
        require(success);
    }

}