import '../../interfaces/IReserveRateStrategy.sol';
import '../../tools/math/WadRayMath.sol';
import '../../tools/math/PercentageMath.sol';
import '../../interfaces/IPriceOracleProvider.sol';
import '../../interfaces/ILendingRateOracle.sol';
