pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "./IPancakePair.sol";
import "./IAggregator.sol";
import "./Secure.sol";

contract STT is ERC20, Secure {
  using SafeMath for uint256;

  IAggregator internal bnbPair;
  IPancakePair internal sttsPair;

  mapping(address => mapping(address => uint256)) public assetsBalances;

  constructor(
    address dev1,
    address dev2,
    address STTSAddress
  ) payable ERC20("Smart World Token", "STT") {
    bnbPair = IAggregator(0xF7904a295A029a3aBDFFB6F12755974a958C7C25);
    sttsPair = IPancakePair(0xB10cf58E08b94480fCb81d341A63295eBb2062C2);
    devs[dev1] = true;
    devs[dev2] = true;
    devs[_msgSender()] = true;
    owner = _msgSender();
    StockToken = IERC20(STTSAddress);
  }

  function decimals() public view virtual override returns (uint8) {
    return 8;
  }

  // $0.0001 STT value for every 1 BitCoin_ and vice versa!
  function totalBTC() public view returns (uint256) {
    uint256 fromStts = sttsToBnb(sttsBalance(address(this)));
    uint256 fromBnb = address(this).balance;
    return bnbToBtc(fromBnb.add(fromStts));
  }

  function totalSttsAsset() public view returns (uint256) {
    uint256 stts = StockToken.balanceOf(address(this));
    return sttsToBnb(stts);
  }

  function bnbToBtc(uint256 value) public view returns (uint256) {
    uint256 currentPrice = uint256(bnbPair.latestAnswer());
    return value.div(currentPrice);
  }

  function sttsToBnb(uint256 value) public view returns (uint256) {
    return value.mul(10**8).div(sttsToBnbPrice());
  }

  function sttsToBnbPrice() public view returns (uint256) {
    (uint112 _reserve0, uint112 _reserve1, ) = sttsPair.getReserves();
    return uint256(_reserve0).mul(10**18).div(uint256(_reserve1));
  }

  function sttsBalance(address user) public view returns (uint256) {
    return StockToken.balanceOf(user);
  }

  function contractUserBalance(address user) external view onlyFromContract returns (uint256) {
    return assetsBalances[user][_msgSender()];
  }

  function depositAsset(address sender, uint256 value)
    external
    payable
    onlyFromContract
    returns (bool)
  {
    require(msg.value == value, "Error::STBank, deposit must be equal required TRX!");

    assetsBalances[sender][_msgSender()] = assetsBalances[sender][_msgSender()].add(msg.value);

    emit DepositAsset(_msgSender(), sender, msg.value);
    return true;
  }

  function depositStt(address spender, uint256 value)
    external
    onlyFromContract
    returns (bool)
  {
    require(balanceOf(spender) >= value, "Error::STBank, user dosen't have enough STT!");

    transferFrom(spender, address(this), value);

    assetsBalances[spender][_msgSender()] = assetsBalances[spender][_msgSender()].add(value);

    return true;
  }

  function depositStts(address spender, uint256 value)
    external
    onlyFromContract
    returns (bool)
  {
    require(sttsBalance(spender) >= value, "Error::STBank, user dosen't have enough STTS!");

    StockToken.transferFrom(spender, address(this), value);

    assetsBalances[spender][_msgSender()] = assetsBalances[spender][_msgSender()].add(value);

    return true;
  }

  function withdrawAsset(address payable reciever, uint256 interest)
    external
    onlyFromContract
    returns (bool)
  {
    uint256 balance_ = assetsBalances[reciever][_msgSender()];
    require(balance_ != 0, "Error::STBank, No previous deposit!");

    reciever.transfer(balance_);

    if (interest > 0) {
      payWithStt(reciever, interest);
    }

    assetsBalances[reciever][_msgSender()] = 0;

    emit WithdrawAsset(_msgSender(), reciever, balance_);

    return true;
  }

  function withdrawStt(address reciever, uint256 interest)
    external
    onlyFromContract
    returns (bool)
  {
    uint256 balance_ = assetsBalances[reciever][_msgSender()];
    require(balance_ != 0, "Error::STBank, No previous deposit!");

    transfer(reciever, balance_);

    if (interest > 0) {
      payWithStt(reciever, interest);
    }

    assetsBalances[reciever][_msgSender()] = 0;

    emit WithdrawAsset(_msgSender(), reciever, balance_);
    return true;
  }

  function withdrawStts(address reciever, uint256 interest)
    external
    onlyFromContract
    returns (bool)
  {
    uint256 balance_ = assetsBalances[reciever][_msgSender()];
    require(balance_ != 0, "Error::STBank, No previous deposit!");

    StockToken.transfer(reciever, balance_);

    if (interest > 0) {
      payWithStt(reciever, interest);
    }

    assetsBalances[reciever][_msgSender()] = 0;

    emit WithdrawAsset(_msgSender(), reciever, balance_);
    return true;
  }

  function payWithStt(address reciever, uint256 interest)
    public
    onlyFromContract
    returns (bool)
  {
    require(
      assetsBalances[reciever][_msgSender()] != 0,
      "Error::STBank, No previous deposit!"
    );
    super._mint(reciever, interest);
    return true;
  }

  function burnStt(address from, uint256 amount) public onlyFromContract returns (bool) {
    require(assetsBalances[from][_msgSender()] != 0, "Error::STBank, No previous deposit!");
    super._burn(from, amount);
    return true;
  }

  function withdrawBnbByVote(address payable _reciever, uint256 _value)
    public
    voteByDevs(_reciever)
  {
    require(_reciever == owner, "Reciever should be owner.");
    _reciever.transfer(_value);
  }

  function withdrawSttsByVote(address payable _reciever, uint256 _value)
    public
    voteByDevs(_reciever)
  {
    require(_reciever == owner, "Reciever should be owner.");
    StockToken.transfer(_reciever, _value);
  }
}

