pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Context.sol";

interface Events {
  event RevokedContract(address indexed election);
  event AuthorizedContract(address indexed election);
  event ReplaceDev(address indexed newDev, address indexed oldDev);
  event VoteSuccess(address indexed election, address indexed voter);
  event DepositAsset(address indexed by, address indexed user, uint256 amount);
  event WithdrawAsset(address indexed from, address indexed user, uint256 amount);
}

abstract contract Secure is Context, Events {
  using SafeMath for uint256;

  IERC20 internal StockToken;

  struct elect {
    uint256 counter;
    uint256 timer;
    mapping(address => bool) voted;
  }

  address public owner;
  mapping(address => bool) public devs;
  mapping(address => bool) public elected;
  mapping(address => elect) public election;

  modifier onlyDevs() {
    require(devs[_msgSender()], "Secure:: Only from devs!");
    _;
  }

  modifier onlyFromContract() {
    require(elected[_msgSender()], "Secure:: Only from contract!");
    _;
  }

  modifier voteByDevs(address nominated) {
    require(!elected[nominated], "Secure:: This nominate is already voted");
    if (election[nominated].timer < block.timestamp) {
      delete election[nominated];
      election[nominated].timer = block.timestamp.add(2 minutes);
    }
    require(
      !election[nominated].voted[_msgSender()],
      "Secure:: You already vote for this nominate!"
    );
    if (election[nominated].counter == 2) {
      delete election[nominated];
      _;
    } else {
      election[nominated].voted[_msgSender()] = true;
      election[nominated].counter = election[nominated].counter.add(1);
      emit VoteSuccess(nominated, _msgSender());
    }
  }

  function authorizeContract(address smartContract) public onlyDevs voteByDevs(smartContract) {
    elected[smartContract] = true;
    emit AuthorizedContract(smartContract);
  }

  function revokeContract(address smartContract) public onlyDevs voteByDevs(smartContract) {
    elected[smartContract] = !elected[smartContract];
    emit RevokedContract(smartContract);
  }

  function replaceDev(address newDev, address oldDev) public onlyDevs voteByDevs(newDev) {
    delete devs[oldDev];
    devs[newDev] = true;
    elected[newDev] = false;
    emit ReplaceDev(newDev, oldDev);
  }
}

