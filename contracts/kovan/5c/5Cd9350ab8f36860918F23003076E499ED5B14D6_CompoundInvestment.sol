pragma solidity ^0.5.0;

import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/token/ERC20/SafeERC20.sol';
import '@openzeppelin/contracts/math/SafeMath.sol';
import '@openzeppelin/contracts/utils/ReentrancyGuard.sol';
import '@openzeppelin/contracts/ownership/Ownable.sol';
import './interfaces/CTokenInterface.sol';

import "hardhat/console.sol";

contract CompoundInvestment is Ownable, ReentrancyGuard {

    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    
    mapping (address => mapping(address => uint256)) public tokensInvested;    // user-token invest mapping
    mapping (address => uint256) public totalInvested;     // token- total invest mapping
    mapping (address => bool) public cTokenRegistry;     // token-registry mapping

    event AdminClaimed(address indexed admin, address indexed token, uint256 amount);
    event Invest(address indexed user, address indexed token, uint256 amount);
    event Withdraw(address indexed user, address indexed token, uint256 amount);

    constructor () public {}

    modifier registeredCToken(address _cToken) {
        require(cTokenRegistry[_cToken], "CompoundInvestment::registeredCToken: Invalid cToken, not registered");
        _;
    }

    /// @notice Admin registers which Ctokens to invest in compound-finance
    /// @dev Admin registers which Ctokens to invest in compound-finance
    /// @param _cToken address of the cToken from compound-finance
    function registerCToken(address _cToken) external onlyOwner {
        cTokenRegistry[_cToken] = true;
    }

    /// @notice Lends the user's fund into compound-finance
    /// @dev Track user funds and safe check compound's error type while minting cTokens
    /// @param _cToken address of the cToken from compound-finance
    /// @param _amount amount of the underlying token of the respective cToken
    function invest(address _cToken, uint256 _amount) external registeredCToken(_cToken) {
        CTokenInterface cToken = CTokenInterface(_cToken);
        IERC20 token = IERC20(address(cToken.underlying()));

        token.safeTransferFrom(msg.sender, address(this), _amount);
        token.safeApprove(_cToken, _amount);
        uint result = cToken.mint(_amount);
        require(result == 0, "CompoundInvestment::invest: cToken#mint() failed");

        tokensInvested[msg.sender][address(token)] = tokensInvested[msg.sender][address(token)].add(_amount);
        totalInvested[address(token)] = totalInvested[address(token)].add(_amount);

        emit Invest(msg.sender, address(token), _amount);
    }

    /// @notice Withdraws the user's fund from compound-finance
    /// @dev Track user funds and safe check compound's error type while redeeming underlying
    /// @param _cToken address of the cToken from compound-finance
    /// @param _amount amount of the underlying token of the respective cToken
    function withdraw(address _cToken, uint256 _amount) external nonReentrant registeredCToken(_cToken) {
        CTokenInterface cToken = CTokenInterface(_cToken);
        IERC20 token = IERC20(address(cToken.underlying()));

        require(tokensInvested[msg.sender][address(token)] >= 0, "CompoundInvestment::withdraw: Withdrawing more than invested");

        uint256 amount;
        if(_amount > tokensInvested[msg.sender][address(token)]) {
            amount = tokensInvested[msg.sender][address(token)];
        }else{
            amount = _amount;
        }

        uint result = cToken.redeemUnderlying(amount);
        require(result == 0, "CompoundInvestment::withdraw: cToken#redeemUnderlying() failed");

        token.safeTransfer(msg.sender, amount);
        tokensInvested[msg.sender][address(token)] = tokensInvested[msg.sender][address(token)].sub(amount);
        totalInvested[address(token)] = totalInvested[address(token)].sub(amount);

        emit Withdraw(msg.sender, address(token), amount);
    }

    /// @notice Admin can claim the accrued interest on behalf of all user funds
    /// @dev Track user funds and safe check for claimable amount if any
    /// @param _cToken address of the cToken from compound-finance
    function adminClaim(address _cToken) external onlyOwner nonReentrant registeredCToken(_cToken) {
        CTokenInterface cToken = CTokenInterface(_cToken);
        IERC20 token = IERC20(cToken.underlying());
        
        uint256 underLyingAmount = cToken.balanceOfUnderlying(address(this));
        uint claimableAmount = underLyingAmount.sub(totalInvested[address(token)]);

        require(claimableAmount > 0, "CompoundInvestment::adminClaim: No available amount for claim");
        uint result = cToken.redeemUnderlying(claimableAmount);
        require(result == 0, "CompoundInvestment::adminClaim: cToken#redeemUnderlying() failed");

        token.safeTransfer(msg.sender, claimableAmount);

        emit AdminClaimed(msg.sender, address(token), claimableAmount);
    }
}

