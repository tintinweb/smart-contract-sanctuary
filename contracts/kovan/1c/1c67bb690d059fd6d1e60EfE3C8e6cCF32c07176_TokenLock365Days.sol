pragma solidity ^0.6.0;

import "./Ownable.sol";
import "./SafeMath.sol";
import "./IUniswapV2Pair.sol";
import "./SafeERC20.sol";

contract TokenLock365Days is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IUniswapV2Pair;

    IUniswapV2Pair public lpToken = IUniswapV2Pair(address(0));

    constructor(address _lpToken) public {
        require(_lpToken != address(0), "Invalid Contract Address");

        lpToken = IUniswapV2Pair(_lpToken);
    }

    function withdraw() public onlyOwner {
        uint256 contractBalance = lpToken.balanceOf(address(this));

        lpToken.transfer(msg.sender, contractBalance);
    }
}

