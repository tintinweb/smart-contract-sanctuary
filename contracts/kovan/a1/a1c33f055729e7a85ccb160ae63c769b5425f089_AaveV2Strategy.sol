// IMPORTANT: Changes from v1 to v2 
// See https://docs.aave.com/developers/change-from-v1-to-v2)
contract AaveV2Strategy is IRhoStrategy, PausableUpgradeable, AccessControlEnumerableUpgradeable {
    using SafeERC20Upgradeable for IERC20MetadataUpgradeable;

    bytes32 public constant MANAGER_ROLE = keccak256("MANAGER_ROLE");
    bytes32 public constant VAULT_ROLE = keccak256("VAULT_ROLE");
    bytes32 public constant SWEEPER_ROLE = keccak256("SWEEPER_ROLE");

    IERC20MetadataUpgradeable public override underlying;
    address public aTokenAddr;
    IAToken public aToken;
    IProtocolDataProvider public protocolDataProvider; 
    ILendingPoolAddressesProvider public lpAddrProvider;

    uint256 public override switchLockDuration;
    uint256 public override switchLockedUntil;
    uint256 public override rewardConversionThreshold;
    uint256 public override switchingLockTarget;

    function initialize(
        address _underlyingAddr,
        address _protocolDataProviderAddr,
        uint256 _lockDurationInBlock
    ) public initializer {
        PausableUpgradeable.__Pausable_init();
        AccessControlEnumerableUpgradeable.__AccessControlEnumerable_init();

        underlying = IERC20MetadataUpgradeable(_underlyingAddr);
        protocolDataProvider = IProtocolDataProvider(_protocolDataProviderAddr);
        (aTokenAddr, , ) = protocolDataProvider.getReserveTokensAddresses(_underlyingAddr);
        aToken = IAToken(aTokenAddr);
        lpAddrProvider = ILendingPoolAddressesProvider(protocolDataProvider.ADDRESSES_PROVIDER());

        _setSwitchLockDuration(_lockDurationInBlock);
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
    }

    function _supplyRate() private view returns (uint256) {
        (, , , uint256 rateInRay, , , , , , ) = protocolDataProvider.getReserveData(address(underlying));
        return (rateInRay / 1e9);
    }

    function supplyRate() external override view returns (uint256) {
        return _supplyRate();
    }

    function effectiveSupplyRate() external override view returns (uint256) {
        return _supplyRate();
    }

    /**
     * @notice get only the data required by Flurry
     */
    function getReserveDataInternal() private view returns (
        uint256 availableLiquidity,
        uint256 totalStableDebt,
        uint256 totalVariableDebt,
        uint256 liquidityRate,
        uint256 avgStableRate
    ) {
        (uint256 _availableLiquidity, uint256 _totalStableDebt, uint256 _totalVariableDebt, uint256 _liquidityRate, , , uint256 _avgStableRate, , , ) =
            protocolDataProvider.getReserveData(address(underlying));
        return (_availableLiquidity, _totalStableDebt, _totalVariableDebt, _liquidityRate, _avgStableRate);
    }

    function getReserveFactorInternal() private view returns (uint256 reserveFactor) {
        (, , , , reserveFactor, , , , , ) = protocolDataProvider.getReserveConfigurationData(address(underlying));
        return reserveFactor;
    }

    function effectiveSupplyRate(uint256 delta, bool isPositive) external override view returns (uint256) {
        ILendingPool lendingPool = ILendingPool(lpAddrProvider.getLendingPool());
        DataTypes.ReserveData memory reserveData = lendingPool.getReserveData(address(underlying));
        
        (uint256 newAvailableLiquidity, uint256 totalStableDebt, uint256 totalVariableDebt, , uint256 avgStableRate) =
            getReserveDataInternal();
        newAvailableLiquidity = isPositive ? newAvailableLiquidity + delta : newAvailableLiquidity - delta;
        
        (uint256 rateInRay, ,) = IReserveInterestRateStrategy(reserveData.interestRateStrategyAddress).calculateInterestRates(
            address(underlying),
            newAvailableLiquidity,
            totalStableDebt,
            totalVariableDebt,
            avgStableRate,
            getReserveFactorInternal()
        );
        return rateInRay / 1e9;
    }

    function balanceOfUnderlying() external view override returns (uint256) {
        return aToken.balanceOf(address(this)); // 1:1 pegged
    }

    function updateBalanceOfUnderlying() external override view returns (uint256) {
        return aToken.balanceOf(address(this)); // always return (principal + interest)
    }

    function deploy(uint256 _amount) public override onlyRole(VAULT_ROLE) {
        require(_amount > 0, "AaveV2Strategy: deploy amount must be positive");
        ILendingPool lendingPool = ILendingPool(lpAddrProvider.getLendingPool());
        underlying.safeIncreaseAllowance(lpAddrProvider.getLendingPool(), _amount);
         // `onBehalfOf` = address(this) because aToken is managed by this strategy, NOT the vault
        lendingPool.deposit(address(underlying), _amount, address(this), 0);
        emit Deploy(_amount);
    }

    function withdrawUnderlying(uint256 _amount) public override onlyRole(VAULT_ROLE) {
        require(_amount > 0, "AaveV2Strategy: withdraw amount must be positive");
        if (_amount != type(uint).max) {
            require(aToken.balanceOf(address(this)) >= _amount, "AaveV2Strategy: cannot withdraw more than balance");
        }
        ILendingPool lendingPool = ILendingPool(lpAddrProvider.getLendingPool());
        lendingPool.withdraw(address(underlying), _amount, _msgSender());
        emit WithdrawUnderlying(_amount);
    }

    function withdrawAll() external override onlyRole(VAULT_ROLE) {
        withdrawUnderlying(type(uint).max);
        emit WithdrawAll();
    }

    function collectRewardToken() external override onlyRole(VAULT_ROLE) {
    }

    function setRewardConversionThreshold(uint256 _threshold) external override onlyRole(VAULT_ROLE) {
        rewardConversionThreshold = _threshold;
    }

    // withdraw random token transfer into this contract
    function sweepERC20Token(address token, address to) external override onlyRole(SWEEPER_ROLE) {
        require(token != address(underlying), "!safe");
        require(token != address(aToken), "!safe");
        IERC20Upgradeable tokenToSweep = IERC20Upgradeable(token);
        tokenToSweep.transfer(to, tokenToSweep.balanceOf(address(this)));
    }

    function setSwitchLockDuration(uint256 numberOfBlocks) external override onlyRole(MANAGER_ROLE) {
        _setSwitchLockDuration(numberOfBlocks);
    }

    function _setSwitchLockDuration(uint256 numberOfBlocks) internal {
        switchLockDuration = numberOfBlocks;
    }

    function switchingLock(uint256 target) external override onlyRole(VAULT_ROLE) {
        switchingLockTarget = target;
        switchLockedUntil = block.number + switchLockDuration;
    }
}

