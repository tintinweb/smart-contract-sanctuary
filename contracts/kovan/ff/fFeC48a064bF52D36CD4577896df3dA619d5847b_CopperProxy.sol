pragma solidity ^0.8.7;
import {TransferHelper} from "@uniswap/lib/contracts/libraries/TransferHelper.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "hardhat/console.sol";

interface LBPFactory {
    function create(
        string memory name,
        string memory symbol,
        address[] memory tokens,
        uint256[] memory weights,
        uint256 swapFeePercentage,
        address owner,
        bool swapEnabledOnStart
    ) external returns (address);
}

interface Vault {
    struct JoinPoolRequest {
        address[] assets;
        uint256[] maxAmountsIn;
        bytes userData;
        bool fromInternalBalance;
    }

    struct ExitPoolRequest {
        address[] assets;
        uint256[] minAmountsOut;
        bytes userData;
        bool toInternalBalance;
    }

    function joinPool(
        bytes32 poolId,
        address sender,
        address recipient,
        JoinPoolRequest memory request
    ) external;

    function exitPool(
        bytes32 poolId,
        address sender,
        address recipient,
        ExitPoolRequest memory request
    ) external;
    
    function getPoolTokens(bytes32 poolId)
        external
        view
        returns (
            IERC20[] memory tokens,
            uint256[] memory balances,
            uint256 lastChangeBlock
        );
}

interface LBP {
    function updateWeightsGradually(
        uint256 startTime,
        uint256 endTime,
        uint256[] memory endWeights
    ) external;

    function setSwapEnabled(bool swapEnabled) external;

    function getPoolId() external returns (bytes32 poolID);

    function getGradualWeightUpdateParams() external view
        returns (
            uint256 startTime,
            uint256 endTime,
            uint256[] memory endWeights
        );

    function getNormalizedWeights() external view returns (uint256[] memory);

    function getRate() external view returns (uint256);

    function getInvariant() external view returns (uint256);
    function getLastInvariant() external view returns (uint256);
}

contract CopperProxy {
    address public constant VaultAddress = address(0xBA12222222228d8Ba445958a75a0704d566BF2C8);

    mapping(address => address) public poolOwner;

    address public immutable feeRecipient;
    address public immutable LBPFactoryAddress;

    constructor(address _feeRecipient, address _LBPFactoryAddress) {
        feeRecipient = _feeRecipient;
        LBPFactoryAddress = _LBPFactoryAddress;
    }

    // Pool access control
    modifier onlyPoolOwner(address pool) {
        require(msg.sender == poolOwner[pool], "!owner");
        _;
    }

    function getPoolOwner(address pool) external view returns (address) {
        return poolOwner[pool];
    }

    struct PoolConfig {
        string name;
        string symbol;
        address[] tokens;
        uint256[] amounts;
        uint256[] weights;
        uint256 swapFeePercentage;
        address owner;
        bytes userData;
        bool fromInternalBalance; // should always be false, remove it from params
        uint256 startTime;
        uint256 endTime;
        uint256[] endWeights;
    }

    function createAuction(PoolConfig memory poolConfig) external returns (address) {
        // 1: deposit tokens and approve vault
        for (uint256 index = 0; index < poolConfig.tokens.length; index++) {
            TransferHelper.safeTransferFrom(
                poolConfig.tokens[index],
                msg.sender,
                address(this),
                poolConfig.amounts[index]
            );
            TransferHelper.safeApprove(
                poolConfig.tokens[index],
                VaultAddress,
                poolConfig.amounts[index]
            );
        } 
        // 2: pool creation
        address pool = LBPFactory(LBPFactoryAddress).create(
            poolConfig.name,
            poolConfig.symbol,
            poolConfig.tokens,
            poolConfig.weights,
            poolConfig.swapFeePercentage,
            address(this), // owner set to this proxy // TODO THIS PROB needs to change to the user wallet
            false // swaps disabled on start
        );
        poolOwner[pool] = poolConfig.owner;

        // 3: deposit tokens into pool
        Vault(VaultAddress).joinPool(
            LBP(pool).getPoolId(),
            address(this), // sender
            msg.sender, // recipient
            Vault.JoinPoolRequest(
                poolConfig.tokens,
                poolConfig.amounts,
                poolConfig.userData,
                poolConfig.fromInternalBalance
            )
        );
        // 4: configure weights
        LBP(pool).updateWeightsGradually(
            poolConfig.startTime,
            poolConfig.endTime,
            poolConfig.endWeights
        );
        console.log('pool: ', pool);
        return pool;
    }

    function setSwapEnabled(address pool, bool swapEnabled) external onlyPoolOwner(pool) {
        // todo remove me
        uint256[] memory nweights = LBP(pool).getNormalizedWeights();
        console.log('normalizedWeights: ', nweights[0]);

        (,, uint256[] memory endWeights) = LBP(pool).getGradualWeightUpdateParams();
        console.log('getGradualWeightUpdateParams: ', endWeights[0]);
        
        console.log('rate: ', LBP(pool).getRate());
        console.log('invariant (current one): ', LBP(pool).getInvariant());
        console.log('last invariant (when join/exit): ', LBP(pool).getLastInvariant());
        //
        LBP(pool).setSwapEnabled(swapEnabled);
    }

    struct ExitPoolConfig {
        address poolAddress;
        address[] tokens;
        bytes userData;
        uint256[] amounts;
        uint256 bptAmount;
    }

    enum ExitKind { EXACT_BPT_IN_FOR_ONE_TOKEN_OUT, EXACT_BPT_IN_FOR_TOKENS_OUT, BPT_IN_FOR_EXACT_TOKENS_OUT }

    function exitPool(ExitPoolConfig memory exitPoolConfig) external onlyPoolOwner(exitPoolConfig.poolAddress) {
        // 1. Move BPT tolken to proxy. BPT token has to be approved for this contract previously
        require(IERC20(exitPoolConfig.poolAddress).transferFrom(msg.sender, address(this), exitPoolConfig.bptAmount));
        (IERC20[] memory poolTokens,,) = Vault(VaultAddress).getPoolTokens(LBP(exitPoolConfig.poolAddress).getPoolId());
        uint256[] memory minAmountsOut = new uint256[](2);
        minAmountsOut[0] = 0; // ONLY DO THIS FOR EXACT_BPT_IN_FOR_TOKENS_OUT
        minAmountsOut[1] = 0; // ONLY DO THIS FOR EXACT_BPT_IN_FOR_TOKENS_OUT
        // 2. Specify the exact BPT amount to burn
        bytes memory userData = abi.encode(
            ExitKind.EXACT_BPT_IN_FOR_TOKENS_OUT,
            exitPoolConfig.bptAmount
        );
        Vault.ExitPoolRequest memory exitRequest = Vault.ExitPoolRequest(
            exitPoolConfig.tokens,
            minAmountsOut,
            userData,
            false
        );
        // 3. Exit pool and keep tokens in contract
        Vault(VaultAddress).exitPool(
            LBP(exitPoolConfig.poolAddress).getPoolId(),
            address(this),
            payable(address(this)),
            exitRequest
        );

        // 4. Transfer to user
        for (uint256 i = 0; i < poolTokens.length; i++) {
            uint256 poolTokenBalance = poolTokens[i].balanceOf(address(this));
            if (poolTokenBalance != 0) {
                poolTokens[i].transfer(msg.sender, poolTokenBalance);
            }
        }
    }

}

