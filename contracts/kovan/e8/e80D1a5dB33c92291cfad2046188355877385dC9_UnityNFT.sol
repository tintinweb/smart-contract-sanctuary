pragma solidity >=0.8.4;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import '@chainlink/contracts/src/v0.8/ChainlinkClient.sol';
import "@openzeppelin/contracts/utils/Strings.sol";

contract UnityNFT is ChainlinkClient, ERC721 {
    using Chainlink for Chainlink.Request;
    using Strings for uint8;

    uint256 private oraclePayment;
    address private oracle;
    bytes32 private jobId;

    string public baseURI;
    uint8 public URI_ID;
    uint8 NFT = 1;
    uint256 public interval;


    event BaseURIChanged(string _baseURI);
    event TokenURIChanged(uint256 _tokenId, string _tokenURI);
    event IntervalTimeChanged(uint256 _interval);

    modifier onlyOwner {
        require(msg.sender == ownerOf(NFT), "Owner Only function.");
        _;
    }

    constructor(string memory _baseURI, uint256 _interval) ERC721("UnityNFT", "UNFT") {
        setPublicChainlinkToken();
        oraclePayment = 0.1 * 10 ** 18; // 0.1 LINK = 0.1 * 10 ** 18;
        oracle = 0x2f90A6D021db21e1B2A077c5a37B3C7E75D15b7e; 
        jobId = "a7ab70d561d34eb49e9b1612fd2e044b";

        baseURI = _baseURI;
        interval = _interval;

        _safeMint(msg.sender, NFT);
        emit BaseURIChanged(_baseURI);
        emit IntervalTimeChanged(_interval); 
    }

    function startCycle()
    onlyOwner
    public{
        Chainlink.Request memory req = buildChainlinkRequest(jobId, address(this), this.fulfill.selector);  
        req.addUint("until", block.timestamp + interval);
        sendChainlinkRequestTo(oracle, req, oraclePayment);
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        return bytes(baseURI).length > 0 ? 
        string(abi.encodePacked(baseURI, "/1/", URI_ID.toString())) : "";
    }

    function fulfill(bytes32 _requestId) public recordChainlinkFulfillment(_requestId) {
        URI_ID = (URI_ID + 1) % 7;

        startCycle();

        emit TokenURIChanged(1, bytes(baseURI).length > 0 ? 
        string(abi.encodePacked(baseURI, "/1/", URI_ID.toString())) : "");
    }

    function changeBaseUri(string memory _baseURI)
    onlyOwner
    public{
        baseURI = _baseURI;
        emit BaseURIChanged(_baseURI);
    }

    function changeIntervalTime(uint256 _interval)
    onlyOwner
    public{
        interval = _interval;
        emit IntervalTimeChanged(_interval);
    }

    function getOracle()
    onlyOwner
    public
    view
    returns(address){
        return(oracle);
    }

    function getJobId()
    onlyOwner
    public
    view
    returns(bytes32){
        return(jobId);
    }

    function getOraclePayment()
    onlyOwner
    public
    view
    returns(uint256){
        return(oraclePayment);
    }

    function changeOracleData(address _oracle, bytes32 _jobId, uint256 _oraclePayment)
    onlyOwner
    public{
        oracle = _oracle;
        jobId = _jobId;
        oraclePayment = _oraclePayment;
    }
}

