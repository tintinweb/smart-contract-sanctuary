    //     // require(deviation <= maxTwapDeviation, "PSC");
    // }

    /// @dev Fetches time-weighted average price in ticks from Uniswap pool for specified duration
    function getTwap(address _pool, uint32 tick, uint32 tickAmount) external view returns (int56[] memory) {
        IUniswapV3Pool pool = IUniswapV3Pool(_pool);
        uint32[] memory secondsAgo = new uint32[](tickAmount);
        for(uint32 i=tickAmount; i>=0; i-=1) {
            secondsAgo[i] = i * tick;
        }

        (int56[] memory tickCumulatives, ) = pool.observe(secondsAgo);
        return tickCumulatives;
    }
}