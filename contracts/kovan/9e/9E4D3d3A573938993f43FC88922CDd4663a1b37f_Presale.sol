import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155Receiver.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/math/Math.sol";
import "@openzeppelin/contracts/utils/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/ReentrancyGuard.sol";
import "./DateControl.sol";

/* TO DO LIST 
 *   
 * 
 * 
 */

contract Presale is Ownable, DateControl, ReentrancyGuard, ERC1155Receiver {
    using SafeMath for uint256; 
    using Math for uint256; 
    using EnumerableSet for EnumerableSet.UintSet; 
    using EnumerableSet for EnumerableSet.AddressSet;

    /* ========== STATE VARIABLES ========== */
    IERC1155 public cryptoBud;

    uint256 public pricePerBud = 0.01 ether;
    uint256 public maxBuyCount = 5;

    // Mapping from address to bud count 
    mapping(address => uint256) private accountBuyCount;
    // Set of bud IDs this contract owns 
    EnumerableSet.UintSet private presaleBudIDs;
    // Set of address that are approved to purchase bud 
    EnumerableSet.AddressSet private whitelist;

    /* ========== CONSTRUCTOR ========== */
    constructor(IERC1155 _cryptoBud) { 
        cryptoBud = _cryptoBud; 
        }
    /* ========== OWNER ONLY ========== */
   

    /** * @dev Allow owner to change bud price 
    */ 
    function setPricePerBud(uint256 _price) public onlyOwner { 
        pricePerBud = _price; 
        }



    /** * @dev Allow owner to update maximum number bud a given user can buy 
    */ 
    function setMaxBuyCount(uint256 _maxBuyCount) public onlyOwner { 
        maxBuyCount = _maxBuyCount; 
        }



    /** * @dev Allow owner to transfer a bud that didn't get buyed during presale 
    */ 
    function reclaim(uint256 _id, address _to) public onlyOwner whenEnded {
        cryptoBud.safeTransferFrom(address(this), _to, _id, 1, "");
        }



    /** * @dev Allow owner to transfer all bud that didn't get buyed during presale 
    */ 
    function reclaimAll(address _to) public onlyOwner whenEnded { 
        uint256 length = presaleBudIDs.length(); 
        uint256[] memory ids = new uint256[](length); 
        uint256[] memory amount = new uint256[](length); 
        for (uint256 i = 0; i < length; i++) { 
            ids[i] = presaleBudIDs.at(i); 
            amount[i] = 1; 
            }
            cryptoBud.safeBatchTransferFrom(address(this), _to, ids, amount, ""); 
        }


    /** * @dev Allows owner to withdrawal the presale balance to an account. 
    */ 
    function withdraw(address payable _to) external onlyOwner { 
        _to.transfer(address(this).balance); 
        }


    /* ========== EXTERNAL MUTATIVE FUNCTIONS ========== */

    /** * @dev Buy _count number of bud 
    */ 
    function buyBud(uint256 _count) public payable whenInProgress nonReentrant {
        require(_count > 0, "BudPresale: must buy at least one bud");
        
        address account = msg.sender; 
        uint256 credit = canBuyCount(account); 

        require( _count <= credit, "BudPresale: buyion count larger than maximum buyion limit");
        require(msg.value >= getBuyPrice(_count), "BudPresale: insufficient funds" );

        uint256[] memory ids = new uint256[](_count); 
        uint256[] memory counts = new uint256[](_count); 

        for (uint256 i = 0; i < _count; i++) { 
            ids[i] = _randRemoveBud(); 
            counts[i] = 1; 
            }
        accountBuyCount[account] += _count; // Accumulate the number of buys
        cryptoBud.safeBatchTransferFrom(address(this), account, ids, counts, "" );
        
        }

    /* ========== VIEW ========== */
    
    /** * @dev returns number of _account has buyed presale bud 
    */ 
    function getBuyCount(address _account) public view returns (uint256) { 
        return accountBuyCount[_account]; 
        }

    /** * @dev total buyion price if buy _count many
    */ 
    function getBuyPrice(uint256 _count) public view returns (uint256) { 
        return _count.mul(pricePerBud); 
        }
    /** * @dev number of presale bud this contract owns 
    */ 
    function getPresaleBudCount() public view returns (uint256) { 
        return presaleBudIDs.length(); 
        }
    /** * @dev how many more _account can buy bud 
    */ 
    function canBuyCount(address _account) public view returns (uint256) { 

        uint256 credit = maxBuyCount.sub(accountBuyCount[_account]);
        uint256 budCount = presaleBudIDs.length();
        return credit.min(budCount);
        }

    /** * @dev onERC1155Received implementation per IERC1155Receiver spec 
    */ 
    function onERC1155Received( address, address, uint256 id, uint256, bytes calldata ) external override returns (bytes4) {
        require( 
            msg.sender == address(cryptoBud), "BudPresale: received bud from unauthenticated contract" );
            uint256[] memory ids = new uint256[](1); ids[0] = id;
            _receivedBud(ids);
            return bytes4( 
                keccak256( 
                    "onERC1155Received(address,address,uint256,uint256,bytes)"
                    ) 
                ); 
        }

    /** * @dev onERC1155BatchReceived implementation per IERC1155Receiver spec 
    */ 
    function onERC1155BatchReceived( address, address, uint256[] calldata ids, uint256[] calldata, bytes calldata ) external override returns (bytes4) { 
        require( 
            msg.sender == address(cryptoBud), "BudPresale: received bud from unauthenticated contract" );
            _receivedBud(ids);
            return bytes4( 
                keccak256( 
                    "onERC1155BatchReceived(address,address,uint256[],uint256[],bytes)" 
                    ) 
                ); 
        }

    /* ========== PRIVATE ========== */

    /** * @dev randomly select and remove a bud 
    * returns selected bud ID 
    */ 
    function _randRemoveBud() private returns (uint256) {
        require(presaleBudIDs.length() > 0, "No more presale bud");
        uint256 totalLength = presaleBudIDs.length();
        uint256 randIndex = uint256(blockhash(block.number - 1)); 

        randIndex = uint256(keccak256(abi.encodePacked(randIndex, totalLength))).mod(totalLength);

        uint256 randID = presaleBudIDs.at(uint256(randIndex));
        require(presaleBudIDs.remove(randID));
        
            return randID; 
        }


    function _receivedBud(uint256[] memory ids) private { 
        for (uint256 i = 0; i < ids.length; i++) { 
            presaleBudIDs.add(ids[i]);
                } 
            } 

}

