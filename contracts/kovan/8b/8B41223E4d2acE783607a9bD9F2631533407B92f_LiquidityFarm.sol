// Kovan balancer test tokens:
//t0 -> 0x0db0a80b3aCF626Bba34B144A222fCd803BEB97e
//t1 -> 0x41F1C72dB73f2EB8bE303F43FCACCfd0CBFD4477

enum AMM{Uniswap, Balancer}

/**
* @title Trader *
* @author Carson Case ([emailÂ protected])
* @notice Trader acts as an interface between all Roci contracs and AMMs
 */
contract Trader is Ownable, ITrader, UniTrader, BalTrader{

    AMM public currentAMM;

    constructor() Ownable(){
        currentAMM = AMM.Uniswap;
    }

    /**
    * @dev owner can set AMM
    * @param _amm new AMM
     */
    function setAMM(AMM _amm)external onlyOwner{
        currentAMM = _amm;
    }

    /**
    * @dev for now just uniswap. But will eventually have several amms
    * @param _in is token to go in
    * @param _out is the token to come out
    * @param _amount is the amount of in to send
     */
    function trade(address _in, address _out, uint _amount) override external {
        IERC20(_in).transferFrom(msg.sender, address(this), _amount);
        if(currentAMM == AMM.Uniswap){
            _tradeUniswap(_in, _out, _amount, address(this));
        }
        IERC20 t = IERC20(_out);
        t.transfer(msg.sender, t.balanceOf(address(this)));
    }
    
    /**
    * @dev zaps tokens to lp. Sells half for _t1 and deposits lp with the both of them
    * @param _t0 is the token the user sends in
    * @param _t1 is the token to sell half of _t0 for and deposit with the other half as LP
    * @param _t0Am is the amount of _t0 to initially send in
    * @param _receiver is the receiver of the LP tokens
     */
    function zap(address _t0, address _t1, uint _t0Am, address _receiver) external{
        IERC20(_t0).transferFrom(msg.sender, address(this), _t0Am);
        if(currentAMM == AMM.Uniswap){
            _uniZap(_t0, _t1, _t0Am, _receiver);
        }    
    }

    function zap(address _lpToken, address _from, address _to, address _token, uint _amount) override external {
        address t0 = IUniswapV2Pair(_lpToken).token0();
        address t1 = IUniswapV2Pair(_lpToken).token1();
        bool inToken0 = t0 == _token;
        
        IERC20(_token).transferFrom(_from, address(this), _amount);
        if (currentAMM == AMM.Uniswap) {
            // swap 50% of _token to another token
            // add token0/token1 to pool's liquidity, send LP tokens to _to
            _uniZap(t0, t1, inToken0, _amount, _to);
        }
    }

    function zapLPtoTokens(address _lpToken, uint _amount, address _outToken, address _receiver) override external {
        if(currentAMM == AMM.Uniswap){
            _uniZapLPtoTokens(_lpToken, _amount, _outToken, _receiver);
        }  
    }

    /**
    * @dev for now Uniswap. swaps tokens to lp
    * @param _token0 is first token that makes up LP
    * @param _token1 is the other half of the LP
    * @param _amount0 is the amount of token0 t0 deposit
    * @param _amount1 is the amount of token1 to deposit
     */
    function zapTokensToLP(address _token0, address _token1, uint _amount0, uint _amount1) external{
        IERC20(_token0).transferFrom(msg.sender, address(this), _amount0);
        IERC20(_token1).transferFrom(msg.sender, address(this), _amount1);
        if(currentAMM == AMM.Uniswap){
            _uniZapTokensToLP(_token0, _token1, _amount0, _amount1, msg.sender);
        }
    }
    
    /**
    * @dev used to create a new pair on an exchange
    * @param _token0 is the first part of the pair
    * @param _token1 is the second part
    * @return the pair address
     */
    function initPair(address _token0, address _token1) override external returns (address) {
        if(currentAMM == AMM.Uniswap){
            return(_uniInitPair(_token0, _token1));
        }
        if(currentAMM == AMM.Balancer){
            return(_balInitPair(_token0, _token1, msg.sender));
        }
    }

    /**
    * @dev used to fetch a tokens exchange price
    * @param _in token in
    * @param _out token out
    * @param _amount amount of token in
    * @return the amount of tokens you would receive out for this trade
     */
    function getAmountOut(address _in, address _out, uint _amount) override external view returns (uint) {
        if(currentAMM == AMM.Uniswap){  
            return _uniGetAmountOut(_in, _out, _amount);
        }
    }
    
    /**
    * @dev before trade hook. Executed before buys and sells
     */
    function _beforeTrade() internal override(UniTrader, BalTrader){
        // nothing for now
    }
}

