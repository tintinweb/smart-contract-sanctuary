/**
* @title UniTrader
* @author Carson Case ([emailÂ protected])
* @dev function descriptions are better in trader where their public versions exist
 */
abstract contract UniTrader{
    using SafeERC20 for IERC20;

    IUniswapV2Router02 public constant uniswap = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
    IUniswapV2Factory private factory = IUniswapV2Factory(uniswap.factory());

    /*
            EVENTS
    */
    event UniswapTrade(address _in, address _out, uint _amount);
    event UniswapPairInitialization(address token0, address token1);
    event UniswapLPDeposit(address token0, address token1, uint amount0, uint amount1, uint liquidity);
    event UniswapLPWithdrawal(address token0, address token1, uint amount0, uint amount1);

    /**
    * @dev _uniZap based off the lp token address
     */
    function _uniZap(address _lpToken, uint _amount, address _caller) internal{
        address t0 = IUniswapV2Pair(_lpToken).token0();
        address t1 = IUniswapV2Pair(_lpToken).token1();
        _uniZap(t0,t1, _amount, _caller);
    }

    /**
    * @dev zap with uniswap
     */
    function _uniZap(address _token0, address _token1, uint _amount, address _caller) internal{
        _tradeUniswap(_token0, _token1, _amount/2, address(this));

        uint t0Am = (_amount - (_amount/2));
        uint t1Am = IERC20(_token1).balanceOf(address(this));
        _uniZapTokensToLP(_token0, _token1, t0Am, t1Am, _caller);
    }

    /**
     * @dev zap with uniswap
     */
    function _uniZap(address _token0, address _token1, bool _inToken0, uint _amount, address _caller) internal{
        address inToken = _inToken0 ? _token0 : _token1;
        address outToken = _inToken0 ? _token1: _token0;

        _tradeUniswap(inToken, outToken, _amount / 2, address(this));

        uint t0Am = _inToken0 ? (_amount - (_amount / 2)) : IERC20(outToken).balanceOf(address(this));
        uint t1Am = _inToken0 ? IERC20(outToken).balanceOf(address(this)) : (_amount - (_amount / 2));
        _uniZapTokensToLP(_token0, _token1, t0Am, t1Am, _caller);
    }

    /**
    * @dev _zaps two tokens to LP with uniswap
     */
    function _uniZapLPtoTokens(address _lpToken, uint _amount, address _outToken, address _receiver) internal{
        address token0 = IUniswapV2Pair(_lpToken).token0();
        address token1 = IUniswapV2Pair(_lpToken).token1();
        address inToken = token0 == _outToken ? token1 : token0;
        
        // remove liquidity
        IERC20(_lpToken).approve(address(uniswap), _amount);
        (uint amount0, uint amount1) = 
            uniswap.removeLiquidity(
                token0,
                token1,
                _amount,
                0,
                0,
                address(this),
                block.timestamp + 30
        );

        // send out token
        IERC20(_outToken).safeTransfer(_receiver, token0 == _outToken ? amount0 : amount1);
        
        // swap another token for out token
        _tradeUniswap(inToken, _outToken, token0 == _outToken ? amount1 : amount0, _receiver);
    }

    /**
    * @dev swaps tokens to lp
     */
    function _uniZapTokensToLP(address _token0, address _token1, uint _amount0, uint _amount1, address _receiver) internal{
        IERC20(_token0).approve(address(uniswap), _amount0);
        IERC20(_token1).approve(address(uniswap), _amount1);
        (uint amount0, uint amount1, uint liquidity) = 
            uniswap.addLiquidity(
                _token0,
                _token1,
                _amount0,
                _amount1,
                0,
                0,
                _receiver,
                block.timestamp + 30
            );
        emit UniswapLPDeposit(_token0, _token1, amount0, amount1, liquidity);
    }
    
    /**
    * @dev used to create a new pair on an exchange
     */
    function _uniInitPair(address _token0, address _token1) internal returns(address){
        emit UniswapPairInitialization(_token0, _token1);
        return(factory.createPair(_token0, _token1));
    }

    /**
    * @dev used to fetch a tokens exchange price
     */
    function _uniGetAmountOut(address _in, address _out, uint _amount) internal view returns(uint){
        IUniswapV2Pair pair = IUniswapV2Pair(factory.getPair(_in,_out));
        (uint reserveA, uint reserveB, uint x) = pair.getReserves();
        return uniswap.getAmountOut(_amount, reserveA, reserveB);
    }

    /**
    * @dev preforms a sell
     */
    function _tradeUniswap(address _in, address _out, uint _amount, address _receiver) internal{
        _beforeTrade();
        address[] memory path = new address[](2);
        path[0] = _in;
        path[1] = _out;
        IERC20(_in).approve(address(uniswap), _amount);
        uniswap.swapExactTokensForTokens(_amount, 0, path, _receiver, block.timestamp + 20);

        emit UniswapTrade(_in, _out, _amount);
    }

    /**
    * @dev before trade hook. Executed before buys and sells
     */
    function _beforeTrade() internal virtual{
        // nothing for now
    }

}

