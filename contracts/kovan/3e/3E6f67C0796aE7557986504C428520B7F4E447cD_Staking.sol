library SnapshotLib {
    using SafeMath for uint256;
    using Arrays for uint256[];

    struct SnapshotBalance {
        mapping(uint256 => uint256) snapshot;
        uint256[] heights;
        uint256 balance;
    }

    function add(SnapshotBalance storage balance, uint256 amount) public {
        balance.balance = balance.balance.add(amount);
        balance.snapshot[block.number] = balance.balance;

        if (balance.heights.length == 0) {
            balance.heights.push(block.number);
        } else if (lastHeight(balance) < block.number) {
            balance.heights.push(block.number);
        }
    }

    function sub(SnapshotBalance storage balance, uint256 amount) public {
        balance.balance = balance.balance.sub(amount);
        balance.snapshot[block.number] = balance.balance;

        if (balance.heights.length == 0) {
            balance.heights.push(block.number);
        } else if (lastHeight(balance) < block.number) {
            balance.heights.push(block.number);
        }
    }

    function lastHeight(SnapshotBalance storage balance)
        public
        view
        returns (uint256)
    {
        return balance.heights[balance.heights.length.sub(1)];
    }

    function heightPos(SnapshotBalance storage balance, uint256 height)
        public
        view
        returns (uint256)
    {
        require(balance.heights.length > 0, "the array is empty");

        return balance.heights.findUpperBound(height);
    }
}



contract Staking is ReentrancyGuard {
    using SafeMath for uint256;

    uint256 public _startTime;
    uint256 public _endTime;
    uint256 public _rewardRate;

    IERC20 public _stakingToken;
    IERC20 public _rewardToken;

    uint256 public _rewardPerTokenStored;
    uint256 public _lastUpdateTime;

    mapping(address => uint256) public _rewards;
    mapping(address => uint256) public _userRewardPerTokenPaid;

    uint256 public _supply;
    mapping(address => uint256) public _balance;

    mapping(address => LockAmount) public _lockAmount;

    struct LockAmount {
        uint256[] amount;
        uint256 releaseTime;
        uint256 pos;
    }

    constructor(
        uint256 startTime_,
        uint256 endTime_,
        uint256 rewardRate_,
        address stakingToken_,
        address rewardToken_
    ) {
        _startTime = startTime_;
        _endTime = endTime_;
        _rewardRate = rewardRate_;
        _stakingToken = IERC20(stakingToken_);
        _rewardToken = IERC20(rewardToken_);
    }

    function balanceOf(address account) public view returns (uint256) {
        return _balance[account];
    }

    modifier updateReward(address account) {
        _rewardPerTokenStored = rewardPerToken();
        _lastUpdateTime = lastTimeRewardApplicable();
        if (account != address(0)) {
            _rewards[account] = earned(account);
            _userRewardPerTokenPaid[account] = _rewardPerTokenStored;
        }
        _;
    }

    function lockedIncomeBalanceOf(address account) public view returns (LockAmount memory) {
        return _lockAmount[account];
    }

    function earned(address account) public view returns (uint256) {
        return
            balanceOf(account)
                .mul(rewardPerToken().sub(_userRewardPerTokenPaid[account]))
                .div(1e18)
                .add(_rewards[account]);
    }

    function stake(uint256 amount) public updateReward(msg.sender) {
        require(block.timestamp < _endTime, "the end");
        require(amount > 0, "amount 111");
        _balance[msg.sender] = _balance[msg.sender].add(amount);

        require(
            _stakingToken.transferFrom(msg.sender, address(this), amount),
            "transferFrom fail"
        );
        _supply = _supply.add(amount);
    }

    function withdraw(uint256 amount) public updateReward(msg.sender) {
        console.log("withdraw");
        require(amount > 0, "cannot withdraw 0");
        _balance[msg.sender] = _balance[msg.sender].sub(amount);
        require(_stakingToken.transfer(msg.sender, amount), "withdraw fail");
    }

    function exit() public {
        withdraw(balanceOf(msg.sender));
        getReward();
    }

    function getReward() public updateReward(msg.sender) {
        uint256 reward = _rewards[msg.sender];
        LockAmount storage lockAmount = _lockAmount[msg.sender];
        uint256 amount = 0;
        if (reward > 0) {
            _rewards[msg.sender] = 0;

            if (lockAmount.amount.length == 0) {
                lockAmount.releaseTime = block.timestamp + 1 weeks;
                lockAmount.pos = 0;
                for (uint256 i = 0; i < 19; i++) {
                    lockAmount.amount.push();
                }
            }

            // 20 , 
            uint256 part = reward.div(20);
            uint256 pos = 0;
            for (uint256 i = 0; i < 18; i++) {
                pos = lockAmount.pos.add(i).mod(lockAmount.amount.length);
                lockAmount.amount[pos] = lockAmount.amount[pos].add(part);
            }

            // ,
            pos = pos.add(1).mod(lockAmount.amount.length);
            lockAmount.amount[pos] = reward.sub(part.mul(19));

            // 
            amount = part;
        }

        // 
        uint256 releaseAmount = amount;
        // 0, 
        if (lockAmount.releaseTime > 0 ) {
            while (lockAmount.releaseTime <= block.timestamp) {

                uint256 pos = lockAmount.pos;
                releaseAmount = releaseAmount.add(lockAmount.amount[pos]);

                lockAmount.amount[pos] = 0;
                lockAmount.releaseTime = lockAmount.releaseTime + 1 weeks;

                pos++;
                if (pos == lockAmount.amount.length) {
                    lockAmount.pos = 0;
                } else {
                    lockAmount.pos = pos;
                }
            }
        }

        if (releaseAmount > 0) {
            require(
                _rewardToken.transfer(msg.sender, releaseAmount),
                "reward token fail"
            );
        }
    }

    function lastTimeRewardApplicable() public view returns (uint256) {
        return Math.max(_startTime, Math.min(block.timestamp, _endTime));
    }

    function rewardPerToken() public view returns (uint256) {
        if (_supply == 0) {
            return _rewardPerTokenStored;
        }
        return
            _rewardPerTokenStored.add(
                lastTimeRewardApplicable()
                    .sub(_lastUpdateTime)
                    .mul(_rewardRate)
                    .mul(1e18)
                    .div(_supply)
            );
    }
}