pragma solidity ^0.6.0;
pragma experimental ABIEncoderV2;

library ToHexString {
    bytes16 private constant _HEX_SYMBOLS = "0123456789abcdef";
    
    function bytesToHexString(bytes memory data) internal pure returns (string memory){
        return stringArraytoString(bytes32ArrayToStringArray(bytesToBytes32Array(data)));
    }
    function bytesToBytes32Array(bytes memory data) internal pure returns (bytes32[] memory){
    // Find 32 bytes segments nb
    uint256 dataNb = data.length / 32;
    // Create an array of dataNb elements
    bytes32[] memory dataList = new bytes32[](dataNb);
    // Start array index at 0
    uint256 index = 0;
    // Loop all 32 bytes segments
    for (uint256 i = 32; i <= data.length; i = i + 32) {
        bytes32 temp;
        // Get 32 bytes from data
        assembly {
            temp := mload(add(data, i))
        }
        // Add extracted 32 bytes to list
        dataList[index] = temp;
        index++;
    }
    // Return data list
    return dataList;
    }
    function bytes32ArrayToStringArray(bytes32[] memory _bytes32Array) internal pure returns (string[] memory){
        uint256 len = _bytes32Array.length;
        string[] memory stringArray = new string[](len);
        for (uint256 i = 0; i < len; i++) {
            stringArray[i] = bytes32toHexStringNo0x(_bytes32Array[i]);
        }
        return stringArray;
    }
    function stringArraytoString(string[] memory _array) internal pure returns (string memory) {
        if (_array.length == 1) { 
            return string(abi.encodePacked(_array[0])); 
        }
        bytes memory stringAll;
        for (uint256 i=0; i < _array.length; i++){
            stringAll = abi.encodePacked(stringAll, _array[i]); 
        }
        return string(stringAll);
    }
    function bytes32toHexStringNo0x(bytes32 _value) internal pure returns (string memory){
        uint256 value = uint256(_value);
        bytes memory buffer = new bytes(64);
        for (uint256 i = 64; i > 0; --i) {
            buffer[i-1] = _HEX_SYMBOLS[value & 0xf];
            value >>= 4;
        }
        require(value == 0, "Strings: hex length insufficient");
        return string(buffer);
    }
    function addressToHexString(address _address) internal pure returns (string memory) {
        uint256 value = uint256(_address);
        bytes memory buffer = new bytes(42);
        buffer[0] = "0";
        buffer[1] = "x";
        for (uint256 i =  41; i > 1; --i) {
            buffer[i] = _HEX_SYMBOLS[value & 0xf];
            value >>= 4;
        }
        require(value == 0, "Strings: hex length insufficient");
        return string(buffer);
    }
}

