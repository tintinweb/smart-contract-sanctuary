pragma solidity >=0.6.2 <0.7.0;

import "./interfaces/IUniswapV2Pair.sol";
import "./interfaces/IWrapped777.sol";

contract UniswapReader {

address ERC20Token; // 0xa986ebe429a5b2e59699fa7974947b033878154f
address ERC777Token; // 0xA5AE1805e449371A3f6C71b3516A4Ef94c7a9A6c

    function setUniV2Address (address _ER20Token) public payable {
        ERC20Token = _ER20Token;
    }
    
    function setUniV2777Address (address _ERC777Token) public payable {
        ERC777Token = _ERC777Token;
    }

    function getTotalSupply () external view returns (uint) {
        return IUniswapV2Pair(ERC20Token).totalSupply();
    }

    function getReserves () external view returns (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast) {
        return IUniswapV2Pair(ERC20Token).getReserves();
    }

    function getBalanceOf (address owner) external view returns (uint) {
        return IWrapped777(ERC777Token).balanceOf(owner);
    }
}

