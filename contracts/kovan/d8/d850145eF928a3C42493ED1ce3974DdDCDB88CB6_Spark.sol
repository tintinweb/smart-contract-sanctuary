        // calculate the claimable amount
        uint amountClaimed = _userDepo[staker][index].amountClaimed;
        uint claimableAmt = computeUnitRewardFor(staker, index).sub(amountClaimed);
        if (claimableAmt <= 0) {
            emit TokensClaimed(staker, index, 0);
            return 0;
