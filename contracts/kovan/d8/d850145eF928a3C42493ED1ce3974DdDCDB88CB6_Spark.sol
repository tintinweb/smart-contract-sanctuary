        _userDepo[staker][index].amountClaimed = _userDepo[staker][index].amountClaimed.add(claimableAmt);
        _userProf[staker].cumuReward = _userProf[staker].cumuReward.add(claimableAmt);
        // 2. update global stats
        totalRewardGiven = totalRewardGiven.add(claimableAmt);

        // interaction
        _rewardToken.safeTransferFrom(REWARD_TOKEN_POOL, staker, claimableAmt);

        emit TokensClaimed(staker, index, claimableAmt);

        return claimableAmt;
    }

