pragma solidity 0.6.12;
pragma experimental ABIEncoderV2;

import "hardhat/console.sol";
import "@boringcrypto/boring-solidity/contracts/libraries/BoringERC20.sol";
import "@sushiswap/core/contracts/uniswapv2/interfaces/IUniswapV2Pair.sol";

contract SwapStrategy {
    
    using BoringERC20 for IERC20;
    
    bytes32 public constant EXECUTE_TYPEHASH = keccak256("Swap(address strategy,address owner,uint256 nonce,uint256 deadline,address feeToken,uint256 feeAmount,address inputToken,uint256 inputAmount,address outputToken,uint256 minOutputAmount)");
    bytes32 public immutable DOMAIN_SEPARATOR;
    bytes32 public immutable pairCodeHash;
    address public immutable factory;
    
    constructor (bytes32 _domain_separator, address _factory, bytes32 _pairCodeHash) public {
        DOMAIN_SEPARATOR = _domain_separator;
        pairCodeHash = _pairCodeHash;
        factory = _factory;
    }
    
    struct SwapTransaction {
        address strategy;
        address owner;
        uint256 nonce;
        uint256 deadline;
        IERC20 feeToken;
        uint256 feeAmount; 
        IERC20 inputToken;
        uint256 inputAmount;
        IERC20 outputToken;
        uint256 minOutputAmount;
        uint8 v;
        bytes32 r; 
        bytes32 s;
        address feeTo;
        IERC20 inbetweenToken;
    }

    function execute(bytes memory _data) public {
        (address owner,
        IERC20 feeToken, 
        uint256 feeAmount,
        IERC20 inputToken,
        uint256 inputAmount,
        IERC20 outputToken,
        uint256 minOutputAmount,
        address feeTo,
        IERC20 inbetweenToken) = checkSignature(_data);

        if (address(inbetweenToken) == address(0)) {
            directSwap(
                owner,
                feeToken,
                feeAmount,
                inputToken,
                inputAmount,
                outputToken,
                minOutputAmount,
                feeTo);
        } else {
            swapWithHop(
                owner,
                feeToken, 
                feeAmount,
                inputToken,
                inputAmount,
                outputToken,
                minOutputAmount,
                feeTo,
                inbetweenToken);
        }
    }
    
    function directSwap(
        address owner,
        IERC20 feeToken, 
        uint256 feeAmount,
        IERC20 inputToken,
        uint256 inputAmount,
        IERC20 outputToken,
        uint256 minOutputAmount,
        address feeTo
    ) internal {
        
        uint256 received;
            
        IUniswapV2Pair pair = getPair(inputToken, outputToken);
        
        inputToken.safeTransferFrom(owner, address(pair), inputAmount);
        
        if (feeToken == outputToken) {
        
            received = _swap(inputToken, outputToken, pair, address(this), inputAmount);
            require(received > feeAmount);
            received -= feeAmount;
            outputToken.safeTransfer(owner, received);
            outputToken.safeTransfer(feeTo, feeAmount);
        
        } else {
            
            received = _swap(inputToken, outputToken, pair, owner, inputAmount);
            feeToken.safeTransferFrom(owner, feeTo, feeAmount);
            
        }
        
        require(received > minOutputAmount);
    }
    
    // @notice instead of passing a path array for swaps, pass an optional parameter inbetweenToken
    // @notice this allows the following trade: inputToken -> inbetweenToken -> outputToken
    // @notice the inbetweenToken is not part of the signature
    function swapWithHop(
        address owner,
        IERC20 feeToken, 
        uint256 feeAmount,
        IERC20 inputToken,
        uint256 inputAmount,
        IERC20 outputToken,
        uint256 minOutputAmount,
        address feeTo,
        IERC20 inbetweenToken
    ) internal {
        
        uint256 received;
        
        IUniswapV2Pair pair0 = getPair(inputToken, inbetweenToken);
        IUniswapV2Pair pair1 = getPair(inbetweenToken, outputToken);
        
        inputToken.safeTransferFrom(owner, address(pair0), inputAmount);
            
        received = _swap(inputToken, inbetweenToken, pair0, address(pair1), inputAmount);
            
        if (feeToken == outputToken) {
            
            received = _swap(inbetweenToken, outputToken, pair1, address(this), received);
            require(received > feeAmount, "!output");
            received -= feeAmount;
            outputToken.safeTransfer(owner, received);
            outputToken.safeTransfer(feeTo, feeAmount);
        
        } else {
            
            received = _swap(inbetweenToken, outputToken, pair1, owner, received);
            feeToken.safeTransferFrom(owner, feeTo, feeAmount);
            
        }
        
        require(received > minOutputAmount, "!minOutput");
    }
    
    function checkSignature(bytes memory _data) public view returns(address, IERC20, uint256, IERC20, uint256, IERC20, uint256, address, IERC20) {

        SwapTransaction memory swapTx = abi.decode(_data, (SwapTransaction));

        bytes32 digest = keccak256(abi.encodePacked(
            "\x19\x01",
            DOMAIN_SEPARATOR,
            keccak256(abi.encode(
                EXECUTE_TYPEHASH,
                swapTx.strategy,
                swapTx.owner,
                swapTx.nonce,
                swapTx.deadline,
                swapTx.feeToken,
                swapTx.feeAmount,
                swapTx.inputToken,
                swapTx.inputAmount,
                swapTx.outputToken,
                swapTx.minOutputAmount
            ))
        ));

        require(swapTx.owner == ecrecover(digest, swapTx.v, swapTx.r, swapTx.s), "!signature");

        return (
            swapTx.owner,
            swapTx.feeToken, 
            swapTx.feeAmount,
            swapTx.inputToken,
            swapTx.inputAmount,
            swapTx.outputToken,
            swapTx.minOutputAmount,
            swapTx.feeTo,
            swapTx.inbetweenToken
        );
    }
    
    // calculate the output amount for the xy=k amm trade given the input amount
    function getAmountOut(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    ) internal pure returns (uint256 amountOut) {
        uint256 amountInWithFee = amountIn * 997;
        uint256 numerator = amountInWithFee * reserveOut;
        uint256 denominator = reserveIn * 1000 + amountInWithFee;
        amountOut = numerator / denominator;
    }
    
    // execute the swap
    // assume tokens are already transfered to the SushiSwap pair
    function _swap(
        IERC20 inputToken,
        IERC20 outputToken,
        IUniswapV2Pair pair,
        address receiver,
        uint256 amountIn
    ) internal returns (uint256 amountOut) {
        
        (uint256 reserve0, uint256 reserve1, ) = pair.getReserves();
        
        if (inputToken < outputToken) {
            amountOut = getAmountOut(amountIn, reserve0, reserve1);
            pair.swap(0, amountOut, receiver, new bytes(0));
        } else {
            amountOut = getAmountOut(amountIn, reserve1, reserve0);
            pair.swap(amountOut, 0, receiver, new bytes(0));
        }
        
    }
    
    function getPair(IERC20 token0, IERC20 token1) public view returns (IUniswapV2Pair) {
        (IERC20 _token0, IERC20 _token1) = token0 < token1 ? (token0, token1) : (token1, token0);
        return IUniswapV2Pair(address(uint256(keccak256(abi.encodePacked(
            hex"ff",
            factory,
            keccak256(abi.encodePacked(_token0, _token1)),
            pairCodeHash
        )))));
    }
    
}

