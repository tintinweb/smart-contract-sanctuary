contract Constants {
    bytes32 internal constant _permissionless_  = 'permissionless';
    //bytes32 internal constant _mintFeeRate_         = 'mintFeeRate';
    bytes32 internal constant _mintFeeTo_           = 'mintFeeTo';
    //bytes32 internal constant _WETH_            = 'WETH';
    //bytes32 internal constant _swapFactory_     = 'swapFactory';
    //bytes32 internal constant _swapRouter_      = 'swapRouter';
    //bytes32 internal constant _isAuthority_     = 'isAuthority';
    
    uint256 internal constant MAX_FEE_RATE      = 0.10 ether;   // 10%
    
    bytes4 internal constant _INTERFACE_ID_ERC721 = 0x80ac58cd;
}


contract NDR is ERC20UpgradeSafe {
    using SafeERC20 for IERC20;
    using SafeMath for uint;
    using Strings for uint;
    
    address public factory;
    address public nft;
    uint public tokenId;

    function __NDR_init(address nft_, uint tokenId_) external initializer {
        (string memory name, string memory symbol) = spellNameAndSymbol(nft_, tokenId_);
        __ERC20_init(name, symbol);
        //_setupDecimals(18);

        factory = _msgSender();
        nft = nft_;
        tokenId = tokenId_;
    }
    
    function spellNameAndSymbol(address nft_, uint tokenId_) public view returns (string memory name, string memory symbol) {
        string memory sid = tokenId_.toString();
        name = string(abi.encodePacked('CryptoShow.art NDR: ', IERC721Metadata(nft_).name(), ' [', sid, ']'));
        symbol = string(abi.encodePacked(IERC721Metadata(nft_).symbol(), '[', sid, ']'));
    }

    function setNameAndSymbol(string memory name, string memory symbol) external {
        require(_msgSender() == Factory(factory).governor() || _msgSender() == __AdminUpgradeabilityProxy__(address(uint160(factory))).__admin__());
        _name = name;
        _symbol = symbol;
    }

    function _transfer(address sender, address recipient, uint256 amount) virtual override internal {
        uint fee = 0;
        
        if(!Factory(factory).inFreeList(sender, recipient))
            fee = amount.mul(Factory(factory).txFeeRate()).div(1e18);

        _beforeTokenTransfer(sender, recipient, amount);

        _balances[sender] = _balances[sender].sub(amount, "ERC20: transfer amount exceeds balance");
        _balances[recipient] = _balances[recipient].add(amount).sub(fee);
        emit Transfer(sender, recipient, amount);
        if(fee > 0) {
            _balances[address(0)] = _balances[address(0)].add(fee);
            emit Transfer(recipient, address(0), fee);
        }
    }
    
    modifier onlyFactory {
        require(_msgSender() == factory, 'Only Factory');
        _;
    }
    
    function withdraw_(address to) external onlyFactory {
        IERC721(nft).safeTransferFrom(address(this), to, tokenId);
    }

    function transfer_(address sender, address recipient, uint256 amount) external onlyFactory {
        _transfer(sender, recipient, amount);
    }
    
    function mint_(address _to, uint volume) external onlyFactory {
        _mint(_to, volume);
    }
    
    function burn_(address _from, uint volume) external onlyFactory {
        _burn(_from, volume);
    }

    // Reserved storage space to allow for layout changes in the future.
    uint256[46] private ______gap;
}


contract Factory is Configurable, ContextUpgradeSafe, Constants {
    using SafeERC20 for IERC20;
    using SafeMath for uint;
    using Address for address;

    address public productImplementation;
    mapping(address => mapping(uint => address)) public ndrs;     // uft => tokenId => ndr
    address[] public allNdrs;
    
    function length() public view returns (uint) {
        return allNdrs.length;
    }
    
    mapping(address => bool) public freeList;
    uint public txFeeRate;
    uint public mintFeeRate;
    
    function setFee(uint txFeeRate_, uint mintFeeRate_, address mintFeeTo) public governance {
        require(txFeeRate_ <= MAX_FEE_RATE && mintFeeRate_ <= MAX_FEE_RATE);
        txFeeRate = txFeeRate_;
        mintFeeRate = mintFeeRate_;
        config[_mintFeeTo_] = uint(mintFeeTo);
    }
    
    function addFreeList(address addr) external governance {
        require(!freeList[addr], 'already in freeList');
        freeList[addr] = true;
    }
    
    function removeFreeList(address addr) external governance {
        require(freeList[addr], 'not in freeList');
        freeList[addr] = false;
    }
    
    function inFreeList(address sender, address recipient) public view returns (bool) {
        return freeList[sender] || freeList[recipient];
    }

    function __Factory_init(address governor, address implNDR, address mintFeeTo/*, address swapRouter*/) public initializer {
        __Governable_init_unchained(governor);
        __Context_init_unchained();
        __Factory_init_unchained(implNDR, mintFeeTo/*, swapRouter*/);
    }

    function __Factory_init_unchained(address implNDR, address mintFeeTo/*, address swapRouter*/) public governance {
        productImplementation  = implNDR;
        setFee(0.025 ether, 0.05 ether, mintFeeTo);     // 2.5%, 5%
        //config[_swapRouter_]            = uint(swapRouter);
        //config[_WETH_]                  = uint(IUniswapV2Router01(swapRouter).WETH());
    }
    
    function upgradeProductImplementationTo(address implNDR) external governance {
        productImplementation = implNDR;
    }
    
    function createNDR(address nft, uint tokenId, uint totalSupply, address to) public returns (address ndr) {
        require(nft != address(0), 'ZERO_ADDRESS');
        require(nft.isContract(), 'nft should isContract');
        require(IERC165(nft).supportsInterface(_INTERFACE_ID_ERC721), 'nft should supportsInterface(_INTERFACE_ID_ERC721)');
        require(config[_permissionless_] != 0 || _msgSender() == governor);

        require(ndrs[nft][tokenId] == address(0), 'the NDR exist already');

        bytes memory bytecode = type(InitializableProductProxy).creationCode;

        bytes32 salt = keccak256(abi.encodePacked(nft, tokenId));
        assembly {
            ndr := create2(0, add(bytecode, 32), mload(bytecode), salt)
        }
        InitializableProductProxy(payable(ndr)).__InitializableProductProxy_init(address(this), 0, abi.encodeWithSignature('__NDR_init(address,uint256)', nft, tokenId));

        IERC721(nft).transferFrom(_msgSender(), address(this), tokenId);

        uint fee = totalSupply.mul(mintFeeRate).div(1e18);
        NDR(ndr).mint_(address(config[_mintFeeTo_]), fee);
        NDR(ndr).mint_(to, totalSupply.sub(fee));
        
        ndrs[nft][tokenId] = ndr;
        allNdrs.push(ndr);
        emit CreateNDR(_msgSender(), nft, tokenId, ndr, allNdrs.length);
    }
    event CreateNDR(address indexed creator, address indexed nft, uint indexed tokenId, address ndr, uint count);

    // Reserved storage space to allow for layout changes in the future.
    uint256[44] private ______gap;
}