pragma solidity ^0.8.7;

import "hardhat/console.sol";
import "contracts/Token.sol";
import "contracts/Nft.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";

contract MisteryBox is VRFConsumerBase, Ownable {
    string private greeting;
    uint256 internal fee;
    bytes32 internal keyHash;
    uint256 public tokenPrice;
    uint256 public ethPrice;
    EggsNFT public nftContract;
    Token public token;
    mapping(bytes32 => Order) private orders;
    mapping(uint8 => EggsNFT.Element) private elementMap;
    mapping(uint8 => EggsNFT.MagicSchool) private magicSchoolMap;

    enum Currency {
        ETH,
        TOKEN
    }

    struct Order {
        address buyer;
        Currency currency;
        uint8 quantity;
    }

    event Purchased(address buyer, bytes32 requestId);

    constructor(
        address _tokenAddress,
        address vrfCoordinator,
        address link,
        bytes32 _keyHash
    ) VRFConsumerBase(vrfCoordinator, link) {
        token = Token(_tokenAddress);
        keyHash = _keyHash;
        fee = 0.1 * 10**18;
        tokenPrice = 1;
        ethPrice = 1;
        setMappings();
    }

    // owner only stuff
    function setTokenPrice(uint256 newPrice) public onlyOwner {
        tokenPrice = newPrice;
    }

    function setEthPrice(uint256 newPrice) public onlyOwner {
        ethPrice = newPrice;
    }

    function setNftContractAddress(address _nftContract) public onlyOwner {
        nftContract = EggsNFT(_nftContract);
    }

    function withdraw() public onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    // internals

    function setMappings() internal {
        elementMap[0] = EggsNFT.Element.FIRE;
        elementMap[1] = EggsNFT.Element.WATER;
        elementMap[2] = EggsNFT.Element.ICE;
        elementMap[3] = EggsNFT.Element.WIND;
        elementMap[4] = EggsNFT.Element.ETHEREAL;
        elementMap[5] = EggsNFT.Element.ABYSS;
        magicSchoolMap[0] = EggsNFT.MagicSchool.Leaf;
        magicSchoolMap[1] = EggsNFT.MagicSchool.Wave;
        magicSchoolMap[2] = EggsNFT.MagicSchool.Cloud;
    }

    function checkLink() internal view {
        require(LINK.balanceOf(address(this)) >= fee, "Out of LINKS");
    }

    // this is here just so we can mock it in the testableMisteryBox
    function randomRequest(bytes32 _keyHash, uint256 _fee)
        internal
        virtual
        returns (bytes32 requestId)
    {
        return requestRandomness(_keyHash, _fee);
    }

    function setOrders(
        address buyer,
        Currency currency,
        bytes32 requestId,
        uint8 quantity
    ) internal {
        orders[requestId] = Order({
            buyer: buyer,
            currency: currency,
            quantity: quantity
        });
    }

    function animalForRandomness(uint256 randomNumber)
        internal
        pure
        returns (EggsNFT.Animal)
    {
        if (randomNumber < 10) {
            // .1%
            return EggsNFT.Animal.RedPanda;
        } else if (randomNumber < 100) {
            // .99%
            return EggsNFT.Animal.Bear;
        } else if (randomNumber < 2000) {
            // 19%
            return EggsNFT.Animal.Falcon;
        } else if (randomNumber < 5000) {
            // 30%
            return EggsNFT.Animal.Moth;
        } else if (randomNumber < 9950) {
            return EggsNFT.Animal.Narwhal;
        } else {
            return EggsNFT.Animal.Turtle;
        }
    }

    function typeForRandomness(uint256 randomNumber)
        internal
        view
        returns (EggsNFT.Element)
    {
        return elementMap[SafeCast.toUint8(randomNumber % 6)];
    }

    function magicSchoolForRandomness(uint256 randomNumber)
        internal
        view
        returns (EggsNFT.MagicSchool)
    {
        return magicSchoolMap[SafeCast.toUint8(randomNumber % 3)];
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness)
        internal
        override
    {
        Order memory order = orders[requestId];

        // start at 0, allow up to 10000 for .01% chances
        uint128 modIncrementer = 10000;

        for (uint256 i = 0; i < order.quantity; i++) {
            modIncrementer++;

            uint256 animalRandom = randomness % modIncrementer;
            modIncrementer++;

            uint256 typeRandom = randomness % modIncrementer;
            modIncrementer++;

            uint256 schoolRandom = randomness % modIncrementer;
            modIncrementer++;

            uint256 mindRandom = randomness % modIncrementer;
            modIncrementer++;

            uint256 bodyRandom = randomness % modIncrementer;
            modIncrementer++;

            uint256 spiritRandom = randomness % modIncrementer;
            modIncrementer++;

            uint256 chiRandom = randomness % modIncrementer;
            modIncrementer++;

            nftContract.safeMint(
                order.buyer,
                animalForRandomness(animalRandom % 10000),
                typeForRandomness(typeRandom % 100),
                magicSchoolForRandomness(schoolRandom % 100),
                SafeCast.toUint8(mindRandom % 100),
                SafeCast.toUint8(bodyRandom % 100),
                SafeCast.toUint8(spiritRandom % 100),
                SafeCast.toUint8(chiRandom % 100)
            );
        }
    }

    function validateOrder(uint256 amount) internal view {
        require(amount <= 100, "Cannot order more than 100");
        require(amount > 0, "You need to buy at least 1");
        checkLink();
    }

    // public interface

    function buyWithToken(uint8 amount) public returns (bytes32 requestId) {
        validateOrder(amount);
        uint256 totalPrice = amount * tokenPrice;
        require(
            token.balanceOf(msg.sender) >= totalPrice,
            "You dont have enough tokens"
        );
        token.transferFrom(msg.sender, address(this), totalPrice);
        requestId = randomRequest(keyHash, fee);
        setOrders(msg.sender, Currency.TOKEN, requestId, amount);
        emit Purchased(msg.sender, requestId);
        return requestId;
    }

    function buyWithEth(uint8 amount)
        public
        payable
        returns (bytes32 requestId)
    {
        validateOrder(amount);
        require(msg.value == ethPrice * amount, "You didn't send enough ETH");
        requestId = randomRequest(keyHash, fee);
        setOrders(msg.sender, Currency.ETH, requestId, amount);
        emit Purchased(msg.sender, requestId);
        return requestId;
    }
}

