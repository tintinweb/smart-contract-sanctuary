// Purely for Creating sales , handle price.
contract ECFMarket1155 is Ownable, ERC1155Holder {
    using SafeMath for uint256;
    // Card related
    struct CardStructure {
        uint256 maxPrice;
        uint256 quantity;
        string cardType;
    }
    mapping(uint256 => CardStructure) CardData;

    uint256 public sale_start;
    uint256 public sale_end;
    IGeneric1155 generic;
    mapping(address => bool) auth;
    address payable public wallet;

    event Refund(
        address buyer,
        uint256 sent,
        uint256 purchased,
        uint256 refund
    );
    event newSalesEndDate(uint256 endDate);

    modifier sale_active() {
        require(getTimestamp() >= sale_start, "Sales not started");
        require(getTimestamp() <= sale_end, "Sale ended");
        _;
    }

    modifier onlyAuth() {
        require(auth[msg.sender] || (msg.sender == owner()), "Not Authorised");
        _;
    }

    // Start
    constructor(
        uint256 _start,
        uint256 _end,
        address payable _wallet,
        IGeneric1155 _generic
    ) public {
        wallet = _wallet;
        sale_start = _start;
        sale_end = _end;
        generic = _generic;
    }

    function mintNew(
        uint256 quantity,
        string memory ipfsHash,
        string memory _cardType,
        uint256 maxPrice
    ) external onlyAuth {
        uint256 tokenIndex = generic.tokenCount();
        generic.mint(quantity, ipfsHash, _cardType);
        CardData[tokenIndex].maxPrice = maxPrice;
        CardData[tokenIndex].cardType = _cardType;
        CardData[tokenIndex].quantity = quantity;
    }

    function buyCard(uint256 _cardID) external payable sale_active() {
        uint256 tokenIndex = generic.tokenCount();
        require(_cardID < tokenIndex, "Invalid card type");
        require(getCardRemaining(_cardID) > 0, "No remaining cards");
        uint256 balance = msg.value;
        uint256 price = getCard_price(_cardID);
        if (balance < price) {
            revert("Didn't send enough");
        }
        for (uint256 j = 0; j < 20; j++) {
            // Will run out of gas, if more than 60.
            price = getCard_price(_cardID);
            // Refund
            if (balance < price) {
                payable(wallet).transfer(msg.value.sub(balance));
                payable(msg.sender).transfer(balance);
                emit Refund(msg.sender, msg.value, j, balance);
                return;
            }

            generic.safeTransferFrom(
                address(this),
                msg.sender,
                _cardID,
                1,
                " "
            );
            balance = balance.sub(price);
        }
        // To refund the left over back to Buyer.
        payable(wallet).transfer(msg.value.sub(balance));
        payable(msg.sender).transfer(balance);
        emit Refund(msg.sender, msg.value, 20, balance);
    }

    function getCard_price(uint256 _cardID) public view returns (uint256) {
        // Expontential decay.
        uint256 initialBalance = getCardQuantity(_cardID);
        uint256 remainingCards = getCardRemaining(_cardID);
        uint256 multipler = initialBalance - remainingCards;
        uint256 maxPrice = CardData[_cardID].maxPrice;
        uint256 finalPrice = ((multipler) / (initialBalance / (maxPrice)));
        /*
        console.log(
            " Card price %s  , remaining %s , initial %s ",
            finalPrice,
            remainingCards,initialBalance
        );
        */
        return ((finalPrice + 1) * 1 ether);

        /*
        
        */
    }

    function getCardRemaining(uint256 _cardID) public view returns (uint256) {
        return (generic.balanceOf(address(this), _cardID));
    }

    function getCardQuantity(uint256 _cardID) public view returns (uint256) {
        return CardData[_cardID].quantity;
    }

    function getCardTypeData(uint256 _cardID)
        public
        view
        returns (string memory)
    {
        return CardData[_cardID].cardType;
    }

    function mintTheRest(uint256 _cardID) external onlyOwner {
        require(sale_is_over(), " sales is not over yet ");
        generic.safeTransferFrom(
            address(this),
            owner(),
            _cardID,
            getCardRemaining(_cardID),
            " "
        );
    }

    // This function is called for plain Ether transfers, i.e.
    // for every call with empty calldata.
    receive() external payable {
        // React to receiving ether
    }

    // Viewer

    function getTimestamp() public view virtual returns (uint256) {
        return block.timestamp;
    }

    function how_long_more(bool _end)
        public
        view
        returns (
            uint256 Days,
            uint256 Hours,
            uint256 Minutes,
            uint256 Seconds
        )
    {
        uint256 gap;
        if (_end) {
            require(getTimestamp() < sale_end, "Sales ended");
            gap = sale_end - getTimestamp();
        } else {
            require(getTimestamp() < sale_start, "Missed It");
            gap = sale_start - getTimestamp();
        }
        Days = gap / (24 * 60 * 60);
        gap = gap % (24 * 60 * 60);
        Hours = gap / (60 * 60);
        gap = gap % (60 * 60);
        Minutes = gap / 60;
        Seconds = gap % 60;
        return (Days, Hours, Minutes, Seconds);
    }

    function sale_is_over() public view returns (bool) {
        return (getTimestamp() > sale_end);
    }

    function drain(xERC20 _token) external onlyOwner {
        if (address(_token) == 0x0000000000000000000000000000000000000000) {
            payable(owner()).transfer(address(this).balance);
        } else {
            _token.transfer(owner(), _token.balanceOf(address(this)));
        }
    }

    function retrieve721(address _tracker, uint256 _id) external onlyOwner {
        IERC721(_tracker).transferFrom(address(this), msg.sender, _id);
    }

    function extendSales(uint256 _seconds) external onlyOwner {
        // extend no of weeks.
        sale_end = sale_end.add(_seconds);
        emit newSalesEndDate(sale_end);
    }

    function stopSales() external onlyOwner {
        // end sales.
        sale_end = getTimestamp();
        emit newSalesEndDate(sale_end);
    }
}

