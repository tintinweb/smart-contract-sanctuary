pragma solidity ^0.8.0;
//SPDX-License-Identifier: UNLICENSED

import "./interfaces/IDAOVoting.sol";
import "./interfaces/IAskoDAO.sol";
import "../proposals/interfaces/IProposalManager.sol";
import "../utils/interfaces/ITimelock.sol";
import "../utils/interfaces/IDAOAccess.sol";
import "../tokens/interfaces/IxASKO.sol";
import "../proposals/Proposals.sol";

import "hardhat/console.sol";

contract DAOVoting is IDAOVoting {
    using Proposals for Proposals.ProposalReceipt;
    using Proposals for Proposals.EntityType;
    using Proposals for Proposals.ProposalState;
    using Proposals for Proposals.ProductProposal;
    using Proposals for Proposals.DevelopmentProposal;

    IProposalManager public proposalManager;
    ITimelock public timelock;
    IxASKO public _xAsko;
    IDAOAccess public daoAccess;

    address public daoAddress;

    struct TimelockProposal {
        address target;
        uint256 value;
        string signature;
        bytes calldatas;
        uint256 eta;
    }

    /// @notice mapping of proposalid to next timelockAction
    mapping(uint256 => TimelockProposal) public proposalActions;

    /// @notice reciept map for proposals, third mapping is for currentInterval's use for continuation voting, will be 0 for all else
    mapping(address => mapping(uint256 => mapping(uint256 => Proposals.ProposalReceipt[6])))
        public proposalReciepts;

    address[] hasVotedCont;

    /// @notice Cast yes vote event
    event CastYesVote(
        uint256 proposalId,
        address voter,
        uint256 votes,
        bool support
    );

    /// @notice Cast approval vote development proposal event
    event CastApprovalVote(uint256 proposalId, address voter, bool support);

    /// @notice Cast appproval vote product proposal event
    event CastVetoVote(uint256 proposalId, address voter, bool support);

    /// @notice Open proposal event
    event OpenProposal(uint256 proposalId, address admin);

    /// @notice Accepted proposal event
    event AcceptProposal(uint256 proposalId);

    /// @notice Accepted proposal event
    event ProposalVetod(uint256 proposalId);

    /**
     * Constructs the ASKO DAO.
     * See RateGovernor for information on the parameters.
     */
    constructor(
        address daoAccessAddress,
        address proposalManagerAddress,
        address timelockAddress,
        address xAskoAddress
    ) {
        daoAccess = IDAOAccess(daoAccessAddress);
        proposalManager = IProposalManager(proposalManagerAddress);
        timelock = ITimelock(timelockAddress);
        _xAsko = IxASKO(xAskoAddress);
    }

    modifier onlyDAO() {
        require(
            msg.sender == daoAddress,
            "only the DAO contract can call this function"
        );
        _;
    }

    modifier onlyGuardian() {
        require(
            daoAccess.isGuardian(msg.sender),
            "Must have guardian permissions"
        );
        _;
    }

    modifier onlyTimelock() {
        require(msg.sender == address(timelock));
        _;
    }

    function setDAO(address _daoAddress) public onlyGuardian() {
        daoAddress = _daoAddress;
    }

    function removeProposal(uint256 proposalId) internal {
        IAskoDAO(daoAddress).returnBudget(proposalId);
        proposalManager.resetHasActiveProposal(proposalId);
        proposalManager.setState(
            proposalId,
            uint256(Proposals.ProposalState.REMOVED)
        );
    }

    /**
     * Approves product proposal for ASKO DAO voting
     * @param proposalId proposal ID to approve.
     */
    function openProposal(uint256 proposalId, address sender)
        public
        override
        onlyDAO
    {
        _openProposal(proposalId, sender);
    }

    function _openProposal(uint256 proposalId, address admin) internal {
        require(
            proposalManager.getState(proposalId) ==
                uint256(Proposals.ProposalState.PROPOSED),
            "Has already been opened."
        );

        // set timelock for close vote here
        uint256 votingPeriod = 0 days;
        string memory signature = "closeDDFVote(uint256)";
        uint256 state = 2;

        if (
            proposalManager.getType(proposalId) ==
            uint256(Proposals.EntityType.PRODUCT)
        ) {
            signature = "closeYesVote(uint256)";
            state = 1;
        }

        proposalManager.setState(proposalId, state);

        timelock.queueTransaction(
            address(this),
            0,
            signature,
            abi.encode(proposalId),
            block.timestamp + votingPeriod
        );
        proposalActions[proposalId] = TimelockProposal({
            target: address(this),
            value: 0,
            signature: signature,
            calldatas: abi.encode(proposalId),
            eta: block.timestamp + votingPeriod
        });

        emit OpenProposal(proposalId, admin);
    }

    function closeYesVote(uint256 proposalId) public onlyTimelock {
        //console.log("closing yes vote");
        require(
            proposalManager.getState(proposalId) ==
                uint256(Proposals.ProposalState.OPEN),
            "Proposal has not been approved by an admin/already been executed or removed"
        );

        // check if vote passes treshold * 100
        //console.log(proposalManager.getVotes(proposalId, 5, true) * 100);
        //console.log(proposalManager.getVotes(proposalId, 2, false) * 100);
        //console.log(proposalManager.getVoteThreshold(proposalId, 5));
        if (
            //check yes votes pass threshold for yesRate
            (proposalManager.getVotes(proposalId, 2, true) * 100) >
            proposalManager.getVoteThreshold(proposalId, 2) *
                _xAsko.getPriorSupply(
                    proposalManager.getVotingStartBlock(proposalId)
                ) &&
            //check yes votes outnumber no votes
            (proposalManager.getVotes(proposalId, 2, true) >
                proposalManager.getVotes(proposalId, 2, false)) &&
            //check not enough admins have voted to veto
            (proposalManager.getVotes(proposalId, 5, true) * 100) <
            proposalManager.getVoteThreshold(proposalId, 5) * 100
        ) {
            proposalManager.setState(
                proposalId,
                uint256(Proposals.ProposalState.ACCEPTED)
            );

            //queue closing of DDFVote in timelock
            uint256 votingPeriod = 0 days;
            timelock.queueTransaction(
                address(this),
                0,
                "closeDDFVote(uint256)",
                abi.encode(proposalId),
                block.timestamp + votingPeriod
            );
            proposalActions[proposalId] = TimelockProposal({
                target: address(this),
                value: 0,
                signature: "closeDDFVote(uint256)",
                calldatas: abi.encode(proposalId),
                eta: block.timestamp + votingPeriod
            });
            

            emit AcceptProposal(proposalId);
        } else {
            removeProposal(proposalId);
        }
    }

    function closeDDFVote(uint256 proposalId) public onlyTimelock {
        //console.log("closing DDF vote");
        require(
            proposalManager.getState(proposalId) ==
                uint256(Proposals.ProposalState.ACCEPTED),
            "Proposal has not been approved by the DDF/already been executed or removed"
        );

        // check if vote passes treshold * 100
        //console.log(proposalManager.getVotes(proposalId, 3, true) * 100);
        //console.log(proposalManager.getVotes(proposalId, 3, false) * 100);
        //console.log(proposalManager.getVoteThreshold(proposalId, 3));
        if (
            ((proposalManager.getVotes(proposalId, 3, true) * 100) >
                proposalManager.getVoteThreshold(proposalId, 3)) &&
            (proposalManager.getVotes(proposalId, 3, true) >
                proposalManager.getVotes(proposalId, 3, false))
        ) {
            if (
                proposalManager.getType(proposalId) ==
                uint256(Proposals.EntityType.DEVELOPMENT)
            ) {
                proposalManager.setState(
                    proposalId,
                    uint256(Proposals.ProposalState.EXECUTED)
                );
                proposalManager.resetHasActiveProposal(proposalId);
                // send tokens to developer address, store in askoDAO for time being
                _xAsko.transferFrom(
                    daoAddress,
                    proposalManager.getReceiver(proposalId),
                    proposalManager.getBudget(proposalId)
                );
            } else {
                // send initial budget slice
                (uint256 devDays, uint256 interval, ) = proposalManager
                .getDevDays(proposalId);
                _xAsko.transferFrom(
                    daoAddress,
                    proposalManager.getReceiver(proposalId),
                    proposalManager.getBudget(proposalId) / interval
                );
                proposalManager.incrementInterval(proposalId);
                // queue continuation vote in timelock
                uint256 votingPeriod = ((devDays * 86400) / (interval - 1));
                //console.log(devDays/(interval-1) * currentInterval);
                timelock.queueTransaction(
                    address(this),
                    0,
                    "closeContVote(uint256)",
                    abi.encode(proposalId),
                    block.timestamp + votingPeriod
                );
                proposalActions[proposalId] = TimelockProposal({
                    target: address(this),
                    value: 0,
                    signature: "closeContVote(uint256)",
                    calldatas: abi.encode(proposalId),
                    eta: block.timestamp + votingPeriod
                });

                proposalManager.setState(
                    proposalId,
                    uint256(Proposals.ProposalState.APPROVED)
                );
            }

            emit AcceptProposal(proposalId);
        } else {
            removeProposal(proposalId);
        }
    }

    function closeContVote(uint256 proposalId) public onlyTimelock {
        //console.log("closing Continuation vote");
        require(
            proposalManager.getState(proposalId) ==
                uint256(Proposals.ProposalState.APPROVED),
            "Proposal has not been approved by the DDF/already been executed or removed"
        );
        // check if vote passes treshold * 100
        //console.log(proposalManager.getVotes(proposalId, 4, true) * 100);
        console.log(proposalManager.getVotes(proposalId, 4, false) * 100);
        console.log(proposalManager.getVoteThreshold(proposalId, 4));
        if (
            (proposalManager.getVotes(proposalId, 4, false) * 100) <
            (100 - proposalManager.getVoteThreshold(proposalId, 4)) *
                _xAsko.getPriorSupply(
                    proposalManager.getVotingStartBlock(proposalId)
                ) ||
            (proposalManager.getVotes(proposalId, 4, true) >
                proposalManager.getVotes(proposalId, 4, false))
        ) {
            //check if this is the last interval
            (
                uint256 devDays,
                uint256 interval,
                uint256 currentInterval
            ) = proposalManager.getDevDays(proposalId);
            uint256 budget = proposalManager.getBudget(proposalId);
            _xAsko.transferFrom(
                daoAddress,
                proposalManager.getReceiver(proposalId),
                budget / interval
            );
            if (currentInterval == interval-1) {
                //console.log("end it");
                uint256 remainingTokens = budget - ((budget/interval)*interval);
                console.log(remainingTokens);
                 _xAsko.transferFrom(
                daoAddress,
                proposalManager.getReceiver(proposalId),
                remainingTokens
            );
                proposalManager.setState(
                    proposalId,
                    uint256(Proposals.ProposalState.EXECUTED)
                );
                proposalManager.resetHasActiveProposal(proposalId);
            } else {
                uint256 votingPeriod = 0 /*((devDays * 86400) / (interval - 1))*/;
                //console.log(devDays/(interval-1) * currentInterval);
                timelock.queueTransaction(
                    address(this),
                    0,
                    "closeContVote(uint256)",
                    abi.encode(proposalId),
                    block.timestamp + votingPeriod
                );
                proposalActions[proposalId] = TimelockProposal({
                    target: address(this),
                    value: 0,
                    signature: "closeContVote(uint256)",
                    calldatas: abi.encode(proposalId),
                    eta: block.timestamp + votingPeriod
                });
                proposalManager.incrementInterval(proposalId);
                proposalManager.setVotes(proposalId, 4, true, 0);
                proposalManager.setVotes(proposalId, 4, false, 0);
                // for(uint i = 0; i < hasVotedCont.length; i++)
                // {
                //     proposalReciepts[hasVotedCont[i]][proposalId][0][4].hasVoted = false;
                // }
            }
            emit AcceptProposal(proposalId);
        } else {
            removeProposal(proposalId);
        }
    }

    function executeCloseVote(uint256 proposalId)
        public
        payable
        override
        onlyDAO
    {
        TimelockProposal storage proposal = proposalActions[proposalId];
        timelock.executeTransaction(
            proposal.target,
            proposal.value,
            proposal.signature,
            proposal.calldatas,
            proposal.eta
        );
    }

    /**
     * Cast a yes vote on a proposal.
     * @param proposalId proposal ID to approve.
     */
    function voteYes(
        uint256 proposalId,
        bool support,
        address sender
    ) public override onlyDAO {
        _weightedVote(proposalId, support, sender, 2);
    }

    function contVote(
        uint256 proposalId,
        bool support,
        address sender
    ) public override onlyDAO {
        _weightedVote(proposalId, support, sender, 4);
    }

    function _weightedVote(
        uint256 proposalId,
        bool support,
        address sender,
        uint8 rate
    ) internal {
        // pass checks
        uint256 currentInterval = 0;
        if(rate == 4)
        {
            (,,currentInterval) = proposalManager.getDevDays(proposalId);
        }
        require(
            proposalManager.getType(proposalId) ==
                uint256(Proposals.EntityType.PRODUCT),
            "Must be a product proposal"
        );
        require(
            !proposalReciepts[sender][proposalId][currentInterval][rate].hasVoted,
            "Cannot vote again"
        );
        require(
            (rate == 2 &&
                proposalManager.getState(proposalId) ==
                uint256(Proposals.ProposalState.OPEN)) ||
                (rate == 4 &&
                    proposalManager.getState(proposalId) ==
                    uint256(Proposals.ProposalState.APPROVED)),
            "Proposal is in the incorrect state"
        );

        // console.log(
        //     _xAsko.getPriorVotes(
        //         sender,
        //         proposalManager.getVotingStartBlock(proposalId)
        //     )
        // );

        // cast vote
        proposalManager.setVotes(
            proposalId,
            rate,
            support,
            proposalManager.getVotes(proposalId, rate, support) +
                _xAsko.getPriorVotes(
                    sender,
                    proposalManager.getVotingStartBlock(proposalId)
                )
        );
        proposalReciepts[sender][proposalId][currentInterval][rate].hasVoted = true;
        if(rate == 4){
            hasVotedCont.push(sender);
        }

        emit CastYesVote(
            proposalId,
            sender,
            _xAsko.getPriorVotes(
                sender,
                proposalManager.getVotingStartBlock(proposalId)
            ),
            support
        );
    }

    function DDFVote(
        uint256 proposalId,
        bool support,
        address sender
    ) public override onlyDAO {
        _unweightedVote(proposalId, support, sender, 3);
    }

    function AdminVeto(
        uint256 proposalId,
        bool support,
        address sender
    ) public override onlyDAO {
        _unweightedVote(proposalId, support, sender, 5);
    }

    function _unweightedVote(
        uint256 proposalId,
        bool support,
        address sender,
        uint8 rate
    ) internal {
        require(
            !proposalReciepts[sender][proposalId][0][3].hasVoted,
            "Cannot vote again"
        );
        uint256 state = proposalManager.getState(proposalId);
        require(
            (rate == 3 && state == uint256(Proposals.ProposalState.ACCEPTED)) ||
                (rate == 5 && state == uint256(Proposals.ProposalState.OPEN)),
            "Proposal is in the incorrect state for this vote"
        );

        proposalManager.setVotes(
            proposalId,
            rate,
            support,
            proposalManager.getVotes(proposalId, rate, support) + 1
        );
        proposalReciepts[sender][proposalId][0][rate].hasVoted = true;

        emit CastApprovalVote(proposalId, sender, support);
    }
}

