pragma solidity 0.8.4;

import "@openzeppelin/contracts/interfaces/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/interfaces/IERC721Enumerable.sol";
import "@openzeppelin/contracts/interfaces/IERC721Receiver.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

contract PixelVerse is Ownable, IERC721Receiver, ReentrancyGuard {
  using SafeMath for uint256;

  struct AuctionInfo {
    address seller;
    address nft;
    uint itemId;
    uint price;
    address bidder;
    uint createdAt;
    uint updatedAt;
    uint start;
    uint end;
  }

  AuctionInfo[] public auctions;
  uint256 constant public fee = 125; // constatnly 1.25%

  address public reserve;
  address private immutable _token;

  bytes4 private constant _ERC721_RECEIVED = 0x150b7a02;

  event CreateAuction(address indexed sender, uint auctionId, uint itemId, uint start, uint end);
  event Bid(address indexed sender, uint auctionId, uint price, uint bidAt);
  event EndAuction(address indexed sender, uint auctionId);
  event SetReserve(address indexed sender, address reserve);

  constructor (address __token, address _reserve) public {
    require(__token != address(0), "PixelVerse: Invalid address");
    require(_reserve != address(0x0), "PixelVerse: Invalid address");

    reserve = _reserve;
    _token = __token;
  }

  modifier validId(uint _auctionId) {
    require(_auctionId < auctions.length, "PixelVerse: Invalid Auction Id");
    _;
  }
  
  modifier validSeller(uint _auctionId) {
    AuctionInfo storage auction = auctions[_auctionId];
    require(auction.seller == _msgSender(), "PixelVerse: Invalid Permission");
    _;
  }

  function createAuction(address _nft, uint _itemId, uint _price,  uint _start, uint _end) external {
    require(_start < _end, "PixelVerse: Period is not valid");
    require(_end > block.timestamp, "PixelVers: End is not valid");
    AuctionInfo memory newAuction = AuctionInfo(
                                      _msgSender(),
                                      _nft,
                                      _itemId,
                                      _price,
                                      address(0),
                                      block.timestamp,
                                      block.timestamp,
                                      _start,
                                      _end
                                    );
    auctions.push(newAuction);
    uint256 auctionId = auctions.length - 1;

    IERC721(_nft).safeTransferFrom(_msgSender(), address(this), _itemId);
    emit CreateAuction(_msgSender(), auctionId, _itemId, _start, _end);
  }

  function bid(uint _auctionId, uint _price) external validId(_auctionId) nonReentrant {
    AuctionInfo storage auction = auctions[_auctionId];
    require(_msgSender() != auction.seller, "PixelVerse: Invalid Bidder");
    require(_msgSender() != auction.bidder, "PixelVerse: Invalid Bidder");
    require(block.timestamp >= auction.start, "PixelVerse: Auction is not started");
    require(block.timestamp <= auction.end, "PixelVerse: Auction is Over");
    require(_price > auction.price, "PixelVerse: Price is low");

    if (auction.bidder != address(0)) {
      require(IERC20(_token).transfer(auction.bidder, auction.price), "PixelVerse: token transfer failed");
    }

    auction.bidder = _msgSender();
    auction.price = _price;
    auction.updatedAt = block.timestamp;

    require(IERC20(_token).transferFrom(_msgSender(), address(this), _price), "PixelVerse: token transfer failed");

    emit Bid(_msgSender(), _auctionId, _price, block.timestamp);
  }

  function setReserve(address _reserve) external onlyOwner {
    require(_reserve != address(0x0), "PixelVerse: Invalid address");
    reserve = _reserve;

    emit SetReserve(_msgSender(), reserve);
  }

  function token() public view returns (address) {
    return _token;
  }

  function getAuctionCount() external view returns (uint256) {
    return auctions.length;
  }

  function endAuction(uint _auctionId) external validId(_auctionId) nonReentrant {
    AuctionInfo storage auction = auctions[_auctionId];
    require(auction.end < block.timestamp, "PixelVerse: Not ended yet");
    require((_msgSender() == auction.seller) || (_msgSender() == auction.bidder) , "PixelVerse: Invalid Permission");

    if(auction.bidder == address(0)) {
      IERC721(auction.nft).safeTransferFrom(address(this), auction.seller, auction.itemId);
    } else {
      uint256 feeAmount = auction.price.mul(fee).div(10000);
      uint256 amountForSeller = auction.price.sub(feeAmount);
      require(IERC20(_token).transfer(auction.seller, amountForSeller), "PixelVerse: token transfer failed");
      require(IERC20(_token).transfer(reserve, feeAmount), "PixelVerse: token transfer failed");
      IERC721(auction.nft).safeTransferFrom(address(this), auction.bidder, auction.itemId);
    }

    emit EndAuction(_msgSender(), _auctionId);
  }

  function onERC721Received(address, address, uint256, bytes calldata) external override pure returns (bytes4) {
    return _ERC721_RECEIVED;
  }
}

