import "hardhat/console.sol";

contract MorganHarper is ERC1155Supply, Ownable, IERC2981 {
  event Morganized(uint256 indexed tokenId);

  address public immutable royaltyReceiver;
  uint256 public immutable royaltyPercentage;
  uint256 public constant ROYALTY_BASE = 10000;
  uint256 public immutable maxSupplyPerAsset;

  string private _baseURI = "https://arweave.net/";
  string private _contractURIHash = "_HTAWY7JE5FCNkbyhQ9F6okDE80eV4or3fXgYOnNzj0";

  string[7] public assetURIHashes = [
    "-7GLCh5VUzmljNLOYOppwFJynwYsiEtJOVuibU8R94I", // 0 - Bucket Hat
    "TpkrWqVnRcMd_5QwmvOc_Ofz7z1fNeXfFZ4mbuGntyQ", // 1 - Graffiti Wall
    "3-zWqK27r9XddAw-K2H5yTZKhRWfaSPQjBr9Z82cLpw", // 2 - Jean Jacket
    "lM6yVMEs5KVqx2-pnzUPKGW95KGBElhJPNrY5EsQj9I", // 3 - Newsstand
    "sIM0phucexQDkyIf9BAs_Wmnd-xRjvoiLup-I9moCb4", // 4 - Old School Desktop
    "VR5FKnOxJSk6WgX-p3onKZOjy737E2kUQce0uDWecuU" // 5 - Digital Pet
  ];
  uint256 public constant FLOWER_ASSET_ID = 6;

  constructor(
    uint256 _maxSupplyPerAsset,
    address _royaltyReceiver,
    uint256 _royaltyPercentage
  ) ERC1155("") {
    require(_royaltyReceiver != address(0), "royaltyReceiver is address zero");
    require(_royaltyPercentage <= ROYALTY_BASE, "royaltyPercentage is too high");

    maxSupplyPerAsset = _maxSupplyPerAsset;
    royaltyReceiver = _royaltyReceiver;
    royaltyPercentage = _royaltyPercentage;
  }

  function contractURI() public view returns (string memory) {
    return string(abi.encodePacked(_baseURI, _contractURIHash));
  }

  function uri(uint256 assetId) public view virtual override returns (string memory) {
    require(assetId < assetURIHashes.length, "Asset does not exist");
    if (assetId == FLOWER_ASSET_ID) {
      return getFlowerURI(svgToImageURI(assetURIHashes[FLOWER_ASSET_ID]));
    } else {
      return string(abi.encodePacked(_baseURI, assetURIHashes[assetId]));
    }
  }

  /**
   * @dev Minting function.
   * @param assetId The asset to be minted.
   */
  function morganize(uint256 assetId) public payable returns (uint256) {
    require(assetId < assetURIHashes.length, "Asset does not exist");
    require(totalSupply(assetId) < maxSupplyPerAsset, "Maximum supply reached");

    require(balanceOf(msg.sender, assetId) < 1, "Asset mint limit of 1 reached");
    require(totalBalanceOf(msg.sender) < 3, "Wallet mint limit of 3 reached");
    if (assetId == FLOWER_ASSET_ID) require(msg.value >= 0.05 ether, "0.05 ether mint fee required");

    _mint(msg.sender, assetId, 1, ""); // recipient, asset_id, quantity, data
    emit Morganized(assetId);
    return assetId;
  }

  /**
   * @dev Sets the base URI for all token IDs. Only the owner can call this function.
   * @param newBaseURI The new base URI
   */
  function setBaseURI(string memory newBaseURI) external onlyOwner {
    _baseURI = newBaseURI;
  }

  /**
   * @dev Sets the contract URI for the collection. Only the owner can call this function.
   * @param newContractURIHash The new contract URI
   */
  function setContractURIHash(string memory newContractURIHash) external onlyOwner {
    _contractURIHash = newContractURIHash;
  }

  /**
   * @dev Sets the asset URI hash for a given asset ID. Only the owner can call this function.
   * @param assetId The asset ID for which to change the key
   * @param assetURIHash The new asset URI hash
   */
  function setAssetURIHash(uint256 assetId, string memory assetURIHash) external onlyOwner {
    assetURIHashes[assetId] = assetURIHash;
  }

  /**
   * @dev Sets the base URI for all token IDs. Only the owner can call this function.
   * @param addr Address of wallet
   * @return count - The count of all NFTs owned by this address
   */
  function totalBalanceOf(address addr) public view returns (uint256) {
    uint256 count = 0;
    for (uint256 assetId = 0; assetId < assetURIHashes.length; assetId++) {
      count += balanceOf(addr, assetId);
    }
    return count;
  }

  /**
   * @dev Interface implementation for the NFT Royalty Standard (ERC-2981).
   * Called by marketplaces that supports the standard with the sale price to determine how much royalty is owed and
   * to whom.
   * The first parameter tokenId (the NFT asset queried for royalty information) is not used as royalties are
   * calculated equally for all tokens.
   * @param salePrice The sale price of the NFT asset specified by `tokenId`
   * @return receiver Recipient of hte royalties
   * @return royaltyAmount Value percentage (using 2 decimals - 10000 = 100, 0 = 0)
   */
  function royaltyInfo(uint256, uint256 salePrice) external view override returns (address, uint256) {
    return (royaltyReceiver, (salePrice * royaltyPercentage) / ROYALTY_BASE);
  }

  /**
   * @dev See {IERC165-supportsInterface}.
   */
  function supportsInterface(bytes4 interfaceId) public view virtual override(IERC165, ERC1155) returns (bool) {
    return interfaceId == type(IERC2981).interfaceId || super.supportsInterface(interfaceId);
  }

  /**
   * @dev Gets a base-64 encoded image URI for on-chain assets from an SVG.
   * @param svg The SVG for which to generate an image URI.
   */
  function svgToImageURI(string memory svg) public pure returns (string memory) {
    string memory svgBase64Encoded = Base64.encode(bytes(string(abi.encodePacked(svg))));
    return string(abi.encodePacked("data:image/svg+xml;base64,", svgBase64Encoded));
  }

  /**
   * @dev Gets a base-64 encoded image URI into base-64 encoded JSON metadata.
   * @param imageURI The base-64 encoded image URI.
   */
  function getFlowerURI(string memory imageURI) public pure returns (string memory) {
    string memory name = "Morgan Harper OC Flower";
    string memory description = "Limited edition On-chain Morgan Harper Flower";
    string memory encodedMetadata = Base64.encode(
      abi.encodePacked('{"name":"', name, '", "description":"', description, '", "image": "', imageURI, '"}')
    );
    return string(abi.encodePacked("data:application/json;base64,", encodedMetadata));
  }
}

