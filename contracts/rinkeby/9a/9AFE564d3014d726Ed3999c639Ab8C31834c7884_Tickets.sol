pragma solidity 0.8.6;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./ILottery.sol";
import "../library/HeapSort.sol";
import "hardhat/console.sol";

struct Ticket {
    uint16[] ticketNumbers;
    bool isClaimed;
}

contract Tickets is ERC1155, AccessControl {
    bytes32 constant CONTRACT_ROLE = bytes32(keccak256("CONTRACT_ROLE"));

    event TicketClaimed(address claimer, uint256 lotteryId, uint256 tokenId);
    event TicketBuyed(
        address buyer,
        uint256 lotteryId,
        uint256 tokenId,
        uint16[] ticketNumber,
        uint256 selTicketCounter
    );

    ILottery lottery;
    IERC20 public winr;

    /**
     *userTickets   LotteryId => TokenId => Ticket
     *selTicketCounter LotteryId => numbersArray => counter
     */

    mapping(uint256 => mapping(uint256 => Ticket)) public userTickets;
    mapping(uint256 => mapping(bytes => uint256)) public selTicketCounter;

    uint256 public tokenId;

    constructor(string memory _uri) ERC1155(_uri) {
        tokenId = 0;
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function mintTicket(address _account) internal {
        tokenId++;
        _mint(_account, tokenId, 1, "");
    }

    function getOwnerOfTicket(uint256 _tokenId, address _address)
        external
        view
        returns (bool)
    {
        uint256 balance = balanceOf(_address, _tokenId);
        require(balance > 0, "You do not own this token");
        return true;
    }

    function getTicketNumber(uint256 _tokenId, uint256 _lotteryId)
        external
        view
        returns (uint16[] memory)
    {
        return userTickets[_lotteryId][_tokenId].ticketNumbers;
    }

    function setLotteryAndWinr(address lotteryContract, address _winr)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        winr = IERC20(_winr);
        lottery = ILottery(lotteryContract);
    }

    function buyTicket(uint256 _lotteryId, uint16[] memory _ticketNumbers)
        external
    {
        LottoInfo memory lottoInf = lottery.getBasicLottoInfo(_lotteryId);
        uint256 ticketPrice = lottoInf.costPerTicket;
        _ticketNumbers = HeapSort.heapSort(_ticketNumbers);

        require(
            lottoInf.lotteryStatus == Status.Open,
            "The lottery is not open"
        );
        require(_ticketNumbers.length == 5, "You can choose only 5 numbers");

        for (uint16 i = 0; i < _ticketNumbers.length; i++) {
            require(
                _ticketNumbers[i] >= 1,
                "Number must be 1 or greater than 1"
            );
            require(_ticketNumbers[i] <= 20, "Number must be less than 20");
            // require(!temp[tokenId][_ticketNumbers[i]],"You can't choose same number");
            // temp[tokenId][_ticketNumbers[i]] = true;
            if (i > 0) {
                require(
                    _ticketNumbers[i] != _ticketNumbers[i - 1],
                    "You can't choose same number"
                );
            }
        }
        require(ticketPrice > 0);
        require(
            winr.balanceOf(msg.sender) >= ticketPrice,
            "Insufficient balance"
        );

        bytes memory bArray = abi.encodePacked(_ticketNumbers);

        winr.transferFrom(msg.sender, address(this), ticketPrice);
        mintTicket(msg.sender);

        uint256 counter = selTicketCounter[_lotteryId][bArray]++;
        userTickets[_lotteryId][tokenId].ticketNumbers = _ticketNumbers;

        emit TicketBuyed(
            msg.sender,
            _lotteryId,
            tokenId,
            _ticketNumbers,
            counter
        );
    }

    /**
     * @notice make sure you authorize the lottery contract before accessing this function
     * @notice claimTicket This function must be only accessed by from lottery contract
     */

    function claimTicket(uint256 _tokenId, uint256 _lotteryId)
        external
        onlyRole(CONTRACT_ROLE)
        returns (bool)
    {
        userTickets[_lotteryId][_tokenId].isClaimed = true;
        emit TicketClaimed(msg.sender, _lotteryId, _tokenId);

        return true;
    }

    function isClaimed(uint256 _tokenId, uint256 _lotteryId)
        external
        view
        returns (
            // onlyRole(CONTRACT_ROLE)
            bool
        )
    {
        return userTickets[_lotteryId][_tokenId].isClaimed;
    }

    function getSellCount(uint256 _lotteryId, uint16[] memory winningNumbers)
        external
        view
        returns (uint256)
    {
        winningNumbers = HeapSort.heapSort(winningNumbers);
        bytes memory bArray = abi.encodePacked(winningNumbers);
        return selTicketCounter[_lotteryId][bArray];
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(AccessControl, ERC1155)
        returns (bool)
    {
        return
            interfaceId == type(IERC1155).interfaceId ||
            interfaceId == type(IERC1155MetadataURI).interfaceId ||
            super.supportsInterface(interfaceId);
    }

    function withdrawWINR(uint256 _amount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        winr.transfer(msg.sender, _amount);
    }
}

