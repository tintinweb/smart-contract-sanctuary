pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";

//import './ExternalStorage.sol';
// @dev
contract AssetContract is Ownable, Pausable, Initializable, ERC721{

    using Strings for uint256;
    using SafeMath for uint256;

    uint256 nextAssetId;
    address proxy;
    //ExternalStorage public externalStorage;

    string public baseURI;

    //event UpgradedExternalStorage(address indexed externalStorage);

    event ChangeLimit(address indexed owner, uint256 indexed tokenId, uint256 indexed limit);

    event ChangeBaseUrl(string indexed baseUrl);

    // Mapping from token ID to token limits
    mapping (uint256 => uint256) private _tokenLimits;

    // Mapping from token ID to token copies
    mapping (uint256 => uint256[]) private _tokenChildren;
    // Mapping from token ID to token parent ID
    mapping (uint256 => uint256) private _tokenCopies;

    // Mapping from token ID to token parent ID
    mapping (uint256 => uint256) private _tokenParents;

    // Mapping from token ID to token metadata
    mapping (uint256 => string) private _tokenMetadata;

    // @dev
    modifier onlyOwnerOrTokenOwner(uint256 tokenId) {
        require( ownerOf(tokenId) == _msgSender() || owner() == _msgSender(), "not the owner and not token owner");
        _;
    }

    constructor() ERC721("AssetContract", "ITM") {
        initialize();
    }

    function initialize()
        public initializer
        returns (bool)
    {
        baseURI = 'https://example.com/asset/';// TODO change baseURI
        return true;
    }

    // @dev Throws if externalStorage set
    /*
    modifier externalStorageSet() {
        require(address (externalStorage) != address(0), "externalStorage not set");
        _;
    }*/

    // @dev Throws if token exist
    modifier assetExists(uint256 tokenId) {
        require(_exists(tokenId), "nonexistent token");
        _;
    }
    /**
        @dev creating an asset
        Limits:
        0 - unlimited
        1 - unique
        >1 - limited
    */

    function createAsset(uint256 limit, string memory metadata)
        virtual
        external whenNotPaused
    {

        require(!_exists(nextAssetId), "tokenId used");
        _mint(_msgSender(), nextAssetId);
        _tokenLimits[nextAssetId] = limit;
        _tokenCopies[nextAssetId] = limit == 0?0:1;
        _tokenMetadata[nextAssetId] = metadata;
        nextAssetId = nextAssetId.add(1);
    }

    function getVersion()
        public pure virtual
        returns (uint)
    {
        return 1;
    }

    // @dev change of base URI
    function changeBaseURI(string memory uri)
        external onlyOwner //externalStorageSet
    {
        //externalStorage.setString(keccak256("baseURI"), uri);
        baseURI = uri;
        emit ChangeBaseUrl(uri);
    }

    // @dev Base URI for computing
    function _baseURI()
        internal view override
        returns (string memory)
    {
        return baseURI;
    }

    // @dev Returns the Uniform Resource Identifier (URI) for `tokenId` token.
    function tokenURI(uint256 tokenId)
        public virtual
        view override assetExists(tokenId)
        returns (string memory)
    {
        return string(abi.encodePacked(baseURI, _tokenMetadata[tokenId]));
    }

    // @dev Returns the parent ID
    function parentId(uint256 tokenId)
        external virtual
        view assetExists(tokenId)
        returns (uint256)
    {
        return  _tokenParents[tokenId];
    }

    // @dev creating a copy of the asset
    function cloneAsset(address to, uint256 tokenId)
        external virtual
        whenNotPaused onlyOwnerOrTokenOwner(tokenId) assetExists(tokenId)
    {
        require(availableClones(tokenId)>0, "token clone limit used");
        require(_isLimitable(tokenId), "not limitable token");
        require(!_exists(nextAssetId), "cloneId used");

        _mint(to, nextAssetId);
        _tokenParents[nextAssetId] = tokenId;
        _tokenCopies[tokenId] = _tokenCopies[tokenId].add(1);
        _tokenChildren[tokenId].push(nextAssetId);
        _tokenMetadata[nextAssetId] = _tokenMetadata[tokenId];
        nextAssetId = nextAssetId.add(1);
    }

    // @dev returns how many more copies you can create
    function availableClones(uint256 tokenId)
        public virtual
        view assetExists(tokenId)
        returns (uint256)
    {
        require(_isLimitable(tokenId), "not limitable token");
        return _tokenLimits[tokenId].sub(_tokenCopies[tokenId]);
    }

    // @dev checks if the asset is NOT unique
    function _isLimitable(uint256 tokenId)
        internal virtual
        view
        returns (bool)
    {
        return _tokenLimits[tokenId] != uint256(0);
    }

    // @dev returns the license type
    function licenseType(uint256 tokenId)
        external virtual
        view assetExists(tokenId)
        returns (string memory)
    {
        return _licenseType(tokenId);
    }

    // @dev returns the license type
    function _licenseType(uint256 tokenId)
        internal virtual
        view
        returns (string memory)
    {
        if(_tokenLimits[tokenId] == 0 && _tokenCopies[tokenId] == 0){
            return 'unlimited';
        }
        if(_tokenLimits[tokenId] == 1){
            return 'unique';
        }
        return 'limited';
    }

    // @dev
    function burnAsset(uint256 tokenId)
        external virtual
        assetExists(tokenId) onlyOwnerOrTokenOwner(tokenId)
    {
        _burnAsset(tokenId);
    }

    // @dev
    function _burnAsset(uint256 tokenId) internal virtual {
        _burn(tokenId);
        delete _tokenMetadata[tokenId];
        delete _tokenLimits[tokenId];
        delete _tokenCopies[tokenId];
        delete _tokenMetadata[tokenId];
    }

    // @dev
    function burnAllCopies(uint256 tokenId)
        external virtual whenNotPaused assetExists(tokenId) onlyOwnerOrTokenOwner(tokenId)
    {
        require(_isLimitable(tokenId), "not limitable token");

        for(uint index=0; index<_tokenChildren[tokenId].length; index++){
            if(_exists(_tokenChildren[tokenId][index])
                && (owner() == _msgSender() || ownerOf(_tokenChildren[tokenId][index]) == _msgSender())
            ){
                _burnAsset(_tokenChildren[tokenId][index]);
            }
        }
        _burnAsset(tokenId);

        delete _tokenChildren[tokenId];
    }

    // @dev
    /*
    function upgrableExternalStore(address _externalStorage)
        external onlyOwner
    {
        require(_externalStorage != address(0));
        externalStorage = ExternalStorage(_externalStorage);
        emit UpgradedExternalStorage(_externalStorage);
    }*/
}

