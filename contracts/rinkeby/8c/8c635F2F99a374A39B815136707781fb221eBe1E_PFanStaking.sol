interface NonfungiblePositionManager {
    //https://rinkeby.etherscan.io/address/0xc36442b4a4522e871399cd717abdd847ab11fe88#code
    //function totalSupply() public returns (uint);
    //function balanceOf(address tokenOwner) public view returns (uint balance);

    function positions(uint256 tokenId)
        external
        view
        returns (
            uint96 nonce,
            address operator,
            address token0,
            address token1,
            uint24 fee,
            int24 tickLower,
            int24 tickUpper,
            uint128 liquidity,
            uint256 feeGrowthInside0LastX128,
            uint256 feeGrowthInside1LastX128,
            uint128 tokensOwed0,
            uint128 tokensOwed1
        );
