contract PFanStaking is Ownable, EIP712 {
    using ECDSA for bytes32;
    using SafeMath for uint256;
    // INonfungiblePositionManager positionmanager;
    INonfungiblePositionManager nPositionsManager;
    address rinkebyPositionManagerAddress =
        0xC36442b4a4522E871399CD717aBDD847Ab11FE88;
    //https://solveforum.com/forums/threads/solved-create-lp-pool-uniswap-v3-compilererror-stack-too-deep-when-compiling-inline-assembly-variable-headstart-is-1-slot-s-too-deep-inside-the.315837/

    struct TxData {
        uint256 timestamp;
        string txHash;
    }

    struct Periods {
        bool firstPeriod;
        bool secondPeriod;
        bool thirdPeriod;
    }

    IERC20 public token;

    uint256 public pfanPool;
    uint256 public totalTokenAmount;

    uint256 public THIRTY_DAYS = 30;

    uint256 public SIXTY_DAYS = 60;

    uint256 public NINETY_DAYS = 90;

    uint256 public THIRTY_DAY_REWARD;

    uint256 public SIXTY_DAY_REWARD;

    uint256 public NINETY_DAY_REWARD;


    IUniswapV3PoolState public pool =
        IUniswapV3PoolState(0x3a861Ff3840787f901dbA5025200D9f1F2Fa27BC);

    mapping(address => bool) public hasClaimed;

    // user address => txHash => status
    mapping(address => mapping(string => Periods)) public claimeableTxs;

    mapping(address => uint256) public totalClaimed;

    mapping(address => uint256) public stakedTime;

    event ADDTOKENS(uint256 indexed tokenAmount);

    constructor(
        address _token, // PFan token address
        uint256 _thirtyDaysReward,
        uint256 _sixtyDaysReward,
        uint256 _ninetyDaysReward
    ) EIP712("POWERFAN", "1") {
        token = IERC20(_token);
        THIRTY_DAY_REWARD = _thirtyDaysReward;
        SIXTY_DAY_REWARD = _sixtyDaysReward;
        NINETY_DAY_REWARD = _ninetyDaysReward;
        nPositionsManager = INonfungiblePositionManager(
            rinkebyPositionManagerAddress
        );
    }

    function getUniswapTransactionData(uint256 tokenId)
        public
        returns (
            uint96 nonce,
            address operator,
            address token0,
            address token1,
            uint24 fee,
            int24 tickLower,
            int24 tickUpper,
            uint128 liquidity,
            uint256 feeGrowthInside0LastX128,
            uint256 feeGrowthInside1LastX128,
            uint128 tokensOwed0,
            uint128 tokensOwed1
        )
    {
        //1. Balance of the liquidity pool provied by sender is still not unstaked yet at UNISWAP_CONTRACT_ADDRESS
        //2. Make sure the transaction hash sender is the current sender
        //address rinkebyPositionManagerAddress = 0xC36442b4a4522E871399CD717aBDD847Ab11FE88;
        //return positionmanager(rinkebyPositionManagerAddress).positions(tokenId);
        //nPositionsManager = NonfungiblePositionManager(rinkebyPositionManagerAddress);
        return nPositionsManager.positions(tokenId);
    }

    function getTokensAmount(uint256 tokenId)
        public
        view
        returns (uint256, uint256)
    {
        (uint160 sqrtPriceX96, , , , , , ) = pool.slot0();
        return PositionValue.total(nPositionsManager, tokenId, sqrtPriceX96);
    }

    function setUniswapContractAddress(address uniswapAddress)
        public
        onlyOwner
    {
        require(totalTokenAmount > 0, "SHOULD_NOT_ZERO");
        token.transferFrom(msg.sender, address(this), totalTokenAmount);
        emit ADDTOKENS(totalTokenAmount);
    }

    /* @notice Admin can add PowerFan tokens to smart contract
     * @dev Only admin can add reward PowerFan tokens
     * @param tokenAmount The amount admin want to add in smart contract
     */
    function addRewardTokens(uint256 tokenAmount) public onlyOwner {
        require(tokenAmount > 0, "SHOULD_NOT_ZERO");
        token.transferFrom(msg.sender, address(this), tokenAmount);
        emit ADDTOKENS(tokenAmount);
    }

    /* @notice Update token reward for 30 days
     * @dev Only admin can update reward tokens for 30 days
     * @param amount The amount admin want to change reward tokens for 30 days
     */
    function update30DayTokenReward(uint256 amount) public onlyOwner {
        require(amount > 0, "SHOULD_NOT_ZERO");
        THIRTY_DAY_REWARD = amount;
    }

    /* @notice Update token reward for 60 days
     * @dev Only admin can update reward tokens for 60 days
     * @param amount The amount admin want to change reward tokens for 60 days
     */
    function update60DayTokenReward(uint256 amount) public onlyOwner {
        require(amount > 0, "SHOULD_NOT_ZERO");
        SIXTY_DAY_REWARD = amount;
    }

    /* @notice Update token reward for 90 days
     * @dev Only admin can update reward tokens for 90 days
     * @param amount The amount admin want to change reward tokens for 90 days
     */
    function update90DayTokenReward(uint256 amount) public onlyOwner {
        require(amount > 0, "SHOULD_NOT_ZERO");
        NINETY_DAY_REWARD = amount;
    }

    /* @notice Withdraw reward tokens
     * @param data contains timestamp and transaction hash
     * @param signature of data signed by user
     */
    function withdrawReward(TxData calldata data, bytes memory signature)
        public
    {
        require(bytes(data.txHash).length == 66, "INVALID_HASH");

        require(_msgSender() == verify(data, signature), "INVALID-SIG");

        // require(!claimeableTxs[_msgSender()][data.txHash], "ALREADY-CLAIMED");
        (
            bool isValidPeriod,
            bool isFirstPeriod,
            bool isSecondPeriod,
            bool isThirdPeriod
        ) = isClaimable(claimeableTxs[_msgSender()][data.txHash]);

        require(isValidPeriod);

        if (isThirtyDaysReward(data.timestamp) && !isFirstPeriod) {
            token.transfer(_msgSender(), THIRTY_DAY_REWARD);
            claimeableTxs[_msgSender()][data.txHash].firstPeriod = true;
            totalClaimed[_msgSender()] = totalClaimed[_msgSender()].add(
                THIRTY_DAY_REWARD
            );
        } else if (isSixtyDaysReward(data.timestamp) && !isSecondPeriod) {
            token.transfer(_msgSender(), SIXTY_DAY_REWARD);
            claimeableTxs[_msgSender()][data.txHash].secondPeriod = true;
            totalClaimed[_msgSender()] = totalClaimed[_msgSender()].add(
                SIXTY_DAY_REWARD
            );
        } else if (isNinetyDaysReward(data.timestamp) && !isThirdPeriod) {
            token.transfer(_msgSender(), NINETY_DAY_REWARD);
            claimeableTxs[_msgSender()][data.txHash].thirdPeriod = true;
            totalClaimed[_msgSender()] = totalClaimed[_msgSender()].add(
                NINETY_DAY_REWARD
            );
        }
    }

    function isClaimable(Periods memory periods)
        internal
        view
        returns (
            bool,
            bool,
            bool,
            bool
        )
    {
        bool validPeriod = (!periods.firstPeriod ||
            !periods.secondPeriod ||
            !periods.thirdPeriod);

        return (
            validPeriod,
            periods.firstPeriod,
            periods.secondPeriod,
            periods.thirdPeriod
        );
    }

    function isThirtyDaysReward(uint256 timestamp)
        internal
        view
        returns (bool)
    {
        return
            calculateDays(timestamp) >= THIRTY_DAYS &&
            calculateDays(timestamp) < SIXTY_DAYS;
    }

    function isSixtyDaysReward(uint256 timestamp) internal view returns (bool) {
        return
            calculateDays(timestamp) >= SIXTY_DAYS &&
            calculateDays(timestamp) < NINETY_DAYS;
    }

    function isNinetyDaysReward(uint256 timestamp)
        internal
        view
        returns (bool)
    {
        return calculateDays(timestamp) >= NINETY_DAYS;
    }

    function calculateDays(uint256 depositTime)
        internal
        view
        returns (uint256)
    {
        return block.timestamp.sub(depositTime).div(1 days);
    }

    function Hash(TxData calldata data) internal view returns (bytes32) {
        return
            _hashTypedDataV4(
                keccak256(
                    abi.encode(
                        keccak256("TxData(uint256 timestamp,string txHash)"),
                        data.timestamp,
                        keccak256(bytes(data.txHash))
                    )
                )
            );
    }

    function verify(TxData calldata data, bytes memory signature)
        internal
        view
        returns (address)
    {
        bytes32 digest = Hash(data);
        return digest.toEthSignedMessageHash().recover(signature);
    }

    function getChainId() internal view returns (uint256 chainId) {
        this; // silence state mutability warning without generating bytecode - see https://github.com/ethereum/solidity/issues/2691
        // solhint-disable-next-line no-inline-assembly
        assembly {
            chainId := chainid()
        }
    }
}

