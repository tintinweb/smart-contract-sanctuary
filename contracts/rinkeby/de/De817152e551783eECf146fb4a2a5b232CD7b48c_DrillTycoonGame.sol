import "hardhat/console.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/interfaces/IERC721.sol";
import "./IDrillTycoon.sol";
import "./IDrillTycoonRenderer.sol";
import "./DrillTycoonLibrary.sol";

contract DrillTycoonGame is Ownable {
    struct DrillMissionData {
        uint8 mission;
        //Current Mission

        uint64 embarkTime;
        //Embark Time

        uint16[9] numberOfMissionsCompleted;
        //Number of missions completed
    }

    struct DrillInventory {
        uint8 refineryLevel;
        //RefineryLevel

        uint8 fuelTankLevel;
        //Fuel Tank Level

        uint16[5] drillParts;
        //Its parts

        uint16[9] resources;
        /*
        0 - FUEL - starts at 0
        1 - STONE - starts at 0
        2 - COAL - starts at 0
        3 - IRONORE - starts at 0
        4 - GOLDORE - starts at 0
        5 - DIAMOND - starts at 0
        6 - IRONINGOT - starts at 0
        7 - GOLDINGOT - starts at 0
        8 - MYSTERIUMINGOT - starts at 0
        */
    }


    //TokenId To Inventory
    mapping(uint256 => uint16[]) public tokenIdToInventory;
    //Trait Id to index in inventory
    mapping(uint256 => mapping(uint16 => uint16)) tokenIdToTraitIdToIndex;
    //Trait Id to index in inventory
    mapping(uint256 => mapping(uint16 => bool)) tokenIdToTraitIdToInInventory;

    struct MarketPlaceEntry {
        uint16 quantity;
        uint8[] resourceTypes;
        uint16[] resourceCosts;
    }

    struct Mission {
        uint16 fuelCost;
        uint8 fuelTankRequired;
        uint8 resourceType;
        uint16 resourceReturnLowerBound;
        uint16 resourceReturnUpperBound;
        uint256 missionLength;
    }

    struct RefineryLevel {
        uint8 resourceCost;
        uint8 resourceType;
        uint8 output;
    }

    struct FuelTankLevel {
        uint8 resourceCost;
        uint8 resourceType;
    }

    mapping(uint256 => DrillInventory) public tokenIdToDrillInventory;
    mapping(uint256 => DrillMissionData) public tokenIdToDrillMissionData;

    mapping(uint32 => MarketPlaceEntry) public traitIdToMarketPlaceEntry;

    mapping(uint8 => Mission) public missions;
    mapping(uint8 => RefineryLevel) public refineryLevels;
    mapping(uint8 => FuelTankLevel) public FuelTankLevels;

    address public DrillTycoonAddress;
    address public DrillTycoonRendererAddress;

    constructor() {
        //Stone Refinery
        refineryLevels[1] = RefineryLevel(20, 1, 100);
        //Iron Refinery
        refineryLevels[2] = RefineryLevel(20, 6, 133);
        //Gold Refinery
        refineryLevels[3] = RefineryLevel(20, 7, 166);
        //Diamond Refinery
        refineryLevels[4] = RefineryLevel(20, 5, 200);

        //Stone Fuel Tank
        FuelTankLevels[1] = FuelTankLevel(20, 1);
        //Iron Fuel Tank
        FuelTankLevels[2] = FuelTankLevel(20, 6);
        //Gold Fuel Tank
        FuelTankLevels[3] = FuelTankLevel(20, 7);
        //Diamond Fuel Tank
        FuelTankLevels[4] = FuelTankLevel(20, 5);

        //Free fuel mission
        missions[1] = Mission(
            0,
            0,
            0,
            5000,
            5000,
            0 /*12 * 60 * 60*/
        );
        //Paid fuel mission
        missions[2] = Mission(
            50,
            0,
            0,
            2000,
            3000,
            0 /*1 * 24 * 60 * 60*/
        );
        //Stone mission
        missions[3] = Mission(
            75,
            0,
            1,
            4500,
            5000,
            0 /*2 * 24 * 60 * 60*/
        );
        //Coal mission
        missions[4] = Mission(
            75,
            1,
            2,
            4000,
            5000,
            0 /*2 * 24 * 60 * 60*/
        );
        //Iron mission
        missions[5] = Mission(
            100,
            1,
            3,
            3000,
            3500,
            0 /*3 * 24 * 60 * 60*/
        );
        //Gold mission
        missions[6] = Mission(
            200,
            2,
            4,
            2500,
            3000,
            0 /*3 * 24 * 60 * 60*/
        );
        //Diamond mission
        missions[7] = Mission(
            300,
            3,
            5,
            2000,
            2200,
            0 /*4 * 24 * 60 * 60*/
        );
    }

    function addResources(
        uint8 resouceIndex,
        uint16 amount,
        uint256 tokenId
    ) internal {
        if (amount == 0) return;
        tokenIdToDrillInventory[tokenId].resources[resouceIndex] += amount;
    }

    function removeResources(
        uint8 resouceIndex,
        uint16 amount,
        uint256 tokenId
    ) internal {
        if (amount == 0) return;
        require(
            amount <= tokenIdToDrillInventory[tokenId].resources[resouceIndex],
            "You don't have enough!"
        );
        tokenIdToDrillInventory[tokenId].resources[resouceIndex] -= amount;
    }

    function embark(uint256 tokenId, uint8 missionIndex) public {
        require(
            missionIndex > 0 && missionIndex <= 7,
            "Mission does not exist."
        );
        //Require they are not currently on a mission
        require(
            tokenIdToDrillMissionData[tokenId].mission == 0,
            "Drill already on mission!"
        );
        //Require they own this drill
        require(
            IERC721(DrillTycoonAddress).ownerOf(tokenId) == msg.sender,
            "You must own this drill!"
        );

        require(
            tokenIdToDrillInventory[tokenId].fuelTankLevel >=
                missions[missionIndex].fuelTankRequired,
            "You don't have the required fuel tank!"
        );

        //Set their current mission.
        tokenIdToDrillMissionData[tokenId].mission = missionIndex;

        //Set when they embarked.
        tokenIdToDrillMissionData[tokenId].embarkTime = uint64(block.timestamp);

        //Subtract the fuel.
        removeResources(0, missions[missionIndex].fuelCost, tokenId);
    }

    function turnBack(uint256 tokenId) public {
        //Require they are currently on a mission
        require(tokenIdToDrillMissionData[tokenId].mission != 0, "Drill not on mission!");
        //Require they own this drill
        require(
            IERC721(DrillTycoonAddress).ownerOf(tokenId) == msg.sender,
            "You must own this drill!"
        );

        //Set their current mission.
        tokenIdToDrillMissionData[tokenId].mission = 0;
    }

    function generateARandomNumberInBounds(
        uint256 tokenId,
        bytes32 entropySeed,
        uint256 missionCount,
        uint8 missionIndex
    ) public view returns (uint16) {
        return
            uint16(
                (uint256(
                    keccak256(
                        abi.encodePacked(
                            tokenId,
                            entropySeed,
                            missionCount,
                            missionIndex
                        )
                    )
                ) %
                    (missions[missionIndex].resourceReturnUpperBound -
                        missions[missionIndex].resourceReturnLowerBound +
                        1)) + missions[missionIndex].resourceReturnLowerBound
            );
    }

    function generateMissionReward(uint256 tokenId, uint8 missionIndex)
        internal
    {
        uint16 emissions = generateARandomNumberInBounds(
            tokenId,
            IDrillTycoon(DrillTycoonAddress).entropySeed(),
            tokenIdToDrillMissionData[tokenId].numberOfMissionsCompleted[missionIndex],
            missionIndex
        );

        addResources(missions[missionIndex].resourceType, emissions, tokenId);
    }

    function finishMission(uint256 tokenId) public {
        //Require they are currently on a mission.
        require(tokenIdToDrillMissionData[tokenId].mission != 0, "Drill not on mission!");
        //Require they own this drill.
        require(
            IERC721(DrillTycoonAddress).ownerOf(tokenId) == msg.sender,
            "You must own this drill!"
        );
        //Require they have completed the mission.
        require(
            tokenIdToDrillMissionData[tokenId].embarkTime +
                missions[tokenIdToDrillMissionData[tokenId].mission].missionLength <=
                block.timestamp,
            "Mission not completed!"
        );

        //Set their rewards
        generateMissionReward(tokenId, tokenIdToDrillMissionData[tokenId].mission);

        //Increment their missions count.
        tokenIdToDrillMissionData[tokenId].numberOfMissionsCompleted[
            tokenIdToDrillMissionData[tokenId].mission
        ]++;

        //Set their current mission.
        tokenIdToDrillMissionData[tokenId].mission = 0;
    }

    function upgradeRefinery(uint256 tokenId) public {
        require(
            msg.sender == IERC721(DrillTycoonAddress).ownerOf(tokenId),
            "You must own this drill"
        );
        require(
            tokenIdToDrillInventory[tokenId].refineryLevel != 4,
            "Can't upgrade furnace further."
        );

        //Upgrade their refinery
        tokenIdToDrillInventory[tokenId].refineryLevel++;

        //If they can call this, they have enough
        removeResources(
            refineryLevels[tokenIdToDrillInventory[tokenId].refineryLevel].resourceType,
            refineryLevels[tokenIdToDrillInventory[tokenId].refineryLevel].resourceCost,
            tokenId
        );
    }

    function upgradeFuelTank(uint256 tokenId) public {
        require(
            msg.sender == IERC721(DrillTycoonAddress).ownerOf(tokenId),
            "You must own this drill"
        );
        require(
            tokenIdToDrillInventory[tokenId].fuelTankLevel != 4,
            "Can't upgrade fuel tank further."
        );

        //Upgrade their refinery.
        tokenIdToDrillInventory[tokenId].fuelTankLevel++;

        //If they can call this, they have enough.
        //If they can't, it reverts.
        removeResources(
            FuelTankLevels[tokenIdToDrillInventory[tokenId].fuelTankLevel].resourceType,
            FuelTankLevels[tokenIdToDrillInventory[tokenId].fuelTankLevel].resourceCost,
            tokenId
        );
    }

    function smeltBatch(
        uint8 resourceToSmelt,
        uint16 quantityToSmelt,
        uint256 tokenId
    ) public {
        require(
            tokenIdToDrillInventory[tokenId].refineryLevel != 0,
            "You haven't built a refinery yet!"
        );
        require(
            msg.sender == IERC721(DrillTycoonAddress).ownerOf(tokenId),
            "You must own this drill"
        );
        require(
            resourceToSmelt == 3 || resourceToSmelt == 4,
            "Unsmeltable item!"
        );

        //Remove this much of that item.
        removeResources(resourceToSmelt, quantityToSmelt, tokenId);
        //Remove this much coal
        removeResources(2, quantityToSmelt, tokenId);

        //Add this much of it's smelted version
        addResources(
            resourceToSmelt + 3,
            (quantityToSmelt *
                refineryLevels[tokenIdToDrillInventory[tokenId].refineryLevel].output) /
                uint8(100),
            tokenId
        );
    }

    function buyPart(
        uint256 tokenId,
        uint16 traitId,
        uint16 traitTypeIndex
    ) public {
        require(
            msg.sender == IERC721(DrillTycoonAddress).ownerOf(tokenId),
            "You must own this drill"
        );

        require(
            !tokenIdToTraitIdToInInventory[tokenId][traitId],
            "This is already in your inventory!"
        );
        require(
            !(IDrillTycoonRenderer(DrillTycoonAddress)
                .getTraitOfTokenByIndex(tokenId, traitTypeIndex)
                .traitId == traitId),
            "This is currently equipped!"
        );

        for (
            uint8 i = 0;
            i < traitIdToMarketPlaceEntry[traitId].resourceTypes.length;
            i++
        ) {
            //Remove these items
            removeResources(
                traitIdToMarketPlaceEntry[traitId].resourceTypes[i],
                traitIdToMarketPlaceEntry[traitId].resourceCosts[i],
                tokenId
            );
        }

        //Put it in their inventory
        tokenIdToInventory[tokenId].push(traitId);

        //Set it as being in their inventory
        tokenIdToTraitIdToInInventory[tokenId][traitId] = true;

        //Set the current index of this trait in their inventory to length - 1.
        tokenIdToTraitIdToIndex[tokenId][traitId] = uint16(
            tokenIdToInventory[tokenId].length - 1
        );

        //Remove a part from the market
        traitIdToMarketPlaceEntry[traitId].quantity--;
    }

    function swapPart(
        uint256 tokenId,
        uint16 newTraitId,
        uint16 traitTypeIndex
    ) public {
        require(
            msg.sender == IERC721(DrillTycoonAddress).ownerOf(tokenId),
            "You must own this drill"
        );

        require(
            tokenIdToTraitIdToInInventory[tokenId][newTraitId],
            "You don't own this part."
        );

        /*
            Add the old token to where the new token WAS in inventory


            Replace their old trait with their new trait in their inventory
            Set that index.


            Overwrite their old trait with their new trait.
        */

        //Get this trait Id
        uint16 oldTraitId = IDrillTycoonRenderer(DrillTycoonAddress)
            .getTraitOfTokenByIndex(tokenId, traitTypeIndex)
            .traitId;

        //Replace the current trait Id with the old trait Id, as you know where the current trait resides in the inventory.
        tokenIdToInventory[tokenId][
            tokenIdToTraitIdToIndex[tokenId][newTraitId]
        ] = oldTraitId;

        //New trait is no longer in your inventory
        tokenIdToTraitIdToInInventory[tokenId][newTraitId] = false;

        //The index of the trait going into the inventory, should be the index of where the new trait was.
        tokenIdToTraitIdToIndex[tokenId][oldTraitId] = tokenIdToTraitIdToIndex[
            tokenId
        ][newTraitId];

        //Overwrite the drill to reflect the new trait.
        tokenIdToDrillInventory[tokenId].drillParts[traitTypeIndex] = newTraitId;
    }

    function alchemize(uint256 tokenId, uint16 quantity) public {
        require(
            msg.sender == IERC721(DrillTycoonAddress).ownerOf(tokenId),
            "You must own this drill"
        );

        //Remove this much of gold, iron, diamond.
        removeResources(5, 100 * quantity, tokenId);
        removeResources(6, 100 * quantity, tokenId);
        removeResources(7, 100 * quantity, tokenId);

        //Add this much mysterium ingot.
        addResources(8, quantity, tokenId);
    }

    function getTraitIdOfDrillByTraitTypeIndex(
        uint256 tokenId,
        uint16 traitTypeIndex
    ) public view returns (uint16) {
        return tokenIdToDrillInventory[tokenId].drillParts[traitTypeIndex];
    }

    function getMission(uint256 tokenId) public view returns (uint8) {
        return tokenIdToDrillMissionData[tokenId].mission;
    }

    function getEmbarkTime(uint256 tokenId) public view returns (uint64) {
        return tokenIdToDrillMissionData[tokenId].embarkTime;
    }

    function getRefineryLevel(uint256 tokenId) public view returns (uint8) {
        return tokenIdToDrillInventory[tokenId].refineryLevel;
    }

    function getFuelTankLevel(uint256 tokenId) public view returns (uint8) {
        return tokenIdToDrillInventory[tokenId].fuelTankLevel;
    }

    function getDrillParts(uint256 tokenId)
        public
        view
        returns (uint16[5] memory)
    {
        return tokenIdToDrillInventory[tokenId].drillParts;
    }

    function getNumberOfMissionsCompleted(uint256 tokenId)
        public
        view
        returns (uint16[9] memory)
    {
        return tokenIdToDrillMissionData[tokenId].numberOfMissionsCompleted;
    }

    function getResources(uint256 tokenId)
        public
        view
        returns (uint16[9] memory)
    {
        return tokenIdToDrillInventory[tokenId].resources;
    }

    function getInventory(uint256 tokenId)
        public
        view
        returns (uint16[] memory)
    {
        return tokenIdToInventory[tokenId];
    }

    function addTraitsForSale(
        uint16 traitId,
        uint16 quantity,
        uint8[] memory resourceTypes,
        uint16[] memory resourceCosts
    ) public onlyOwner {
        traitIdToMarketPlaceEntry[traitId] = MarketPlaceEntry(
            quantity,
            resourceTypes,
            resourceCosts
        );
    }

    function setDrillTycoonAddress(address newAddress) public onlyOwner {
        DrillTycoonAddress = newAddress;
    }

    function setDrillTycoonRendererAddress(address newAddress)
        public
        onlyOwner
    {
        DrillTycoonRendererAddress = newAddress;
    }
}

