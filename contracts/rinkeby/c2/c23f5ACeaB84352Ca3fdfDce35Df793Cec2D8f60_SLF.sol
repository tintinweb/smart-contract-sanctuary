    address public AUCTION_DEPLOYED_ADDRESS;
    
    /*Deployed Address of SLC contract*/
    mapping(string => address) public PROPERTY_ERC20_ADDRESS;
    
    /*Token IDs*/
    uint TOKEN_ID = 0;
    
    /*To store property info according to minted NFT TOKEN_ID*/
    mapping(string => PROPERTY_DETAILS) public PROP_INFO;
    
    event PROPERTY_TOKENS_MINTED(string ID);
    
    constructor() public{
        CONTRACT_OWNER = msg.sender;
        mintNFT nft = new mintNFT();
        MINTNFT_DEPLOYED_ADDRESS = address(nft);
    }
    
    /*To check whether a function is call by contract owner or not*/
    modifier onlyOwner{
        require(msg.sender == CONTRACT_OWNER,"caller is not contract owner");
        _;
    }
    
   
    /*Store mintNFT Contract Deployed Address*/
    function SET_MINTNFT_DEPLOYED_ADDRESS(address _address) public onlyOwner returns(bool){
        MINTNFT_DEPLOYED_ADDRESS = _address;
        return true;
    }
    
    /*Store Auction Contract Deployed Address*/
    function SET_AUCTION_DEPLOYED_ADDRESS(address _address) public onlyOwner returns(bool){
        AUCTION_DEPLOYED_ADDRESS = _address;
        return true;
    }
    
    
    /*Only an Admin can call this function to List property details to mint tokens*/
    function ListProperty_details(uint _origValue,
                                  uint _currValue,
                                  uint _coins_issued,
                                  uint _equity_at_issuance,
                                  uint _varriation,
                                  uint _current_coin_value,
                                  uint _orig_issue_rate,
                                  uint _next_schedule_reevaluation,
                                  string memory token_uri,
                                  string memory propertyID) public onlyOwner returns(bool){
        require(MINTNFT_DEPLOYED_ADDRESS != address(0),"MINT NFT ADDRESS IS ZERO");
        require(AUCTION_DEPLOYED_ADDRESS != address(0),"AUCTION ADDRESS IS ZERO");
        SLC s = new SLC("SLC","S",18,_origValue,msg.sender,AUCTION_DEPLOYED_ADDRESS);
        PROPERTY_ERC20_ADDRESS[propertyID] = address(s);
        require(Auction(AUCTION_DEPLOYED_ADDRESS).Store_SLC_Contract_Address(address(s),propertyID),"SLC address not stored");
        ++TOKEN_ID;
        mintNFTcontract(MINTNFT_DEPLOYED_ADDRESS).mint(msg.sender,TOKEN_ID,token_uri);
        PROPERTY_DETAILS memory info = PROPERTY_DETAILS({
        origValue:_origValue,
        currValue:_currValue,
        coins_issued:_coins_issued,
        equity_at_issuance:_equity_at_issuance,
        Total_Current_Value:_currValue,
        Varriation:_varriation,
        Current_Coin_Value:_current_coin_value,
        Original_Issuance_Rate:_orig_issue_rate,
        Next_Schedule_Revaluation:_next_schedule_reevaluation
        });
        PROP_INFO[propertyID] = info;
        emit PROPERTY_TOKENS_MINTED(propertyID);
        return true;
    } 
    
