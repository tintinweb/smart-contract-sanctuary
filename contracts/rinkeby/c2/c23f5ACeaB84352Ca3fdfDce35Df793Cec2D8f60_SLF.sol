    struct PROPERTY_DETAILS  {
        uint origValue;
        uint currValue;
        uint coins_issued;
        uint equity_at_issuance;
        uint Total_Current_Value;
        uint Varriation;
        uint Current_Coin_Value;
        uint Original_Issuance_Rate;
        uint Next_Schedule_Revaluation;
    }
    
