import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import '@openzeppelin/contracts/utils/structs/EnumerableSet.sol';
import '@openzeppelin/contracts/token/ERC1155/IERC1155.sol';
import '@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol';

import '@openzeppelin/contracts/utils/cryptography/ECDSA.sol';

contract Breed is ERC721, Ownable{

    /*
     ____  _ ___  _ _____ _       ____  _     ____  ____ 
    /  __\/ \\  \///  __// \     /  __\/ \ /\/  __\/ ___\
    |  \/|| | \  / |  \  | |     |  \/|| | |||  \/||    \
    |  __/| | /  \ |  /_ | |_/\  |  __/| \_/||  __/\___ |
    \_/   \_//__/\\\____\\____/  \_/   \____/\_/   \____/
    */  

    using EnumerableSet for EnumerableSet.UintSet;

    //IERC1155 public constant OPENSEA_STORE = IERC1155(0x495f947276749Ce646f68AC8c248420045cb7b5e);
    IERC1155 public OPENSEA_STORE = IERC1155(0x88B48F654c30e99bc2e4A1559b4Dcf1aD93FA656);
    address public constant burn = address(0x000000000000000000000000000000000000dEaD);
    address public waddress = 0x4599C61283e0eF125572eBEB3EB50dc128834A2C;

    mapping(uint256 => bool) public _creators;

    mapping(address => EnumerableSet.UintSet) private _deposits;
    mapping(uint256 => uint256) public _deposit_expiry_times;
    uint256 public secondsToBreed = 60 * 5;

    string public baseTokenURI;
    uint256 numTokens = 0;
    bool public onSale = false;

    //Presale
    uint256 preSaleLimit = 50;
    uint preSalePerLimit = 3;
    address public _signerAddress = 0xF6bA99F2E6ce96B74d4df667C01c5ed2EF249be2;
    bool public onSalePrivate = true;
    mapping(address => uint) public mintedPre;

    uint256 public price = 0.03 ether;
    uint256 public maxTokensPurchase = 10;
    uint public MAX_PUBLIC_TOKENS = 500;
    uint public MAX_TOKEN_SUPPLY = 1000;

    constructor() ERC721("ERC721 Token", "MEME") {
        baseTokenURI = "ipfs://QmWEFSMku6yGLQ9TQr66HjSd9kay8ZDYKbBEfjNi4pLtrr/";
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenURI;
    }

    function mintPresale(uint256 _amount, bytes32 hash, uint8 _signatureV, bytes32 _signatureR, bytes32 _signatureS) public payable {
        require(onSalePrivate,"Presale must be active");
        require(numTokens + _amount <= preSaleLimit, "Purchase would exceed max supply of Tokens");
        require(msg.value >= price * _amount, "Ether value sent is not correct");
        require(mintedPre[msg.sender] + _amount <= preSalePerLimit, "Purchase would exceed max tokens for presale");

        address signer = ECDSA.recover(hash, _signatureV,  _signatureR,  _signatureS);
        
        //pubhash = hash;
        _signerAddress = signer;

        require(signer == _signerAddress, "Invalid signature");
        
        for(uint i; i < _amount; i++){
            _safeMint(msg.sender, numTokens + 1);
            numTokens = numTokens + 1;
        }

        mintedPre[msg.sender] += _amount;
    }

    function mint(uint numberOfTokens) public payable {
        require(onSale, "Sale must be active to mint");
        require(numberOfTokens <= maxTokensPurchase, "Exceed Max Per");
        require(numTokens + numberOfTokens <= MAX_PUBLIC_TOKENS, "Exceed Max Supply");
        require(msg.value >= (price * numberOfTokens), "Ether value sent is not correct");
        
        for(uint i = 0; i < numberOfTokens; i++){
            if (numTokens <= MAX_TOKEN_SUPPLY)
            {
                _safeMint(msg.sender, numTokens + 1);
                numTokens = numTokens + 1;
            }
        }
    }

    function adminMint(uint numberOfTokens) public onlyOwner(){
        for(uint i = 0; i < numberOfTokens; i++){
            if (numTokens <= MAX_TOKEN_SUPPLY)
            {
                _safeMint(msg.sender, numTokens + 1);
                numTokens = numTokens + 1;
            }
        }
    }

    function depositBreed(uint256 maleTokenID, uint256 femaleTokenID) public{
        require(isValidDoge(maleTokenID), "Non Valid Male Doge");
        require(isValidDoge(femaleTokenID), "Non Valid Female Doge");
        require(checkDogeBreedDate(maleTokenID) != 0, "Male Doge Already Bred");
        require(checkDogeBreedDate(femaleTokenID) != 0, "Female Doge Already Bred");

        OPENSEA_STORE.safeTransferFrom(msg.sender, address(this), maleTokenID, 1, "");
        OPENSEA_STORE.safeTransferFrom(msg.sender, address(this), femaleTokenID, 1, "");

        _deposits[msg.sender].add(maleTokenID);
        _deposits[msg.sender].add(femaleTokenID);

        _deposit_expiry_times[maleTokenID] = block.timestamp + secondsToBreed;
        _deposit_expiry_times[femaleTokenID] = block.timestamp + secondsToBreed;
    }

    function withdrawBreed(uint256 maleTokenID, uint256 femaleTokenID) public{
        require(_deposits[msg.sender].contains(maleTokenID), "Male Token not deposited");
        require(_deposits[msg.sender].contains(femaleTokenID), "Female Token not deposited");
        require(_deposit_expiry_times[maleTokenID] <= block.timestamp, "Male Token not yet breeded");
        require(_deposit_expiry_times[femaleTokenID] <= block.timestamp, "Female Token not yet breeded");

        _deposits[msg.sender].remove(maleTokenID);
        _deposits[msg.sender].remove(femaleTokenID);

        OPENSEA_STORE.safeTransferFrom(address(this), msg.sender, maleTokenID, 1, "");
        OPENSEA_STORE.safeTransferFrom(address(this), msg.sender, femaleTokenID, 1, "");

        _safeMint(msg.sender, numTokens + 1);
        numTokens = numTokens + 1;
    }

    function reroll(uint256 firstTokenID, uint256 secondTokenID) public {
        require(ownerOf(firstTokenID) == msg.sender);
        require(ownerOf(secondTokenID) == msg.sender);

        _transfer(
            msg.sender,
            burn,
            firstTokenID
        );

        _transfer(
            msg.sender,
            burn,
            secondTokenID
        );

        _safeMint(msg.sender, numTokens + 1);
        numTokens = numTokens + 1;
    }

    //Read Functions
    function isValidDoge(uint256 _id) view internal returns(bool) {
        uint256 creator = (_id >> 96);
        return _creators[creator];
	}

    function checkDogeBreedDate(uint256 tokenID) public view returns(uint256){
        return _deposit_expiry_times[tokenID];
    }

    function checkDepositedDoges(address _check) public view returns (uint256 [] memory) {
        uint setLength = _deposits[_check].length();

        uint[] memory ids = new uint[](setLength);

        for(uint i = 0; i < setLength; i++)
        {
            ids[i] = _deposits[_check].at(i);
        }

        return ids;
    }

    //Setter Functions
    function setBaseURI(string memory _baseTokenURI) public onlyOwner {
        baseTokenURI = _baseTokenURI;
    }

    function changeOnSale() public onlyOwner() {
        onSale = !onSale;
    }

    function setMaxTokens(uint256 _newMaxTokens) public onlyOwner() {
        MAX_TOKEN_SUPPLY = _newMaxTokens;
    }

    function _addCreator(address _toAdd) public onlyOwner(){
        uint256 newCreator = uint256(uint160(_toAdd));
        _creators[newCreator] = true;
    }

    //Utilities
    function withdrawAll() public payable onlyOwner() {
        require(payable(waddress).send(address(this).balance));
    }

    function onERC1155Received(address _operator, address _from, uint256 _id, uint256 _value, bytes calldata _data) external returns(bytes4) {
		require(msg.sender == address(OPENSEA_STORE), "Doge: not opensea asset");
		return IERC1155Receiver.onERC1155Received.selector;
	}
}

