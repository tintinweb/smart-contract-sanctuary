pragma solidity ^0.8.7;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./AlbertKit.sol";


contract AlbertGear is ERC721Enumerable, Ownable {
    using SafeMath for uint256;
    
    AlbertKit private ak;
    string private baseURI;
    uint256 private nextIndex;

    uint public constant totalPartNum = 5;
    uint256 public constant MAX_ELEMENTS = 44440; //  8888 * 5
    uint256[] public parentId;
    address public finalOperator;

    event Minted(uint256 startTokenId, address owner);
    
    constructor(address _addr, string memory _uri) ERC721("AlbertGear", "AP") {
        ak = AlbertKit(_addr);
        nextIndex = 1;
        baseURI = _uri;
    }

    function takeApart(uint256 akId) public  {
        require(akId >=1 && akId <= 8888, "Invalid token Id");
        require(ak.ownerOf(akId) == msg.sender, "your are not owner of the nft");
       
        parentId.push(akId); // parentId would have 8888 only. 
        // Gas optimization
        uint256 _nextTokenId = nextIndex;
        for (uint i = 0; i < totalPartNum; i++) {
            _safeMint(msg.sender, _nextTokenId + i);
        }
        
        nextIndex = _nextTokenId + totalPartNum;
        ak.burnByGameOperator(akId, msg.sender);
        emit Minted(_nextTokenId, msg.sender);
    }

    
    function getPartDetail(uint256 tokenId, uint256 displayType) public view returns (string memory) {
        require(tokenId >=1 && tokenId <= MAX_ELEMENTS, "invalid tokenId");

        uint256 akId = parentId[(tokenId - 1).div(totalPartNum)];
        uint256 partType = tokenId.sub(1).mod(totalPartNum);
        return ak.getPartDetail(akId, partType, displayType);
    }

    function tokenURI(uint256 tokenId) override public view returns (string memory) {
        // require(_exists(tokenId),  "does not exists");
        uint256 partTypePic = tokenId.sub(1).mod(totalPartNum) +1;

        string memory json = Base64.encode(bytes(string(abi.encodePacked('{"name": "Albert Gear #', toString(tokenId), '", "description": "Albert Gear.", "attributes": ['
        , getPartDetail(tokenId, 1) ,'],"image": "', baseURI, toString(partTypePic), getPartDetail(tokenId, 0), '.png"}'))));
        return string(abi.encodePacked('data:application/json;base64,', json));
    }
    
    function setFinalOperator(address _add) onlyOwner public {
        finalOperator = _add;
    }
   
    function burnByFinalOperator(uint256[] memory tokenIds, address ownerAddress) public {
        require (msg.sender == finalOperator, "not authorized");
        for (uint256 i=0; i<tokenIds.length; i++) {
            require (ownerOf(tokenIds[i]) == ownerAddress, "not owner");
            _burn(tokenIds[i]);
        }
    }

    function setBaseURI(string memory uri) external onlyOwner {
        baseURI = uri;
    }

    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT license
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol
        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
}

