pragma solidity ^0.8.7;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "./AlbertGear.sol";

contract AlbertAssemble is ERC721Enumerable, Ownable {
    using SafeMath for uint256;
  
    AlbertGear private ag;
    string private baseURI1;
    string private baseURI2;
    uint256 private nextIndex;
    uint256[] public surprise; // save for future
    address public treasury;

    uint256 constant public abPartNum = 5;
    uint256 public constant MAX_ELEMENTS = 44440; //  8888 * 5
    uint256 public constant PRICE = 5e16;  //0.05eth
    mapping(uint256 => uint256[]) public partsInfo;

    event Minted(uint256 startTokenId, address owner);

    constructor(
        address _ag_addr, 
        address _treasury, 
        string memory uri1,
        string memory uri2) ERC721("AlbertAssemble", "AA") {
        ag = AlbertGear(_ag_addr);
        baseURI1 = uri1;
        baseURI2 = uri2;
        nextIndex = 1;
        treasury = _treasury;
    }

    function assemble(uint256[] memory agIds) public payable {
        require(agIds.length == abPartNum,  "invlaid part ids lengths. require 5");
        for (uint256 i =0; i < abPartNum; i++) {
            require(agIds[i] >=1 && agIds[i] <= MAX_ELEMENTS, "Invalid part Id");
            require(ag.ownerOf(agIds[i]) == msg.sender, "your are not owner of the nft");
            require(agIds[i].sub(1).mod(abPartNum) == i, "please provide part in order");
        }
        require(msg.value >= PRICE || msg.sender == owner(), "ether value sent is below the price");
        
        //never leave money in the contract
        payable(treasury).transfer(msg.value);

        partsInfo[nextIndex] = agIds;
        _safeMint(msg.sender, nextIndex);
        
        ag.burnByFinalOperator(agIds, msg.sender);
        emit Minted(nextIndex, msg.sender);

        nextIndex ++;
    }

    function tokenURI(uint256 tokenId) override public view returns (string memory) {
        require(tokenId >= 1,  "invlaid tokenid");

        uint256[] memory pids = partsInfo[tokenId];
        string memory displayId;
        string memory displayMsg;
        for (uint i = 0; i < pids.length; i++) {
            uint256 pid = pids[i];
            // uint256 abId = pid.div(abPartNum);
            // uint256 partType = pid.mod(abPartNum);
            displayId = string(abi.encodePacked(displayId, ag.getPartDetail(pid, 0)));
            if (i + 1 == abPartNum) {
                displayMsg = string(abi.encodePacked(displayMsg, ag.getPartDetail(pid, 1)));
            } else {
                displayMsg = string(abi.encodePacked(displayMsg, ag.getPartDetail(pid, 1), ","));
            }
        }
        string memory surprise_id = "";
        if (surprise.length != 0) {
            surprise_id = toString(surprise[tokenId]);
        }
        
        string memory uri;
        if (bytes(displayId)[0] == "3") {
            uri = baseURI2;
        } else {
            uri = baseURI1;
        }

        string memory json = Base64.encode(bytes(string(abi.encodePacked('{"name": "Albert Assemble #', toString(tokenId), '", "description": "Albert Assemble.", "attributes": ['
        , displayMsg ,'],"image": "', uri, displayId, surprise_id, '.png"}'))));
        return string(abi.encodePacked('data:application/json;base64,', json));
    }


    function setBaseURI(string memory uri1, string memory uri2) external onlyOwner {
        baseURI1 = uri1;
        baseURI2 = uri2;
    }

    function setSurprise(uint256[] memory _surpirse) external onlyOwner {
        surprise = _surpirse;
    }

    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT license
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol
        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
}

