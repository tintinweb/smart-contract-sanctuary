// File: contracts/Oneday.sol

pragma solidity ^0.8.7;







contract Oneday is ERC721, ERC721Enumerable, Ownable {
    using Counters for Counters.Counter;
    using DateTime for uint;
    
   // view contract
    address private _contractURI = address(0);
    Counters.Counter private _tokenIdCounter;
    // token <=> date
    mapping(uint256 => uint256) private _tokenDate;
    mapping(uint256 => uint256) private _dateToken;
    // title of token
    mapping(uint256 => bytes32) private _tokenTitle;
    // luck prop.
    mapping(uint256 => Luck) private _tokenLuck;

    event Rarity(uint256 indexed tokenId, uint8 indexed rarity, address owner);
    event Cheer(uint256 indexed tokenId, uint8 indexed cheers, address cheerer);

    // error info:
    // E0: invalid contract
    // E1: invalid date
    // E2: had claimed
    // E3: not existed
    // E4: not owner of token
    // E5: reached max

    constructor() ERC721("Oneday", "DAY") {}

    // claim
    function claim() external {
        claim(0, address(0), '');
    }
    function claim(uint256 date, address addr, string memory title) public {
        // default : today
        uint256 curDate = DateTime.timestampToDateString(block.timestamp);
        if (date == 0) {
            date = curDate;
        }
        require(DateTime.isValidDate(date) && date <= curDate, "E1");
        // default : self
        if (addr == address(0)) {
            addr = msg.sender;
        }
        // had claimed
        require(_dateToken[date] == 0 && _tokenDate[0] != date, "E2");

        // mint
        uint256 tokenId = _tokenIdCounter.current();
        _safeMint(addr, tokenId);
        _tokenIdCounter.increment();

        _tokenDate[tokenId] = date;
        _dateToken[date] = tokenId;
        // set luck
        _tokenLuck[tokenId] = _getLuck();
        // rarity ?
        if (_tokenLuck[tokenId].rarity > 0) {
            emit Rarity(tokenId, _tokenLuck[tokenId].rarity, ERC721.ownerOf(tokenId));
        }

        _setTitle(tokenId, title);
    }
    // get luck
    function _getLuck() private view returns (Luck memory luck) {
        uint256 luckNumber = uint(keccak256(abi.encodePacked(block.timestamp, block.number, msg.data, msg.sender))) % 1000000;
        luck = Luck({
            cheers : 1,
            health : uint8(luckNumber / 10000),
            freedom : uint8((luckNumber % 10000) / 100),
            happiness : uint8(luckNumber % 100),
            rarity : 0
        });
        luck.rarity = _getRarity(luck);
    }
    // get rarity
    function _getRarity(Luck memory luck) private pure returns (uint8 rarity) {
        uint8 bounce = luck.cheers / 3;
        // rarity :
        // 0 - Common
        // 1 - Rare
        // 2 - Epic
        // 3 - Legendary
        rarity = (((luck.health + bounce) >= 99)? 1 : 0) 
            + (((luck.freedom + bounce) >= 99)? 1 : 0) 
            + (((luck.happiness + bounce) >= 99)? 1 : 0);
    }
    // cheer
    function cheer(uint256 tokenId) external {
        require(_exists(tokenId), "E3");
        // max cheer 255
        require(_tokenLuck[tokenId].cheers < 255, "E5");

        _tokenLuck[tokenId].cheers += 1;
        emit Cheer(tokenId, _tokenLuck[tokenId].cheers, msg.sender);
        uint8 rarity = _getRarity(_tokenLuck[tokenId]);
        if (_tokenLuck[tokenId].rarity < rarity) {
            _tokenLuck[tokenId].rarity = rarity;
            emit Rarity(tokenId, rarity, ERC721.ownerOf(tokenId));
        }
    }
    // set title
    function setTitle(uint256 tokenId, string memory title) external {
        require(ERC721.ownerOf(tokenId) == msg.sender, "E4");
        _setTitle(tokenId, title);
    }

    function _setTitle(uint256 tokenId, string memory title) private {
        bytes32 t;
        assembly {
            t := mload(add(title, 32))
        }        
        _tokenTitle[tokenId] = t;
    }

    function dateToken(uint256 date) external view returns (uint256 tokenId) {
        tokenId = _dateToken[date];
    }
    function setURI(address addr) external onlyOwner {
        // is contract?
        uint256 size;
        assembly { size := extcodesize(addr) }
        require(size > 0, "E0");        
        _contractURI = addr;
    }

    function tokenURI(uint256 tokenId) override public view returns (string memory uri) {
        require(_exists(tokenId), "E3");
        require(_contractURI != address(0),"E0");
        uri = IURI(_contractURI).get(tokenId, _tokenDate[tokenId],
            Luck({
                cheers : _tokenLuck[tokenId].cheers,
                health : _tokenLuck[tokenId].health,
                freedom : _tokenLuck[tokenId].freedom,
                happiness : _tokenLuck[tokenId].happiness,
                rarity : _tokenLuck[tokenId].rarity
            }),
            _tokenTitle[tokenId]);
    }
    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}