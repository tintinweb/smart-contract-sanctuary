pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";

import "./Token.sol";

contract Rewarder is Ownable, ReentrancyGuard {
    using SafeMath for uint;

    uint256 public rewardPerSecond;

    mapping(address => uint256) public claims;

    MetisEarthToken public immutable token;
    IERC721 public immutable nft;

    // EVENTS
    event Claim(address indexed claimer, uint256 tokens);
    event RewardPerSecondChange(uint256 newReward);
    // ------

    constructor(address _token, address _erc721) {
        token = MetisEarthToken(_token);
        nft = IERC721(_erc721);
    }

    function setReward(uint256 newReward) public onlyOwner {
        emit RewardPerSecondChange(newReward);
        rewardPerSecond = newReward;
    }

    function pendingReward(address user) public view returns (uint256 reward) {
        uint256 claimedAt = claims[user];
        if (claimedAt == 0) {
            claimedAt = block.timestamp;
        }

        reward = block.timestamp.sub(claimedAt).mul(rewardPerSecond).mul(nft.balanceOf(user));
    }

    function claim() public nonReentrant {
        if (claims[msg.sender] == 0) {
            claims[msg.sender] = block.timestamp;
        }

        uint256 reward = pendingReward(msg.sender);
        token.mint(reward);
        token.transfer(msg.sender, reward);

        claims[msg.sender] = block.timestamp;

        emit Claim(msg.sender, reward);
    }
}

