import "./ERC721/ERC721.sol";

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

import "@rarible/royalties/contracts/impl/RoyaltiesV2Impl.sol";
import "@rarible/royalties/contracts/LibRoyaltiesV2.sol";
import "@rarible/royalties/contracts/LibPart.sol";

// import "@openzeppelin/contracts/token/ERC721/extensions/IERC721Metadata.sol";

contract TestNFT is ERC721, Ownable, RoyaltiesV2Impl {
    using Counters for Counters.Counter;
    mapping(address => uint256) private _mintFeeOfToken;
    Counters.Counter private _tokenIds;

    constructor(IERC20[] memory paymentMethods_, uint256[] memory mintFeeOfTokens_)
        ERC721("Test NFT", "TEST")
    {
        require(
            paymentMethods_.length == mintFeeOfTokens_.length,
            "Lengths of payment methods and mint fee should be equal."
        );

        for (uint128 i = 0; i < paymentMethods_.length; i++) {
            require(mintFeeOfTokens_[i] > 0, "Fee cannot be zero.");
            _mintFeeOfToken[address(paymentMethods_[i])] = mintFeeOfTokens_[i];

            emit AddedPaymentMethod(msg.sender, address(paymentMethods_[i]), mintFeeOfTokens_[i]);
        }

        /** Setting URIs */
        // _setBaseURI("https://creatures-api.opensea.io/api/creature/");
        // _setBaseURI("https://gateway.pinata.cloud/ipfs/QmNQYaaWe6gFoiZjrngTyxCh9b7z2KaeVQaBqg622QdUPq/{id}.json");
        _setBaseURI("https://gateway.pinata.cloud/ipfs/QmNQYaaWe6gFoiZjrngTyxCh9b7z2KaeVQaBqg622QdUPq/");
    }

    function mint(address to, address paymentMethod) external returns (uint256) {
        IERC20 paymentToken = _getPaymentMethodToken(paymentMethod);
        uint256 fee = _mintFeeOfToken[paymentMethod];

        require(
            paymentToken.balanceOf(msg.sender) >= fee,
            "Insufficient token balance of selected payment method."
        );

        require(
            paymentToken.allowance(msg.sender, address(this)) >= fee,
            "NFT Contract has insufficient token allowance for spending."
        );

        paymentToken.transferFrom(msg.sender, address(this), fee);

        _tokenIds.increment();

        uint256 id = _tokenIds.current();
        _safeMint(to, id);

        emit PaidMintFee(msg.sender, paymentMethod, fee, id);

        return id;
    }

    /** WIP ROYALTIES */
    function setRoyalties(
        uint256 _tokenId,
        address payable _royaltiesReceipientAddress,
        uint96 _percentageBasisPoints
    ) public onlyOwner {
        LibPart.Part[] memory _royalties = new LibPart.Part[](1);
        _royalties[0].value = _percentageBasisPoints;
        _royalties[0].account = _royaltiesReceipientAddress;
        _saveRoyalties(_tokenId, _royalties);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        if (interfaceId == LibRoyaltiesV2._INTERFACE_ID_ROYALTIES) {
            return true;
        }
        return super.supportsInterface(interfaceId);
    }

    function addPaymentMethod(address paymentMethod, uint256 mintFee) external onlyOwner {
        require(mintFee > 0, "Mint fee cannot be zero.");
        _mintFeeOfToken[paymentMethod] = mintFee;

        emit AddedPaymentMethod(msg.sender, paymentMethod, mintFee);
    }

    function _getPaymentMethodToken(address paymentMethod) private view returns (IERC20) {
        require(_mintFeeOfToken[paymentMethod] > 0, "Invalid payment method.");
        return IERC20(paymentMethod);
    }

    function withdraw(address paymentMethod) external onlyOwner {
        IERC20 paymentToken =  IERC20(paymentMethod);
        uint256 balance = paymentToken.balanceOf(address(this));

        paymentToken.transfer(msg.sender, balance);
    }

    event AddedPaymentMethod(address indexed sender, address paymentMethod, uint256 mintFee);
    event PaidMintFee(address indexed sender, address paymentMethod, uint256 mintFee, uint256 id);
}

