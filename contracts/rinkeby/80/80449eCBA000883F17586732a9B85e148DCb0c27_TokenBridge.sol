pragma solidity 0.8.10;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";

contract TokenBridge is
  AccessControlEnumerable,
  Ownable,
  ERC721Holder,
  ERC1155Holder
{
  bytes32 public constant MODERATOR_ROLE = keccak256("MODERATOR_ROLE");

  bytes4 public constant INTERFACE_ID_IERC721 = type(IERC721).interfaceId;
  bytes4 public constant INTERFACE_ID_IERC1155 = type(IERC1155).interfaceId;

  // Struct to track token info
  struct Token {
    bytes32 interfaceId;
    address tokenAddress;
    address payable owner;
    uint256 tokenId;
  }

  // Struct to manage a token contract address
  struct TokenContract {
    bytes32 interfaceId;
    address tokenAddress;
  }

  // Mapping owner address to list of owned tokens
  mapping(address => mapping(uint256 => Token)) private _ownedTokens;

  // Mapping token id to owner
  mapping(address => mapping(uint256 => address payable)) private _tokenOwners;

  // Mapping token id to index of owner tokens
  mapping(address => mapping(uint256 => uint256)) private _ownedTokensIndex;

  // Mapping owner address to token count
  mapping(address => uint256) private _balances;

  // Mappings to manage token contract addresses
  mapping(uint256 => TokenContract) private _tokenContracts;
  mapping(address => uint256) private _tokenContractIndexes;
  uint256 public tokenContractCount;

  event Imported(address indexed tokenAddress, address indexed from, uint256 tokenId);
  event Exported(address indexed tokenAddress, address indexed to, uint256 tokenId);
  event TokenOwnerChanged(address indexed tokenAddress, uint256 tokenId, address indexed from, address indexed to);

  /***
   * Public functions
   */
  constructor() {
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(MODERATOR_ROLE, msg.sender);
  }

  function balanceOf(address owner) public view virtual returns (uint256) {
    require(owner != address(0), "TokenBridge: balance query for the zero address");
    return _balances[owner];
  }

  function tokenOfOwnerByIndex(address owner, uint256 index) public view virtual returns (Token memory) {
    require(index < balanceOf(owner), "TokenBridge: owner index out of bounds");
    return _ownedTokens[owner][index];
  }

  function ownerOf(address tokenAddress, uint256 tokenId) public view virtual returns (address) {
    return _tokenOwners[tokenAddress][tokenId];
  }

  function tokenContractByIndex(uint256 index) public view virtual returns (TokenContract memory) {
    require(index < tokenContractCount, "TokenBridge: index out of bounds");
    return _tokenContracts[index];
  }

  function onERC721Received(address, address from, uint256 tokenId, bytes memory) public virtual override(ERC721Holder) returns (bytes4) {
    require(from != address(0), "TokenBridge: cannot mint directly to the bridge");

    bytes32 interfaceId = INTERFACE_ID_IERC721;
    address tokenAddress = msg.sender;

    _addTokenToRepository(interfaceId, tokenAddress, payable(from), tokenId);
    _addTokenContract(interfaceId, tokenAddress);

    emit Imported(tokenAddress, from, tokenId);

    return this.onERC721Received.selector;
  }

  function onERC1155Received(address, address from, uint256 tokenId, uint256, bytes calldata) public virtual override(ERC1155Holder) returns (bytes4) {
    require(from != address(0), "TokenBridge: cannot mint directly to the bridge");

    bytes32 interfaceId = INTERFACE_ID_IERC1155;
    address tokenAddress = msg.sender;

    _addTokenToRepository(interfaceId, tokenAddress, payable(from), tokenId);
    _addTokenContract(interfaceId, tokenAddress);

    emit Imported(tokenAddress, from, tokenId);

    return this.onERC1155Received.selector;
  }

  function exportToken(address tokenAddress, address to, uint256 tokenId) public {
    address operator = msg.sender;
    address from = address(this);
    address owner = _tokenOwners[tokenAddress][tokenId];
    uint256 index = _ownedTokensIndex[tokenAddress][tokenId];
    Token memory token = tokenOfOwnerByIndex(owner, index);
    bool isTokenOwner = (operator == token.owner && tokenId == token.tokenId);
    require(isTokenOwner || hasRole(MODERATOR_ROLE, operator), "TokenBridge: must be the token owner or moderator");

    if (token.interfaceId == INTERFACE_ID_IERC721) {
      IERC721(tokenAddress).safeTransferFrom(from, to, tokenId);
    }
    else if (token.interfaceId == INTERFACE_ID_IERC1155) {
      IERC1155(tokenAddress).safeTransferFrom(from, to, tokenId, 1, "");
    }
    else {
      revert("TokenBridge: token contract interface not supported");
    }

    _removeTokenFromRepository(tokenAddress, owner, tokenId);
    emit Exported(tokenAddress, to, tokenId);
  }

  function changeTokenOwner(address tokenAddress, uint256 tokenId, address from, address to) public {
    require(hasRole(MODERATOR_ROLE, msg.sender), "TokenBridge: must have moderator role");

    uint256 index = _ownedTokensIndex[tokenAddress][tokenId];
    Token memory token = _ownedTokens[from][index];
    bytes32 interfaceId = token.interfaceId;
    require(tokenId == token.tokenId && from == token.owner, "TokenBridge: from-address must be the token owner");

    _removeTokenFromRepository(tokenAddress, from, tokenId);
    _addTokenToRepository(interfaceId, tokenAddress, payable(to), tokenId);

    emit TokenOwnerChanged(tokenAddress, tokenId, from, to);
  }

  function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControlEnumerable, ERC1155Receiver) returns (bool) {
    return super.supportsInterface(interfaceId);
  }

  /***
   * Private functions
   */
   function _addTokenToRepository(bytes32 interfaceId, address tokenAddress, address payable owner, uint256 tokenId) private {
     uint256 index = balanceOf(owner);

     _ownedTokens[owner][index] = Token(interfaceId, tokenAddress, owner, tokenId);
     _tokenOwners[tokenAddress][tokenId] = owner;
     _ownedTokensIndex[tokenAddress][tokenId] = index;
     _balances[owner] += 1;
   }

   function _removeTokenFromRepository(address tokenAddress, address owner, uint256 tokenId) private {
     uint256 lastIndex = balanceOf(owner) - 1;
     uint256 index = _ownedTokensIndex[tokenAddress][tokenId];

     if (index != lastIndex) {
         Token memory lastToken = _ownedTokens[owner][lastIndex];
         uint256 lastTokenId = lastToken.tokenId;

         _ownedTokensIndex[tokenAddress][lastTokenId] = index;
         _ownedTokens[owner][index] = lastToken;
     }

     delete _ownedTokensIndex[tokenAddress][tokenId];
     delete _tokenOwners[tokenAddress][tokenId];
     delete _ownedTokens[owner][lastIndex];
     _balances[owner] -= 1;
   }

   function _tokenContractExists(address tokenAddress) private view returns (bool) {
     uint256 index = _tokenContractIndexes[tokenAddress];
     return _tokenContracts[index].tokenAddress == tokenAddress;
   }

   function _addTokenContract(bytes32 interfaceId, address tokenAddress) private {
     if (_tokenContractExists(tokenAddress))  return;

     uint256 index = tokenContractCount;
     _tokenContracts[index] = TokenContract(interfaceId, tokenAddress);
     _tokenContractIndexes[tokenAddress] = index;
     tokenContractCount += 1;
   }

   function _removeTokenContract(address tokenAddress) private {
     if (!_tokenContractExists(tokenAddress))  return;

     uint256 lastIndex = tokenContractCount - 1;
     uint256 index = _tokenContractIndexes[tokenAddress];

     if (index != lastIndex) {
         TokenContract memory lastContract = _tokenContracts[lastIndex];
         address lastTokenAddress = lastContract.tokenAddress;

         _tokenContractIndexes[lastTokenAddress] = index;
         _tokenContracts[index] = lastContract;
     }

     delete _tokenContractIndexes[tokenAddress];
     delete _tokenContracts[lastIndex];
     tokenContractCount -= 1;
   }

}

