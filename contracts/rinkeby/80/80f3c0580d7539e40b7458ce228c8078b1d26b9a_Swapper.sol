pragma abicoder v2;

import "hardhat/console.sol";
import "@uniswap/v3-periphery/contracts/interfaces/ISwapRouter.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@uniswap/v3-periphery/contracts/libraries/TransferHelper.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract Swapper is AccessControl, Ownable {

    using SafeMath for uint256;

    // https://docs.openzeppelin.com/contracts/4.x/api/access#AccessControl
    // Create a new role identifier for the minter role
    bytes32 public constant SWAP_BOT = keccak256("SWAP_BOT");

    struct SwapInfo {
        address tokenIn;
        address tokenOut;
        uint256 amountIn;
        uint256 amountOutMin;
        uint24 poolFee;
    }

    struct ArbitrageInfo {
        uint8 ammStart;
        uint8 ammEnd;
        address tokenIn;
        address tokenOut;
        uint256 amountIn;
        uint256 tokenOutAmountMin; // swap start: tokenOutAmountMin = tokenOutPriceStart * amountIn
        uint256 tokenInAmountMin; // swap end: tokenInAmountMin = tokenInPriceEnd * swapOutput
        uint24 poolFeeStart;
        uint24 poolFeeEnd;
    }

    ISwapRouter public uniswapRouter;
    IUniswapV2Router02 public sushiswapRouter;
    uint8 constant UNISWAP = 2;
    uint8 constant SUSHISWAP = 3;
    mapping(address => bool) public whitelistedTokens;

    constructor(ISwapRouter _uniswap, IUniswapV2Router02 _sushiswap) public {
        uniswapRouter = ISwapRouter(_uniswap);
        sushiswapRouter = IUniswapV2Router02(_sushiswap);

        // Grant the contract deployer the default admin role: it will be able
        // to grant and revoke any roles
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function performArbitrage(ArbitrageInfo calldata args) external onlyRole(SWAP_BOT) {

        require(whitelistedTokens[args.tokenIn] == true, "tokenIn not whitelisted");
        require(whitelistedTokens[args.tokenOut] == true, "tokenOut not whitelisted");

        uint256 swapOutput;

        SwapInfo memory swap1 = SwapInfo({
        tokenIn : args.tokenIn,
        tokenOut : args.tokenOut,
        amountIn : args.amountIn,
        amountOutMin : args.tokenOutAmountMin,
        poolFee : args.poolFeeStart
        });

        if (args.ammStart == UNISWAP) {
            swapOutput = usingUniswap(swap1);
        } else {
            swapOutput = usingSushiswap(swap1);
        }

        SwapInfo memory swap2 = SwapInfo({
        tokenIn : args.tokenOut,
        tokenOut : args.tokenIn,
        amountIn : swapOutput,
        amountOutMin : args.tokenInAmountMin,
        poolFee : args.poolFeeEnd
        });

        if (args.ammEnd == UNISWAP) {
            usingUniswap(swap2);
        } else {
            usingSushiswap(swap2);
        }
    }

    /// @notice swapExactInputSingle swaps a fixed amount of DAI for a maximum possible amount of WETH9
    /// using the DAI/WETH9 0.3% pool by calling `exactInputSingle` in the swap router.
    /// @dev The calling address must approve this contract to spend at least `amountIn` worth of its DAI for this function to succeed.
    /// @return amountOut The amount of WETH9 received.
    function usingUniswap(SwapInfo memory swap) private returns (uint256) {

        // Approve the router to spend tokenIn
        TransferHelper.safeApprove(swap.tokenIn, address(uniswapRouter), swap.amountIn);

        // Naively set amountOutMinimum to 0. In production,
        // use an oracle or other data source to choose a safer value for amountOutMinimum.
        // We also set the sqrtPriceLimitx96 to be 0 to ensure we swap our exact input amount.
        ISwapRouter.ExactInputSingleParams memory params =
        ISwapRouter.ExactInputSingleParams({
        tokenIn : swap.tokenIn,
        tokenOut : swap.tokenOut,
        fee : swap.poolFee,
        recipient : address(this),
        deadline : block.timestamp,
        amountIn : swap.amountIn,
        amountOutMinimum : swap.amountOutMin,
        sqrtPriceLimitX96 : 0
        });

        // The call to `exactInputSingle` executes the swap.
        uint256 uniAmountOut = uniswapRouter.exactInputSingle(params);
        return uniAmountOut;
    }

    function usingSushiswap(SwapInfo memory swap) private returns (uint256) {
        // Approve the router to spend tokenIn
        TransferHelper.safeApprove(swap.tokenIn, address(sushiswapRouter), swap.amountIn);
        address[] memory path = new address[](2);
        path[0] = swap.tokenIn;
        path[1] = swap.tokenOut;
        uint[] memory amounts = sushiswapRouter.swapExactTokensForTokens(
            swap.amountIn, swap.amountOutMin, path, address(this), block.timestamp + 1000
        );
        return amounts[1];
    }

    function addWhitelistedToken(address _token)  external onlyRole(DEFAULT_ADMIN_ROLE) {
        whitelistedTokens[_token] = true;
    }

    function removeWhitelistedToken(address _token) external onlyRole(DEFAULT_ADMIN_ROLE) {
        delete whitelistedTokens[_token];
    }

    function withdrawToken(address tokenAddress, uint256 amount) external onlyRole(DEFAULT_ADMIN_ROLE) {
        TransferHelper.safeTransfer(tokenAddress, msg.sender, amount);
    }
}

