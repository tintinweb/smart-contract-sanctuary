     * @param index the punk index, 0 <= index < 10000
     */
    function punkAttributes(uint16 index)
        external
        view
        returns (string memory text)
    {
        require(index >= 0 && index < 10000);
        uint8 cell = uint8(index / 100);
        uint256 offset = (index % 100) * 8;
        for (uint256 j = 0; j < 8; j++) {
            uint8 asset = uint8(punks[cell][offset + j]);
            if (asset > 0) {
                if (j > 0) {
                    text = string(
                        abi.encodePacked(text, ", ", assetNames[asset])
                    );
                } else {
                    text = assetNames[asset];
                }
            } else {
                break;
            }
        }
    }

    function composite(
        bytes1 index,
        bytes1 yr,
        bytes1 yg,
        bytes1 yb,
        bytes1 ya
    ) internal view returns (bytes4 rgba) {
        uint256 x = uint256(uint8(index)) * 4;
