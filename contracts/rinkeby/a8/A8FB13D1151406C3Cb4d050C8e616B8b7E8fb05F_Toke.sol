        // purple_punch
        mintMaster(20, address(0xD55Eb5Bae961a39C8923c809fB0E12F6164D7f9f));

        // sour_diesel
        mintMaster(21, address(0xC1ba9285ACBae7dC403153F9E5c2B4108AB5ACdc));    
