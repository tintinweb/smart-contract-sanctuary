        // alien_dawg
        mintMaster(5, address(0x1AF69C824E0B8c0E433b744A674FCbb1Ea67Ce60));

        // star_dawg
        mintMaster(6, address(0x8aF339E7D066A8743E90d3475da42F5723e4db5A));

        // purple_haze
