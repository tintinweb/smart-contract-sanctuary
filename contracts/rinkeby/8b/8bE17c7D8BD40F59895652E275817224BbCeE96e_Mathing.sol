contract Mathing {
    using SafeMath for uint256;
    using Address for address;

    // Uniswap
    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair;

    uint256 public maxTxCoolDownnumber;
    uint256 public TestPrint;

    uint256 timeLockAfterPurchaise = 604800;
    uint256 private buyLockPercentage = 1000;
    uint256 public maxTxPercent = 10;
    uint256 public maxTxCoolDownTime = 86400;
    mapping(address => TransactionLockData) public transactionslock;

    uint256 public _tTotal = 609_069_609_069_609_069 * 10**9;

    struct AntiRugLockData {
        uint256 time;
        uint256 amount;
    }
    uint256 antiRugCoolDownTime = 60 * 60 * 12;
    uint256 private antiRugPercentage = 2000;
    mapping(address => AntiRugLockData) public antiruglock;

    function antiRug(address sender, uint256 amount) public {
        uint256 currentBalance = 1000;
        uint256 antiRugCoolDown = (currentBalance * antiRugPercentage) / 10000;

        AntiRugLockData storage antirugtransaction = antiruglock[sender];
        // If the seller sells, add ammount sold and time.
        // Check if wallet has prior transactions if
        if (antirugtransaction.time == 0) {
            antirugtransaction.time = block.timestamp + antiRugCoolDownTime;
            require(amount <= antiRugCoolDown);
            antirugtransaction.amount = amount;
        }
        //  If the ammount is greater than the allowed ammount, don't let the seller sell.
        // If wallet has prior transactions
        // If the seller sells again adjust amount sold based on time
        else {
            uint256 coolDownRemaining;
            // Calculate how much time and lock is left
            if (antirugtransaction.time > block.timestamp) {
                coolDownRemaining = antirugtransaction.time - block.timestamp;
            } else {
                coolDownRemaining = 0;
            }
            // If there is no cooldown, reset balance.
            // TestPrint = block.timestamp;
            if (coolDownRemaining == 0) {
                delete antiruglock[sender];
            }
            // If there is cooldown calculate remainig locked.
            // If the seller sells again adjust amount sold based on time
            else {
                uint256 remainingPercentage = coolDownRemaining
                .mul(10**9)
                .div(antiRugCoolDownTime)
                .mul(10000)
                .div(10**9);

                antirugtransaction.amount = antirugtransaction
                .amount
                .mul(remainingPercentage)
                .div(10000);
            }

            antirugtransaction.time = block.timestamp.add(antiRugCoolDownTime);

            uint256 check = currentBalance
            .add(antirugtransaction.amount)
            .mul(antiRugPercentage)
            .div(10000)
            .sub(antirugtransaction.amount);

            require(amount <= check);
            antirugtransaction.amount = antirugtransaction.amount.add(amount);
        }
    }

    // Transaction Data tracker.
    struct TransactionDataMaxTX {
        uint256 time;
        uint256 amount;
    }
    mapping(address => TransactionDataMaxTX) public transactionsMaxTX;

    // Transaction Data tracker.
    struct TransactionLockData {
        uint256 time;
        uint256 amount;
    }

    function purchaiseCoolDown(
        address sender,
        address recipient,
        uint256 amount
    ) public {
        TransactionLockData storage transactionlocked = transactionslock[
            recipient
        ];
        // Check if wallet has prior transactions if
        if (transactionlocked.time == 0) {
            transactionlocked.time = block.timestamp + timeLockAfterPurchaise;
            transactionlocked.amount = amount.mul(buyLockPercentage).div(10000);
        }
        // If wallet has prior transactions
        else {
            // Calculate how much time and lock is left
            uint256 coolDownRemaining = transactionlocked.time -
                block.timestamp;
            // If there is no cooldown, reset balance.
            if (coolDownRemaining == 0) {
                delete transactionslock[recipient];
            }
            // If there is cooldown calculate remainig locked.
            else {
                uint256 remainingPercentage = coolDownRemaining
                .mul(10**9)
                .div(timeLockAfterPurchaise)
                .mul(10000)
                .div(10**9);

                transactionlocked.amount = transactionlocked
                .amount
                .mul(remainingPercentage)
                .div(10000);
            }

            transactionlocked.time = block.timestamp.add(
                timeLockAfterPurchaise
            );
            transactionlocked.amount = transactionlocked.amount.add(
                amount.mul(buyLockPercentage).div(10000)
            );
        }
    }

    function maxTXCoolDown(
        address sender,
        address recipient,
        uint256 amount
    ) public {
        uint256 maxTxCoolDown = (_tTotal * maxTxPercent) / 10000;
        maxTxCoolDownnumber = maxTxCoolDown;

        TransactionDataMaxTX storage transaction = transactionsMaxTX[sender];
        // If the seller sells, add ammount sold and time.
        // Check if wallet has prior transactions if
        if (transaction.time == 0) {
            transaction.time = block.timestamp + maxTxCoolDownTime;
            require(amount <= maxTxCoolDown);
            transaction.amount = amount;
        }
        //  If the ammount is greater than the allowed ammount, don't let the seller sell.
        // If wallet has prior transactions
        // If the seller sells again adjust amount sold based on time
        else {
            // Calculate how much time and lock is left
            uint256 coolDownRemaining = transaction.time - block.timestamp;
            // If there is no cooldown, reset balance.
            // TestPrint = block.timestamp;
            if (coolDownRemaining == 0) {
                delete transactionsMaxTX[sender];
            }
            // If there is cooldown calculate remainig locked.
            // If the seller sells again adjust amount sold based on time
            else {
                uint256 remainingPercentage = coolDownRemaining
                .mul(10**9)
                .div(maxTxCoolDownTime)
                .mul(10000)
                .div(10**9);

                transaction.amount = transaction
                .amount
                .mul(remainingPercentage)
                .div(10000);
            }

            transaction.time = block.timestamp.add(maxTxCoolDownTime);
            // get total supply value here.
            uint256 newAmount = transaction.amount.add(amount);
            require(newAmount <= maxTxCoolDown);
            transaction.amount = transaction.amount.add(amount);
        }
    }
}

