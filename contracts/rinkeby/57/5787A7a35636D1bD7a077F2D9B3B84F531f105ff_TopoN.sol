      for (uint o = 0; o < perlin_octaves; o += 1) {
        uint oftt = (xi + (yi << PERLIN_YWRAPB)) / 1000;
        //rx * 10 * 1000
        rxf = 5 * (20000 - cos[((xf * 314) % 314000) / 2000]);
        ryf = 5 * (20000 - cos[((yf * 314) % 314000) / 2000]);
        // perlin * 10000
        n1 = perlin[oftt & PERLIN_SIZE];
        n1 =
          (n1 * 100000 + rxf * (perlin[(oftt + 1) & PERLIN_SIZE] - n1)) /
          100000;
        n2 = perlin[(oftt + PERLIN_YWRAP) & PERLIN_SIZE];
        n2 =
          (n2 * 100000 +
            rxf * (perlin[(oftt + PERLIN_YWRAP + 1) & PERLIN_SIZE] - n2)) /
          100000;
        n1 = (n1 * 100000 + ryf * (n2 - n1)) / 100000;

        r = r + n1 / ampl;
        ampl = ampl * 2;
        xi <<= 1;
        xf = xf * 2;
        yi <<= 1;
        yf = yf * 2;

        if (xf >= 1000) {
          xi = xi + 1000;
          xf = xf - 1000;
        }
        if (yf >= 1000) {
          yi = yi + 1000;
          yf = yf - 1000;
        }
      }
      return r;
    }

