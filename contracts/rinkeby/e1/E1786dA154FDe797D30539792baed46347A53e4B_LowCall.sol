contract LowCall {
    using BytesLib for bytes;
    address public impl = 0x24C64FDE8B5C231aAf390573B86BcDFA7420734b;

    fallback() external {
        _callV2(impl);
    }

    function changeImpl(address newImpl) external {
        impl = newImpl;
    }
    function genData(uint256 a, uint256 b, uint256 c, uint256 d) external pure returns (bytes memory) {
        return abi.encode(a,b,c,d);
    }
    
    function decodeData(bytes memory encoded) external pure returns(uint256 a, uint256 b) {
        return (encoded.toUint256(0), encoded.toUint256(32));
    }

    function testLowLevelCall(address to, bytes memory data) external {
        _call(to, bytes4(keccak256("test(uint256,uint256,uint256)")), data);
    }

    function _call(address implementation, bytes4 sig, bytes memory data) internal {
        console.logBytes(data);
        bytes memory test;
        assembly {
            mstore(0,data) //Place signature at begining of empty storage 
            // mstore(0x40, add(x, 0x84))

            let success := call(      //This is the critical change (Pop the top stack value)
                                gas(), //5k gas
                                implementation, //To addr
                                0,    //No value
                                0,    //Inputs are stored at location x
                                0x64, //Inputs are 132 bytes long
                                0,    //Store output over input (saves space)
                                0) //Outputs are 32 bytes long
        }
    }

    function _callV2(address implementation) internal {
        console.logBytes(msg.data);
        assembly {
            // Copy msg.data. We take full control of memory in this inline assembly
            // block because it will not return to Solidity code. We overwrite the
            // Solidity scratch pad at memory position 0.
            calldatacopy(0, 0, calldatasize())

            // Call the implementation.
            // out and outsize are 0 because we don't know the size yet.
            let result := call(gas(), implementation, 0, 0, calldatasize(), 0, 0)

            // Copy the returned data.
            returndatacopy(0, 0, returndatasize())

            switch result
            // delegatecall returns 0 on error.
            case 0 { revert(0, returndatasize()) }
            default { return(0, returndatasize()) }
        }
    }
}

contract Destination {
    uint256 public num;
    function test(uint256 a, uint256 b, uint256 c) external returns (uint256) {
        console.log("a: ", a);
        console.log("b: ", b);
        console.log("c: ", c);
        num = a;
        return a;
    }
}

