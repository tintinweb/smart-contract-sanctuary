import "hardhat/console.sol";
//import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@0xcert/ethereum-erc721/src/contracts/mocks/nf-token-metadata-enumerable-mock.sol";

contract Color is NFTokenMetadataEnumerableMock {
  string[] public colors;
  mapping(string => bool) _colorExists;
  constructor() NFTokenMetadataEnumerableMock("Color","COLOR"){
   }


  function minter(string memory _color) public {
    // Require unique color
    require(!_colorExists[_color]);
    // Color - add it
    colors.push(_color);
    uint _id = colors.length - 1;
    // Call the mint function
    _mint(msg.sender, _id);
    _colorExists[_color] = true;

    // Color - TRACK IT
  }
}

