    Counters.Counter private _tokenIds;

    /// @notice root of the Merkle tree
    bytes32 private _merkleRoot;

    /// @notice public metadata locked flag
    bool public locked;

    /// @notice public revealed state
    bool public revealed;

    /// @notice the current State of the contract
    BoxStatus public _boxStatus;

    /// @notice EIP721-required Base URI
    string private _baseTokenURI;

    /// @notice Base extension for metadata
    string private _baseExtension;

    /// @notice URI to hide NFTS during minting
    string public _notRevealedUri;

    /// @notice Splitter Contract that will collect mint fees
    address payable private _mintingBeneficiary;

    /// @notice max supply of bboxes
    uint32 private _BBOXES;

    /// @notice stealth limit per wallet address
    uint32 private _STEALTH_MINT_LIMIT;

    /// @notice minting price
    uint256 public PRICE;

    /// @notice address claimed its free tokens or no
    mapping(address => bool) private _claimed;

    /**
     * @notice Event published when a box is minted
     * @param to address receiver of token minted
     * @param tokenId token id minted
     **/
    event BoxMinted(address indexed to, uint256 indexed tokenId);

    /**
     * @notice Event published when change in Status
     * @param _boxStatus next status
     **/
    event StatusChanged(BoxStatus indexed _boxStatus);

    /**
     * @notice check minting availibility
     * @param _amount amount to be minted
     **/
    modifier onlyIfAvailable(uint32 _amount) {
        require(_boxStatus != BoxStatus.DONOTOPEN, "BBTest : DO NOT OPEN");
        require(
            (_tokenIds.current()).add(_amount) <= _BBOXES,
            "BBTest: Our supply of Black Boxes has run out"
        );
        require(_amount > 0, "BBTest: Oh so you dont want ANY boxes");
        require(
            msg.value == PRICE.mul(_amount),
            "BBTest: Ether sent mismatch with mint price, silly"
        );
        _;
    }

    /// @notice metadata not locked modifier
    modifier notLocked() {
        require(!locked, "BTTest: Metadata URIs are locked");
        _;
    }

    /// @notice Art not revealed modifier
    modifier notRevealed() {
        require(!revealed, "BTTest: Art is already revealed");
        _;
    }

    /// @notice Art revealed modifier
    modifier Revealed() {
        require(revealed, "BTTest: Art is not revealed");
        _;
    }

    /**
     * @notice constructor
     * @param name the name of the EIP721 Contract
     * @param symbol the token symbol
     * @param baseTokenURI EIP721-required Base URI
     * @param notRevealedUri URI to hide NFTs during minting
     * @param merkleRoot merkle tree root of the hashed whitelist addresses
     * @param mbeneficiary the minting beneficiary that will receive minting funds
     **/
    constructor(
        string memory name,
        string memory symbol,
        string memory baseTokenURI,
        string memory notRevealedUri,
        bytes32 merkleRoot,
        address mbeneficiary
    ) ERC721(name, symbol) {
        _boxStatus = BoxStatus.DONOTOPEN;

        _merkleRoot = merkleRoot;

        PRICE = 0.08 ether;

        _BBOXES = 4096;

        _STEALTH_MINT_LIMIT = 10;

        _baseExtension = ".json";
        _baseTokenURI = baseTokenURI;
        _notRevealedUri = notRevealedUri;

        _mintingBeneficiary = payable(mbeneficiary);
    }

    /// @notice returns the base URI for the contract
    function _baseURI() internal view virtual override returns (string memory) {
        return _baseTokenURI;
    }

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     * @param tokenId get URI of tokenId
     */
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        return
            revealed
                ? string(
                    abi.encodePacked(super.tokenURI(tokenId), _baseExtension)
                )
                : _notRevealedUri;
    }

    /**
     * @notice changes the minting cost
     * @param mintCost new minting cost
     **/
    function changeMintCost(uint256 mintCost) public onlyOwner {
        require(
            PRICE != mintCost,
            "BTTest: mint Cost cannot be same as previous"
        );
        PRICE = mintCost;
    }

    /**
     * @notice Free private mint function
     * @param _to address to send the token
     * @param _amount number of tokens to be minted
     **/
    function Enter(address _to, uint32 _amount) private {
        require(_to != address(0), "BTTest: Address cannot be 0");

        for (uint32 i = _amount; i > 0; i--) {
            _tokenIds.increment();

            _safeMint(_to, _tokenIds.current());

            emit BoxMinted(_to, _tokenIds.current());
        }
    }

    /**
     * @notice Freemint sale for society & freeminters
     * @param _amount amount of tokens to mint
     * @param _proof verify if msg.sender is whitelisted whenever society || freeminters Phase
     * @param freeTokensAmount free amount of tokens to offer
     * @param tier tier of which the user is illegible for free mints
     **/
    function freemint(
        uint32 _amount,
        bytes32[] memory _proof,
        uint32 freeTokensAmount,
        uint32 tier
    ) private {
        require(
            _boxStatus >= BoxStatus(tier),
            "BTTest: please wait the next sale phase to mint"
        );

        if (_claimed[msg.sender]) {
            require(
                balanceOf(msg.sender).add(_amount) <= 10 + freeTokensAmount,
                "BTTest: Limit Exceeded, Ser"
            );

            Enter(msg.sender, _amount);
        } else {
            require(
                balanceOf(msg.sender).add(_amount) <= 10,
                "BTTest: Limit Exceeded, Ser"
            );

            isAllowedFreeMint(_proof, freeTokensAmount, tier);

            _claimed[msg.sender] = true;

            Enter(msg.sender, uint32(_amount.add(freeTokensAmount)));
        }
    }

    /**
     * @notice public mint
     * @param _amount number of tokens to be minted
     **/
    function stealthMint(uint32 _amount) private {
        require(
            balanceOf(msg.sender).add(_amount) <= _STEALTH_MINT_LIMIT,
            "BTTest: YOU HAVE HAD ENOUGH BOXES, RELAX"
        );
        Enter(msg.sender, _amount);
    }

    /// @notice minting function based on boxStatus
    function mint(
        uint32 _amount,
        bytes32[] memory _proof,
        uint32 freeTokensAmount,
        uint32 tier
    ) external payable whenNotPaused onlyIfAvailable(_amount) nonReentrant {
        if (_boxStatus == BoxStatus.STEALTH) {
            stealthMint(_amount);
        } else {
            freemint(_amount, _proof, freeTokensAmount, tier);
        }
    }

    /**
     * @notice a function for admins to mint cost-free
     * @param to the address to send the minted token to
     * @param _amount amount of tokens to be minted
     **/
    function adminMint(address to, uint32 _amount)
        external
        whenNotPaused
        nonReentrant
        onlyOwner
    {
        require(to != address(0), "BTTest: Address cannot be 0");
        require(
            (_tokenIds.current()).add(_amount) <= _BBOXES,
            "BBTest: Supply of Black Boxes has run out"
        );
        require(_amount > 0, "BBTest: Amount must be greater than 0");
        Enter(to, _amount);
    }

    /**
     * @notice Updates the contract satus
     * @param _status the status to be set next
     **/
    function setStatus(BoxStatus _status) external onlyOwner {
        require(_boxStatus != _status, "BBTest : Status cannot be the same");
        _boxStatus = _status;

        emit StatusChanged(_status);
    }

    /**
     * @notice changes the Base URI
     * @param newBaseURI the new Base URI
     **/
    function changeBaseURI(string memory newBaseURI)
        public
        onlyOwner
        notLocked
    {
        require(
            (keccak256(abi.encodePacked((_baseTokenURI))) !=
                keccak256(abi.encodePacked((newBaseURI)))),
            "BTTest: Base URI cannot be same as previous"
        );
        _baseTokenURI = newBaseURI;
    }

    /**
     * @notice changes to not revealed URI
     * @param newNotRevealedUri the new notRevealed URI
     **/
    function changeNotRevealedURI(string memory newNotRevealedUri)
        public
        onlyOwner
        notRevealed
    {
        require(
            (keccak256(abi.encodePacked((newNotRevealedUri))) !=
                keccak256(abi.encodePacked((_notRevealedUri)))),
            "BTTest: Base URI cannot be same as previous"
        );
        _notRevealedUri = newNotRevealedUri;
    }

    /**
     * @notice reveal NFTs
     **/
    function reveal() public onlyOwner notRevealed {
        revealed = true;
    }

    /**
     * @notice lock metadata forever
     **/
    function lockMetadata() public onlyOwner notLocked Revealed {
        locked = true;
    }

    /**
     * @notice changes merkleRoot in case whitelist updated
     * @param merkleRoot root of the Merkle tree
     **/
    function changeMerkleRoot(bytes32 merkleRoot) public onlyOwner {
        require(
            merkleRoot != _merkleRoot,
            "BTTest: Merkle root cannot be same as previous"
        );
        _merkleRoot = merkleRoot;
    }

    /**
     * @notice the public function validating addresses to presale phase
     * @param _proof hashes validating that a leaf exists inside merkle tree aka _merkleRoot
     **/
    function isAllowedFreeMint(
        bytes32[] memory _proof,
        uint32 freeTokensAmount,
        uint32 tier
    ) internal view returns (bool) {
        require(
            MerkleProof.verify(
                _proof,
                _merkleRoot,
                keccak256(abi.encodePacked(msg.sender, freeTokensAmount, tier))
            ),
            "BTTest: Caller is not Allowed free mints"
        );
        return true;
    }

    /**
     * @notice Current totalSupply
     **/
    function currentSupply() external view returns (uint256) {
        return _tokenIds.current();
    }

    /**
     * @notice changes the minting beneficiary payable address
     * @param beneficiary the contract Splitter that will receive minting funds
     **/
    function changeMintBeneficiary(address beneficiary) public onlyOwner {
        require(
            beneficiary != address(0),
            "BBTest: Minting beneficiary cannot be address 0"
        );
        require(
            beneficiary != _mintingBeneficiary,
            "BBTest: beneficiary cannot be same as previous"
        );
        _mintingBeneficiary = payable(beneficiary);
    }

    /// @notice pausing the contract minting and token transfer
    function pause() public virtual onlyOwner {
        _pause();
    }

    /// @notice unpausing the contract minting and token transfer
    function unpause() public virtual onlyOwner {
        _unpause();
    }

    /// @notice Contract state
    function getContractState()
        external
        view
        returns (
            BoxStatus boxStatus_,
            uint256 bboxes_,
            uint256 price_,
            uint256 currentTokenId_,
            uint256 boxesLeft_
        )
    {
        return (
            _boxStatus,
            _BBOXES,
            PRICE,
            _tokenIds.current(),
            _BBOXES.sub(_tokenIds.current())
        );
    }

    function hasFreeMint(
        address account,
        bytes32[] memory _proof,
        uint32 freeTokensAmount,
        uint32 tier
    ) external view returns (bool allowedForFreeMint, bool freeTokensClaimed) {
        return (
            isAllowedFreeMint(_proof, freeTokensAmount, tier),
            _claimed[account]
        );
    }

    /**
     * @notice Checks if a tokenId has been minted
     * @param _tokenId id of the token to be checked
     **/
    function tokenMinted(uint256 _tokenId) public view returns (bool) {
        return _exists(_tokenId);
    }

    /**
     * @notice Determines how ETH is stored/forwarded on purchases.
     **/
    function withdrawAll() external payable onlyOwner {
        require(address(this).balance != 0, "BBTest : Balance is zero");
        /// @notice withdraw funds to beneficiary wallet using CALL to avoid 2300 stipend limit
        (bool success, ) = _mintingBeneficiary.call{
            value: address(this).balance
        }("");
        require(success, "BTTest: Failed to withdraw funds");
    }

    /**
     * @notice before transfer hook function
     * @param from the address to send the token from
     * @param to the address to send the token to
     * @param tokenId to token ID to be sent
     **/
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override(ERC721, ERC721Pausable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }
}