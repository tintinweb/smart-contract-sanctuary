    bool public saleIsActive = false;
    uint256 public mintPrice = 10000000000000000; //0.01 ETH
    uint public constant MAX_BELIEFS_PER_BABEL = 100;

    mapping (uint => uint) private beliefIdToBabelId;
    mapping (uint => uint) private beliefIdToRatingValue;
    mapping (uint => uint) private beliefIdRatingCount;


    constructor() ERC721(tokenName, tokenSymbol) {}

    event SaleStateUpdated(bool indexed _state);
    event mintPriceUpdated(uint256 indexed _price);

    function getContractDimesBalance() public view returns(uint) {
        uint balance = IERC20(DIMES_ADDRESS).balanceOf(address(this));
        return balance;
