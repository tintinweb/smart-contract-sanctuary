    // replace with counter for redeem to save on gas
    function sendDimesToBabelOwner(address _address, uint _reward) internal onlyOwner {
        IERC20(DIMES_ADDRESS).transfer(_address, _reward);
    } 

    function mint( uint _babelTokenId, uint _truthRating ) public payable {
        uint id = totalSupply();
        require(saleIsActive, "Sale is not active");
        require(msg.value >= mintPrice, "Eth value sent is below the mint price");
        // TODO
        // require babelId to exist
        string memory babel = BABEL(BABEL_ADDRESS).getBabelById(_babelTokenId);
        bytes memory b = bytes(babel);
        require(b.length > 0, "Babel token does not exist");
        
        // require _truthRating to be between 0-100
        require(_truthRating >= 0 && _truthRating <= 100, "Belief rating must be a number from 0-100");
        
        // require beliefId to have less than 100 Beliefs assigned
        uint beliefRatingCount = beliefIdRatingCount[_babelTokenId] | 0;
        require(beliefRatingCount < MAX_BELIEFS_PER_BABEL, "There is a 100 belief maximum per babel");
        
        // conditional to send DIMES reward if contract has sufficient balance
        uint dimesBalance = getContractDimesBalance();
        uint reward = 10;
        if (dimesBalance >= reward) {
            address owner = BABEL(BABEL_ADDRESS).ownerOf(_babelTokenId);
            sendDimesToBabelOwner(owner, reward);
        }

        beliefIdToBabelId[id] = _babelTokenId;
        beliefIdToRatingValue[id] = _truthRating;
        beliefIdRatingCount[_babelTokenId] += 1;

        _mint(msg.sender, id);
    }
    
    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT license
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol
        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }

    function buildString(string memory _test, uint _rating) internal pure returns (string memory) {
        uint256 j = 25;
        bytes memory b = bytes(_test);
        string memory textOutput;
        
        //calculate word wrapping 
        uint i = 0;
        uint e = 0;    
        uint ll = 37; //max length of each line
        
        while (true) {
            e = i + ll;
            if (e >= b.length) {
	            e = b.length;
            } else {
        	    while (b[e] != ' ' && e > i) { 
        	        e--;
        	    }
            }
            
            // splice the line in
            bytes memory line = new bytes(e-i);
            for (uint k = i; k < e; k++) {
    	        line[k-i] = b[k];
            }
    
            textOutput = string(abi.encodePacked(textOutput,'<text class="base" x="15" y = "',toString(j),'">',line,'</text>'));

            j += 22;
            if (e >= b.length) break; // finished
            i = e + 1;
        }
        // add Dimenschen
        textOutput = string(abi.encodePacked(textOutput,'<text class="title" alignment-baseline="baseline" x="15" y="330">Dimenschen</text></svg>'));
        // add Truth rating
        textOutput = string(abi.encodePacked(textOutput,'<text class="title" alignment-baseline="baseline" x="250" y="330">',toString(_rating),'% True</text></svg>'));
        return textOutput;
    }
    
    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(tokenId < totalSupply(), "Token id not yet minted");
        
        // get babel id and rating values
        uint babelId = beliefIdToBabelId[tokenId];
        uint ratingValue = beliefIdToRatingValue[tokenId];
        
        // function getBabelById(uint) external view returns (string memory);
        string memory babel = BABEL(BABEL_ADDRESS).getBabelById(babelId);

        // function getImageCode(string memory _babel, uint256 tokenId) external view returns (uint);
        uint imageCode = BABEL(BABEL_ADDRESS).getImageCode(babel, babelId);
        string memory skyColor = BABEL(BABEL_ADDRESS).skyColors(BABEL(BABEL_ADDRESS).getIndexes(imageCode, 0));
        string memory planetColor = BABEL(BABEL_ADDRESS).planetColors(BABEL(BABEL_ADDRESS).getIndexes(imageCode, 1));
        string memory heavenlyBodyColor = BABEL(BABEL_ADDRESS).heavenlyBodyColors(BABEL(BABEL_ADDRESS).getIndexes(imageCode, 2));
        string memory heavenlyReflectionColor = BABEL(BABEL_ADDRESS).heavenlyReflectionColors(BABEL(BABEL_ADDRESS).getIndexes(imageCode, 3));
        
        // return finalized token visuals and metadata
        // start svg
        string memory output = string(abi.encodePacked('<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 350 350"><defs><linearGradient id="rectGradient" gradientTransform="rotate(90)"><stop offset="69%"  stop-color="',skyColor,'" /><stop offset="79%" stop-color="',planetColor,'" /></linearGradient><linearGradient id="circleGradient" gradientTransform="rotate(315)"><stop offset="10%" stop-color="',heavenlyBodyColor,'" /><stop offset="90%" stop-color="',heavenlyReflectionColor,'" /></linearGradient></defs><style>.title { fill: ',skyColor,'; font-family: Liberation Mono; font-size: 22px; } .base { fill: white; font-family: Liberation Mono; font-size: 18px; font-weight: 300; }</style><rect width="100%" height="100%" fill="url(#rectGradient)" /><circle cx="275" cy="150" r="50" fill="url(#circleGradient)" />'));
        // TEMPORARY
        // output = string(abi.encodePacked(output, '<path d="M325,150 L100,150" stroke="',skyColor,'" stroke-width="0.5" /><path d="M325,155 L100,155" stroke="',skyColor,'" stroke-width="1" /><path d="M325,161 L100,161" stroke="',skyColor,'" stroke-width="1.5" /><path d="M325,167 L100,167" stroke="',skyColor,'" stroke-width="2" /><path d="M325,174 L100,174" stroke="',skyColor,'" stroke-width="2.5" /><path d="M325,181 L100,181" stroke="',skyColor,'" stroke-width="3" /><path d="M325,189 L100,189" stroke="',skyColor,'" stroke-width="3.2" /><path d="M325,197 L100,197" stroke="',skyColor,'" stroke-width="3.5" />'));
        
        // add text to image
        string memory textString = buildString(babel, ratingValue);
        output = string(abi.encodePacked(output, textString));

        // Add metadata to json
        string memory jsonMeta = string(abi.encodePacked('{"name": "Belief #',toString(tokenId),'", "description": "Beliefs are not private. Beliefs are not free.", "image": "data:image/svg+xml;base64,', Base64.encode(bytes(output)), '",'));
        jsonMeta = string(abi.encodePacked(jsonMeta, ' "attributes": [{ "trait_type": "BABEL ID", "value": "',toString(babelId),'" }, { "trait_type": "RATING", "value": "',toString(ratingValue),'" }]}'));
        string memory json = Base64.encode(bytes(jsonMeta));
        output = string(abi.encodePacked('data:application/json;base64,', json));
        
        return output;
    }
    
    function tokensOfOwner(address _owner) external view returns(uint256[] memory ) {
        uint256 tokenCount = balanceOf(_owner);
        if (tokenCount == 0) {
            // Return an empty array
            return new uint256[](0);
        } else {
            uint256[] memory result = new uint256[](tokenCount);
            uint256 index;
            for (index = 0; index < tokenCount; index++) {
                result[index] = tokenOfOwnerByIndex(_owner, index);
            }
            return result;
        }
    }
    
