pragma solidity 0.8.4;


import "./SafeMath.sol";
import "./Context.sol";     
import "./IERC20.sol";      // Need this to withdraw certain tokens
import "./Ownable.sol";     // need owner because of Initialization Functions
import "./SafeERC20.sol";   // withdraw airdropped token



contract AHOY is Context, Ownable {

    using SafeMath for uint256;
    using SafeERC20 for IERC20; // this is for IERC20 tokens that you can store in the airdrop contract


    ////////////////////////////AIRDROP CONTRACT INFO VARS////////////////////////////  
    uint256 public releaseDateUnixTimeStamp = block.timestamp;     // Version 1 Release Date! gets the block timestamp so we can know when it was deployed
    ////////////////////////////AIRDROP CONTRACT INFO VARS////////////////////////////  





    ////////////////////////////AIRDROP TOKEN INFO VARS////////////////////////////  
    // Instructions - Load up Info Variables here for the Token you want to Airdrop
    // address public addressOfAirdropToken = 0x051cf5178f60e9def5d5a39b2a988a9f914107cb; 
    address public addressOfAirdropToken = 0xBf69A81BfbD5320C0AbcF1acB36B7A74F7d95300;       //CHANGEIT - You will need to change this for the real thing to he real address
    string public nameOfAirdropToken = "Epstein Token";  
    string public symbolOfAirdropToken = "EPSTEIN";  
    uint256 public decimalsOfAirdropToken = 10**18;  
    uint256 public totalSupplyOfAirdropToken = decimalsOfAirdropToken.mul(69000000000000);
    ////////////////////////////AIRDROP TOKEN INFO VARS////////////////////////////  





    ////////////////////////////AIRDROP FUNCTIONALITY VARS////////////////////////////  
    bool public isAirdropEnabled = true;    // Change if you want the airdrop enabled or disabled at the start
    mapping(address => uint256) public airDropTokensTotal;
    ////////////////////////////AIRDROP FUNCTIONALITY VARS////////////////////////////  







 
    ////////////////////////////EVENTS////////////////////////////
    event AirdropEnabled(address indexed ownerAddress, uint256 currentBlockTime);
    event AirdropDisabled(address indexed ownerAddress, uint256 currentBlockTime);
    event InitilizationOfAirdrops(address indexed ownerAddress, uint256 currentBlockTime);
    event AirDropClaimed(address indexed claimer, uint256 indexed amountOfAirdropTokenToClaim, uint256 currentBlockTime);
    event ETHwithdrawnRecovered(address indexed claimerWalletOwner, uint256 indexed ethClaimedRecovered, uint256 currentBlockTime);
    event ERC20tokenWithdrawnRecovered(address indexed claimerWalletOwner, uint256 indexed balanceClaimedRecovered, uint256 currentBlockTime);
    ////////////////////////////EVENTS////////////////////////////







    ////////////////////////////AIRDROP CONTRACT INFO FUNCTIONS////////////////////////////  
    function nameOfAirdropContract() public pure returns (string memory) {
        return "AHOY! Airdrop Contract ";
    }

    function symbolOfAirdropContract() public pure returns (string memory) {
        return "AAC";
    }

    function versionOfAirdropContract() public pure returns (string memory) {
        return "1.0";
    }
    ////////////////////////////AIRDROP CONTRACT INFO FUNCTIONS////////////////////////////  









    ////////////////////////////AIRDROP TOKEN INFO FUNCTIONS////////////////////////////  
    function currentAirdropTokenSupply(IERC20 tokenForAirdrop) public view returns (uint256) {
        return tokenForAirdrop.balanceOf(address(this));
    }


    function balanceOfAirdropTokenForAddress(address account) public view returns (uint256) {
        return airDropTokensTotal[account];  
    }
    ////////////////////////////AIRDROP TOKEN INFO FUNCTIONS////////////////////////////  











    ////////////////////////////AIRDROP FUNCTIONS////////////////////////////  
    function enableAirdrop() public onlyOwner {
        isAirdropEnabled = true;
        emit AirdropEnabled(owner(), getCurrentBlockTime());
    }


    function disableAirdrop() public onlyOwner {
        isAirdropEnabled = false;
        emit AirdropDisabled(owner(), getCurrentBlockTime());
    }

    
    function airDropClaim() public {    
        require(isAirdropEnabled, "AirDrop must be enabled. It is currently disabled. Contact the admin.");  

        address claimer = _msgSender();

        require(airDropTokensTotal[claimer] > 0,"You have no airdrop to claim.");   // this address is not valid to claim so return it. Needs to have an amount over 0 to claim

        require(currentAirdropTokenSupply(IERC20(addressOfAirdropToken)) >= airDropTokensTotal[claimer],"Not enough Airdrop Token in Contract");   // There needs to be enough token in the contract for the airdrop to be claimed 

        uint256 amountOfAirdropTokenToClaim = airDropTokensTotal[claimer];  // get it into a uint256 var for ease of use

        airDropTokensTotal[claimer] = 0;    // set to zero, more can be initialized later if needed

        withdrawAirdropTokenToAddress(IERC20(addressOfAirdropToken), claimer, amountOfAirdropTokenToClaim);

        emit AirDropClaimed(claimer, amountOfAirdropTokenToClaim, getCurrentBlockTime());


        // approveInternal(address(this), claimer, amountToGiveClaimer.mul(10**9));        // approves amount to claim from owner address
        // transferFromIgnoreMaxLimit(address(this), claimer, amountToGiveClaimer.mul(10**9));       // transfers the airdrop amount from the owner address to the claimer
        // approveInternal(address(this), claimer, 0);        // sets the approved amount back to zero      
    }


    function initializeAirDropAddressesAndAmounts(address[] memory addressesToAirDrop, uint256[] memory amountsToAirDrop) external onlyOwner{ 
        for(uint i = 0; i < addressesToAirDrop.length; i++){
            airDropTokensTotal[addressesToAirDrop[i]] = amountsToAirDrop[i];
        }
        emit InitilizationOfAirdrops(owner(), getCurrentBlockTime());
    }


    function payableInputAddress(address inputAddress) internal pure returns (address payable) {   // gets the sender of the payable address
        address payable payableInAddress = payable(address(inputAddress));
        return payableInAddress;
    }


    function withdrawAirdropTokenToAddress(IERC20 tokenForAirdrop, address receiverAddress, uint256 amountToClaim) internal {
        tokenForAirdrop.safeTransfer(payableInputAddress(receiverAddress), amountToClaim );
    }
    ////////////////////////////AIRDROP FUNCTIONS////////////////////////////  










    ////////////////////////////TOKEN RECOVERY FUNCTIONS////////////////////////////  
    function payableOwnerWalletAddr() internal view returns (address payable) {   // gets the sender of the payable address
        address currentOwnerAddress = owner();
        address payable payableOwnerAddress = payable(address(currentOwnerAddress));
        return payableOwnerAddress;
    }

    function withdrawETHSentToContractAddress() external onlyOwner  {   
        payableOwnerWalletAddr().transfer(address(this).balance);
        emit ETHwithdrawnRecovered(owner(), address(this).balance, getCurrentBlockTime());
    }

    function withdrawERC20SentToContractAddress(IERC20 tokenToWithdraw) external onlyOwner {
        tokenToWithdraw.safeTransfer(payableOwnerWalletAddr(), tokenToWithdraw.balanceOf(address(this)));
        emit ERC20tokenWithdrawnRecovered(owner(), tokenToWithdraw.balanceOf(address(this)), getCurrentBlockTime());
    }
    ////////////////////////////TOKEN RECOVERY FUNCTIONS////////////////////////////  








    ////////////////////////////MISC INFO FUNCTIONS////////////////////////////  
    function getCurrentBlockTime() public view returns (uint) {
        return block.timestamp;     // gets the current time and date in Unix timestamp
    }
    ////////////////////////////MISC INFO FUNCTIONS////////////////////////////  








    receive() external payable { }      // oh it's payable alright
}






