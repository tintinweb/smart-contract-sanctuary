/**
* @dev Define the Collectible Traits that will be stored on chain
*/
struct CollectibleTraits {
    string name;
    string collectibleType;
    uint256 maxBoostLevel;
    uint256 blocksToMaxBoost;
    uint256 expiryDate;
    string uri;
}

contract FarmlandCollectible is ERC721, Ownable {
    using Counters for Counters.Counter;
    using SafeMath for uint256;
    Counters.Counter private _tokenIds;

    constructor() ERC721("Farmland Collectible", "FARMLAND") public {
        _setBaseURI("ipfs://");
    }

    /**
     * @dev PUBLIC: Stores the key traits for Farmland Collectibles, by making the structs public we can read the attributes through the contract view
    */
    mapping(uint256 => CollectibleTraits) public collectibleTraits;
   
    /**
     * @dev Owner only can mint new Farmland Collectibles NFTs, its a standard NFT extended to store key traits on chain this is required to enable boosts
    */
    function mintCollectible(
        address receiver, 
        string memory tokenURI, 
        string memory collectibleName,
        string memory collectibleType,
        uint256 maxBoostLevel, 
        uint256 blocksToMaxBoost, 
        uint256 expiryDate
        ) 
    external 
    onlyOwner 
    returns (uint256) {
    
    _tokenIds.increment();
    uint256 id = _tokenIds.current();

    // Store Key Equipment traits in on chain
    CollectibleTraits storage collectibleTrait = collectibleTraits[id];
    collectibleTrait.name = collectibleName;
    collectibleTrait.collectibleType = collectibleType;
    collectibleTrait.maxBoostLevel = maxBoostLevel;
    collectibleTrait.blocksToMaxBoost = blocksToMaxBoost;
    collectibleTrait.expiryDate = 0;
    if ( expiryDate > 0 ) { 
        collectibleTrait.expiryDate = expiryDate; 
    }
    collectibleTrait.uri = tokenURI;
    _safeMint(receiver, id);
    _setTokenURI(id, tokenURI);
    return id;
    } 
}

