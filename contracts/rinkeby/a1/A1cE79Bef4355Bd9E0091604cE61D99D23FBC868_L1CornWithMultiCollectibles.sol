        if (requiredState) {
            require(
                ownerOfFarm[farmAddress].numberOfCollectibles != 0,
                "You need a collectible on your farm"
            );
        } else {
            require(
                ownerOfFarm[farmAddress].numberOfCollectibles == 0,
                "Ensure you release your collectible(s) first"
            );
        }
        _; // Call the actual code
    }

    /**
