        uint256 _myRatio = getAddressRatio(farmAddress);
        uint256 _globalRatio = getGlobalRatio();

        // Avoid division by 0 & ensure 1x boost if nothing is locked
        if (_globalRatio == 0 || _myRatio == 0) {
