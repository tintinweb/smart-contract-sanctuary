    //////////////////// GETTERS //////////////////////

    /**
     * @dev Return the amount available to harvest at a specific block by farm
     */
    function getHarvestAmount(address farmAddress, uint256 targetBlock)
        private
        view
        returns (uint256 availableToHarvest)
    {
        Farm memory farm = ownerOfFarm[farmAddress]; // Shortcut accessor

        // Ensure this address has a farm on the LAND
        if (farm.amount == 0) {
            return 0;
        }

        require(
            targetBlock <= block.number,
            "You can only calculate up to current block"
        );
        require(
            farm.lastHarvestedBlockNumber <= targetBlock,
            "You can only specify blocks at or ahead of last harvested block"
        );

        // set the end of growth cycle ... calculate last block
        uint256 lastBlockInGrowthCycle = farm.lastHarvestedBlockNumber.add(_maxGrowthCycle);

        // Owning a farmer increase the length of the growth cycle
        if ( getFarmCollectibleType(farmAddress,"Farmer") ) { 
            lastBlockInGrowthCycle = farm.lastHarvestedBlockNumber.add(_maxGrowthCycleWithFarmer);
        }

        uint256 blocksMinted = _maxGrowthCycle;

        if (targetBlock < lastBlockInGrowthCycle) {
