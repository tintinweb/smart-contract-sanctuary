            // Only count collectibles that haven't already expired
            if (ownerOfCollectibles[farmAddress][i].expiry < block.timestamp) {
                expired = true;
            }
            if ( ownerOfCollectibles[farmAddress][i].expiry == 0 ) {
                expired = false;
            }
