    //////////////////// SETTERS //////////////////////

    /**
     * @dev PUBLIC: Allocate LAND to farm for growing crops with the specified address as the harvester.
     */
    function allocate(address farmAddress, uint256 amount)
        public
        nonReentrant()
        preventSameBlock(_msgSender())
    {
        require(
            amount > 0,
            "You must provide a positive amount of LAND to build a farm"
        );

        // Ensure you can only lock up to a limited amount of LAND during failsafe period
        if (block.number < failsafeTargetBlock) {
            require(amount <= _failsafeMaxAmount,"You can only allocate a maximum of 1000 LAND during failsafe.");
        }

        Farm storage farm = ownerOfFarm[_msgSender()]; // Shortcut accessor

        // Limit the max farm size for farms without NFTs
        uint256 _maxFarmSizeWithoutFarmer = 50000 * (10**18); // TODO: consider a global constant
        if ( farm.amount.add(amount) > _maxFarmSizeWithoutFarmer && !getFarmCollectibleType(farmAddress,"Farmer") ) {
            revert('You need a farmer to build a farm this size');
        }
        uint256 _maxFarmSizeWithoutEquipment = 150000 * (10**18); // TODO: consider a global constant
        if ( farm.amount.add(amount) > _maxFarmSizeWithoutEquipment && !getFarmCollectibleType(farmAddress,"Equipment") ) {
            revert('You need a farmer & equipment to build a farm this size');
        }
       
        if (farm.amount == 0 && !getFarmCollectibleType(farmAddress,"Farmer")) { // Check to see if there is LAND in the farm
            farm.amount = amount; // Stores the LAND AMOUNT
            farm.blockNumber = block.number; // Block when farm first setup
            farm.harvesterAddress = farmAddress; // Stores the farmers address
            _globalCompostedAmount = _globalCompostedAmount.add(farm.compostedAmount); // retains any composted crops for returning farmers
            _globalTotalFarms = _globalTotalFarms.add(1); // Increment the total farms counter
        } else {
            farm.amount = farm.amount.add(amount); // Adds additional LAND
        }

        _globalAllocatedAmount = _globalAllocatedAmount.add(amount);
        farm.lastHarvestedBlockNumber = block.number; // Reset the last harvest height to the new LAND allocation height

        emit Allocated(_msgSender(), block.number, farmAddress, amount, farm.compostedAmount);

        // Send [amount] of LAND token from the address that is calling this function to crop smart contract.
        IERC777(token).operatorSend(_msgSender(), address(this), amount, "", "" ); // [RE-ENTRANCY WARNING] external call, must be at the end
    }

    /**
     * @dev PUBLIC: Releasing a farm returns LAND to the owners
     */
    function release()
        public
        nonReentrant()
        preventSameBlock(_msgSender())
        requireFarm(_msgSender(), true) // Ensure the address you are releasing has a farm on the LAND
    {
        Farm storage farm = ownerOfFarm[_msgSender()]; // Shortcut accessor
        uint256 amount = farm.amount;
        farm.amount = 0;
        _globalAllocatedAmount = _globalAllocatedAmount.sub(amount);
        _globalCompostedAmount = _globalCompostedAmount.sub(farm.compostedAmount);
        _globalTotalFarms = _globalTotalFarms.sub(1);

        emit Released(_msgSender(), amount, farm.compostedAmount);

        // Send back the LAND amount to person calling the method
        IERC777(token).send(_msgSender(), amount, ""); // [RE-ENTRANCY WARNING] external call, must be at the end
    }

    /**
     * @dev PUBLIC: Composting a crop fertilizes a farm at specific address
     */
    function compost(address farmAddress, uint256 amount)
        public
        nonReentrant()
        requireFarm(farmAddress, true) // Ensure the address you are composting to has a farm on the LAND
    {
        require(amount > 0, "Nothing to compost");

        Farm storage farm = ownerOfFarm[farmAddress]; // Shortcut accessor, pay attention to farmAddress here
        farm.compostedAmount = farm.compostedAmount.add(amount);
        _globalCompostedAmount = _globalCompostedAmount.add(amount);

        // Get a composting bonus if you own a farmer or equipment
        if ( getFarmCollectibleType(farmAddress,"Farmer") || getFarmCollectibleType(farmAddress,"Equipment") ){
            uint256 equipmentBonusBoost = 1000; // 10%
            if (getFarmCollectibleType(farmAddress,"Equipment")) {
                equipmentBonusBoost = 2500; // 25%
            }
            uint256 bonusAmount = equipmentBonusBoost.mul(amount).div(_percentMultiplier); // Add an additional 10% for farms with equipment 
            farm.compostedAmount = farm.compostedAmount.add(bonusAmount);
            _globalCompostedAmount = _globalCompostedAmount.add(bonusAmount);
        }

        emit Composted(_msgSender(), farmAddress, amount);

        // Call the normal ERC-777 burn (this will destroy a crop token). We don't check address balance for amount because the internal burn does this check for us.
        _burn(_msgSender(), amount, "", ""); // [RE-ENTRANCY WARNING] external call, must be at the end
    }

    /**
     * @dev PUBLIC: Harvests crops from the Farm to a target address UP TO the target block (target address can be used to harvest to an alternative address)
     */
    function harvest(
        address farmAddress,
        address targetAddress,
        uint256 targetBlock
    )
        public
        nonReentrant()
        preventSameBlock(farmAddress)
        requireFarm(farmAddress, true) // Ensure the adress that is being harvested has a farm on the LAND
    {
        require(
            targetBlock <= block.number,
            "You can only harvest up to current block"
        );

        Farm storage farm = ownerOfFarm[farmAddress]; // Shortcut accessor, pay attention to farmAddress here

        require(
            farm.lastHarvestedBlockNumber < targetBlock,
            "You can only harvest ahead of last harvested block"
        );
        require(
            farm.harvesterAddress == _msgSender(),
            "You must be the owner of the farm to harvest"
        );

        uint256 mintAmount = getHarvestAmount(farmAddress, targetBlock);
        require(mintAmount > 0, "Nothing to harvest");

        farm.lastHarvestedBlockNumber = targetBlock; // Reset the last harvested height

        emit Harvested(_msgSender(),block.number,farmAddress,targetAddress,targetBlock,mintAmount);

        // Call the normal ERC-777 mint (this will harvest crop tokens to targetAddress)
        _mint(targetAddress, mintAmount, "", ""); // [RE-ENTRANCY WARNING] external call, must be at the end
    }

    /**
     * @dev PUBLIC: Harvest & Compost in a single call for a farms with a farmer & equipment.
     */
    function directCompost(
        address farmAddress,
        uint256 targetBlock
    )   
        public
        nonReentrant()
        requireFarm(farmAddress, true) // Ensure the adress that is being harvested has a farm on the LAND
        requireFarmer(farmAddress) // Ensure theres a farmer on the farm
        requireEquipment(farmAddress) // Ensure there Equipment on the farm
    {
        require(
            targetBlock <= block.number,
            "You can only harvest & compost up to current block"
        );

        Farm storage farm = ownerOfFarm[farmAddress]; // Shortcut accessor

        require(
            farm.lastHarvestedBlockNumber < targetBlock,
            "You can only harvest & compost ahead of last harvested block"
        );
        require(
            farm.harvesterAddress == _msgSender(),
            "You must be the owner of the farm to harvest & compost"
        );
        // Ensure there is crop to harvest 
        uint256 amount = getHarvestAmount(farmAddress, targetBlock);
        require(amount > 0, "Nothing to compost");

        // Reset the last harvested height
        farm.lastHarvestedBlockNumber = targetBlock;

        // Update farm & global composted amounts variables
        farm.compostedAmount = farm.compostedAmount.add(amount);
        _globalCompostedAmount = _globalCompostedAmount.add(amount);

        // Get a composting bonus if you own a farmer or equipment
        if ( getFarmCollectibleType(farmAddress,"Farmer") || getFarmCollectibleType(farmAddress,"Equipment") ){
            uint256 equipmentBonusBoost = 1000; // 10%
            if ( getFarmCollectibleType(farmAddress,"Equipment") ) {
                equipmentBonusBoost = 2500; // 25%
            }
            uint256 bonusAmount = equipmentBonusBoost.mul(amount).div(_percentMultiplier); // Add an bonus for farms with equipment 
            farm.compostedAmount = farm.compostedAmount.add(bonusAmount);
            _globalCompostedAmount = _globalCompostedAmount.add(bonusAmount);
        }

        emit Composted(_msgSender(), farmAddress, amount);
    }

    function equipCollectible(uint256 tokenID)
        public
        nonReentrant()
        preventSameBlockCollectible(_msgSender())
        requireFarm(_msgSender(), true) // You can't add a collectible if you don't have a farm
        {
        require(tokenID > 0, "You must provide a tokenID of the Collectible");

        // Retrieve collectible details from NFT
        (string memory _name, string memory _type, uint256 _maxBoostLevel,uint256 _blocksToMaxBoost,uint256 _expiry, string memory _uri) = nftToken.collectibleTraits(tokenID);
  
        Farm storage farm = ownerOfFarm[_msgSender()]; // Shortcut accessor
        // Increment number of collectibles owned by that address
        farm.numberOfCollectibles = farm.numberOfCollectibles.add(1);

        // Add details to Collectibles
        ownerOfCollectibles[_msgSender()].push(Collectible(tokenID, _name, _type, _maxBoostLevel, _blocksToMaxBoost, block.number, _expiry, _uri));
        
        emit CollectibleEquipped(_msgSender(),block.number,tokenID,_name);

        // Send [amount] of LP token from the address that is calling this function to crop smart contract.
        IERC721(nftToken).safeTransferFrom(_msgSender(),address(this),tokenID); // [RE-ENTRANCY WARNING] external call, must be at the end
    }

    function releaseCollectible(uint256 index)
        public
        nonReentrant()
        preventSameBlockCollectible(_msgSender())
        requireCollectible(_msgSender(), true) // You can't remove collectible if you don't have any
        requireFarm(_msgSender(), false) // You need to release your Land before you can release Collectibles - to stop people sharing the NFTs
        {
        Farm storage farm = ownerOfFarm[_msgSender()];
        uint256 tokenID = ownerOfCollectibles[_msgSender()][index].id;
        string memory collectibleName = ownerOfCollectibles[_msgSender()][index].name;

        // Ensure that the token id is greater than 0 else the call fails
        require ( tokenID > 0 , "Unable to locate tokenID" );

        // Swap and delete
        ownerOfCollectibles[_msgSender()][index] = ownerOfCollectibles[_msgSender()][ownerOfCollectibles[_msgSender()].length.sub(1)];
        ownerOfCollectibles[_msgSender()].pop();
        
        // Update number of collectibles
        farm.numberOfCollectibles = farm.numberOfCollectibles.sub(1);

        emit CollectibleReleased(_msgSender(),block.number,tokenID,collectibleName);
        // Return Collectible to the address that is calling this function
        IERC721(nftToken).safeTransferFrom(address(this),_msgSender(),tokenID); // [RE-ENTRANCY WARNING] external call, must be at the end
    }

