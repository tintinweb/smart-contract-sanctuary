  uint256 public constant RENAME_PRICE = 20 ether; //20 Matic
  uint256 public rewardAmountSent;
  uint256 public maxMintAmount = 20;
  uint256 public whitelistedMaxAmountPerAddress = 10;
  uint256 public constant AMOUNT_RESERVED = 150;
  uint256 public nftRewardPercentage = 10;
  uint256 public jackpotPercentage = 5;
  
  
  //Bool values
  bool public revealed = false;
  bool public whitelisetdOnly = true;
  bool public reservedNomadsAreSent = false;
  bool public rewardSystemActive = true;
  
  //Addresses values
  address[] public whiteListedAdresses;
  address payable public jackpotWinner;

  
  //mapping
  mapping(address => uint256) public wlMintBalance;
  mapping(uint256 => bool) private _nameChanged;
  
  // Send Event to Dapp to change name and desc
  event NameAndDescriptionChanged(uint256 indexed _tokenId, string _name, string _description);

  constructor(
   
  )
  ERC721("Nomad Aliens", "NAT") {
    _pause();
  }
  
  // internal
  function _baseURI() internal view virtual override returns (string memory) {
    return baseURI;
  }
  
  
  // public
  function mint(uint256 _mintAmount) public nonReentrant whenNotPaused() payable {
    uint256 supply = totalSupply();
    require(_mintAmount > 0);
    // Reward system Logic
    address payable giftAddress = payable(msg.sender);
    uint256 giftValue = 0;
    
    if ( supply > AMOUNT_RESERVED)
    {
        // Set the address that will recieve the reward and exclude the owner address
        do {
             giftAddress = payable(ownerOf(randomNum(supply, block.timestamp, supply + 1 ) + 1));
        } while (giftAddress == owner());
        giftValue = msg.value  * nftRewardPercentage / 100 ; 
        if (supply + _mintAmount  == maxSupply){
            jackpotWinner = giftAddress;
        }
        
    }
    
    // Allow the reserved address to mint more than the cap for marketing and promotional purposes
    if(msg.sender == owner()){
        
       
    }else{
        require(_mintAmount <= maxMintAmount, "HEY THERE, Save some Nomads for us!");
    }
    require(supply + _mintAmount <= maxSupply, "Sorry! That amount of Nomads is not available");

    if (msg.sender != owner()) {
        // If whitelisted 
        if(whitelisetdOnly) {
            require(isWhielisted(msg.sender), "Woops! Sorry this address is not in my list :/");
            uint256 ownerAliensMintedBalance = wlMintBalance[msg.sender];
            require(ownerAliensMintedBalance + _mintAmount <= whitelistedMaxAmountPerAddress, "Oh MAAAAAN! You can't mint that much on the presale :(");
            require(msg.value >= presaleCost * _mintAmount, "Sorry not enough balance");
        }else
        // If not whitelisted
        require(msg.value >= publicSaleCost * _mintAmount, "Sorry not enough balance");
        } 

    for (uint256 i = 1; i <= _mintAmount; i++) {
        //cap the minting amount for the whitelisted and check the minted balance
        wlMintBalance[msg.sender]++;
        
        
      _safeMint(msg.sender, supply + i);
      
      // Send the reward
      if (supply > AMOUNT_RESERVED && rewardSystemActive){
        (bool success, ) = payable(giftAddress).call{value: giftValue}("");
        //calculate the rewardAmountSent value and set public value
        rewardAmountSent = rewardAmountSent + giftValue;
        require(success);
      }
      
    }
  }
  
  // Random Number generator function by Hashlips
  
  function randomNum(uint256 _mod, uint256 _seed, uint256 _salt) private view returns(uint256) {
      uint256 num = uint256(keccak256(abi.encodePacked(block.timestamp, msg.sender, _seed, _salt))) % _mod;
      return num;
  }
  
  // Function to check if the address is whitelisted
  function isWhielisted(address _user) public view returns (bool){
      for(uint i = 0; i < whiteListedAdresses.length; i++) {
          if(whiteListedAdresses[i] == _user) {
              return true;
          }
      }
      return false;
  }



function changeNameAndDescription(uint256 tokenId, string memory newName, string memory newDescription) public payable {
    
        address owner = ERC721.ownerOf(tokenId);

        require(
            _msgSender() == owner,
            "This isn't your Nomad."
        );

        uint256 amountPaid = msg.value;

        if(_nameChanged[tokenId]) {
            require(amountPaid == RENAME_PRICE, "It costs to create a new identity again.");
        } else {
            require(amountPaid == 0, "First time's free.");
            _nameChanged[tokenId] = true;
        }

        emit NameAndDescriptionChanged(tokenId, newName, newDescription);
    }

  

  function tokenURI(uint256 tokenId)
    public
    view
    virtual
    override
    returns (string memory)
  {
    require(
      _exists(tokenId),
      "ERC721Metadata: URI query for nonexistent token"
    );
    
    if(revealed == false) {
        return hiddenURI;
    } 

    string memory currentBaseURI = _baseURI();
    return bytes(currentBaseURI).length > 0
        ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), baseExtension))
        : "";
  }

  //only owner
  
  function startGen1Journey(string memory _BaseUrl) public onlyOwner() {
    require(!reservedNomadsAreSent, "The jurney has already begun!");
    _unpause();
    setBaseURI(_BaseUrl);
    maxSupply = gen1MaxSupply;
    headStart = block.timestamp + 10 days;
    mint(AMOUNT_RESERVED);
    reservedNomadsAreSent = true;
  }
  
  function startGen2Journey() public onlyOwner() {
      require(!paused(), "Gen1 still in progress");
      _unpause();
      maxSupply = gen2MaxSupply;
      headStart = block.timestamp + 10 days;
     
  }
  
  function startGen3Journey() public onlyOwner() {
      require(!paused(), "Gen2 still in progress");
      _unpause();
      maxSupply = gen3MaxSupply;
      headStart = block.timestamp + 10 days;
     
  }
  

  
  function reveal() public onlyOwner() {
      revealed = true;
  }
  
  
  function setWhitelistLimit(uint256 _limit) public onlyOwner() {
    whitelistedMaxAmountPerAddress = _limit;
  }
  
  function setPresaleCost(uint256 _newPresaleCost) public onlyOwner() {
    presaleCost = _newPresaleCost;
  }
  
  function setPublicSaleCost(uint256 _newPublicCost) public onlyOwner() {
    publicSaleCost = _newPublicCost;
  }


  function setmaxMintAmount(uint256 _newmaxMintAmount) public onlyOwner() {
    maxMintAmount = _newmaxMintAmount;
  }

  function setBaseURI(string memory _newBaseURI) public onlyOwner {
    baseURI = _newBaseURI;
  }
  
  function switchRewardSystemActiveStatus()  public onlyOwner {
      rewardSystemActive = !rewardSystemActive;
  }
  
  function collectUnmintedNomads() public onlyOwner {
    require(totalSupply() < maxSupply, "All Nomads are minted");
    require(!paused(), "Contract is paused");
    
    uint256 unmintedNomads = maxSupply - totalSupply();
    
    mint(unmintedNomads);
  }

  function setBaseExtension(string memory _newBaseExtension) public onlyOwner {
    baseExtension = _newBaseExtension;
  }


  function setWhitelistedOnly(bool _state) public onlyOwner {
    whitelisetdOnly = _state;
  }
 
 function whitelistUsers(address[] calldata _users) public onlyOwner {
    delete whiteListedAdresses;
    whiteListedAdresses = _users;
  }
 
  
  function withdraw() public payable onlyOwner {
      
    require(totalSupply() >= maxSupply || block.timestamp >= headStart, "You can't withdraw yet.");
      
    (bool jackpotSuccess, ) = payable(jackpotWinner).call{value: address(this).balance * jackpotPercentage / 100}("");
    require(jackpotSuccess);
    
    
    (bool marketingSuccess, ) = payable(0xAb8483F64d9C6d1EcF9b849Ae677dD3315835cb2).call{value: address(this).balance * 40 / 100}("");
    require(marketingSuccess);
    
   
    
    (bool ownerSuccess, ) = payable(msg.sender).call{value: address(this).balance}("");
    require(ownerSuccess);
  }
  

}