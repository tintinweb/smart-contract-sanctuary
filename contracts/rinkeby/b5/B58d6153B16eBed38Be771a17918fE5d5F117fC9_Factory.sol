pragma solidity 0.8.5;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

contract Collection is ERC1155, AccessControl {

    uint256 constant MysterySeed = 1;
    uint256 constant BlackOnyxSeed = 2;
    uint256 constant RoseGoldSeed = 3;
    uint256 constant TerracottaSeed = 4;
    uint256 constant DiamondBlueSeed = 5;
    uint256 constant PlatiniumWhiteSeed = 6;

    event Minted(
        address indexed operator,
        address indexed to,
        uint256 indexed id,
        uint256 seed,
        uint256 weight
    );

    event MintedBatch(
        address indexed operator,
        address indexed to,
        uint256 [] indexed id,
        uint256[] seedIds,
        uint256[] weights
    );

   
    mapping(uint256 => uint256) public seedIdByTokenAttribute;
    mapping(uint256 => uint256) public weightByTokenAttribute;

    using EnumerableSet for EnumerableSet.UintSet;

    EnumerableSet.UintSet mintedSeeds;

    address factory;
    constructor( 
        string memory uri,
        address _admin,
        address _factory
        ) public 
        ERC1155(uri){

        factory = _factory;

        _setupRole(DEFAULT_ADMIN_ROLE, _admin);
    }

    modifier factoryOrAdmin() {
      require(msg.sender == factory || hasRole(DEFAULT_ADMIN_ROLE, msg.sender) ,"This function can only be called by factory contract or Admin");
      _;
    }

    
    function addSeedIdsBatch(uint256[] memory tokenIds, uint256[] memory seedIds) internal {
        require(tokenIds.length == seedIds.length, "NftStakingMock: inconsistent array lengths");
        for (uint256 i = 0; i < tokenIds.length; ++i) {
           seedIdByTokenAttribute[tokenIds[i]] = seedIds[i];
        }
      
    }

    function addSeedId(uint256 _tokenId, uint256  _seedId) internal {
        seedIdByTokenAttribute[_tokenId] = _seedId;

    }

    function addWeight(uint256 _seedId, uint256  _weigth) external onlyRole(DEFAULT_ADMIN_ROLE) {
        weightByTokenAttribute[_seedId] = _weigth;

    }

    function mint(
        address _to, 
        uint256 _seedId, 
        uint256 _id
    ) external factoryOrAdmin {
        require(!(mintedSeeds.contains(_id)), "This seed id already minted");

        _mint(_to, _id, 1, "");

        addSeedId(_id, _seedId);
   
        mintedSeeds.add(_id);
        uint256 weight = weightByTokenAttribute[_seedId];
        emit Minted(address(this), _to, _id, _seedId, weight);
    }

    function mintBatch(
        address _to, 
        uint256[] calldata _seedIds, 
        uint256[] calldata _ids,  
        uint256[] calldata _amount
    ) external factoryOrAdmin {
        uint256[] memory weights = new uint256[](_ids.length);
        for(uint256 i=0; i < _ids.length; i++){
            require(!(mintedSeeds.contains(_ids[i])), "This seed id already minted");
            weights[i] = weightByTokenAttribute[_seedIds[i]];
        }
       

        _mintBatch(_to, _ids, _amount, "");

        addSeedIdsBatch(_ids, _seedIds);

        for(uint256 i=0; i < _ids.length; i++){
            mintedSeeds.add(_ids[i]);
       
        }
        
        emit MintedBatch(address(this), _to, _ids, _seedIds, weights);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC1155, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

   
    

}

pragma solidity 0.8.5;

import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./Collection.sol";
import "./ERC20Mock.sol";
import "./UintSet.sol";
import "hardhat/console.sol";


struct Stakers{
    address owner;
    uint256 seedId;
    uint256 weight;
    uint256 startTimestamp;
    uint256 exitTimestamp;
    uint256 lastClaimed;
    uint256 accumulatedTime;
    uint256 currentStage;
    bool isFinished;
    bool exist;
}
contract NftStaking is AccessControl, IERC1155Receiver{
    using UintSet for Set;

    event NftStaked(address staker, address collection, uint256 tokenId, uint256 seedId, uint256 weight);
    event NftUnstaked(address staker, address collection, uint256 tokenId, uint256 seedId, uint256 weight);
    event NftsBatchStaked(address staker, address collection, uint256[] tokenIds, uint256[] seedIds, uint256[] weights);
    event NftsBatchUnstaked(address staker, address collection, uint256[] tokenIds, uint256[] seedIds, uint256[] weights);
    event RewardsClaimed(address staker, uint256 amount);
    event Started();
    // seedId => months => shareOfMÄ±ntableToken 
    //example MysterySeedId=1 => 1.ay => 851,064
    mapping(uint256 => mapping(uint256 => uint256)) public getTotalMintPerMonths;
    mapping(uint256 => Stakers) public staker;
    mapping(address => Set) internal stakerTokenId;

    Collection public collection;
    IERC20 public rewardToken;
    address [] public whitelistedNftContract;
    uint256 startTimestamp;
    bytes4 internal constant _ERC1155_RECEIVED = 0xf23a6e61;
    bytes4 internal constant _ERC1155_BATCH_RECEIVED = 0xbc197c81;
    bytes4 internal constant _ERC1155_REJECTED = 0xffffffff;

    constructor() public{
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    modifier hasStarted() {
        require(startTimestamp != 0, "NftStaking: staking not started");
        _;
    }

    modifier hasNotStarted() {
        require(startTimestamp == 0, "NftStaking: staking has started");
        _;
    }

    function start() public onlyRole(DEFAULT_ADMIN_ROLE) hasNotStarted {
        startTimestamp = block.timestamp;
        emit Started();
    }

    function setRewardToken(address token) external onlyRole(DEFAULT_ADMIN_ROLE) {
        rewardToken = IERC20(token);
    }

    function addReward(
        uint256 _seedId, 
        uint256 _month, 
        uint256 _totalAmount
    ) external hasNotStarted onlyRole(DEFAULT_ADMIN_ROLE) {
        getTotalMintPerMonths[_seedId][_month] = _totalAmount;
    }
    
    function addWhitelist(address collecAdd) external onlyRole(DEFAULT_ADMIN_ROLE) {
        whitelistedNftContract.push(collecAdd);
    }

    function withdrawEmergency(address token) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 amount = IERC20(token).balanceOf(address(this));
        IERC20(token).transfer(msg.sender, amount);
    }

    
    function _getSeedIds(uint256 tokenId, address collecAdd) internal returns (uint256) {
        collection = Collection(collecAdd);
        return collection.seedIdByTokenAttribute(tokenId);
    }

    function _getWeight(uint256 seedId, address collecAdd) internal returns (uint256) {
        collection = Collection(collecAdd);
        return collection.weightByTokenAttribute(seedId);
    }

    function _stakeNft(
        uint256 tokenId, 
        address owner, 
        address collecAdd
    ) internal hasStarted{
        Stakers storage stakers = staker[tokenId];
        stakers.owner = owner;
        stakers.startTimestamp = block.timestamp;
        stakers.lastClaimed = block.timestamp;

        if(stakers.exist == false){
            stakers.seedId = _getSeedIds(tokenId, collecAdd);
            stakers.weight = _getWeight(stakers.seedId, collecAdd);
            stakers.currentStage = 0;
            stakers.exist = true;

        }
        stakerTokenId[owner].add(tokenId);

        emit NftStaked(owner, collecAdd, tokenId, stakers.seedId, stakers.weight);
    }

    function _batchStakeNfts(
        uint256[] memory tokenIds, 
        address owner, 
        address collecAdd
    ) internal hasStarted{
        uint256 numTokens  = tokenIds.length;
        uint256[] memory seedIds = new uint256[](numTokens);
        uint256[] memory weights = new uint256[](numTokens);
        require(numTokens != 0, "NftStaking: no tokens");
        Stakers storage stakers; 
        for (uint256 index = 0; index < numTokens; ++index) {
            stakers = staker[tokenIds[index]];
            stakers.owner = owner;
            stakers.startTimestamp = block.timestamp;
            stakers.lastClaimed = block.timestamp;

            seedIds[index] = _getSeedIds(tokenIds[index], collecAdd);
            weights[index] = _getWeight(seedIds[index], collecAdd);

            if(stakers.exist == false){
                stakers.seedId = _getSeedIds(tokenIds[index], collecAdd);
                stakers.currentStage = 0;
                stakers.exist = true;
                stakers.weight = weights[index];
            }
            stakerTokenId[owner].add(tokenIds[index]);
        }
        emit NftsBatchStaked(msg.sender, collecAdd, tokenIds, seedIds, weights);
    }

    function unStakeNft(uint256 tokenId, address collecAdd) external {
        Stakers storage stakers = staker[tokenId];

        require(stakers.owner == msg.sender, "NftStaking: not staked for owner");

        stakers.exitTimestamp = block.timestamp;
        stakers.accumulatedTime =  (block.timestamp - stakers.lastClaimed) + stakers.accumulatedTime;
        stakers.owner = address(0);

        IERC1155 _whitelistedNftContract = IERC1155(collecAdd);
        _whitelistedNftContract.safeTransferFrom(address(this),msg.sender,tokenId,1,"");

        uint256 seedId =  _getSeedIds(tokenId, collecAdd);
        uint256 weight = _getWeight(seedId, collecAdd);

        stakerTokenId[msg.sender].remove(tokenId);
       
        emit NftUnstaked(msg.sender, collecAdd, tokenId, seedId, weight);
        
    }

    function batchUnstakeNfts(uint256[] calldata tokenIds, address collecAdd) external {
        uint256 numTokens = tokenIds.length;
        require(numTokens != 0, "NftStaking: no tokens");

        uint256[] memory values = new uint256[](numTokens);
        uint256[] memory seedIds = new uint256[](numTokens);
        uint256[] memory weights = new uint256[](numTokens);

        for (uint256 index = 0; index < numTokens; ++index) {
            
             uint256 tokenId = tokenIds[index];

             Stakers storage stakers = staker[tokenId];

             require(stakers.owner == msg.sender, "NftStaking: not staked for owner");
             
             stakers.exitTimestamp = block.timestamp;
             stakers.accumulatedTime =  (block.timestamp - stakers.lastClaimed) + stakers.accumulatedTime;
             stakers.owner = address(0);

             values[index] = 1;
             seedIds[index] = stakers.seedId;
             weights[index] = _getWeight(seedIds[index], collecAdd);
             stakerTokenId[msg.sender].remove(tokenId);
        }

        IERC1155 _whitelistedNftContract = IERC1155(collecAdd);
        _whitelistedNftContract.safeBatchTransferFrom(address(this),msg.sender,tokenIds,values,"");
    
        emit NftsBatchUnstaked(msg.sender, collecAdd, tokenIds, seedIds, weights);
    }

    function claimRewards(address collecAdd) external hasStarted {
       Stakers storage stakers;
       uint256 totRew;
       uint256 length = stakerTokenId[msg.sender].getArray().length;
       uint256[] memory tokenIds = new uint256[](length);
       tokenIds = stakerTokenId[msg.sender].getArray();
        for(uint256 i =0; i < tokenIds.length; i++){
            stakers = staker[tokenIds[i]];
            require(stakers.owner == msg.sender, "NftStaking: not staked for owner");
            require(stakers.isFinished!=true,"You claimed all Rewards");

            uint256 totalStakedTime = (block.timestamp - stakers.lastClaimed) + stakers.accumulatedTime;
            uint256 totalDay = totalStakedTime / 86400;

            require(totalDay >= 30,"At least one month must pass before the reward can be claimed");

            totalDay > 90 ? 90 : totalDay;

            uint256 totalMonth = totalDay / 30;
            uint256 remainingDay = (totalDay % 10);

            for (uint256 i =0; i < totalMonth; i++){
                totRew += getTotalMintPerMonths[stakers.seedId][1 + stakers.currentStage];
                stakers.currentStage++;
            }

            if(stakers.currentStage == 4){
                stakers.isFinished = true;
            }

            stakers.lastClaimed = block.timestamp;
            stakers.accumulatedTime = remainingDay * 86400;
        }
            rewardToken.transfer(msg.sender, totRew);

            emit RewardsClaimed(msg.sender, totRew);
    }
   
    function getRewards(address owner) external view hasStarted returns(uint256) {
        Stakers memory stakers;
        uint256 totalAmount;
        uint256 length = stakerTokenId[msg.sender].getArray().length;
        uint256[] memory tokenIds = new uint256[](length);
        tokenIds = stakerTokenId[owner].getArray();
        for(uint256 i = 0; i < tokenIds.length; i++){
            stakers = staker[tokenIds[i]];

            require(stakers.isFinished!=true,"You claimed all Rewards");
            require(stakers.owner == owner,"You are not currently staking");

            uint256 totalStakedTime = (block.timestamp - stakers.lastClaimed)+ stakers.accumulatedTime;
            uint256 totalDay = totalStakedTime / 86400;
            uint256 totRew;

            if(totalDay >= 30)
            {
                totalDay > 90 ? 90 : totalDay;
                
                uint256 totalMonth = totalDay / 30;
                uint256 remainingDay = (totalDay % 10);

                for(uint256 i = 0; i < totalMonth;i++){
                    totRew += getTotalMintPerMonths[stakers.seedId][1 + stakers.currentStage];
                    stakers.currentStage++;
                }

                if(stakers.currentStage < 4){
                    totRew += (getTotalMintPerMonths[stakers.seedId][1 + stakers.currentStage] /
                            (86400 * 30)) * 
                            (remainingDay * 86400);
                }

                totalAmount += totRew;
            }
            
            else
            {
                totRew += getTotalMintPerMonths[stakers.seedId][1 + stakers.currentStage] /
                        (86400 * 30);

                totalAmount += totRew * totalStakedTime;
            }
        }

        return totalAmount;   
    }

    function onERC1155Received(
        address, /*operator*/
        address from,
        uint256 id,
        uint256, /*value*/
        bytes calldata /*data*/
    ) external virtual override returns (bytes4) {
        address collecAdd;
        for (uint256 i=0; i<whitelistedNftContract.length; i++){
            if(address(whitelistedNftContract[i]) == msg.sender){
               collecAdd = msg.sender;
               _stakeNft(id, from, collecAdd);
               return _ERC1155_RECEIVED;
            }
        }
        require(false, "NftStaking: contract not whitelisted");
        
    }

    function onERC1155BatchReceived(
        address, /*operator*/
        address from,
        uint256[] calldata ids,
        uint256[] calldata, /*values*/
        bytes calldata /*data*/
    ) external virtual override returns (bytes4) {
        address collecAdd;
        for (uint256 i=0; i<whitelistedNftContract.length; i++){
            if(address(whitelistedNftContract[i]) == msg.sender){
               collecAdd = msg.sender;
               _batchStakeNfts(ids, from, collecAdd);
               return _ERC1155_BATCH_RECEIVED;
            }
        }
        require(false, "NftStaking: contract not whitelisted");
    } 

   
}

