pragma solidity ^0.8.5;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "./Collection.sol";
import "./NftStaking.sol";

contract Factory is AccessControl {
    
    event CollectionCreated(
        address collection,
        string uri
    );

    bytes32 public constant COLLECTION_ROLE = bytes32(keccak256("COLLECTION_ROLE"));
  
    modifier onlyAdmin {
      require(hasRole(DEFAULT_ADMIN_ROLE,msg.sender),"This function can only be called by admin");
      _;
    }

    Collection[] public collections;
    

    constructor() public {
      _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
     }

    function createCollection(
        string calldata _uri,
        address _admin,
        address _stakingAdd
    ) external onlyAdmin() returns (address) {
            
        Collection collection =
            new Collection(_uri, _admin, address(this));

        collections.push(collection);

        NftStaking nftStake = NftStaking(_stakingAdd);
        nftStake.addWhitelist(address(collection));

        emit CollectionCreated(address(collection), _uri);

        return address(collection);
    }

     function collectionLength() external view returns (uint256) {
         return collections.length;
     }
     

     function mint(address _collecAdd, address account, uint256 _id, uint256 _seedId) onlyAdmin() external {
         Collection collection = Collection(_collecAdd);
         
         collection.mint(account, _seedId, _id);
         
     }

    function mintBatch(
        address _collecAdd,
        address account,
        uint256[] calldata _ids, 
        uint256[] calldata _seedIds, 
        uint256[] calldata amount
    ) onlyAdmin()
      external {

         Collection collection = Collection(_collecAdd);
         
         collection.mintBatch(account, _seedIds, _ids, amount);
         
     }

  
}

