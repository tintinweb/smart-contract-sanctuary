pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";


contract NFTFactory is ERC721, Ownable {

    struct Item {
        string name;
        uint256 tokenId;
        string baseURI;
        address owner;
    }
    uint public lastTokenId = 0;

    constructor() ERC721("Barbie Driver Collection", "BDC") {}

    function safeMint(address to, uint256 tokenId) public onlyOwner {
        _safeMint(to, tokenId);
        if (tokenId > lastTokenId) {
            lastTokenId = tokenId;
        }
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://ipfs.io/ipfs/QmWBbfLq4TbTSaugHvbWewvvzb5hnzjki73jk9kQKn73Ab/";
    }

    /// @notice Returns all NFTs and owners
    /// @param pagination Number of nfts per page
    /// @param page Page number
    function allNFTs(uint pagination, uint page) public view 
        returns (Item[] memory) {
        Item[] memory items = new Item[](pagination);
        uint start =  ((page - 1) * pagination) + 1;
        uint end = start + pagination - 1;
        for (uint256 i = start; i <= end; i++) {
            if (i <= lastTokenId) {
                Item memory it = Item({
                    name: name(),
                    tokenId: i,
                    baseURI: _baseURI(),
                    owner: ownerOf(i)
                });
                items[i - 1] = it;
            }
        }
        return items;
    }

}

