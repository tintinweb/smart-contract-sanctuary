pragma solidity ^0.8.0;


import "./NFTFactory.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts-ethereum-package/contracts/GSN/GSNRecipient.sol";


/// @title Marketplace escrow contract for fixed price sales
contract NFTMarketplace is IERC721Receiver, GSNRecipientUpgradeSafe {

    struct Sale {
        uint256 tokenId;
        uint256 price;
        uint256 startTime;  // in epoch seconds
        address owner;
    }

    NFTFactory public nft;
    IERC20 public currency;
    address public creatorAddress;
    address public marketAddress;
    Sale[] public sales;

    mapping(uint256 => uint256) public nftToSale;

    event NFTSale(uint256 indexed tokenId, uint256 price, uint256 saleId);

    event NFTSold(
        uint256 indexed tokenId, uint256 price, address indexed oldOwner, address indexed newOwner
    );

    function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4)
    {
        return this.onERC721Received.selector;
    }

    /// @notice Constructor
    /// @param _nftAddress NFT token address
    /// @param _currencyAddress currency token address
    /// @param _creatorAddress Creator's wallet address
    /// @param _marketAddress Marketplace's wallet address
    constructor(
        address _nftAddress,
        address _currencyAddress,
        address _creatorAddress,
        address _marketAddress
    ) {
        currency = IERC20(_currencyAddress);
        nft = NFTFactory(_nftAddress);
        creatorAddress = _creatorAddress;
        marketAddress = _marketAddress;
        // Populate first sales[] so we know that index 0 is not valid
        // So list begins with index 1
        Sale memory s = Sale({
            tokenId: 0,
            price: 0,
            startTime: 0,
            owner: address(0)
        });
        sales.push(s);
    }

    /// @notice Buy token
    /// @param _saleId Index of sales[]
    function buy(uint256 _saleId) public payable {
        Sale memory s = sales[_saleId];
        // Owners can't buy their own NFTs
        require(s.owner != msg.sender, "NFT already owned by sender");
        // Payment must be same as price
        require(msg.value == s.price, "Amount does not match with price");
        // Can only buy after startTime
        require(block.timestamp > s.startTime, "Sale has not started yet");

        // Transfer funds + distribution
        uint256 sellerShare = s.price * 90 / 100;
        uint256 commShare = s.price * 5 / 100;
        currency.transferFrom(msg.sender, s.owner, sellerShare);
        currency.transferFrom(msg.sender, creatorAddress, commShare);
        currency.transferFrom(msg.sender, marketAddress, commShare);
        // currency.transferFrom(msg.sender, creatorAddress, s.price);

        emit NFTSold(s.tokenId, s.price, s.owner, msg.sender);

        // Transfer the NFT
        nft.approve(msg.sender, s.tokenId);
        nft.safeTransferFrom(address(this), msg.sender, s.tokenId);

        // Delete sale
        delete nftToSale[s.tokenId];
        Sale memory replacer = sales[sales.length - 1];
        sales[_saleId] = replacer;
        sales.pop();
    }

    /// @notice Set NFT for sale
    /// @param _tokenId token Id
    /// @param _price Sale price in currency (x10^18)
    /// @param _startTime Starting time in epoch seconds
    function sell(uint _tokenId, uint256 _price, uint _startTime) public {
        // You can only sell your own NFTs
        require(nft.ownerOf(_tokenId) == msg.sender);
        // remember to call setApprovalForAll() in web3
        nft.safeTransferFrom(msg.sender, address(this), _tokenId);

        Sale memory s = Sale({
            tokenId: _tokenId,
            price: _price,
            startTime: _startTime,
            owner: msg.sender
        });

        sales.push(s);
        uint256 saleId = sales.length - 1;
        nftToSale[_tokenId] = saleId;

        emit NFTSale(_tokenId, _price, saleId);
    }

    /// @notice Set batch of NFTs for sale in single call. Must be consecutive
    /// @param _startTokenId Starting token Id
    /// @param _endTokenId Ending token Id
    /// @param _price Sale price in currency
    /// @param _startTime Sale start time in epoch seconds
    function sellBatch(
        uint _startTokenId, uint _endTokenId, uint256 _price, uint _startTime
    ) public {
        // You can only batch-sell your own NFTs
        require(nft.ownerOf(_startTokenId) == msg.sender);

        // Batch-sell
        for (uint256 i = _startTokenId; i <= _endTokenId; i++) {
            sell(i, _price, _startTime);
        }
    }

    /// @notice Remove listing
    /// @param _tokenId token ID
    function cancelSale(uint _tokenId) public {
        require(sales[nftToSale[_tokenId]].owner == msg.sender);

        delete sales[nftToSale[_tokenId]];
        delete nftToSale[_tokenId];

        nft.safeTransferFrom(address(this), msg.sender, _tokenId);
    }

    /// @notice NFTs for sale quantity
    function nSale() public view returns(uint) {
        return sales.length;
    }

    /// @notice All sales
    function getSales() external view returns(Sale[] memory) {
        return sales;
    }

    /// ---------- GSN Related
    function acceptRelayedCall(
        address relay,
        address from,
        bytes calldata encodedFunction,
        uint256 transactionFee,
        uint256 gasPrice,
        uint256 gasLimit,
        uint256 nonce,
        bytes calldata approvalData,
        uint256 maxPossibleCharge
    ) external override view returns (uint256, bytes memory) {
        return _approveRelayedCall();
    }

    // We won't do any pre or post processing, so leave _preRelayedCall and _postRelayedCall empty
    function _preRelayedCall(bytes memory context) internal override returns (bytes32) {
    }

    function _postRelayedCall(bytes memory context, bool, uint256 actualCharge, bytes32) internal override {
    }


}

