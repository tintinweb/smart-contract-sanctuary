pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract RoyaltiesSplitter {
    using SafeMath for uint256;

    uint256 public DENOMINATOR = 10000;
    uint256 public totalRoyalties;

    // Artist's Addresses
    address payable thomas =
        payable(0x4fA7Fc217d2Da64b617eED770286fCC48BaF4022);
    address payable ryan = payable(0x2909Cbe186e86798d45C0b3C2ABf1989e287c7D3);
    address payable terry = payable(0x8E12127a6cC135d5479C6DD1B0776ada92D94FAF);
    address payable TREASURY =
        payable(0x0FF727a618963Af5AbE228AE9C4220F6b47Cfc97);

    receive() external payable {
        uint256 totalReceived = msg.value;
        thomas.call{value: totalReceived.mul(600).div(DENOMINATOR)}("");
        thomas.call{value: totalReceived.mul(800).div(DENOMINATOR)}("");
        thomas.call{value: totalReceived.mul(600).div(DENOMINATOR)}("");
        thomas.call{value: totalReceived.mul(8000).div(DENOMINATOR)}("");
    }
}

