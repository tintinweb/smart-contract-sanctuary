pragma solidity ^0.6.0;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@chainlink/contracts/src/v0.6/interfaces/AggregatorV3Interface.sol";
import "@chainlink/contracts/src/v0.6/ChainlinkClient.sol";

contract Spread is ERC721, ChainlinkClient {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    AggregatorV3Interface internal priceFeed;

    uint256 oraclePayment;

    int public spotOption = 0;
    int public spotPrice = 0;
    int public spotK1 = 0;
    int public spotK2 = 0;

    struct Option {
        address buyer;
        address seller;
        int k1;
        int k2;
        uint expiry;
        uint tokenId;
    }

    uint numOptions;
    mapping (uint => Option) options;

    constructor() ERC721("Call Spread ING", "CSING")  public {
        priceFeed = AggregatorV3Interface(0xdCA36F27cbC4E38aE16C4E9f99D39b42337F6dcf); // USDC/ETH in rinkeby

        setPublicChainlinkToken();
        oraclePayment = 0.1 * 10 ** 18; // 0.1 LINK
    }
    

    function newOption(address buyer, int k1, int k2, uint expiry) public returns (uint optionId) {
        optionId = numOptions++;

        _tokenIds.increment();
        uint256 newTokenId = _tokenIds.current();
        _mint(buyer, newTokenId);

        Option storage op = options[optionId];
        op.buyer = buyer;
        op.seller = _msgSender();
        op.k1 = k1;
        op.k2 = k2;
        op.expiry = expiry;
        op.tokenId = newTokenId;

        // schedule
        bytes32 _jobId = "60f206a5128040fab7a4cd543456c8c0";
        address _oracle = 0x3A56aE4a2831C3d3514b5D7Af5578E45eBDb7a40;
        Chainlink.Request memory req = buildChainlinkRequest(_jobId, address(this), this.expireOption.selector);
        req.addUint("until", block.timestamp + 1 minutes);
        sendChainlinkRequestTo(_oracle, req, oraclePayment);
    }

    function expireOption(bytes32 _requestId)
        public
        recordChainlinkFulfillment(_requestId)
    {
        (
            uint80 roundID, 
            int price,
            uint startedAt,
            uint timeStamp,
            uint80 answeredInRound
        ) = priceFeed.latestRoundData();

        IERC20 usdt = IERC20(address(0xD92E713d051C37EbB2561803a3b5FBAbc4962431)); // TUSDT on rinkeby

        uint optionId = numOptions - 1;
        Option memory op = options[optionId];

        spotPrice = price;
        spotK1 = op.k1;
        spotK2 = op.k2;

        if (price > op.k1 && price < op.k2) {
            spotOption = 1;
            int _amount1 = price - op.k1;
            // usdt.transfer(op.buyer, uint(_amount1));

            int _amount2 = op.k2 - price;
            // usdt.transfer(op.seller, uint(_amount2));
        } else if (price >= op.k2) {
            spotOption = 2;
            int _amount = op.k2 - op.k1;
            // usdt.transfer(op.buyer, uint(_amount));
        } else {
            spotOption = 3;
        }
    }

    /**
     * Returns the latest price
     */
    function getThePrice() public view returns (int) {
        (
            uint80 roundID, 
            int price,
            uint startedAt,
            uint timeStamp,
            uint80 answeredInRound
        ) = priceFeed.latestRoundData();
        return price;
    }
}

