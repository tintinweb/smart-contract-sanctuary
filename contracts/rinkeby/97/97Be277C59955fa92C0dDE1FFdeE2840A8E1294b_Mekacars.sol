contract Mekacars is Ownable, ERC721{
    
    using SafeMath for uint256;
    using Strings for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;
   
    bool public presaleActive;
    bool public mainSaleActive;
    bool public unreveal;
    
    uint256 public reserved;
    uint256 public mainSalePrice;
    uint256 public totalSupply;
    uint256 public supplyLimit;
    uint256 public preSaleMintLimit;
    uint256 public mainSaleMintLimit;
    uint256 mainSaleBlockTime;
    
    uint256 public preSaleLimit;
    uint256 public preSaleCount;
    uint256 public preSalePrice;
    
    
    string public baseURI = "";
    string public unRevealURI = "";
    
    mapping(address => uint256) public preSalebalance;
    mapping(address => uint256) public mainSaleBalance;
    mapping(address => uint256) public sharesOf;
    
    EnumerableSet.AddressSet shareHolders;
    EnumerableSet.AddressSet whiteList;
    constructor() ERC721("MekaCars", "MEKACARS"){
        supplyLimit = 10000;
        preSaleMintLimit = 2;
        mainSaleMintLimit = 10;
        preSaleLimit = 4000;
        baseURI = "_baseURI";
        unRevealURI = "_unRevealURI";
        presaleActive = true;
        mainSalePrice = 0.1 ether;
        
        {
            shareHolders.add(0x1a9d186dc79df52700F1f0E3A523d3D7064A45F4);
            shareHolders.add(0x208205F3F7e758ef67E65d7f6fbE2ed3839Ed8C0);
            shareHolders.add(0xB77d30571fc6C253e645584D4deEF40c139A7900);
            shareHolders.add(0x36c0903Ad6D564F335333344Fab68ebCf736F629);
            shareHolders.add(0x60a4B8c2FC9c3a6Ef951c9402ccE4C3E30996711);
            
            sharesOf[0x1a9d186dc79df52700F1f0E3A523d3D7064A45F4] = 20;
            sharesOf[0xB77d30571fc6C253e645584D4deEF40c139A7900] = 20;
            sharesOf[0x208205F3F7e758ef67E65d7f6fbE2ed3839Ed8C0] = 20;
            sharesOf[0x36c0903Ad6D564F335333344Fab68ebCf736F629] = 20;
            sharesOf[0x60a4B8c2FC9c3a6Ef951c9402ccE4C3E30996711] = 20;
        }
    }
    
    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");
        if(unreveal){
            return bytes(baseURI).length > 0 ? 
        string(abi.encodePacked(baseURI, "/", tokenId.toString(), ".json")) : "";
        }else{
            return bytes(unRevealURI).length > 0 ? unRevealURI : "";
        }
    }
    
    function setBaseURI(string memory _baseURI) external onlyOwner {
        baseURI = _baseURI;
    }
    
    function setUnRevealURI(string memory _unRevealURI) external onlyOwner {
        unRevealURI = _unRevealURI;
    }
    
    function revealURI(bool _state) external onlyOwner{
        unreveal = _state;
    }
    
    function changePreSaleMintLimit(uint256 _preSaleMintLimit) external onlyOwner {
        preSaleMintLimit = _preSaleMintLimit;
    }
    
    function changeMainSaleMintLimit(uint256 _mainSaleMintLimit) external onlyOwner {
        mainSaleMintLimit = _mainSaleMintLimit;
    }
    
    function changeMainSalePrice(uint256 _price) external onlyOwner {
        mainSalePrice = _price;
    }
    
    
    function reserve(uint256 _numberOfTokens) public onlyOwner {
        require(reserved.add(_numberOfTokens) <= 500, "MekaCars::reserved: Exceeding reserve limit.");
        reserved = reserved.add(_numberOfTokens);
        _mint(_numberOfTokens);
    }
    
    function activateMainSale() external onlyOwner {
        presaleActive = false;
        mainSaleActive = true;
        mainSaleBlockTime = block.timestamp;
    }
    
    function addWhitelistUser(address[] memory _users) external onlyOwner {
        uint256 l = _users.length;
        for(uint256 i=0; i< l; i=i+1){
            whiteList.add(_users[i]);
        }
    }
    
    function removeWhitelistUser(address _user) external onlyOwner {
        whiteList.remove(_user);
    }
    
    function getWhiteList() external view returns(address[] memory){
        return whiteList.values();
    }
    
    function preSaleBuy(uint256 _amount) external  payable{
        require(whiteList.contains(_msgSender()), "MekaCars::preSaleBuy: Only for Whitelist users.");
        require(presaleActive, "MekaCars::preSaleBuy: Presale is not active.");
        require(preSaleCount.add(_amount) <= preSaleLimit, "MekaCars::preSaleBuy: exceeding pre-sale limit.");
        require(preSalebalance[_msgSender()].add(_amount) <= preSaleMintLimit, "MekaCars::preSaleBuy: exceeding individual mint limit.");
       
       require(msg.value >= getPrice().mul(_amount), "MekaCars::preSaleBuy: insufficient ethers");
        
        preSalebalance[_msgSender()] = preSalebalance[_msgSender()].add(_amount);
        preSaleCount = preSaleCount.add(_amount);
        _mint(_amount);
    }
    
    function mainSaleBuy(uint256 _amount) external  payable{
        require(mainSaleActive, "MekaCars::mainSaleBuy: Mainsale is not active.");
        require(totalSupply.add(_amount) <= supplyLimit, "MekaCars::mainSaleBuy: exceeding supplyLimit.");
        require(mainSaleBalance[_msgSender()].add(_amount) <= mainSaleMintLimit, "MekaCars::mainSaleBuy: exceeding individual mint limit.");
        
        require(msg.value >= getPrice().mul(_amount), "MekaCars::preSaleBuy: insufficient ethers");
        
        mainSaleBalance[_msgSender()] = mainSaleBalance[_msgSender()].add(_amount);
        _mint(_amount);
    }
    
    function _mint(uint _numberOfTokens) internal{
        uint256 newId = totalSupply;
        for(uint i = 0; i < _numberOfTokens; i++) {
            newId += 1;
            totalSupply = totalSupply.add(1);
            _safeMint(msg.sender, newId);
        }
    }
    
    function withdraw() external onlyOwner{
        require(address(this).balance > 0, "MekaCars::withdraw: insufficient funds in contract.");
        // require(getTotalShares() <= 100, "MekaCars::withdraw: totalShares exceeding 100. Please change the shares respectivly.");
        uint256 balance = address(this).balance;
        (bool success0,) = shareHolders.at(0).call{value: balance.mul(20).div(100)}("");
        (bool success1,) = shareHolders.at(1).call{value: balance.mul(20).div(100)}("");
        (bool success2,) = shareHolders.at(2).call{value: balance.mul(20).div(100)}("");
        (bool success3,) = shareHolders.at(3).call{value: balance.mul(20).div(100)}("");
        (bool success4,) = shareHolders.at(4).call{value: balance.mul(20).div(100)}("");
        require(success0 && success1 && success2 && success3 && success4, "MekaCars::withdraw: withdraw failed.");
    }
    
    function emergencyWithdraw() external onlyOwner {
        require(address(this).balance > 0, "MekaCars::emergencyWithdraw: insufficient funds in contract.");
        (bool success,) = owner().call{value: address(this).balance}("");
        require(success, "MekaCars::withdraw: withdraw failed.");
    }
    
    function getPrice() public view returns(uint256 _price){
        if(presaleActive){
            _price = 0.1 ether;
        }else if(mainSaleActive){
            if(block.timestamp <= mainSaleBlockTime + 1 hours){
                _price = 0.5 ether;
            }else if(block.timestamp <= mainSaleBlockTime + 2 hours){
                _price = 0.4 ether;
            }else if(block.timestamp <= mainSaleBlockTime + 3 hours){
                _price = 0.3 ether;
            }else if(block.timestamp <= mainSaleBlockTime + 4 hours){
                _price = 0.2 ether;
            }else{
                _price = mainSalePrice;
            }
        }
    }
}