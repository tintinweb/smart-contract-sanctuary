contract testEOIV012 is ERC1155Upgradeable, OwnableUpgradeable{
    /**
    * declarating from primitive to complex.
    **/
    using ECDSA for bytes32;

    // bool 16Bit. Block = 48Bit
    bool public isRevealed;
    bool public isWhitelist;
    bool public isInitialized;

    uint128 public limitNftWhitelist;
    uint128 public nftsTotal;
    uint128 public nftsMinted;

    uint256 public cost;

    string private baseURI;
    string private notRevealedUri;
    string private initConstStr;
    string private baseUri;

    // address 20byte
    address private _signerAddress;

    address[] public whitelistedAddresses;

    function initialize(string memory _initConstStr, string memory _baseUri, string memory _notRevealedUri) initializer public {
        initConstStr = _initConstStr;
        baseUri = _baseUri;
        notRevealedUri = _notRevealedUri;
        whitelistedAddresses;
        isRevealed = false;
        isWhitelist = true;
        isInitialized = true;
        nftsTotal = 20;
        nftsMinted = 1;
        cost = 0.09 ether;
        limitNftWhitelist = 2;

        __Ownable_init();
        __ERC1155_init(initConstStr);
        setBaseURI(baseUri);
        setNotRevealedURI(notRevealedUri);
        for(uint256 i = 1; i <= nftsMinted; i++){
            _mint(msg.sender, i, 1, "");
        }
    }

    function matchAddressSigner(bytes32 hash, bytes memory signature) private view returns(bool) {
        return _signerAddress == hash.recover(signature);
    }

    function updateSigner(address _newSigner) internal{
        _signerAddress = _newSigner;
    }


    function minting(bytes32 hash, bytes memory signature, address signerKey, uint256 amountNFT) public payable{
        require(!isWhitelist, "not allowed");
        require(nftsMinted < nftsTotal, "all nfts are minted");
        updateSigner(signerKey);
        require(matchAddressSigner(hash, signature), "DIRECT_MINT_DISALLOWED");
        for(uint256 i = 0; i < amountNFT && nftsMinted < nftsTotal; i++){
            _mint(msg.sender, nftsMinted + 1, 1, "");
            nftsMinted++;
        }
        //require(msg.value >= cost * amountNFT);
    }

    function presaleMinting(bytes32 hash, bytes memory signature, address signerKey, uint256 amountNFT) public payable{
        require(isWhitelist, "not allowed");
        updateSigner(signerKey);
        require(matchAddressSigner(hash, signature), "DIRECT_MINT_DISALLOWED");
        require(checkIfWhitelisted(msg.sender), "current user is not whitelisted");
        require(checkWhitelistMintLimit(msg.sender), "current user has already two minted with whitelist");
        for(uint256 i = 0; i < amountNFT && checkWhitelistMintLimit(msg.sender); i++){
            _mint(msg.sender, nftsMinted + 1, 1, "");
            nftsMinted++;
        }
        //require(msg.value >= cost * amountNFT);
    }

    function countUserNfts(address _acc) public view returns (uint256){
        uint256 counter = 0;
        for(uint256 i = 1; i <= nftsTotal; i++){
            if(balanceOf(_acc, i) == 1){
                counter++;
            }
        }
        return counter;
    }

    /**
    * check if already minted 2 with whitelist
    **/
    function checkWhitelistMintLimit(address _acc) public view returns (bool){
        if(countUserNfts(_acc) < limitNftWhitelist){
            return true;
        }
        return false;
    }

    // update whitelisted user
    function whitelistUsers(address[] calldata _users) public onlyOwner{
        delete whitelistedAddresses;
        whitelistedAddresses = _users;
    }

    function checkIfWhitelisted(address _user) public view returns (bool){
        for(uint256 i = 0; i < whitelistedAddresses.length; i++){
            if(whitelistedAddresses[i] == _user){
                return true;
            }
        }
        return false;
    }

    function getWhitelist() public view returns (address[] memory ){
        return whitelistedAddresses;
    }

    /**
    * update limit of nfts per address at presale
    **/
    function updateNftWhiteListLimit(uint128 _limit) public onlyOwner(){
        limitNftWhitelist = _limit;
    }

    function updateIsWhitelist(bool _state) public onlyOwner(){
        isWhitelist = _state;
    }

    function uri(uint256 tokenId) override public view returns (string memory){
        // continue, if tokenId is existent
        require(
            tokenId >= 1 && tokenId <= nftsTotal,
            "ERC1155Metadata: URI query for nonexistent token"
        );

        if(!isRevealed){
            return notRevealedUri;
        }

        string memory currentBaseURI = _baseURI();
        return bytes(currentBaseURI).length > 0
        ? string(abi.encodePacked(currentBaseURI, StringsUpgradeable.toString(tokenId), ".json"))
        : "";
    }

    function setBaseURI(string memory _newBaseURI) public onlyOwner{ baseURI = _newBaseURI; }

    function setNotRevealedURI(string memory _notRevealedURI) public onlyOwner { notRevealedUri = _notRevealedURI; }

    function reveal() public onlyOwner{ isRevealed = true; }

    function _baseURI() internal view returns (string memory) { return baseURI; }
}