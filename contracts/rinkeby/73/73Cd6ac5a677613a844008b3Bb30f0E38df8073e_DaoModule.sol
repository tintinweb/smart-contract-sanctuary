import {
    ERC20PresetMinterPauser
} from "@openzeppelin/contracts/token/ERC20/presets/ERC20PresetMinterPauser.sol";
import {Create2} from "@openzeppelin/contracts/utils/Create2.sol";
import "hardhat/console.sol";

import {Album} from "./Album.sol";
import {IENS} from "./interfaces/IENS.sol";
import {IENSResolver} from "./interfaces/IENSResolver.sol";
import {IERC721} from "./interfaces/IERC721.sol";
import {IFactorySafeHelper} from "./interfaces/IFactorySafeHelper.sol";

contract AlbumFactory {
    event AlbumCreated(
        string name,
        address album,
        address safe,
        address daoModule,
        address token
    );

    bytes32 public constant DEFAULT_ADMIN_ROLE = 0x00;
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");

    IFactorySafeHelper public immutable FACTORY_SAFE_HELPER;
    IENS public immutable ENS;
    address public immutable ENS_PUBLIC_RESOLVER_ADDRESS;
    bytes32 public immutable BASE_ENS_NODE;

    constructor(
        address factorySafeHelperAddress,
        address ensAddress,
        address ensPublicResolverAddress,
        bytes32 baseEnsNode
    ) {
        console.log("Deploying a Factory");
        FACTORY_SAFE_HELPER = IFactorySafeHelper(factorySafeHelperAddress);
        ENS = IENS(ensAddress);
        BASE_ENS_NODE = baseEnsNode;
        ENS_PUBLIC_RESOLVER_ADDRESS = ensPublicResolverAddress;
    }

    function createAlbum(
        string memory name,
        string calldata symbol,
        // AlbumFactory contract has to be approved to transfer all of these NFTs!
        address[] calldata nftAddresses,
        uint256[] calldata nftIds,
        bytes[] calldata ensResolverData
    ) external {
        console.log("Creating a album with name: ", name);
        require(
            nftAddresses.length == nftIds.length,
            "NFT address and ID arrays must have same lengths"
        );
        bytes32 nameHash = keccak256(abi.encodePacked(name));
        console.log("Got nameHash");
        console.logBytes32(nameHash);
        bytes32 ensSubNode =
            keccak256(abi.encodePacked(BASE_ENS_NODE, nameHash));
        console.log("Got ens subnode");
        console.logBytes32(ensSubNode);
        require(
            ENS.owner(ensSubNode) == address(0),
            "ENS subname is already owned"
        );
        (address safe, address daoModule) =
            FACTORY_SAFE_HELPER.createAndSetupSafe(ensSubNode);
        console.log("Got safe", safe);
        console.log("Got dao module", daoModule);
        address token = createAndSetupToken(name, symbol, safe);
        console.log("Got token", token);
        address album = createAndSetupAlbum(safe, token, nftAddresses, nftIds);
        console.log("Got album", album);

        ENS.setSubnodeOwner(BASE_ENS_NODE, nameHash, address(this));
        ENS.setResolver(ensSubNode, ENS_PUBLIC_RESOLVER_ADDRESS);
        IENSResolver(ENS_PUBLIC_RESOLVER_ADDRESS).multicall(ensResolverData);
        ENS.setOwner(ensSubNode, safe);

        emit AlbumCreated(name, album, safe, daoModule, token);
    }

    function createAndSetupToken(
        string memory name,
        string calldata symbol,
        address safeAddress
    ) internal returns (address) {
        ERC20PresetMinterPauser token =
            new ERC20PresetMinterPauser{salt: ""}(
                // TODO establish what we want recorded token name and symbol to be
                string(abi.encodePacked("sz", name)),
                string(abi.encodePacked("sz", symbol))
            );
        token.grantRole(DEFAULT_ADMIN_ROLE, safeAddress);
        token.grantRole(MINTER_ROLE, safeAddress);
        token.grantRole(PAUSER_ROLE, safeAddress);
        // TODO initialize token, with sale and initial distribution
        // TODO [optional] remove address(this) from all three of these roles

        return address(token);
    }

    function createAndSetupAlbum(
        address safeAddress,
        address tokenAddress,
        address[] calldata nftAddresses,
        uint256[] calldata nftIds
    ) internal returns (address) {
        bytes memory creationCode =
            abi.encodePacked(
                type(Album).creationCode,
                abi.encode(safeAddress, tokenAddress, nftAddresses, nftIds)
            );
        address albumAddr = Create2.computeAddress("", keccak256(creationCode));
        for (uint256 i = 0; i < nftAddresses.length; i++) {
            IERC721(nftAddresses[i]).safeTransferFrom(
                msg.sender,
                albumAddr,
                nftIds[i]
            );
        }
        return Create2.deploy(0, "", creationCode);
    }
}

