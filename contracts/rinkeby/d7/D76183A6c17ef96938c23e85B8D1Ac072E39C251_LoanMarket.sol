// Different EVM based blockchains will need different Dex Strategy
enum PriceStrategy {
    PANCAKE_SWAP,
    UNISWAP
}
contract LoanMarket {
    //Load library structs into contract
    using LoanData for *;
    //saves the transaction hash of the create loan offer transaction as loanId
    //saves information in openLoanOffers when createLoanOffer is called
    mapping(bytes32 => LoanData.LoanDetails) openLoanOffers;
    bytes32[] openLoanOfferKeys;

    GovWorldAdminRegistry govWorldAdminRegistry;
    GovWorldProtocolRegistry govWorldProtocolRegistry;
    //GovWorldTierLevel govWorldTierLevel;
    IDexFactory dexFactory;

    event LoanOfferCreated(
        address  _borrower, 
        address[] _collateralTokens, 
        uint256[] _collateralAmounts, 
        bool _isPrivate,
        bool _isPartialFunding,
        address _borrowStableCoin
    );

    constructor(
        address _govWorldAdminRegistry, 
        address _govWorldProtocolRegistry,
        address _dexFactory
        //address _govWorldTierLevel
    )
    {
        govWorldAdminRegistry = GovWorldAdminRegistry(_govWorldAdminRegistry);
        govWorldProtocolRegistry = GovWorldProtocolRegistry(_govWorldProtocolRegistry);
        //govWorldTierLevel = GovWorldTierLevel(_govWorldTierLevel);
        dexFactory = IDexFactory(_dexFactory);
    }


    function createLoanOfferErc20(
        uint256 _loanAmount,
        uint32 _termsLengthInDays,
        uint32 _apyOffer,
        uint32 _ltv,
        LoanData.LoanType _loanType,
        bool _isPrivate,
        bool _isPartialFunding,
        bool _isInsured,
        address[] memory _stakedCollateralTokens,
        uint256[] memory _stakedCollateralAmounts,
        address _borrowStableCoin
    )
    external
    
    {
        require(_stakedCollateralTokens.length == _stakedCollateralAmounts.length,"GLM: tokens and amounts length must be same");
        //create uniquie loan hash
        bytes32 loanHash = keccak256(abi.encodePacked("SALT","0x01"));


        //loop through all staked collateral tokens.
        for(uint256 i  = 0 ; i  <  _stakedCollateralTokens.length ; i++){
            require(govWorldProtocolRegistry.isTokenApproved(_stakedCollateralTokens[i]),"GLM: One or more tokens not approved.");
            uint256 allowance =  IERC20(_stakedCollateralTokens[i]).allowance(msg.sender, address(this));
            require( allowance >= _stakedCollateralAmounts[i],"GLM: Transfer amount exceeds allowance.");
            //contract will now hold the staked collateral tokens
            IERC20(_stakedCollateralTokens[i]).transferFrom(msg.sender, address(this), _stakedCollateralAmounts[i]);
        }

        LoanData.LoanDetails memory loanDetails = LoanData.LoanDetails(
            _stakedCollateralTokens,
            _stakedCollateralAmounts,
            new bytes32[](0),
            _loanAmount,
            _apyOffer,
            _loanType,
            LoanData.LoanStatus.INACTIVE,
            _termsLengthInDays,
            _ltv,
            _isPrivate,
            _isPartialFunding,
            _isInsured,
            msg.sender,
            _borrowStableCoin
        );
        
        openLoanOffers[loanHash]  = loanDetails;
        //openLoanOfferKeys.push(openLoanOfferKeys);

        emit LoanOfferCreated(
            msg.sender, 
            _stakedCollateralTokens, 
            _stakedCollateralAmounts, 
            _isPrivate,
            _isPartialFunding,
            _borrowStableCoin
        );
    }

    /**
    * @dev makes _admin a pendsing adnmin for approval to be given by
    * all current admins for removing this admnin.
    * @param _tokenA address of alt coin
    * @param _tokenB address of stable coin
    */
    function getTokenPrice(
        address _tokenA,
        address _tokenB
    ) 
        public 
        view
        returns(uint)
    {
        
        IDexPair pair = IDexPair(IDexFactory(dexFactory).getPair(_tokenA, _tokenB));
        IERC20Decimals token1 = IERC20Decimals(pair.token1());
        (uint resToken0, uint resToken1,) = pair.getReserves();
        uint res0 = resToken0*(10**token1.decimals());
        return((10**token1.decimals()*res0)/resToken1); // return amount of token0 needed to buy token1
    }
}

