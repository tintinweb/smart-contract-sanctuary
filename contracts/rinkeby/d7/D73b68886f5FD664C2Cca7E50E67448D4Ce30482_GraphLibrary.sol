    function gerCoordinates(
        uint256 _gridIndex,
        uint256 _xOffset,
        uint256 _yOffset
    ) internal pure returns (uint256, uint256) {
        uint256 column = (_gridIndex % 5); // for x position
        uint256 row = _gridIndex / 5; // for y position
        uint256 cx = column * 100 + _xOffset;
        uint256 cy = row * 100 + _yOffset;
        return (cx, cy);
    }

    function nodeInCell(
        uint256 _gridIndex,
        uint256 _xOffset,
        uint256 _yOffset
    ) internal view returns (string memory) {
        uint256 column = (_gridIndex % 5); // for x position
        uint256 row = _gridIndex / 5; // for y position
        uint256 cx = column * 100 + _xOffset;
        uint256 cy = row * 100 + _yOffset;

        return drawCircle(cx, cy, 25, "#B68558");
    }

