    function decimalTotString(int256 decimal)
        public
        view
        returns (string memory)
    {
        uint256 n = uint256(decimal);

        if (n == 0) {
            return ("0");
        }

        uint256 digits = n / 10**4;
        uint256 sigfigs = n - (digits * 10**4);

        //sigfigs=sigfigs*10**6;
        string memory sigfigsStr = sigfigs == 0
            ? ""
            : string(abi.encodePacked(unicode".", getSigfigs(sigfigs)));

        if (digits < 1) {
            // console.log("<1", string(abi.encodePacked("0", sigfigsStr)));
            return (string(abi.encodePacked("0", sigfigsStr)));
        } else {
            return (
                string(abi.encodePacked(Strings.toString(digits), sigfigsStr))
            );
        }
    }

    function getSigfigs(uint256 sigfigs) internal view returns (string memory) {
        uint256 temp = uint256(sigfigs);
        // console.log("tmp",temp);
        uint256 digits;
        uint8 numSigfigs;
        while (temp != 0) {
            if (numSigfigs > 0) {
                numSigfigs++;
            } else if (temp % 10 != 0) {
                numSigfigs++;
            }
            digits++;
            temp /= 10;
        }
        uint256 nZeroes = 4 - digits;
        uint256 bufferSize = nZeroes + numSigfigs;
        bytes memory buffer = new bytes(bufferSize);

        for (uint256 zerosCursor = 0; zerosCursor < nZeroes; zerosCursor++) {
            buffer[zerosCursor] = bytes1(uint8(48));
        }
        sigfigs = sigfigs / 10**(digits - numSigfigs);
        uint256 indx = bufferSize;
        while (sigfigs > 0) {
            indx--;

            buffer[indx] = bytes1(uint8(uint256(48).add(sigfigs % 10)));

            sigfigs /= 10;
        }

        return (string(buffer));
    }

