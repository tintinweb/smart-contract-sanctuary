    function dydx(
        uint256 cx1,
        uint256 cy1,
        uint256 cx2,
        uint256 cy2
    ) public view returns (int256, int256) {
        int256 _dx;
        int256 _dy;
        int256 m;
        int256 d = 25 * 10**4;

        if (cx1 == cx2) {
            _dy = 0;
            _dx = d;
        } else {
            if (cy1 == cy2) {
                _dy = d;
                _dx = 0;
            } else {
                m =
                    ((int256(cy2) - int256(cy1)) * 10**4) /
                    (int256(cx2) - int256(cx1));

                uint256 r = sqrt(10**8 + uint256(m * m));

                _dy = (d * 10**4) / int256(r);
                _dx = (d * m) / int256(r);
            }
        }

        return (_dx, _dy);
    }

