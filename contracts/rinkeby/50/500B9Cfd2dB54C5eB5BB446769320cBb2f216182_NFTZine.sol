  // Flip to the next issue
  function flipToNextIssue() external {
    require(block.timestamp > issueIDToEndTimeStamp[_currentIssueID], "Sorry, can't flip to the next issue yet!.");

    _currentIssueID++;
    _latestTokenIDFromLastIssue = _tokenIDs.current();
  }

  // A function our user will hit to get their NFT.
  function mintExhibitNFT(uint _gridLocationID) external payable {
     // Get the current tokenId, this starts at 0.
    uint256 newItemId = _tokenIDs.current();

    require(block.timestamp < issueIDToEndTimeStamp[_currentIssueID], "Sorry, this issue of NFT Zine is now locked!"); // test time 
    // require(block.timestamp < issueIDToEndTimeStamp[_currentIssueID], "Sorry, NFT Zine is now locked!"); // real time 

    require(newItemId < issueIDToGridLocationCount[_currentIssueID] + _latestTokenIDFromLastIssue, "Sorry, we are out of NFTs for this issue!");


    require(msg.value >= 100000000000000, "Not enough ETH sent; check price!"); // test price - 0.0001 ETH
    // require(msg.value >= 100000000000000000, "Not enough ETH sent; check price!"); // real price - 0.1 ETH

    


    _safeMint(msg.sender, newItemId);

    // We map the tokenId => their character attributes. More on this in
    // the lesson below.
    tokenIdToAttributes[newItemId] = TokenAttributes({
      imageURI: defaultTokens[newItemId].imageURI,
      gridLocationID: _gridLocationID,
      displayNFTContract: address(0),
      displayNFTID: 0,
      issueID: _currentIssueID
    });

    console.log("An NFT w/ ID %s and grid location %s has been minted to %s", newItemId, _gridLocationID, msg.sender);

    // Keep an easy way to track which toke  ID correponds to a given grid location ID
    tokenIDToGridLocationID[newItemId] = _gridLocationID;

    // Increment the counter for when the next NFT is minted.
    _tokenIDs.increment();
  }


  // Overriding tokenURI function
  function tokenURI(uint256 _tokenId) public view override returns (string memory) {
    TokenAttributes memory tokenAttributes = tokenIdToAttributes[_tokenId];

    string memory imageURI = tokenAttributes.imageURI;
    uint gridLocationID = tokenAttributes.gridLocationID;

    string memory json = Base64.encode(
      bytes(
        string(
          abi.encodePacked(
            '{"name": "EXHIBIT token #',
            Strings.toString(_tokenId),
            '", "description": "EXHIBIT token that lets the owner display an NFT in the NFT Zine", "image": "',
            imageURI,
            '", "attributes": [ { "trait_type": "Grid Location ID", "value": ',Strings.toString(gridLocationID),'}]}'
          )
        )
      )
    );

    string memory output = string(
      abi.encodePacked("data:application/json;base64,", json)
    );
    
    return output;
  }

  function setDisplayNFT(uint _tokenID, address _displayNFTContract, uint _displayNFTID, uint _mintTime) external {
    TokenAttributes storage thisTokenNFT = tokenIdToAttributes[_tokenID];
    console.log("Exhibit NFT attributes imageURI: %s", thisTokenNFT.imageURI);

    require(block.timestamp < issueIDToEndTimeStamp[_currentIssueID], "Sorry, this issue of NFT Zine is now locked!"); // test time 

    require(_mintTime < issueIDToEndTimeStamp[_currentIssueID] && _mintTime > issueIDToEndTimeStamp[_currentIssueID-1], "Sorry, this NFT wasn't minted during the issue time!");

    require (
      _displayNFTContract != address(0),
      "Error: You must provide a contract address for the NFT you want to display." 
    );
    console.log("Display NFT Contract Address: %s", _displayNFTContract);

    require (
      ownerOf(_tokenID) == msg.sender,
      "Error: You must own this Token NFT you want to edit."
    );

    require (
       (IERC1155(_displayNFTContract).balanceOf(msg.sender,_displayNFTID) > 0) || (IERC721(_displayNFTContract).balanceOf(msg.sender) > 0 && IERC721(_displayNFTContract).ownerOf(_displayNFTID) == msg.sender),
      "Error: You must own an NFT from the contract you want to display."
    );
    
    thisTokenNFT.displayNFTContract = _displayNFTContract;
    thisTokenNFT.displayNFTID = _displayNFTID;
    
  }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal virtual override 
    {
        super._beforeTokenTransfer(from, to, tokenId); // Call parent hook
        TokenAttributes storage thisTokenNFT = tokenIdToAttributes[tokenId];
        if (block.timestamp < issueIDToEndTimeStamp[thisTokenNFT.issueID]){ // allow display NFTs to be reset by new owner before the issue locks
          thisTokenNFT.displayNFTContract = address(0);
          thisTokenNFT.displayNFTID = 0;
        }        
    }

    function withdrawMoney() public {
      address withdrawer = 0xD358a57De6ba0c146a7E62746819bcb4Fd60a217; //update with withdrawal IDs
      require (msg.sender == withdrawer, "Can't withdraw funds!"); 
      address payable to = payable(msg.sender);
      to.transfer(address(this).balance);
    }


}

