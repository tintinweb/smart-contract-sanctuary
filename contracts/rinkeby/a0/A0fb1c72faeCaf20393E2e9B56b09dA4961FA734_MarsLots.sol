pragma solidity 0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "./Tesselation.sol";

/*
 * Sets the maximum generation to 22, i.e. it is impossible to split a lot from
 * generation 22. A lot of the last generation will be less approximately half a
 * square meter in surface.
 */
uint constant GEN_MAX = 22;

/**
 * @author Samy Moussally
 * @title A splittable and mergeable NFT that represents lots on
 * planet Mars, following the HEALPix tesselation (Górski et al., 2004)
 */
contract MarsLots is ERC721Enumerable, Ownable {
    
    struct Quadruple {
        uint q1;
        uint q2;
        uint q3;
        uint q4;
    }
    
    constructor() ERC721("MarsLots", "ML") Ownable() {}
    
    /**
     * @dev Checks that function caller is either the owner or approved for a token.
     * @param tokenId the id of the token.
     */
    modifier onlyApprovedOrOwner(uint tokenId) {
        require(_isApprovedOrOwner(msg.sender, tokenId), "ERC721: not owner or approved");
        _;
    }
    
    /**
     * @dev Checks that the 4 tokens have the same owner.
     * @param t1 first token.
     * @param t2 second token.
     * @param t3 third token.
     * @param t4 fourth token.
    */
    modifier onlySameOwner(uint t1, uint t2, uint t3, uint t4) {
        require(ownerOf(t1) == ownerOf(t2) &&
                ownerOf(t2) == ownerOf(t3) &&
                ownerOf(t3) == ownerOf(t4),
                "Lots do not have the same owner"
                );
        _;
    }
    
    /**
     * @dev Mints a new token. Only the contract owner can call
     * this function.
     * @param to send the token to.
     * @param tokenId the token to mint.
     */
    //function safeMint(address to, uint256 tokenId) external onlyOwner() {
    function safeMint(address to, uint256 tokenId) external {
        _safeMint(to, tokenId);
    }
    
    /**
     * @dev Mints a new token and sends it to contract owner. Only the contract owner
     * can call this function.
     * @param tokenId the token to mint.
     */
    //function safeMint(uint256 tokenId) external onlyOwner() {
    function safeMint(uint256 tokenId) external {
        _safeMint(owner(), tokenId);
    }
    
    /**
     * @dev Lists all the token owned by msg.sender.
     * @return an array of uint containting the tokenIds.
     */
    function getMyTokens() external view returns (uint[] memory) {
        uint n = balanceOf(msg.sender);
        uint[] memory tokens = new uint[](n);
        for(uint i = 0; i < n; ++i) {
            tokens[i] = tokenOfOwnerByIndex(msg.sender, i);
        }
        return tokens;
    }
    
    /**
     * @dev Splits the token in four daughters and sends them to the owner
     * of the mother token.
     * @param tokenId the Id of the mother token to be split in four.
     */
    function split(uint tokenId) external onlyApprovedOrOwner(tokenId) {
        require(Tesselation.getGeneration(tokenId) < GEN_MAX, "Lot is too small to be split");
        address tokenOwner = ownerOf(tokenId);
        _burn(tokenId);
        uint[4] memory daughters = getDaughters(tokenId);
        for(uint i = 0; i < 4; ++i) {
            _safeMint(tokenOwner, daughters[i]);
        }
    }
    
    /**
     * @dev Merges the four token into their mother. Only works if the four tokens
     * have the same owner and the same mother (that is, they were obtained from the 
     * same split). This basically reverses a split and send the resulting token to 
     * the (unique) owner of the daughters.
     * @param t1 is the 1st token.
     * @param t2 is the 2nd token.
     * @param t3 is the 3rd token.
     * @param t4 is the 4th token.
     */
    function merge(uint t1, uint t2, uint t3, uint t4) external
        onlySameOwner(t1, t2, t3, t4)
        onlyApprovedOrOwner(t1)
        onlyApprovedOrOwner(t2)
        onlyApprovedOrOwner(t3)
        onlyApprovedOrOwner(t4) {
            Quadruple memory sib = Quadruple(t1, t2 ,t3, t4);
            uint m1 = getMother(sib.q1);
            uint m2 = getMother(sib.q2);
            uint m3 = getMother(sib.q3);
            uint m4 = getMother(sib.q4);
            require(m1 == m2 && m2 == m3 && m3 == m4, "Lots are not sisters");
            address tokenOwner = ownerOf(sib.q1);
            _burn(sib.q1);
            _burn(sib.q2);
            _burn(sib.q3);
            _burn(sib.q4);
            _safeMint(tokenOwner, m1);
    }
    
    /**
     * @dev returns the sisters of a token (including itself), that is it returns
     * the four tokens that can be merged into a mother token.
     * @param tokenId the Id of the token.
     * @return an array of the four sisters.
     */
    function getSisters(uint tokenId) public pure returns (uint[4] memory) {
        require(Tesselation.getGeneration(tokenId) != 0, "first generation");
        return getDaughters(getMother(tokenId));
    }
    
    /**
     * @dev returns the daughters of the token, that is the four tokens that would be 
     * obtained by splitting the token.
     * @param tokenId the Id of the token.
     * @return the four daughters.
     */
    function getDaughters(uint tokenId) public pure returns (uint[4] memory) {
        uint k = Tesselation.getGeneration(tokenId);
        uint p = tokenId + 4 - 4**(k + 1);
        uint d = 4*p + 4**(k+2) -4;
        uint[4] memory daughters;
        for(uint i = 0; i < 4; ++i){
            daughters[i] = d + i;
        }
        return daughters;
    }
    
    /**
     * @dev returns the mother of the token, that is the token that would be obtained by
     * merging this token with its 3 sisters.
     * @param tokenId the tokenId.
     * @return the Id of the mother token.
     */
    function getMother(uint tokenId) public pure returns (uint) {
        uint k = Tesselation.getGeneration(tokenId);
        if (k == 0) {
            //generation 0 so no mother
            return tokenId;
        } else {
            uint p = tokenId + 4 - 4**(k + 1);
            return p/4 + 4**k - 4;
        }
    }
}

