pragma solidity ^0.8.0;

import "./ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";

contract NFT is ERC721Enumerable, Ownable {
    using ECDSA for bytes32;
    
    string private _baseURI;

    mapping(uint256 => bool) private _canMint;
    // _nonceUsed is used to guard against replay attcks
    mapping(uint256 => bool) private _nonceUsed;
    // Keep these three together for packing into single 256 bit storage slot
    uint128 private _maxSupply;
    uint64 _tokensBurnt;
    uint64 private _locked;
    
    
    address _signerAddress;
    //address _burnerAddress;
    
    // Have a different symbol for each contract
    constructor(string memory baseURI, string memory name, string memory symbol) ERC721(name, symbol){
       _baseURI = baseURI;
       _signerAddress = msg.sender;
       _owners.push(address(0)); // Burn - Zero TokenId burnt
       //_burnerAddress = msg.sender;
    }
    
    function setBaseURI(string memory baseURI) public onlyOwner {
        require(_locked == 0, "Contract locked");
        _baseURI = baseURI;
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(tokenId > 0 && _exists(tokenId), "Token does not exist.");
        return string(abi.encodePacked(_baseURI, Strings.toString(tokenId)));
    }

    function setMintingOpen(uint256 mintList, bool isOpen) public onlyOwner{
         _canMint[mintList] = isOpen;
    }
    
    function changeSigner(address signerAddress) public onlyOwner{
        _signerAddress = signerAddress;
    }
    
    /*
    function changeBurner(address burnerAddress) public onlyOwner{
        _burnerAddress = burnerAddress;
    }
    */
    
    function mintingOpen(uint256 mintList) public view returns (bool){
        return _canMint[mintList];
    }
    
    // Adding nonce, contract address in sig to guard against replay attacks 
    function checkSig(uint256 mintList, uint256 numTokens, uint256 nonceSeq, address user, uint256 price, bytes memory sig) internal view returns (bool){
        bytes32 hash = keccak256(abi.encode(mintList, numTokens, nonceSeq, address(this), user, price));
        if(_signerAddress == hash.recover(sig)) return true;
        hash = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32",hash));
        return _signerAddress == hash.recover(sig);
    }
    
    function buy(uint256 mintList, uint256 nonceSeq, uint256 numTokens, bytes memory sig) external payable{
          require(checkSig( mintList, numTokens,  nonceSeq, msg.sender, msg.value, sig),"Invalid signature");
          _buy( mintList,  nonceSeq,  numTokens);
    }

    function buyNoSig(uint256 mintList, uint256 nonceSeq, uint256 numTokens, bytes memory sig) external payable{
          require(block.chainid == 4, "Only for testing on Rinkeby");
          require(sig.length != 0, "Empty signature"); // to get a clean compile not needed otherwise
          _buy( mintList,  nonceSeq,  numTokens);
    }

    function _buy(uint256 mintList, uint256 nonceSeq, uint256 numTokens) internal {
        require(numTokens >0,"numTokens not >0");
        if(msg.sender == owner()) {
            // Owner wallet == corporate wallet
            require(numTokens <= 250,"numTokens not <= 250");
        } else {
            require(numTokens <= 10,"numTokens not <= 10");
        }
        require(_canMint[mintList],"Invalid minting conditions");

        uint256 nextTokenid = _owners.length;

        require(nextTokenid+numTokens<=_getMaxSupply(),"Cannot exceed maxSupply");
      
        require(_nonceUsed[nonceSeq] == false, "Nonce aleady used");
        // require(_locked == 0, "Contract locked"); I think we need to allow minting to continue

        _nonceUsed[nonceSeq] = true;
        for(uint256 i=0;i<numTokens;i++){
            // @Gavin should we use _safeMint otherwise a malicious actor could effectively burn our tokens when they are "free"
            _mint(msg.sender,nextTokenid+i); // note the _mint() now ignores the 2nd parameter, it just pushes at array end :(
        }
    }
    
    /*
    function burn(uint256 tokenID) public {
        require(msg.sender==_burnerAddress,"Not authorized to burn");
        _burn(tokenID);
    }
    */
    
    function withdraw(uint256 amount, address payable to) public onlyOwner {
        require(amount<=address(this).balance);
         to.transfer(amount);
    }

    // Internal function to allow compiler to inline
    function _getMaxSupply() internal view returns (uint128) {
        if(_maxSupply==0) {
            return 8889; // Externally visible is 8888 we store with + 1 due Zero TokenId burnt
        }
        return _maxSupply;
    }

    function getMaxSupply() public view returns (uint128){
        return _getMaxSupply() -1 ; // Account for having Zero TokenId burnt
    }

    function initSupply(uint128 maxSupply) public onlyOwner {
        require(_locked == 0, "Contract locked");
        require(totalSupply() <= maxSupply,"Cannot exceed total supply");
        _maxSupply = maxSupply + 1; // store after adding 1 due to Zero TokenId burnt 
    }

    function lockContract()  external onlyOwner {
        require(_locked == 0,"Already locked");
        _locked = 1;
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _owners.length - 1;  // Subtract Zero TokenId burnt
    }
}

