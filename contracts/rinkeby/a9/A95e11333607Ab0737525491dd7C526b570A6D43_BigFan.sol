pragma solidity ^0.8.0;

import { ReentrancyGuardUpgradeable } from "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import { OpenSeaExtention } from "./OpenSea/OpenSeaExtention.sol";
import { ECDSA } from  "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import { CIDLib } from "./lib/CIDLib.sol";

contract BigFan is ReentrancyGuardUpgradeable, OpenSeaExtention {
	bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
	// fee pricsion (PPM = 1000000)
	uint256 public constant FEE_PRECISION = 1e6;
	// fee in PPM (parts per million)
	uint256 public fee;
	// address for signing buy on mint data
	address public signer;
	// mapping of nonces for mint on buy operations
	mapping (uint256 => bool) public seenNoncesMint;
	// mapping of nonces for mint on buy operations
	mapping (address => mapping(uint256 => bool)) public seenNoncesBuy;
	

	// Modifiers
	modifier onlyAdmin() {
		require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "BigFunContract: Access denied");
		_;
	}

	modifier onlyMinter() {
		require(hasRole(MINTER_ROLE, msg.sender), "BigFunContract: Access denied");
		_;
	}

	modifier ensureValid(uint256 price, uint256 expires) {
		require(msg.value >= price, "BigFan: paid less than price");
		require(block.timestamp <= expires, "BigFan: expired signature");
		_;
	}

	modifier ensureValidNonceMint(uint256 nonce) {
		require(!seenNoncesMint[nonce], "BigFan: already used nonce");
		seenNoncesMint[nonce] = true;
		_;
	}

	modifier ensureValidNonceBuy(address from, uint256 nonce) {
		require(!seenNoncesBuy[from][nonce], "BigFan: already used nonce");
		seenNoncesBuy[from][nonce] = true;
		_;
	}

	// Initialize base contract data
	function initialize(
		address _signer,
		uint256 _fee, 
		string memory _name, 
		string memory _symbol, 
		string memory _contractURI,
		address _openSeaProxy)
		external initializer
	{
		signer = _signer;
		fee = _fee;
		_OpenSeaExtention_init(_name, _symbol, _contractURI, _openSeaProxy);
		__ERC1155AccessControlPauser_init();
		_setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(PAUSER_ROLE, msg.sender);
		_setupRole(MINTER_ROLE, msg.sender);
	}

	/***************************************
					ADMIN
	****************************************/

	function setSigner(address newSigner) 
		external onlyAdmin 
	{
		signer = newSigner;
	}

	function withdraw(address payable to, uint256 amount) 
		external onlyAdmin 
	{
		if(amount == 0) amount = address(this).balance;
		(bool _sent, ) = to.call{value: amount}(new bytes(0));
		require (_sent, "BigFan: withdraw failed");
	}
	
    function mint(address to, uint256 id, uint256 amount, bytes memory data) 
		external onlyMinter 
	{
		_mint(to, id, amount, data);
    }

    function mintBatch( address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
		external onlyMinter 
	{
        _mintBatch(to, ids, amounts, data);
    }

	/***************************************
					USER
	****************************************/

    /**
     * @dev Creates `amount` new tokens for `to`, of token type `id`.
     * Requirements:
     * - msg.value greater or equal price
	 * - unique not used nonce
	 * - block timestamp less or equal expires
	 * - valid signature of all parameters (signed by signer)
     */
    function buyOnMint(
        address to,
        uint256 id,
        uint256 amount,
		bytes memory data,
		uint256 price,
		uint256 expires,
		uint256 nonce,
		bytes memory signature) 
		external payable ensureValid(price, expires) ensureValidNonceMint(nonce)
	{
		bytes32 _hash = keccak256(abi.encodePacked(to, id, amount, price, expires, nonce));
		require(ECDSA.recover(_hash, signature) == signer, "BigFan: wrong signature");
        _mint(to, id, amount, data);
    }

    /**
     * @dev [Batched] variant of {buyOnMint}.
     */
    function buyOnMintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
		bytes memory data,
        uint256 price,
		uint256 expires,
		uint256 nonce,
		bytes memory signature)
		external payable ensureValid(price, expires) ensureValidNonceMint(nonce)
	{
		bytes32 _hash = keccak256(abi.encodePacked(to, ids, amounts, price, expires, nonce));
		require(ECDSA.recover(_hash, signature) == signer, "BigFan: wrong signature");
        _mintBatch(to, ids, amounts, data);
    }

	function buy(
		address payable from, 
		address to,
		uint256 id, 
		uint256 amount,
		bytes memory data,
		uint256 price,
		uint256 expires,
		uint256 nonce,
		bytes memory signature)
		external payable nonReentrant ensureValid(price, expires) ensureValidNonceBuy(from, nonce)
	{
		bytes32 _hash = keccak256(abi.encodePacked(from, to, id, amount, price, expires, nonce));
		require(ECDSA.recover(_hash, signature) == from, "BigFan: wrong signature");
		_safeTransferFrom(from, to, id, amount, data);
		uint256 _amount = price - price * fee / FEE_PRECISION;
		(bool _sent, ) = from.call{value: _amount}(new bytes(0));
		require (_sent, "BigFan: payment failed");
	}

	function buyBatch(
		address payable from, 
		address to,
		uint256[] memory ids, 
		uint256[] memory amounts,
		bytes memory data,
		uint256 price,
		uint256 expires,
		uint256 nonce,
		bytes memory signature)
		external payable nonReentrant ensureValid(price, expires) ensureValidNonceBuy(from, nonce)
	{
		bytes32 _hash = keccak256(abi.encodePacked(from, to, ids, amounts, price, expires, nonce));
		require(ECDSA.recover(_hash, signature) == from, "BigFan: wrong signature");
		_safeBatchTransferFrom(from, to, ids, amounts, data);
		uint256 _amount = price - price * fee / FEE_PRECISION;
		(bool _sent, ) = from.call{value: _amount}(new bytes(0));
		require (_sent, "BigFan: payment failed");
	}

	/***************************************
					GETTERS
	****************************************/

	/**
	 * @dev See {IERC1155MetadataURI-uri}.
	 */
	function uri(uint256 tokenId)
		public pure override returns (string memory)
	{
		string memory prefix = "ipfs://";
		// string memory suffix = "/metadata.json";
		return string(abi.encodePacked(prefix, CIDLib.toCID(tokenId)));
		// return string(abi.encodePacked(prefix, CIDLib.toCID(tokenId), suffix));
	}

	/**
    * Override isApprovedForAll to auto-approve BigFan signer
    */
    function isApprovedForAll(
        address owner,
        address operator
    ) public override virtual view returns (bool) {
        if (operator == signer) {
            return true;
        }
        return super.isApprovedForAll(owner, operator);
    }

}

