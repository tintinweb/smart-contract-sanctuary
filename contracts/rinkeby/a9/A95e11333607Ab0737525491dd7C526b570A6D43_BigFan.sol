pragma solidity ^0.8.0;

import { NativeMetaTransaction } from "./Matic/NativeMetaTransaction.sol";
import { ERC1155AccessControlPauserUpgradeable } from "../ERC1155AccessControlPauserUpgradeable.sol";

contract OpenSeaExtention is NativeMetaTransaction, ERC1155AccessControlPauserUpgradeable {
    // Mapping for total quantity for a token ID
    mapping (uint256 => uint256) public totalSupply;
    // Contract name
    string public name;
    // Contract symbol
    string public symbol;
    // See https://docs.opensea.io/docs/contract-level-metadata
    string public contractURI;
    // OpenSea Proxy contract address
    address public openSeaProxy;

    // Modifiers
	modifier onlyAdminOS() {
		require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "OpenSeaExtention: Access denied");
		_;
	}

    function _OpenSeaExtention_init(
        string memory _name, 
        string memory _symbol, 
        string memory _contractURI, 
        address _openSeaProxy) 
        internal initializer 
    {
        name = _name;
        symbol = _symbol;
        contractURI = _contractURI;
        openSeaProxy = _openSeaProxy;
    }

    /**
    * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
    */
    function _msgSender()
        internal
        override
        view
        returns (address sender)
    {
        if (msg.sender == address(this)) {
            bytes memory array = msg.data;
            uint256 index = msg.data.length;
            assembly {
                // Load the 32 bytes word from memory with the address on the lower 20 bytes, and mask those.
                sender := and(
                    mload(add(array, index)),
                    0xffffffffffffffffffffffffffffffffffffffff
                )
            }
        } else {
            sender = payable(msg.sender);
        }
        return sender;
    }

    function _mint(address to, uint256 id, uint256 amount, bytes memory data) 
		internal
        override 
	{
		super._mint(to, id, amount, data);
        unchecked {
            totalSupply[id] += amount;
        }
    }

    function _mintBatch( address to, uint256[] memory ids, uint256[] memory amounts, bytes memory data)
		internal
        override 
	{
        super._mintBatch(to, ids, amounts, data);
        for (uint256 i = 0; i < ids.length; i++) {
            unchecked {
                totalSupply[ids[i]] += amounts[i];
            }
        }
    }

    function _burn(address from, uint256 id,uint256 amount)
        internal 
        override
    {
        super._burn(from, id, amount);
        unchecked {
            totalSupply[id] -= amount;
        }
    }    

    function _burnBatch(address from, uint256[] memory ids, uint256[] memory amounts)
        internal 
        override
    {
        super._burnBatch(from, ids, amounts);
        for (uint256 i = 0; i < ids.length; i++) {
            unchecked {
                totalSupply[ids[i]] -= amounts[i];
            }
        }
    }
    
    function setContractURI(string memory _contractURI) external onlyAdminOS {
        contractURI = _contractURI;
    }
    
    function setOpenSeaProxy(address _openSeaProxy) external onlyAdminOS {
        openSeaProxy = _openSeaProxy;
    }

    /**
    * Override isApprovedForAll to auto-approve OS's proxy contract
    */
    function isApprovedForAll(
        address _owner,
        address _operator
    ) public override virtual view returns (bool isOperator) {
        // if OpenSea's ERC1155 Proxy Address is detected, auto-return true
        if (_operator == openSeaProxy) {
            return true;
        }
        // otherwise, use the default ERC1155.isApprovedForAll()
        return super.isApprovedForAll(_owner, _operator);
    }

}

