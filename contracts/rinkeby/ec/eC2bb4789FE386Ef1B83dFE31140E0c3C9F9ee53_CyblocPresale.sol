pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "./AccessControl.sol";

abstract contract CyBlocBase is ERC721Enumerable, AccessControl {
    uint256 public MAX_MENTORING = 3;
    uint256 public MENTOR_COOLDOWN_BLOCK = 28800; // 5 days
    uint256 public OPEN_COOLDOWN_BLOCK = 28800; // 5 days

    string public baseURI = "https://tokenmetadata.cyball.com";
    string public contractURIPrefix = "https://tokenmetadata.cyball.com";
    bool public paused = false;

    modifier whenNotPaused() {
        require(!paused, "Paused");
        _;
    }

    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    function contractURI() external view returns (string memory) {
        return contractURIPrefix;
    }    

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal virtual override {
        super._beforeTokenTransfer(from, to, tokenId);

        require(!paused, "token transfer while paused");
    }

    function setMentorCooldown(uint v) external onlyOwner {
        MENTOR_COOLDOWN_BLOCK = v;
    }

    function setOpenCooldown(uint v) external onlyOwner {
        OPEN_COOLDOWN_BLOCK = v;
    }

    function togglePause() external onlyOwner {
        paused = !paused;
    }

    function setBaseURI(string memory _uri) external onlyOwner {
        baseURI = _uri;
    }

    function setContractURI(string memory _uri) external onlyOwner {
        contractURIPrefix = _uri;
    }
}

