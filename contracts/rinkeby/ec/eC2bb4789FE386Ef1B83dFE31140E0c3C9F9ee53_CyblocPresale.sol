pragma solidity ^0.8.0;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "../cyball/CyBlocCore.sol";

contract CyblocPresale is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    struct Proof {
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    struct PackOpen {
        uint256 index;
        uint256[5][3] genes; // [class, trait1, trait2, trait3, seed]
    }

    CyBlocCore public NFTContract;

    uint256 public GENES_COMMIT;

    uint256 public PRESALE_END;
    uint256 public PRESALE_START;

    uint256 TOTAL_PACK;
    uint256 PACK_PRICE = 0.00001 ether;

    address public SIGNER;


    address[] public packBoughtByUser;

    event CyblocPackPurchased(address indexed adopter, uint256 quantity);
    event CyblocPacksOpened(address indexed receiver, PackOpen[] PacksOpen);

    constructor(
        address _NFTContract, 
        address _signer, 
        uint256 _price, 
        uint256 _totalPack, 
        uint256 _commitment,
        uint256 _start,
        uint256 _end
    ) {
        NFTContract = CyBlocCore(_NFTContract);
        SIGNER = _signer;
        PACK_PRICE = _price;
        GENES_COMMIT = _commitment;
        TOTAL_PACK = _totalPack;
        PRESALE_START = _start;
        PRESALE_END = _end;
    }

    modifier notStart() {
        require (block.timestamp < PRESALE_START, "Presale started, Cannot change");
        _;
    }

    modifier started() {
        require (PRESALE_START <= block.timestamp && block.timestamp <= PRESALE_END, "Have not start");
        _;
    }

    function setSigner(address _signer) external onlyOwner {
        SIGNER = _signer;
    }

    function setNFTContract(address _address) external onlyOwner notStart {
        NFTContract = CyBlocCore(_address);
    }

    function setPackages(uint256 _totalPack, uint256 _price, uint256 _commitment) external onlyOwner notStart {
        GENES_COMMIT = _commitment;
        TOTAL_PACK = _totalPack;
        PACK_PRICE = _price;
    }

    function setTime(uint256 _start, uint256 _end) external onlyOwner notStart {
        PRESALE_START = _start;
        PRESALE_END = _end;
    }

    function getChainID() public view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    function numberOfSoldPack() public view returns (uint256) {
        return packBoughtByUser.length;
    }

    function getMyPackageIndex(address _owner, uint n, uint m) public view returns (uint256[60] memory indexs) {
        uint count = 0;
        for (uint i = n; i <= m; i++) {
            if (packBoughtByUser[i] == _owner) {
                indexs[count++] = i;
            }
        }
    }

    function verifyProof(PackOpen memory pack, Proof memory _proof)
        private
        view
        returns (bool)
    {
        if (SIGNER == address(0x0)) {
            return true;
        }

        bytes32 digest = keccak256(
            abi.encodePacked(
                getChainID(),
                address(this),
                pack.index, 
                pack.genes[0],
                pack.genes[1],
                pack.genes[2]
            )
        );
        address signatory = ecrecover(digest, _proof.v, _proof.r, _proof.s);
        return signatory == SIGNER;
    }

    function buyCyblocsPacks(uint256 quantity) external payable started {
        require(quantity > 0, "INVALID QUANTITY");

        require(
            quantity <= TOTAL_PACK.sub(numberOfSoldPack()),
            "NOT ENOUGH PACKS FOR YOU"
        );

        uint totalPrice = PACK_PRICE * quantity;
        
        require(msg.value >= totalPrice, "INVALID MSG.VALUE");

        for (uint256 i = 0; i < quantity; i++) {
            packBoughtByUser.push(msg.sender);
        }

        payable(owner()).transfer(totalPrice);

        if (msg.value > totalPrice) {
            payable(msg.sender).transfer(msg.value.sub(totalPrice));
        }

        emit CyblocPackPurchased(msg.sender, quantity);
    }

    function unpack(PackOpen[] memory packages, Proof[] memory _proofs) external {
        require(packages.length > 0, "INVALID PACKAGE");

        for (uint256 i = 0; i < packages.length; i++) {
            PackOpen memory pack = packages[i];

            require(packBoughtByUser[pack.index] == msg.sender, "Wrong pack");
            // require(verifyProof(packages[i], _proofs[i]), "Wrong proof");
            
            delete packBoughtByUser[pack.index];

            NFTContract.newCyBloc(msg.sender, pack.genes[0], 0, 0);
            NFTContract.newCyBloc(msg.sender, pack.genes[1], 0, 0);
            NFTContract.newCyBloc(msg.sender, pack.genes[2], 0, 0);
        
            emit CyblocPacksOpened(msg.sender, packages);
        }
    }

    function withdraw(address token) external onlyOwner {
        if (token == address(0x0)) {
            payable(msg.sender).transfer(address(this).balance);
        }
        else {
            IERC20(token).safeTransfer(owner(), IERC20(token).balanceOf(address(this)));
        }
    }
}

