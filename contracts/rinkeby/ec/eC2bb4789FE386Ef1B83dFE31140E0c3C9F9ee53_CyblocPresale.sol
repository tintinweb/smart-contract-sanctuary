pragma solidity ^0.8.0;

import "./AccessControlBase.sol";

abstract contract AccessControl is AccessControlBase {
    mapping(address => bool) public spawners;
    mapping(address => bool) public mentorManager;
    

    modifier onlySpawner() {
        require(spawners[msg.sender]);
        _;
    }

    modifier onlyMentorManager() {
        require(spawners[msg.sender]);
        _;
    }

    function setSpawner(address _add, bool _v) external onlyOwner {
        spawners[_add] = _v;
    }

    function setMentorManager(address _add, bool _v) external onlyOwner {
        mentorManager[_add] = _v;
    }
}

// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.0;
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

contract AccessControlBase {
    using SafeERC20 for IERC20;

    address payable public _owner;

    mapping(address => bool) public operators;

    struct Proof {
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    constructor(address _ownerAddress) {
        _owner = payable(_ownerAddress);
    }

    modifier onlyOwner() {
        require(msg.sender == _owner);
        _;
    }

    modifier onlyOperator() {
        require(operators[msg.sender]);
        _;
    }

    function owner() public view returns (address) {
        return _owner;
    }

    function getChainID() public view returns (uint256) {
        uint256 id;
        assembly {
            id := chainid()
        }
        return id;
    }

    function verifyProof(bytes memory encode, Proof memory _proof)
        internal
        view
        returns (bool)
    {
        bytes32 digest = keccak256(
            abi.encodePacked(getChainID(), address(this), encode)
        );
        address signatory = ecrecover(digest, _proof.v, _proof.r, _proof.s);
        return operators[signatory];
    }

    function setOwner(address payable _newOwner) external onlyOwner {
        require(_newOwner != address(0));
        _owner = _newOwner;
    }

    function setOperator(address _operator, bool _v) external onlyOwner {
        operators[_operator] = _v;
    }
    
    function withdraw(address _token, address payable _to) external onlyOwner {
        if (_token == address(0x0)) {
            payable(_to).transfer(address(this).balance);
        }
        else {
            IERC20(_token).safeTransfer(_to, IERC20(_token).balanceOf(address(this)));
        }
    }
}

