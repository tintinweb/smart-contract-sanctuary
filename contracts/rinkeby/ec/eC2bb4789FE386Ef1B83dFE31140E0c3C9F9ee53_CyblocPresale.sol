pragma solidity ^0.8.0;

import "./AccessControl.sol";
import "./CyBlocBase.sol";

interface ICyBlocGeneScientist {
    function mixGenes(uint256 gene, uint256 gene2, uint256 seed) external pure returns (uint256);
    function generateGene(uint256 class, uint256[3] memory traits, uint256 seed) external pure returns (uint256 gene);
}

contract CyBlocCore is CyBlocBase {

    struct CyBloc {
        uint256 gene;
        uint256 cooldownEndBlock;
        uint256 mentorCount;
        uint256 mentoredById;
        uint256 mentoredById2;
    }

    ICyBlocGeneScientist public geneScientist;

    CyBloc[] public cyblocs;
    
    event Spawned(
        uint256 indexed _tokenId,
        address indexed _owner,
        uint256 _gene,
        uint256 _mentor1, 
        uint256 _mentor2
    );

    event StartMentor(uint256 indexed _tokenId, address indexed _owner);
    event FinishMentor(uint256 indexed _tokenId, address indexed _owner);

    constructor(address _owner, address _geneScientist) ERC721("CyBloc", "BLOC") AccessControlBase(_owner) {  
        MAX_MENTORING = 3; 
        MENTOR_COOLDOWN_BLOCK = 28800; // 5 days
        OPEN_COOLDOWN_BLOCK = 28800; // 5 days
        geneScientist = ICyBlocGeneScientist(_geneScientist);

        _spawn(address(0x1), 0x0, 0, 0);
    }

    function setGeneScientist(address _geneScientist) external onlyOwner {
        geneScientist = ICyBlocGeneScientist(_geneScientist);
    }

    function getMentorCount(uint _tokenId) public view returns (uint256) {
        return cyblocs[_tokenId].mentorCount;
    }

    function isReadyMentor(uint _tokenId) public view returns (bool) {
        CyBloc memory _cb = cyblocs[_tokenId];

        return _cb.cooldownEndBlock <= block.number && _cb.mentorCount < MAX_MENTORING && _cb.gene != 0;
    }

    function isReadyGraduation(uint _tokenId) public view returns (bool) {
        CyBloc memory _cb = cyblocs[_tokenId];
        return _cb.gene == 0 && _cb.cooldownEndBlock <= block.number;
    }

    function _spawn(address _owner, uint256 _gene, uint256 _mentoredById, uint256 _mentoredById2) internal returns (uint256) {
        CyBloc memory _newCyBloc = CyBloc({
            gene: _gene,
            cooldownEndBlock: block.number + MENTOR_COOLDOWN_BLOCK,
            mentorCount: 0,
            mentoredById: _mentoredById,
            mentoredById2: _mentoredById2
        });

        cyblocs.push(_newCyBloc);
        
        uint256 newCyblocId = cyblocs.length - 1;
        _mint(_owner, newCyblocId);
        
        emit Spawned(newCyblocId, _owner, _gene, _mentoredById, _mentoredById2);

        return newCyblocId;
    }

    function newCyBloc(address _owner, uint256[5] memory _info, uint256 _mentor1, uint256 _mentor2) external onlySpawner whenNotPaused returns (uint256)
    {
        // info = [class, trait1, trait2, trait3, seed]
        uint gene = geneScientist.generateGene(_info[0], [_info[1], _info[2], _info[3]], _info[4]);
        return _spawn(_owner, gene, _mentor1, _mentor2);
    }

    function startMentor(address _owner, uint256 _mentor, uint256 _mentor2) external whenNotPaused onlyMentorManager returns (uint256) {
        require(_mentor != 0, "Mentor is 0");
        require(ownerOf(_mentor) == _owner, "You are not owner of mentor");
        require(isReadyMentor(_mentor), "Mentor is not ready");

        cyblocs[_mentor].cooldownEndBlock = block.number + MENTOR_COOLDOWN_BLOCK;
        cyblocs[_mentor].mentorCount++;
        
        if (_mentor2 != 0) {
            require(_mentor2 != _mentor, "Use same mentor");
            require(ownerOf(_mentor2) == _owner, "You are not owner of mentor2");
            require(isReadyMentor(_mentor2), "Mentor2 is not ready");

            cyblocs[_mentor2].cooldownEndBlock = block.number + MENTOR_COOLDOWN_BLOCK;
            cyblocs[_mentor2].mentorCount++;
        }

        uint256 newCyblocId = _spawn(_owner, 0, _mentor, _mentor2);
        cyblocs[newCyblocId].cooldownEndBlock = block.number + OPEN_COOLDOWN_BLOCK;
        emit StartMentor(newCyblocId, _owner);

        return newCyblocId;
    }

    function finishMentor(address _owner, uint256 _tokenId, uint256 _seed) external whenNotPaused onlyMentorManager {
        require(_tokenId != 0, "Oops");
        require(ownerOf(_tokenId) == _owner, "Not your token");
        require(isReadyGraduation(_tokenId), "Cannot open");
        
        CyBloc storage _cb = cyblocs[_tokenId];

        uint geneMentor1 = cyblocs[_cb.mentoredById].gene;
        uint geneMentor2 = cyblocs[_cb.mentoredById2].gene;

        _cb.cooldownEndBlock = block.number + MENTOR_COOLDOWN_BLOCK;
        _cb.gene = geneScientist.mixGenes(
            geneMentor1, 
            _cb.mentoredById2 == 0 ? 0 : geneMentor2, 
            _seed
        );

        emit FinishMentor(_tokenId, _owner);
    }
}

