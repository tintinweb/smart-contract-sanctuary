    function palette(bytes memory data) internal view returns (Color [NUM_COLORS] memory) {
        Color [NUM_COLORS] memory colors;
        for (uint16 i = 0; i < NUM_COLORS; i++) {
            colors[i].hexString = Base64.encode(bytes(abi.encodePacked(
                    byteToHexString(data[i*4]),
                    byteToHexString(data[i*4+1]),
                    byteToHexString(data[i*4+2])
                )));
//            console.log(colors[i].hexString);
            colors[i].red = byteToUint(data[i*4]);
            colors[i].green = byteToUint(data[i*4+1]);
            colors[i].blue = byteToUint(data[i*4+2]);
            colors[i].alpha = byteToUint(data[i*4+3]);
        }
        return colors;
    }

    function colorForIndex(bytes[NUM_LAYERS] memory tokenLayers, uint k, uint index, Color [NUM_COLORS][NUM_LAYERS] memory palettes) internal pure returns (string memory) {
        Color memory fg;
        for (uint256 i = NUM_LAYERS-1; i >= 0; i--) {
            if (tokenLayers[i].length > 0) {
                fg = palettes[i][colorIndex(tokenLayers[i], k, index)];
                if (fg.alpha == 255) {
                     return fg.hexString;
                } else if (fg.alpha == 0) {
                    continue;
                } else {
                    for (uint256 j = i-1; j >=0; j--) {
                        if (tokenLayers[j].length > 0) {
                            Color memory bg = palettes[j][colorIndex(tokenLayers[j], k, index)];
                            if (bg.alpha > 0) {
                                return Base64.encode(bytes(blendColors(fg, bg)));
                            }
                        }
                    }
                }
            }
        }
        return fg.hexString;
    }

    function colorIndex(bytes memory data, uint k, uint index) internal pure returns (uint8) {
        if (index == 0) {
            return uint8(data[k]) >> 5;
        } else if (index == 1) {
            return (uint8(data[k]) >> 2) % 8;
        } else if (index == 2) {
            return((uint8(data[k]) % 4) * 2) + (uint8(data[k+1]) >> 7);
        } else if (index == 3) {
            return (uint8(data[k+1]) >> 4) % 8;
        } else if (index == 4) {
            return (uint8(data[k+1]) >> 1) % 8;
        } else if (index == 5) {
            return ((uint8(data[k+1]) % 2) * 4) + (uint8(data[k+2]) >> 6);
        } else if (index == 6) {
            return (uint8(data[k+2]) >> 3) % 8;
        } else {
            return uint8(data[k+2]) % 8;
        }
    }

