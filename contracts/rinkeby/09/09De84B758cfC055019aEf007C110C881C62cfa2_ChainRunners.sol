        require(totalSupply() + _count <= _maxRunners, "Attempted to mint more runners than total supply");
        require(_count == 1 || _count == 3 || _count == 10, "The provided count is invalid");
        require(msg.value == 0.07 ether * _count, "Incorrect amount of ether sent");

        for (uint256 i = 0; i < _count; i++) {
            getRandomNumber();
        }
    }

