    function dnaFor(uint input, uint length)
        internal
        pure
        returns (uint)
    {
        return uint(keccak256(abi.encodePacked(input))) % length;
    }

    function dnaChunk(uint dna, uint start)
        private
        pure
        returns (uint)
    {
        uint offset = 10 ** (12  - start);
        uint i = dna / offset;
        return start > 2 ? i % 100 : i;
    }

    function getNose(uint dna)
        public
        view
        returns (string memory)
    {
        uint i = dnaChunk(dna, 2) % 8 + 1;
        return noseArr[i];
    }

    function getCheeks(uint dna)
        public
        view
        returns (string memory)
    {
        uint i = dnaChunk(dna, 4) % 8 + 1;
        return cheekArr[i];
    }

    function getEyes(uint dna)
        public
        view
        returns (string memory)
    {
        uint i = dnaChunk(dna, 6) % 8 + 1;
        return eyesArr[i];
    }

    function getMouth(uint dna)
        public
        view
        returns (string memory)
    {
        uint i = dnaChunk(dna, 8) % 8 + 1;
        return mouthArr[i];
    }

    function getBackground(uint dna)
        public
        view
        returns (string memory)
    {
        uint i = dnaChunk(dna, 10) % 8 + 1;
        return backgroundArr[i];
    }

    function _makeParts(uint dna)
        internal
        view
        returns (string[7] memory)
    {
        string[7] memory parts;
        parts[0] = '<svg width="400" height="400" viewBox="0 0 400 400" fill="none" xmlns="http://www.w3.org/2000/svg">';
        parts[1] = getBackground(dna);
        parts[2] = getCheeks(dna);
        parts[3] = getEyes(dna);
        parts[4] = getNose(dna);
        parts[5] = getMouth(dna);
        parts[6] = "</svg>";
        return parts;
    }

