    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(_exists(tokenId), "Face does not exist");

        uint digits = 12;
        uint length = 10 ** digits;
        uint dna = dnaFor(tokenId, length);

        string[7] memory parts = _makeParts(dna);
        string memory output = string(
            abi.encodePacked(
                parts[0],
                parts[1],
                parts[2],
                parts[3],
                parts[4],
                parts[5],
                parts[6]
            )
        );

        string memory json = Base64.encode(bytes(string(abi.encodePacked(
            '{"name": "Face #',
            toString(tokenId),
            '", "description": "A face", "dna": "',
            toString(dna),
            '", "image": "data:image/svg+xml;base64,',
            Base64.encode(bytes(output)),
            '"',
            "}"
        ))));

        output = string(abi.encodePacked(
            'data:application/json;base64,',
            json
        ));

        return output;
    }

    function mint(uint256 tokenId) public nonReentrant {
        require(
            !_exists(tokenId) && tokenId > 0 && tokenId < 1024,
            "Face id is bad"
        );
        _safeMint(msg.sender, tokenId);
    }

