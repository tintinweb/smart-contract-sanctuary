pragma solidity ^0.8.5;
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@chainlink/contracts/src/v0.8/dev/VRFConsumerBase.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "./libs/AdressSet.sol";
import "hardhat/console.sol";

// Represents the status of the raffle
enum Status {
    NotStarted, // The raffle has not started yet
    Open, // The raffle is open for ticket purchases
    Closed, // The raffle is no longer open for ticket purchases
    Completed // The raffle has been closed and the numbers drawn
}

struct RaffleInfo {
    address collection;
    uint256 raffleID; // ID for raffle
    Status raffleStatus; // Status for raffle
    uint256 costPerTicket; // Cost per ticket in $ern
    uint256 startingTimestamp; // Block timestamp for star of raffle
    uint256 closingTimestamp; // Block timestamp for end of entries
    address[] participants;
    Set differentPartic;
    Set winners;
    uint256[] cardIds;
    uint256 totalParticipiants;
    uint256 totalCard;
    uint256 randomNumber;
}

struct Winner {
    uint256 claimableCardId;
    bool isClaimed;
} 

struct VRF {
    bytes32 keyHash;
    uint256 fee;
    address token;
    address coordinator;
}

contract Raffle is AccessControl, IERC1155Receiver, VRFConsumerBase {
    using AddressSet for Set;

    event RaffleOpen(uint256 raffleId, uint256 startingTimestamp, uint256 closingTimestamp, uint256 costPerTicket);
    event RaffleClose(uint256 raffleId);
    event RaffleCompleted(uint256 raffleId, address[] wnngAddress);
    event RandomNumber(uint256 number, uint256 raffleId);
    event TicketBuyed(address buyer, uint256 amount);
    event RaffleCardAdded(uint256 cardId, address collection);
    event RaffleCardBatchAdded(uint256[] cardIds, address collection);
    event CardClaimed(address winner, address collection, uint256 cardId);

    uint256 public raffleIdCounter;
    uint256[] winningUsers;
    Set whitelistedNftContract;

    bytes4 internal constant _ERC1155_RECEIVED = 0xf23a6e61;
    bytes4 internal constant _ERC1155_BATCH_RECEIVED = 0xbc197c81;
    bytes4 internal constant _ERC1155_REJECTED = 0xffffffff;

    VRF private _vrf;
    IERC20 public tokenForSales;
    
    mapping(uint256 => mapping(uint256 => bool)) private winngNumb;
    mapping(uint256 => mapping(address => bool)) private winngAdd;
    mapping(uint256 => mapping(address => Winner)) public winnerCard;
    mapping(uint256 => RaffleInfo) internal allRaffles;
    mapping(bytes32 => uint256) public requestAndRaffleId;

    constructor(VRF memory vrf) VRFConsumerBase(vrf.coordinator, vrf.token) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _vrf = vrf;
    }


    function getRandomNumber(uint256 _raffleId) internal returns (bytes32 requestId){
        require(
            LINK.balanceOf(address(this)) >= _vrf.fee,
            "Not enough LINK - fill contract with faucet"
        );
        bytes32 requestId = requestRandomness(_vrf.keyHash, _vrf.fee);
        requestAndRaffleId[requestId] = _raffleId;
        return requestId;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness)
        internal
        override
    {   
        uint256 raffleId = requestAndRaffleId[requestId];
        allRaffles[raffleId].randomNumber = randomness;
        
        emit RandomNumber(randomness, raffleId);
    }

    function withdrawLink() external onlyRole(DEFAULT_ADMIN_ROLE) {
        LINK.transfer(msg.sender, LINK.balanceOf(address(this)));
    }

    function addWhitelist(address collecAdd) external onlyRole(DEFAULT_ADMIN_ROLE) {
        whitelistedNftContract.add(collecAdd);
    }

    function setTokenAdd(address ernAdd) external onlyRole(DEFAULT_ADMIN_ROLE) {
        tokenForSales = IERC20(ernAdd);
    }

    function withdrawERC20(address tokenAdd) external onlyRole(DEFAULT_ADMIN_ROLE) {
        uint256 balance = IERC20(tokenAdd).balanceOf(address(this));
        IERC20(tokenAdd).transfer(msg.sender,balance);
    }

    function emergncyWithdrawNFT(address collecAdd, uint256 cardId) external onlyRole(DEFAULT_ADMIN_ROLE) {
        IERC1155 collec = IERC1155(collecAdd);
        collec.safeTransferFrom(address(this),msg.sender,cardId,1,"");
    }
    
    
    function createRaffle(
        uint256 _startingTimestamp,
        uint256 _closingTimestamp,
        uint256 _costPerTicket,
        address _collection
    )   external 
        onlyRole(DEFAULT_ADMIN_ROLE)
    { 
        if(raffleIdCounter >= 1){
        require(allRaffles[raffleIdCounter].raffleStatus == Status.Completed,
            "You can't create multiple Raffles at the same time");
        }

        require(_startingTimestamp >= block.timestamp,
            "Start time should be bigger than the current time");

        require(_startingTimestamp < _closingTimestamp,
            "End time should be greater than the start time");

        raffleIdCounter++;

        allRaffles[raffleIdCounter].collection = _collection;
        allRaffles[raffleIdCounter].raffleID = raffleIdCounter;
        allRaffles[raffleIdCounter].raffleStatus = Status.Open;
        allRaffles[raffleIdCounter].startingTimestamp = _startingTimestamp;
        allRaffles[raffleIdCounter].closingTimestamp = _closingTimestamp;
        allRaffles[raffleIdCounter].costPerTicket = _costPerTicket;

        emit RaffleOpen(
            raffleIdCounter, 
            _startingTimestamp, 
            _closingTimestamp, 
            _costPerTicket 
        );
    }

    function changeRaffleTime(
        uint256 _raffleId,
        uint256 _startingTimestamp, 
        uint256 _closingTimestamp
    )   external
        onlyRole(DEFAULT_ADMIN_ROLE)
    { 
        require(_startingTimestamp >= block.timestamp,
            "Raffle start time should be greater than current time");

        require(_startingTimestamp < _closingTimestamp,
            "end time should be greater than start time");

        allRaffles[_raffleId].startingTimestamp = _startingTimestamp;
        allRaffles[_raffleId].closingTimestamp = _closingTimestamp;
    }

    function finishTheRaffle(uint256 _raffleId) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            allRaffles[_raffleId].closingTimestamp <=
                block.timestamp,
            "Cannot set winning numbers during Raffle"
        );
        require(
            allRaffles[_raffleId].raffleStatus == Status.Open,
            "Raffle State incorrect for draw"
        );
        uint256 diffParticp =  allRaffles[_raffleId].differentPartic.getArray().length;
        require(
           diffParticp >= allRaffles[_raffleId].totalCard,
            "The number of different participants must be more than the number of cards");
        // Sets Raffle status to closed
        allRaffles[_raffleId].raffleStatus = Status.Closed;
        // Requests a random number from the generator

        //############################################## 
        //THIS LINE SHOULD BE COMMENT LINE FOR LOCAL TESTING !!!!!
        getRandomNumber(_raffleId);
        //##############################################
        // Emits that random number has been requested
        emit RaffleClose(_raffleId);
    }

    function pickWinners(uint256 _raffleId) 
        external 
        onlyRole(DEFAULT_ADMIN_ROLE) 
        returns (address[] memory) {
        RaffleInfo storage raffleInf = allRaffles[_raffleId];
        bool _continue = true;
     
        uint256 index = 0;
        uint256 hashIndex = 0;
        address[] memory winningAddress = new address[](raffleInf.totalCard);

        while (_continue == true) {
            bytes32 hashOfRandom = keccak256(
                abi.encodePacked(raffleInf.randomNumber, hashIndex)
            );
            uint256 numberRepresentation = uint256(hashOfRandom);
            uint256 pickedNumber = ((numberRepresentation) % raffleInf.totalParticipiants);
            address winner = raffleInf.participants[pickedNumber];
            
            if (winngNumb[_raffleId][pickedNumber] == false && winngAdd[_raffleId][winner] == false) {
                winngNumb[_raffleId][pickedNumber] = true;
                winngAdd[_raffleId][winner] = true;
                raffleInf.winners.add(winner);
                winningAddress[index] = winner;
                winnerCard[_raffleId][winner].claimableCardId = raffleInf.cardIds[index];

                index++;
            }
            hashIndex++;

            if (index == raffleInf.totalCard) {
                _continue = false;
            }
        }
      
        allRaffles[_raffleId].raffleStatus = Status.Completed;
    
        //getWinnerNumber[_raffleId] = winningNumbers;
        emit RaffleCompleted(_raffleId, winningAddress);
        
        return winningAddress;

    }

    function buyTicket(uint256 _raffleId) external {
        RaffleInfo storage raffleInf = allRaffles[_raffleId];

        require(raffleInf.raffleStatus == Status.Open,"This raffle closed");
        require(tokenForSales.balanceOf(msg.sender) >= raffleInf.costPerTicket,"insufficient amount");
        
        tokenForSales.transferFrom(msg.sender,address(this),raffleInf.costPerTicket);

        raffleInf.participants.push(msg.sender);
        raffleInf.differentPartic.add(msg.sender);
        raffleInf.totalParticipiants++;
        emit TicketBuyed(msg.sender, raffleInf.costPerTicket);
    }

    function claimCard(uint256 _raffleId) external {
        RaffleInfo storage raffleInf = allRaffles[_raffleId];

        require(raffleInf.winners.contains(msg.sender),"You didn't win a card");
        require(winnerCard[_raffleId][msg.sender].isClaimed == false,"You have already claimed");

        uint256 cardId = winnerCard[_raffleId][msg.sender].claimableCardId;
        winnerCard[_raffleId][msg.sender].isClaimed = true;

        IERC1155 _whitelistedNftContract = IERC1155(raffleInf.collection);
        _whitelistedNftContract.safeTransferFrom(address(this),msg.sender,cardId,1,"");

        emit CardClaimed(msg.sender, raffleInf.collection, cardId);
    }

    function registerCard(uint256 cardId, address collection) internal {
        allRaffles[raffleIdCounter].cardIds.push(cardId);
        allRaffles[raffleIdCounter].totalCard++;

        emit RaffleCardAdded(cardId, collection);
    }

    function registerCardBatch(uint256[] memory cardIds, address collection) internal {
        for (uint256 i = 0; i < cardIds.length; i++) {
            allRaffles[raffleIdCounter].cardIds.push(cardIds[i]);
            allRaffles[raffleIdCounter].totalCard++;
        }

        emit RaffleCardBatchAdded(cardIds, collection);
    }

    function getRaffleInfo(uint256 _raffleId)
        external
        view
        returns (
            address, 
            uint256, 
            Status, 
            uint256, 
            uint256,
            uint256, 
            address[] memory, 
            uint256[] memory,
            uint256,
            uint256,
            uint256
        ) {
        RaffleInfo storage raffleInf =allRaffles[_raffleId];
        return(
            raffleInf.collection,
            raffleInf.raffleID,
            raffleInf.raffleStatus,
            raffleInf.costPerTicket,
            raffleInf.startingTimestamp,
            raffleInf.closingTimestamp,
            raffleInf.participants,
            raffleInf.cardIds,
            raffleInf.totalParticipiants,
            raffleInf.totalCard,
            raffleInf.randomNumber
        );
    }

    function getWinners(uint256 _raffleId) external view returns(address[] memory){
        RaffleInfo storage raffleInf =allRaffles[_raffleId];
        return(raffleInf.winners.getArray());
    }

    function onERC1155Received(
        address, /*operator*/
        address from,
        uint256 id,
        uint256, /*value*/
        bytes calldata /*data*/
    ) external virtual override returns (bytes4) {
        if(whitelistedNftContract.contains(msg.sender)){
            registerCard(id, msg.sender);
            return _ERC1155_RECEIVED;
        }
        require(false, "NftStaking: contract not whitelisted");
        
    }

    function onERC1155BatchReceived(
        address, /*operator*/
        address from,
        uint256[] calldata ids,
        uint256[] calldata, /*values*/
        bytes calldata /*data*/
    ) external virtual override returns (bytes4) { 
        if(whitelistedNftContract.contains(msg.sender)){
            registerCardBatch(ids, msg.sender);
            return _ERC1155_BATCH_RECEIVED;
        }
        
        require(false, "NftStaking: contract not whitelisted");
    } 

}

