/// @title Based Token ERC20
/// @author Mason Hall
/// @notice ERC20 token representing a share of BasedDAO
contract BasedToken is ERC20, ERC20Permit, ERC20Votes {
	/// ERRORS ///

	/// @notice Thrown when trying to directly call the mint or burn functions
	error Unauthorized();

	/// @notice The manager of this DAO. Should be a multi-sig until a Governor contract is introduced
	address public immutable manager;

	/// @notice Deploys a BasedToken instance with the specified name and symbol
	/// @param name The name of the deployed token
	/// @param symbol The symbol of the deployed token
	/// @dev Deployed from the constructor of the BasedDAO contract
	constructor(string memory name, string memory symbol) 
		ERC20(name, symbol)
		ERC20Permit(name)
	{
		manager = msg.sender;
	}

	/// @notice Grants the specified address a specified amount of tokens
	/// @param to The address that will receive the tokens
	/// @param amount the amount of tokens to receive
	/// @dev This function should be called from within BasedDAO, and will revert if manually accessed
	function mint(address to, uint256 amount) public payable {
		if (msg.sender != manager) revert Unauthorized();

		_mint(to, amount);
	}

	/// @notice Burns a specified amount of tokens from a specified address' balance
	/// @param from The address that will get their tokens burned
	/// @param amount the amount of tokens to burn
	/// @dev This function should be called from within BasedDAO, and will revert if manually accessed
	function burn(address from, uint256 amount) public payable {
		if (msg.sender != manager) revert Unauthorized();

		_burn(from, amount);
	}

	// The functions below are overrides required by Solidity.

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override(ERC20, ERC20Votes) {
        super._afterTokenTransfer(from, to, amount);
    }

	function _mint(address to, uint256 amount)
        internal
        override(ERC20, ERC20Votes)
    {
        super._mint(to, amount);
    }

    function _burn(address account, uint256 amount)
        internal
        override(ERC20, ERC20Votes)
    {
        super._burn(account, amount);
    }
}

/// @title BasedDAO
/// @author Mason Hall
/// @notice DAO with a new address-based PoW mechanism, BasedDAO. Has Juicebox functionality for the treasury
contract BasedDAO {
	/// ERRORS ///

	/// @notice Thrown when trying to change the state of the campaign or renounce the contract without being the manager
	error Unauthorized();

	/// @notice Thrown when trying to claim a refund while refunds are closed
	error RefundsClosed();

	/// @notice Thrown when trying to contribute while contributions are closed
	error ContributionsClosed();

	/// @notice Thrown when trying to contribute with an address that is not ba5ed
	error NotBased();

	/// EVENTS ///

	/// @notice Emitted when the manager renounces the contract, locking its current state forever
	event Renounced();

	/// @notice Emitted when the manager withdrawns a share of the raised funds
	/// @param amount The amount of ETH withdrawn
	event Withdrawn(uint256 amount);

	/// @notice Emitted when the state of the campaign is changed
	/// @param state The new state of the campaign
	event StateUpdated(State state);

	/// @notice Emitted when the token multiplier of the DAO is changed
	/// @param tokenMultiplier The new token multiplier
	event TokenMultiplierUpdated(uint256 tokenMultiplier);

	/// @notice Emitted when a contributor successfully claims a refund
	/// @param contributor The address of the contributor
	/// @param amount The amount of ETH refunded
	event Refunded(address indexed contributor, uint256 amount);

	/// @notice Emitted when a user contributes to the campaign
	/// @param contributor The address of the contributor
	/// @param amount The amount of ETH contributed
	event Contributed(address indexed contributor, uint256 amount);

	/// @notice Emitted when the manager assigns a new manager for the DAO
	/// @param newManager The new manager of the DAO. Should only be used to make the manager a multi-sig or Governor contract
	event ManagerUpdated(address newManager);

	/// @notice Possible states of DAO fundraising
	enum State {
		CLOSED,
		OPEN,
		REFUNDING
	}

	/// @notice The address of the user who can withdraw funds and change the state of the DAO fundraise
	address public manager;

	/// @notice The current state of the DAO fundraise
	/// @dev This automatically generates a getter for us!
	State public getState;

	/// @notice The amount of ERC20 tokens to issue per ETH received before bonus multiplier
	uint256 public getBaseEthRatio;

	/// @notice The address of the ERC20 token representing shares of the DAO
	BasedToken public immutable token;
	
	/// @notice The largest target a based address can have
	uint256 public constant MAX_TARGET = 0xFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF;

	/// @notice The log of the largest target a based address can have
	uint256 public constant MAX_TARGET_LOG = 140;

	/// @notice Keeps track of how much ETH each address has contributed
	mapping (address => uint256) public  getEthContributed;

	/// @notice Deploys a BasedDAO instance with the specified name and symbol
	/// @param name The name of the ERC20 token
	/// @param symbol The symbol of the ERC20 token
	constructor(string memory name, string memory symbol) payable {
		manager = msg.sender;
		getState = State.OPEN;
		getBaseEthRatio = 20_000;
		token = new BasedToken(name, symbol);
	}

	/// @notice Contribute to the DAO by sending ETH, if contributions are open
	function contribute() public payable {
		if (getState != State.OPEN) revert ContributionsClosed();
		if (!_isBased()) revert NotBased();
		if (getEthContributed[msg.sender] + msg.value >= 1e18) revert ContributionsClosed();
		emit Contributed(msg.sender, msg.value);

		getEthContributed[msg.sender] += msg.value;
		token.mint(msg.sender, msg.value * _getTokenMultiple());
	}

	/// @notice check if the contributor has a ba5ed address
	/// @return bool denoting if addresses first 5 bytes are ba5ed hex code 
	function _isBased() internal view returns (bool) {
		return uint160(msg.sender) >> 140 == 0xba5ed;
	}

	/// @notice get remainder of address after prefix
	/// @return the last 140 bits of the uint160 of msg.sender
	function _getRemainder() internal view returns (uint160) {
		return uint160(msg.sender) & ((1 << 140) - 1);
	}

	/// @notice calculates the $BASED/ETH ratio ~based~ on your address
	/// @return the number of tokens to give in return for ETH
	function _getTokenMultiple() internal view returns (uint256) {
		uint160 remainder = _getRemainder();
		uint256 multiple = MAX_TARGET_LOG - _getIntLog(remainder) + 1;
		return getBaseEthRatio * multiple;
	}


	/// @notice get integer part of log
	/// @return the integer part of log
	function _getIntLog(uint160 x) internal pure returns (uint256) {
		uint256 n = 0;	
		if (x >= 2**128) { x >>= 128; n += 128; }
		if (x >= 2**64) { x >>= 64; n += 64; }
		if (x >= 2**32) { x >>= 32; n += 32; }
		if (x >= 2**16) { x >>= 16; n += 16; }
		if (x >= 2**8) { x >>= 8; n += 8; }
		if (x >= 2**4) { x >>= 4; n += 4; }
		if (x >= 2**2) { x >>= 2; n += 2; }
		if (x >= 2**1) { x >>= 1; n += 1; }

		return n;
	}

	/// @notice Receive a refund for your contribution to the DAO, if refunds are open
	function refund(uint256 amount) public {
		if (getState != State.REFUNDING) revert RefundsClosed();
		if (!_isBased()) revert NotBased();
		uint256 refundETH;
		uint256 tokenMultiple = _getTokenMultiple();
		assembly {
			refundETH := div(amount, tokenMultiple)
		}

		token.burn(msg.sender, amount);
		emit Refunded(msg.sender, refundETH);

		SafeTransferLib.safeTransferETH(msg.sender, refundETH);
	}

	/// @notice Withdraw a share of the raised funds, only available to the manager of the DAO fundraise
	function withdraw() public {
		if (msg.sender != manager) revert Unauthorized();

		uint256 amount = address(this).balance;

		emit Withdrawn(amount);
		SafeTransferLib.safeTransferETH(msg.sender, amount);
	}

	/// @notice Update the state of the DAO fundraise, only available to the manager of the DAO fundraise
	/// @param state The new state of the DAO fundraise
	function setState(State state) public {
		if (msg.sender != manager) revert Unauthorized();

		getState = state;
		emit StateUpdated(state);
	}

	/// @notice Update the token multiplier for the DAO
	/// @param tokenMultiplier The new token multiplier
	function setTokenMultiplier(uint256 tokenMultiplier) public {
		if (msg.sender != manager) revert Unauthorized();

		getBaseEthRatio = tokenMultiplier;
		emit TokenMultiplierUpdated(tokenMultiplier);
	}

	/// @notice Renounce ownership of the DAO, effectively locking all settings in place. Only available to the manager of the DAO fundraise
	function renounce() public {
		if (msg.sender != manager) revert Unauthorized();

		emit Renounced();
		manager = address(0);
	}

	/// @notice Set a new manager (multi-sig or Governor contract). Only available to the manager of the DAO fundraise
	function setManager(address newManager) public {
		if (msg.sender != manager) revert Unauthorized();
		require(manager != newManager, "New manager = old manager");
		require(newManager != address(0), "Invalid new manager");

		emit ManagerUpdated(newManager);
		manager = newManager;
	}

	/// @dev This function ensures this contract can receive ETH
	receive() external payable {}
}

