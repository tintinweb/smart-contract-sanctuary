//import "hardhat/console.sol";
import "prb-math/contracts/PRBMathUD60x18.sol";

// описывает логику налогов
contract TaxContract {
    using PRBMathUD60x18 for uint256;

    event OnReward(address indexed account, uint256 value); // кто и сколько получил вознаграждения
    event OnTaxInterval(uint256 indexed number, uint256 poolSize, uint256 totalOnAccounts); // когда сменился налоговый интервал

    // настройки
    uint256 _TaxIntervalDays = 7; // интервал налогооблажения
    uint256 _ConstantTaxPercent = 1e16; // константная составляющая налога 100%=1e18
    uint256 _MinBalancePercentTransactionForReward = 5e15; // мин процент от баланса, который нужно провести, чтобы получить награду
    uint256 _RewardStopAccountPercent = 1e16; // при каком проценте от общей эмиссии (кроме обменника), на момент перехода к новому налоговому интервалу, награда станет 0 (награда падает, при приближении размера кошелька к этому проценту)
    uint256 _MaxTaxPercent = 1e17; // максимальный процент налога 100%=1e18

    mapping(address => uint256) _taxGettings; // time when account was collected fee last time
    mapping(address => uint256) _LastRewardedTransactions; // когда были сделаны последние транзакции, которые моглибы участвовать в получении наград (см правило мин размера транзакции от размера кошелька)
    uint256 _currentTaxPool; // how much tax fee collected in this tax getting interval
    uint256 internal _lastTaxPool; // how much tax fee collected in last tax getting interval
    uint256 _lastTaxTime; // last time when tax gettings interval was switched
    uint256 _lastTotalOnAccounts; // how much was in the hands of users at the time of completion of the previous tax collection
    uint256 _givedTax; // сколько налогов раздали (раздается из пула прошлого налогооблажения)
    uint256 _TaxIntervalNumber; // current tax getting interval number

    constructor() {
        _lastTaxTime = block.timestamp;
    }

    // задает настройки налогооблажения
    function InitializeTax(
        uint256 taxIntervalDays,
        uint256 constantTaxPercent,
        uint256 minBalancePercentTransactionForReward,
        uint256 rewardStopAccountPercent,
        uint256 maxTaxPercent
    ) internal {
        _TaxIntervalDays = taxIntervalDays;
        _ConstantTaxPercent = constantTaxPercent;
        _MinBalancePercentTransactionForReward = minBalancePercentTransactionForReward;
        _RewardStopAccountPercent = rewardStopAccountPercent;
        _MaxTaxPercent = maxTaxPercent;
        _lastTaxTime = block.timestamp;
    }

    // когда были сделаны последние транзакции, которые моглибы участвовать в получении наград (см правило мин размера транзакции от размера кошелька)
    function GetLastRewardedTransactionTime(address account)
        public
        view
        returns (uint256)
    {
        return _LastRewardedTransactions[account];
    }

    // возвращает время, когда указанный аккаунт получил налог или 0, если он не получал налог
    function GetLastRewardTime(address account) public view returns (uint256) {
        return _taxGettings[account];
    }

    // сколько налогов собрано за предыдущий интервал налогооблажения
    function GetLastTaxPool() public view returns (uint256) {
        return _lastTaxPool;
    }

    // возвращает время, когда произошла последняя смена интервала налогооблажения или 0, если смены еще небыло
    function GetLastTaxPoolTime() public view returns (uint256) {
        return _lastTaxTime;
    }

    // сколько было на всех аккаунтах (кроме обменника) в момент смены налогового интервала
    function GetLastTotalOnAccounts() public view returns (uint256) {
        return _lastTotalOnAccounts;
    }

    //returns the currently set tax interval in days
    function GetTaxIntervalDays() public view returns (uint256) {
        return _TaxIntervalDays;
    }

    // мин процент от баланса, который нужно провести, чтобы получить награду 1%=1e18
    function GetMinBalancePercentTransactionForReward()
        public
        view
        returns (uint256)
    {
        return _MinBalancePercentTransactionForReward;
    }

    // сколько налогов собрано за текущий интервал налогооблажения
    function GetCurrentTaxPool() public view returns (uint256) {
        return _currentTaxPool;
    }

    // сколько налогов раздали (раздается из пула прошлого налогооблажения)
    function GetGivedTax() public view returns (uint256) {
        return _givedTax;
    }

    // returns the current tax interval number
    // сейчас везде сделано с учетом того, что 0 - пока не пройдет нуливой налоговый интервал, потом 1 итд
    function GetTaxIntervalNumber() public view returns (uint256) {
        return _TaxIntervalNumber;
    }

    // сколько суммарно имеется нерозданного налога
    function GetTotalTax() public view returns (uint256) {
        return _currentTaxPool + _lastTaxPool - _givedTax;
    }

    // попытка перехода к след интервалу налогооблажения
    // tokensOnAccounts - сколько суммарно токенов есть на всех аккаунтах, подлежащих раздаче налога (все кроме обменника)
    function TryNextTaxInterval(uint256 tokensOnAccounts) internal {
        if (block.timestamp < _lastTaxTime + _TaxIntervalDays * 1 days) return;

        //console.log("NEXT_TIME");

        // we transfer all the collected money for distribution, plus what was not distributed
        _lastTaxPool = _lastTaxPool - _givedTax;
        _lastTaxPool += _currentTaxPool;
        // we reset the collection of funds and how much was issued
        _currentTaxPool = 0;
        _givedTax = 0;
        // we remember how much money the participants have in their hands (to calculate the share of each)
        _lastTotalOnAccounts = tokensOnAccounts;
        // we write the current date when the tax interval was changed
        _lastTaxTime = block.timestamp;
        // update the counter of tax intervals
        ++_TaxIntervalNumber;
        emit OnTaxInterval(_TaxIntervalNumber, _lastTaxPool, _lastTotalOnAccounts);
    }

    // прибавляет количество выданного из собранного пула
    // если добавляется больше чем _lastTaxPool то добавит сколько не хватает до _lastTaxPool
    // вернет сколько добавилось
    function AddGivedTax(uint256 amount) internal returns (uint256) {
        uint256 last = _givedTax;
        _givedTax += amount;
        if (_givedTax > _lastTaxPool) _givedTax = _lastTaxPool;
        return _givedTax - last;
    }

    // добавляет налог в налоговый пул
    function AddTaxToPool(uint256 tax) internal {
        _currentTaxPool += tax;
    }

    // если истина, то за транзакцию можно получить вознаграждение
    function IsRewardedTransaction(
        uint256 accountBalance,
        uint256 transactionAmount
    ) public view returns (bool) {
        // если на акке небыло ничего то он награды не получает
        if (accountBalance == 0) return false;
        // сколько процентов транзакция из общего баланса аккаунта 1%=1e18
        uint256 accountTransactionPercent = PRBMathUD60x18.div(
            transactionAmount,
            accountBalance
        );
        return
            accountTransactionPercent >= _MinBalancePercentTransactionForReward;
    }

    // обновляет инфу о том, кто и сколько получил награды
    // бросает эвент OnReward, если было не нуливое вознаграждение
    // возвращает сколько налога должен получить указанный аккаунт
    // возвращает 0 - если вознаграждения нет
    // за 1 налоговый интервал вернет значение >0 только раз
    // accountBalance - баланс аккаунта
    // в 0 налоговом интервале налоги не раздаются
    function UpdateAccountReward(
        address account, // аккаунт
        uint256 accountBalance, // баланс аккаунта
        uint256 transactionAmount // сумма перевода
    ) internal returns (uint256) {
        // limiter if the time of the last receipt of tax is greater than or equal to the time of the last transition to a new taxation cycle
        if (account == address(0)) return 0;
        // если транзакция меньше мин процента то она не считается
        if (!IsRewardedTransaction(accountBalance, transactionAmount)) {
            //console.log("is not rewarded transaction!");
            return 0;
        }
        // запоминаем, что выполнили транзакцию, которая может участвовать в раздаче вознаграждения
        //console.log("rewarded transaction!");
        _LastRewardedTransactions[account] = block.timestamp;
        // ограничитель, если аккаунт уже получал награду
        if (_taxGettings[account] >= _lastTaxTime + 1 days) return 0;
        // если 0 налоговый интервал
        if (_TaxIntervalNumber == 0) return 0;
        // не нужно?
        if (_lastTotalOnAccounts == 0) return 0;
        // все раздали
        if (_givedTax >= _lastTaxPool) return 0; // if you have issued more than the tax pool, then we will not issue anything further
        // пишем, когда получал награду (или ее не получил, тк след ограничители сработали)
        _taxGettings[account] = block.timestamp;

        // сколько процентов на моментсмены налогового интервала от всей эмиссии имел аккаунт
        uint256 percentOfTotal = PRBMathUD60x18.div(
            accountBalance,
            _lastTotalOnAccounts
        );

        // calculate reward
        uint256 reward = PRBMathUD60x18.mul(percentOfTotal, _lastTaxPool);

        // расчет коэффициента умножения налога (чем ближе у аккаунта к 1% от всех аккаунтов тем меньше коэффициент те 1%=0 0%=1e18)
        reward = PRBMathUD60x18.mul(
            reward,
            GetRewardDynamicKoef(accountBalance)
        );

        // we increase the counter of the issued tax
        _givedTax += reward;
        //console.log("reward=", reward);
        if (reward > 0) emit OnReward(account, reward);
        return reward;
    }

    // возвращает коэффициент наград
    // 1 (1e18) - когда 0 от общего числа на аккаунтах
    // 0 - когда 1% отобщей эмиссии (на руках)
    function GetRewardDynamicKoef(uint256 accountBalance)
        public
        view
        returns (uint256)
    {
        // сколько процентов на моментсмены налогового интервала от всей эмиссии имел аккаунт
        uint256 percentOfTotal = PRBMathUD60x18.div(
            accountBalance,
            _lastTotalOnAccounts
        );

        if (percentOfTotal >= _RewardStopAccountPercent) return 0;
        return
            1e18 -
            PRBMathUD60x18.div(percentOfTotal, _RewardStopAccountPercent);
    }

    // возвращает налог на указанное количество токенов
    function GetTax(
        uint256 numTokens, // сколько переводим
        uint256 accountBalance, // баланс на аккаунте, который переводит
        uint256 totalOnAccounts // вся эмиссия без обменника
    ) public view returns (uint256) {
        // вычисляем налог
        uint256 tax = GetStaticTax(numTokens) +
            GetDynamicTax(numTokens, accountBalance, totalOnAccounts);
        // ограничиваем налог сверху
        uint256 maxTax = PRBMathUD60x18.mul(numTokens, _MaxTaxPercent);
        if (tax > maxTax) tax = maxTax;
        // вывод результата
        return tax;
    }

    // возвращает статическую составляю налога на транзакцию
    function GetStaticTax(
        uint256 numTokens // сколько переводим
    ) public view returns (uint256) {
        return PRBMathUD60x18.mul(numTokens, _ConstantTaxPercent);
    }

    // возвращает динамическую составляющую налога на транзакцию
    function GetDynamicTax(
        uint256 numTokens, // сколько переводим
        uint256 accountBalance, // баланс на аккаунте, который переводит
        uint256 totalOnAccounts // вся эмиссия без обменника
    ) public pure returns (uint256) {
        // если тотальная эмиссия 0
        if (totalOnAccounts == 0) return 0;

        // расчет динамической составляющей налога (от размера транзакции 100% всего токена заплатит 100% сверху)
        // коэффициент размера транзакции
        uint256 transactionK = PRBMathUD60x18.div(numTokens, totalOnAccounts);
        // добавляем налог
        uint256 tax = PRBMathUD60x18.mul(transactionK, numTokens);

        // расчет динамической составляющей налога (от размера кошелька аккаунт из 100% всего токена заплатит 100% сверху)
        transactionK = PRBMathUD60x18.div(accountBalance, totalOnAccounts);
        // добавляем налог
        tax += PRBMathUD60x18.mul(transactionK, numTokens);

        // вывод результата
        return tax;
    }

    // возвращает процент константной составляющей налога 100%=1e18
    function GetConstantTaxPercent() public view returns (uint256) {
        return _ConstantTaxPercent;
    }

    // возвращает при каком проценте динамическая награда становится 0 1e18=100%
    function GetRewardStopAccountPercent() public view returns (uint256) {
        return _RewardStopAccountPercent;
    }

    // максимальный процент налога 100%=1e18
    function GetMaxTaxPercent() public view returns (uint256) {
        return _MaxTaxPercent;
    }
}

