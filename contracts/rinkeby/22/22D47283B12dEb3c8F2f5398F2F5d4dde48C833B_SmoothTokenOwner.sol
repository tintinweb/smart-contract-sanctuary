// токен эталон
contract SmoothToken is LimboContract("Smooth", "SMTH") {
    using PRBMathUD60x18 for uint256;
    //using Pricing for Pricing.PricingData;
    using SellLimit for SellLimit.SellLimitData;
    using GlobalLimits for GlobalLimits.GlobalLimitsData;
    using SafeMath for uint256;

    // настройки
    uint256 constant MAX_PRICE_FALL_PERCENT = 1e15; // 1e18=100% макс процент, на который может обрушить цену один аккаунт при одной транзакции
    uint256 constant MAX_FALL_PERCENT_GLOBAL = 1e16; // (1%)how much max the rate can collapse
    uint256 constant PRICE_CONTROL_TIME_INTERVAL_MINUTES = 60; // time interval for price control (after this interval, a trace price measurement occurs)
    uint256 constant MIN_CELL_INTERVAL_SEC = 60; // мин интервал, после которого можно делать продажу
    uint256 constant MAX_ONE_ACCOUNT_AMMOUNT_PERCENT = 1e16; // сколько макс от totalSupply может быть на одном кошельке (100%=1e18)

    // события
    event OnIncrementGlobalLimits(uint256 maxFallPercent); // инкремент глобальных лимитов
    event OnDecrementGlobalLimits(uint256 maxFallPercent); // декремент глобальных лимитов
    event OnIncrementSellLimit(uint256 maxFallPercent); // лимит обрушения курса при продаже увеличен
    event OnDecrementSellLimit(uint256 maxFallPercent); // лимит обрушения курса при продаже уменьшен

    uint256 public _TotalOnAccounts; // сколько всего токена есть на всех аккаунтах (кроме налоговых пулов)
    mapping(address => uint256) _LastBuyTimes; // во сколько была последняя покупка на каждый аккаунт (кроме обменника)
    //Pricing.PricingData internal _Pricing; // текущая цена токена
    SellLimit.SellLimitData internal _SellLimit; // current limit on token withdraw for single account
    GlobalLimits.GlobalLimitsData internal _GlobalLimits; // global limit on token withdraw
    IUniswapV2Router02 public _UniswapRouter =
        IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D); // адрес роутера юнисвапа
    IUniswapV2Pair public _UniswapV2Pair; // пара

    constructor() {
        // инициализация ограничителя обвала курса
        _SellLimit.Initialize(MAX_PRICE_FALL_PERCENT);
        // инициализация библиотеки, определяющей уменьшение или увеличение максобвала курса
        _GlobalLimits.Initialize(
            MAX_FALL_PERCENT_GLOBAL,
            PRICE_CONTROL_TIME_INTERVAL_MINUTES * 1 minutes,
            1 // мин возможное число
        );
    }

    // текущая средняя цена
    function GetCurrentMidPrice() public view returns (uint256) {
        if (address(_UniswapRouter) == address(0)) return 0;
        (uint256 RouterToken, uint256 RouterEth, ) = _UniswapV2Pair.getReserves();
        return PRBMathUD60x18.div(RouterToken, RouterEth);
    }

    // сколько суммарно есть токена, вместе с налогом, кроме лимбо
    function totalSupply() public view override returns (uint256) {
        return _TotalOnAccounts + GetTotalTax();
    }

    // сколько макс от totalSupply может быть на одном кошельке (100%=1e18)
    function GetMaxOneAccountPercent() public pure returns (uint256) {
        return MAX_ONE_ACCOUNT_AMMOUNT_PERCENT;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        // ограничители
        require(from != to, "ERROR: Sending to yourself is not allowed");

        // обработка минта и берна
        if (from == address(0)) {
            _TotalOnAccounts += amount;
            return;
        }
        if (to == address(0)) {
            _TotalOnAccounts = _TotalOnAccounts.sub(amount);
            return;
        }

        // обновление глобальных лимитов и зависимых от них локальных лимитов
        UpdateSellLimits(GetCurrentMidPrice());

        // ограничитель быстрой продажи
        QuickSaleLimit(from, to);
        // ограничитель макс возможного кошелька
        MaxAccountAmmountLimit(from, to, amount);
        // получаем количество токена и эфира на роутере
        (uint256 RouterToken, uint256 RouterEth, ) = _UniswapV2Pair.getReserves();
        // ограничитель обвала курса (ограничения продажи)
        if (from != address(0) && to == address(_UniswapRouter)) {
            _SellLimit.CheckLimit(
                RouterEth,
                RouterToken,
                amount
            );
        }

        // делаем каждый аккаунт известным
        if (from != address(_UniswapRouter)) TryAddToAccountList(from);
        if (to != address(_UniswapRouter)) TryAddToAccountList(to);

        // попытка перехода к новому налоговому интервалу
        TryNextTaxInterval(_TotalOnAccounts.sub(RouterToken));

        // минт, берн и передача с роутера далее не обрабатывается (далее обработка если токен передается, но не с обменника)
        if (from == address(0) || to == address(0) || from == address(_UniswapRouter))
            return;

        // попытка выдать вознаграждение и выйти из лимбо
        if (from != address(_UniswapRouter)) TryGetRewardAndLimboOut(from, amount);
        if (to != address(_UniswapRouter)) TryGetRewardAndLimboOut(to, amount);

        // вычисляем налог
        uint256 tax = GetTax(amount, balanceOf(from), _TotalOnAccounts);

        // попытка замайнить лимбо с других аккаунтов (в размере половины налога)
        uint256 minedLimbo = LimboMining(from, tax / 2);
        // уменьшаем налог, на размер замайненого лимбо
        tax = tax.sub(minedLimbo);
        // жжем налог с транзакции
        if (tax > 0) _burn(from, tax);
        // добавляем собранный налог в налоговый пул
        AddTaxToPool(tax / 2);
    }

    // обновление глобальных лимитов и зависимые от них локальных лимитов
    function UpdateSellLimits(uint256 newPrice) internal {
        //console.log("newPrice: ", newPrice);
        int256 globalLimits = _GlobalLimits.Update(newPrice);
        if (globalLimits > 0) {
            if (globalLimits == 2)
                emit OnIncrementGlobalLimits(_GlobalLimits._MaxFallPercent);
            if (_SellLimit.IncrementLimits())
                emit OnIncrementSellLimit(_SellLimit.MaxFallPercent);
        }
        if (globalLimits < 0) {
            //console.log(2);
            if (globalLimits == -2)
                emit OnDecrementGlobalLimits(_GlobalLimits._MaxFallPercent);
            if (_SellLimit.DecrementLimits())
                emit OnDecrementSellLimit(_SellLimit.MaxFallPercent);
        }
        //console.log(3);
    }

    // производит проверку аккаунта на предмет быстрой продажи
    // при обнаружении быстрой продажи выдает исключение "quick sale"
    function QuickSaleLimit(address from, address to) internal {
        // ограничители
        if (from == address(0) || to == address(0) || from == to) return;
        // если была покупка с обменника то запоминаем когда она была и далее ничего не делаем
        if (from == address(_UniswapRouter)) {
            _LastBuyTimes[to] = block.timestamp; // запоминаем когда была последняя покупка
            return;
        }
        // если идет передача на обменник (продажа токена)
        if (to == address(_UniswapRouter)) {
            uint256 timeInterval = block.timestamp - _LastBuyTimes[from];
            require(
                timeInterval >= MIN_CELL_INTERVAL_SEC * 1 seconds,
                "quick sale"
            );
        }
    }

    // ограничитель макс возможного кошелька
    // выдает ошибку "account has too much token" если на адрес to нельзя добавить addamount токенов
    function MaxAccountAmmountLimit(
        address from, // от кого
        address to, // кому
        uint256 addamount // сколько добавляется
    ) internal view {
        if (from == address(0) || to == address(_UniswapRouter)) return;
        require(
            balanceOf(to) + addamount <= MaxAccountAmmount(),
            "account has too much token"
        );
    }

    // сколько токена можно держать на одном аккаунте
    function MaxAccountAmmount() public view returns (uint256) {
        return
            PRBMathUD60x18.mul(totalSupply(), MAX_ONE_ACCOUNT_AMMOUNT_PERCENT);
    }

    // возвращает макс интервал в секундах, после которого можно совершить продажу после покупки
    function GetMinCellIntervalSec() public pure returns (uint256) {
        return MIN_CELL_INTERVAL_SEC;
    }

    // максимальный процент, на который можно обвалить курс 1e18=1%
    function GetMaxFallPercent() public view returns (uint256) {
        return _SellLimit.MaxFallPercent;
    }
}

