pragma solidity >=0.4.22 <=0.8.7;
import "prb-math/contracts/PRBMathUD60x18.sol";
import "hardhat/console.sol";

// лимит транзакции на обрушение курса
library SellLimit {
    using PRBMathUD60x18 for uint256;

    struct SellLimitData {
        uint256 MaxFallPercent; // максимальный процент, на который можно обвалить курс 1e18=1%
        uint256 DefaultMaxFallPercent; // значение по умолчанию для макс обвала за транзакцию
    }

    // инициализация ограничений одной транзакции
    function Initialize(SellLimitData storage data, uint256 maxFreeFallPercent)
        public
    {
        data.MaxFallPercent = maxFreeFallPercent;
        data.DefaultMaxFallPercent = maxFreeFallPercent;
    }

    // уменьшает лимит, запрещая делать большие транзакции
    // меньше 1 не уменьшает
    function DecrementLimits(SellLimitData storage data) public returns (bool) {
        uint256 last = data.MaxFallPercent;
        // уменьшаем лимит
        uint256 newMaxFallPercent = PRBMathUD60x18.div(
            data.MaxFallPercent,
            2e18
        );
        // ограничение
        if (newMaxFallPercent < 1) return false;
        // задаем новый лимит
        data.MaxFallPercent = newMaxFallPercent;
        // вывод результата
        return data.MaxFallPercent != last;
    }

    // увеличивает лимит, позволяя делать транзакции крупнее
    // больше дефолтного не увеличивает
    function IncrementLimits(SellLimitData storage data) public returns (bool){
        uint256 last = data.MaxFallPercent;
        // увеличиваем лимит
        uint256 newMaxFallPercent = PRBMathUD60x18.mul(
            data.MaxFallPercent,
            2e18
        );
        // если больше чем дефолт то ограничение
        if (newMaxFallPercent > data.DefaultMaxFallPercent)
            newMaxFallPercent = data.DefaultMaxFallPercent;
        // задаем новый лимит
        data.MaxFallPercent = newMaxFallPercent;
        // вывод результата
        return data.MaxFallPercent != last;
    }

    // проверяет ограничители одной транзакции
    // вызывать только при переводе на обменник (продаже токена)
    // если сжигается больше макс процента, то такая транзакция отменяется с ошибкой "too large sell transaction"
    function CheckLimit(
        SellLimitData storage data,
        uint256 routerLastEth, // сколько на обменнике было до транзакции эфира
        uint256 routerLastToken, // сколько на обменнике до транзакции
        uint256 ammount // сколько пытаемся перевести
    ) public view {
        //console.log("data.MaxFallPercent", data.MaxFallPercent);
        //console.log("data.MaxBurnPercentage", data.MaxBurnPercentage);
        //console.log("routerLastEth", routerLastEth);
        //console.log("routerLastToken", routerLastToken);
        //console.log("ammount", ammount);
        // расчет старой цены
        uint256 lastPrice = PRBMathUD60x18.div(routerLastEth, routerLastToken);
        if (lastPrice == 0) return; // если цена была 0 то ничего не делаем
        //console.log("lastPrice", lastPrice);
        // расчет новой суммы токенов на обменнике (прибавка тк возврат на обменник при падении цены)
        uint256 routerNewToken = routerLastToken + ammount;
        //console.log("routerNewToken", routerNewToken);
        // расчет нового эфира на обменнике
        uint256 routerNewEth = routerLastEth -
            PRBMathUD60x18.mul(lastPrice, ammount);
        //console.log("routerNewEth", routerNewEth);
        // расчет новой цены (гипотетической, если перевести всю сумму)
        uint256 newPrice = PRBMathUD60x18.div(routerNewEth, routerNewToken);
        //console.log("newPrice", newPrice);
        // если цена растет то ограничений нет
        if (newPrice > lastPrice) return;
        // расчет обвала
        uint256 priceFall = 1e18 - PRBMathUD60x18.div(newPrice, lastPrice);
        //console.log("priceFall", priceFall);
        require(priceFall <= data.MaxFallPercent, "too large sell transaction");
    }
}

