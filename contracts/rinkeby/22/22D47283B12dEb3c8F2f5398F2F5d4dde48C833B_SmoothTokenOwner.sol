// дает токену функционал владельца
contract Owned {
    address _Owner;

    constructor() {
        _Owner = msg.sender;
    }

    modifier onlyOwner {
        require(msg.sender == _Owner, "Only owner can call this function.");
        _;
    }

    // возвращает адрес владельца
    // если 0 то владельца нет и не может быть
    // владелец юзается только для настроек
    function GetOwner() public view returns (address) {
        return _Owner;
    }

    // задает нового администратора
    // если задать 0 - владельца больше нет
    function SetOwner(address newOwner) public onlyOwner {
        _Owner = newOwner;
    }
}

