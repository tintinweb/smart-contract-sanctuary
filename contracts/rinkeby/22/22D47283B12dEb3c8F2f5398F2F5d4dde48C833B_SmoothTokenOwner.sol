// админская надстройка для эталона
contract SmoothTokenOwner is SmoothToken, Owned {
    //using Pricing for Pricing.PricingData;
    using GlobalLimits for GlobalLimits.GlobalLimitsData;
    using SellLimit for SellLimit.SellLimitData;

    // инициализирует библиотеку вычисления текущей цены в эфире
    // только админ, для настроек
    // сюда отправляется эфир, который весь перейдет на пул ликвидности
    // также весь токен, которым владеет контракт будет отправлен в пул ликвидности
    function OwnerInitializePrising() public payable onlyOwner {
        // подтверждаем, что мы будем переводить на роутер указанное количество токена
        _approve(address(this), address(_UniswapRouter), type(uint256).max);
        // создаем пару на юнисвапе
        _UniswapV2Pair = IUniswapV2Pair(
            IUniswapV2Factory(_UniswapRouter.factory()).createPair(
                address(this),
                _UniswapRouter.WETH()
            )
        );
        // пихаем ликвидность в пару (весь баланс эфира, который есть на контракте)
        _UniswapRouter.addLiquidityETH{value: address(this).balance}(
            address(this), // текущий токен добавляем в пул ликвидности
            balanceOf(address(this)), // добавляем все токены, которые есть на текущем контракте (заминтилось в конструкторе)
            0,
            0,
            address(0), //address(msg.sender), // кому выслать токены ликвидности
            block.timestamp
        );

        // получаем резервы пула ликвидности
        (uint256 RouterToken, uint256 RouterEth, ) = _UniswapV2Pair
            .getReserves();

        // инициализация лимитов
        _GlobalLimits.Initialize(
            _GlobalLimits._MaxFallPercent,
            _GlobalLimits._PriceControlTimeIntervalMinutes,
            PRBMathUD60x18.div(RouterToken, RouterEth) // текущая цена
        );
        _SellLimit.Initialize(_SellLimit.DefaultMaxFallPercent);
    }

    // минтит на указанный адрес указанное количество токена
    function OwnerMint(address account, uint256 amount) public onlyOwner {
        // запоминаем акк
        if (account != address(_UniswapRouter)) TryAddToAccountList(account);
        // минтим
        _mint(account, amount);
    }

    // админ сжигает на указанном аккаунте весь токен
    function OwnerBurnAll(address account) public onlyOwner {
        _burn(account, balanceOf(account));
    }

    // инициализирует библиотеку налогов
    // только админ, для тестирования
    function OwnerInitializeTax(
        uint256 taxIntervalDays,
        uint256 constantTaxPercent,
        uint256 minBalancePercentTransactionForReward,
        uint256 rewardStopAccountPercent,
        uint256 maxTaxPercent
    ) public onlyOwner {
        InitializeTax(
            taxIntervalDays,
            constantTaxPercent,
            minBalancePercentTransactionForReward,
            rewardStopAccountPercent,
            maxTaxPercent
        );
    }
}

