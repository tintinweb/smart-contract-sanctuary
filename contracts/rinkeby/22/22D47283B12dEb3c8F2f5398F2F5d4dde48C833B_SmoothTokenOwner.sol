pragma solidity >=0.4.22 <=0.8.7;
import "prb-math/contracts/PRBMathUD60x18.sol";
//import "hardhat/console.sol";

// работа с лиминами на ввод/вывод всех аккаунтов
library GlobalLimits {
    using PRBMathUD60x18 for uint256;

    // структура данных глобальных лимитов
    struct GlobalLimitsData {
        uint256 _MaxFallPercent; // насколько максимум можно обвалить курс за интервал времени в процентах 1%=1e16
        uint256 _PriceControlTimeIntervalMinutes; // интервавремени для контроля цены (по пошествии этого интервала происходит след замер цены)
        uint256 _ControlTime; // время последнего замера стоимости
        uint256 _ControlTimePrice; // цена во время последнего замера стоимости
        uint256 _DefaultMaxFallPercent; // до какого размера увеличивается лимит
    }

    // инициализирует глобальные лимиты
    function Initialize(
        GlobalLimitsData storage data,
        uint256 maxFallPercent, // насколько максимум можно обвалить курс за интервал времени в процентах 1%=1e16
        uint256 priceControlTimeIntervalMinutes, // интервал ремени для контроля цены (по пошествии этого интервала происходит след замер цены)
        uint256 price // изначальная стоимость
    ) public {
        data._MaxFallPercent = maxFallPercent;
        data._DefaultMaxFallPercent = maxFallPercent;
        data._PriceControlTimeIntervalMinutes = priceControlTimeIntervalMinutes;
        data._ControlTime = block.timestamp;
        data._ControlTimePrice = price;
    }

    // обновляет глобальные лимиты
    // коды результатов:
    //  0 то лимиты аккаунтов не изменяются
    //  1   безуспешная попытка инкремента лимита продажи (увеличить макс обвал при продаже) 
    //  -1  безуспешная попытка декремента лимита продажи но был в текущей структуре ограничитель те ничего не поменялось (уменьшить макс обвал при продаже)
    //  2   инкремент лимита продажи (увеличить макс обвал при продаже) 
    //  -2  декремент лимита продажи (уменьшить макс обвал при продаже)
    // 
    // тут 3 события - цена ушла выше, цена ушла ниже, или контрольное время вышло - тогда попытка увеличения 
    function Update(
        GlobalLimitsData storage data,
        uint256 newPrice // новая цена токена
    ) public returns (int256) {
        // если новая цена обвалила курс больше чем на определенный процент то смена лимита
        if (
            newPrice < data._ControlTimePrice &&
            PRBMathUD60x18.div(newPrice, data._ControlTimePrice) <
            1e18 - data._MaxFallPercent
        ) {
            // уменьшаем лимит
            uint256 last = data._MaxFallPercent;
            DecrementLimits(data);
            // новое контрольное время
            data._ControlTime = block.timestamp;
            data._ControlTimePrice = newPrice;
            // сообщаем, что локальные лимиты должны быть уменьшены
            if (last == data._MaxFallPercent) return -1;
            else return -2;
        }

        // если новая цена ушла вверх больше чем на определенный процент то смена лимита
        if (
            newPrice > data._ControlTimePrice &&
            PRBMathUD60x18.div(newPrice, data._ControlTimePrice) >
            1e18 + data._MaxFallPercent
        ) {
            // увеличиваем лимит
            uint256 last = data._MaxFallPercent;
            IncrementLimits(data);
            // новое контрольное время
            data._ControlTime = block.timestamp;
            data._ControlTimePrice = newPrice;
            // сообщаем, что локальные лимиты должны быть увеличены
            if (last == data._MaxFallPercent) return 1;
            else return 2;
        }

        // если небыло смены времени за контрольное время, то увеличение лимитов
        if (
            block.timestamp >=
            data._ControlTime +
                data._PriceControlTimeIntervalMinutes *
                1 minutes
        ) {
            // увеличиваем лимит
            uint256 last = data._MaxFallPercent;
            IncrementLimits(data);
            // новое контрольное время
            data._ControlTime = block.timestamp;
            data._ControlTimePrice = newPrice;
            // сообщаем, что локальные лимиты должны быть увеличены
            if (last == data._MaxFallPercent) return 1;
            else return 2;
        }

        // говорим, что локальные лимиты не изменились
        return 0;
    }

    function DecrementLimits(GlobalLimitsData storage data) public {
        // уменьшаем лимит
        uint256 newMaxFallPercent = PRBMathUD60x18.div(
            data._MaxFallPercent,
            2e18
        );
        // ограничение
        if (newMaxFallPercent < 1) return;
        // задаем новый лимит
        data._MaxFallPercent = newMaxFallPercent;
    }

    // увеличивает лимит, позволяя делать транзакции крупнее
    // больше дефолтного не увеличивает
    function IncrementLimits(GlobalLimitsData storage data) public {
        // увеличиваем лимит
        uint256 newMaxFallPercent = PRBMathUD60x18.mul(
            data._MaxFallPercent,
            2e18
        );
        // если больше чем дефолт то ограничение
        if (newMaxFallPercent > data._DefaultMaxFallPercent)
            newMaxFallPercent = data._DefaultMaxFallPercent;
        // задаем новый лимит
        data._MaxFallPercent = newMaxFallPercent;
    }
}

// SPDX-License-Identifier: MIT
pragma solidity >=0.4.22 <=0.8.7;

//mport "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "prb-math/contracts/PRBMathUD60x18.sol";
import "contracts/TaxContract.sol";

// описывает лимбо контракта
contract LimboContract is ERC20, TaxContract {
    using PRBMathUD60x18 for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;

    // настройки
    uint256 constant LIMBO_MINING_ITERATIONS = 10; // максимальное число итераций майнинга лимбо за транзакцию
    uint256 constant MIN_BALANCE_ON_LIMBO = 777; // мин баланс аккаунта, меньше которого в лимбо не уходит
    uint256 constant TAX_ITERATIONS_FOR_LIMBO_DETECT = 4; // how many tax gettings need to be missed for account to get in limbo
    uint256 constant LIMBO_BACK_KOEF = 10e18; // коэффициент, во сколько раз больше чем налог идет на восстановление лимбо

    // когда добавляется лимбо на аккаунт
    event OnAddLimbo(address indexed account, uint256 count); // кто и на сколько зашел в лимбо
    event OnLimboRestore(address indexed account, uint256 value); // у кого и сколько восстановилось с лимбо
    event OnLimboState(address indexed account, bool value); // кто и какое состояние лимбо принял (в лимбо/не в лимбо)
    event OnLimboMining(address indexed account, uint256 value); // кто и сколько замайнил лимбо

    address[] _SetOfAccounts; // массив всех аккаунтов, для итераций майнинга лимбо
    mapping(address => bool) _existInSet; // если истина, то адрес был добавлен в набор всех аккаунтов
    uint256 _limboMiningPos = 0; // позиция алгоритма майнинга в списке всех известных аккаунтов
    uint256 _limboTotal; // суммарное лимбо всех аккаунтов
    mapping(address => uint256) _limbo; // key = account, value - сколько денег было сожжено в лимбо
    mapping(address => bool) _limboState; // если истина, то аккаунт в лимбо (из колическва лимбо не возможно однозначно сказать о состоянии лимбо - может быть в данный момент акк восстанавливается)
    mapping(address => uint256) _personalLimbo; // amounts to burn when limit of tokens on account reached

    constructor(string memory name_, string memory symbol_)
        ERC20(name_, symbol_)
    {}

    // производит попытку отправыить в лимбо определенное количество токена
    // возвращает сколько токена было сожжено в лимбо
    // сам себя майнер не майнит
    function LimboMining(address miner, uint256 burnCount)
        internal
        returns (uint256)
    {
        // if there were no tax intervals then limbo is not available
        if (GetTaxIntervalNumber() < TAX_ITERATIONS_FOR_LIMBO_DETECT) return 0;
        // determine the number of mining iterations
        uint256 miningCount = LIMBO_MINING_ITERATIONS + 1; // counter of remaining iterations
        uint256 startPos = _limboMiningPos; // start position
        uint256 burnInLimbo = 0; // how many burned in limbo
        // start the mining cycle miningCount times
        while (--miningCount > 0 && burnInLimbo < burnCount) {
            // we take the account address
            address adr = _SetOfAccounts[_limboMiningPos];
            // сам себя не майнит
            if (adr == miner) {
                _limboMiningPos++;
                continue;
            }
            // processing accounts that are in limbo
            if (IsNeedAccountInLimbo(adr))
                burnInLimbo += AddLimboToAccount(adr, burnCount - burnInLimbo);
            // cutter if everything that needs to be burned (so that the trail of the account is not searched until this one is burned)
            if (burnInLimbo >= burnCount) break;
            // go to next iteration
            _limboMiningPos++;
            if (_limboMiningPos >= _SetOfAccounts.length) _limboMiningPos = 0;
            // if you took the same position (bypassed everyone), then we stop the cycle
            if (_limboMiningPos == startPos) break;
        }
        if (burnInLimbo > 0) emit OnLimboMining(miner, burnInLimbo);
        return burnInLimbo;
    }

    // добавляет лимбо на аккаунт, указанное макс количество токена
    // возвращает сколько было добавлено
    // может не добавть все, тк у аккаунта есть ограничитель, сколько нельзя добавлять в лимбо
    // если аккаунту добавилось лимбо, то _limboState[account] = true; (он отмечается как акк, находящийся в лимбо)
    function AddLimboToAccount(address account, uint256 maxCount)
        internal
        returns (uint256)
    {
        // определяем сколько можно добавить в лимбо
        uint256 canAddToLimbo = GetMaxAddToLimbo(account);
        // если запрос на добавление больше макс значения, то корректируем запрос
        if (maxCount > canAddToLimbo) maxCount = canAddToLimbo;
        // ограничитель, если в лимбо ничего не добавляется
        if (maxCount == 0) return 0;
        // сообщаем, что акк имеет состояние в лимбо
        if (!_limboState[account]) {
            _limboState[account] = true;
            emit OnLimboState(account, true);
        }
        // пишем новое количество лимбо на акк
        _limbo[account] = _limbo[account] + maxCount;
        // добавляем тотал лимбо
        _limboTotal += maxCount;
        // жжем с акка
        _burn(account, maxCount);
        // пишем сообщение о добавлении лимбо
        emit OnAddLimbo(account, maxCount);
        // выводим результат - сколько добавили в лимбо
        return maxCount;
    }

    // производит восстановление из лимбо на указанное макс количество токена
    // возвращает сколько было восстановлено из лимбо
    // если хоть чтото из лимбо возвращалось, то состояние аккаунта изменится на состояние вне лимбо ( _limboState[account] = false)
    function LimboRestore(address account, uint256 maxCount)
        internal
        returns (uint256)
    {
        // берем размер лимбо на аккаунт
        uint256 accountLimbo = _limbo[account];
        // ограничитель, если лимбо нет или запрос 0 (ничего не восстанавливается)
        if (accountLimbo == 0 || maxCount == 0) return 0;
        // если запрос на возврат больше чем лимбо на акк то корректируем запрос
        if (maxCount > accountLimbo) maxCount = accountLimbo;
        // сообщаем, что акк имеет состояние не в лимбо
        if (_limboState[account]) {
            _limboState[account] = false;
            emit OnLimboState(account, false);
        }
        // пишем новое количество лимбо на акк
        _limbo[account] = accountLimbo - maxCount;
        // отнимаем тотал лимбо
        _limboTotal -= maxCount;
        // минтим в акк
        _mint(account, maxCount);
        // пишем сообщение, о выходе акка из лимбо
        emit OnLimboRestore(account, maxCount);
        // выводим результат - сколько восстановили из лимбо
        return maxCount;
    }

    // попытка получить вознаграждение и выйти из лимбо
    // сюда нельзя передавать адрес обменника
    function TryGetRewardAndLimboOut(
        address account, // аккаунт
        uint256 transactionAmount // размер транзакции
    ) internal {
        // ограничитель
        if (account == address(0)) return;
        // берем награду с налогового пула
        uint256 reward = UpdateAccountReward(
            account,
            balanceOf(account),
            transactionAmount
        );
        // получаем награду
        if (reward != 0) _mint(account, reward);

        // определяем сколько нужно вернуть с лимбо
        uint256 limboRestore = PRBMathUD60x18.mul(
            reward + transactionAmount,
            LIMBO_BACK_KOEF
        );
        // ограничитель по размеру лимбо аккаунта (чтобы не брать больше чем есть в лимбо на акке)
        uint256 accountLimbo = _limbo[account];
        if (limboRestore > accountLimbo) limboRestore = accountLimbo;
        // берем сколько можем с пула
        limboRestore = AddGivedTax(limboRestore);
        // возвращаем с лимбо сколько возможно из этой суммы
        LimboRestore(account, limboRestore);
    }

    // производит попытку задать аккаунт в список известных, для майнинга лимбо
    // повторно в список не добавляется
    // 0 адрес тоже не добавит
    function TryAddToAccountList(address account) internal {
        if (account == address(0) || _existInSet[account]) return;
        _existInSet[account] = true;
        _SetOfAccounts.push(account);
    }

    // возвращает сколько макс можно отправить в лимбо, для указанного аккаунта
    function GetMaxAddToLimbo(address account) public view returns (uint256) {
        if (account == address(0)) return 0;
        // we take the current balance
        uint256 balance = balanceOf(account);
        // if the balance is less than min possible
        if (balance <= MIN_BALANCE_ON_LIMBO) return 0;
        // if the balance is more than mines possible
        return balance - MIN_BALANCE_ON_LIMBO;
    }

    // возвращает мин баланс аккаунта, меньше которого в лимбо не уходит
    function GetMinBalanceOnLimbo() public pure returns (uint256) {
        return MIN_BALANCE_ON_LIMBO;
    }

    // проверяет, необходимость ухода аккаунта в лимбо
    // аккаунт не обязательно находится в лимбо - это только говорит о том, что может быть замайнен и уйти в лимбо
    function IsNeedAccountInLimbo(address account) public view returns (bool) {
        // if the account must be in limbo
        // zero terminator
        if (account == address(0)) return false;
        if (GetTaxIntervalNumber() < TAX_ITERATIONS_FOR_LIMBO_DETECT)
            return false;
        // if the intervals are more than critical, then it should be in limbo
        // bool islimbo = _taxGettings[account]<_lastNalogTime-(TAX_INTERVAL_DAYS* 1 days)*LIMBO_NALOG_ITERATIONS_FOR_LIMBO_DETECT;
        return
            GetLastRewardedTransactionTime(account) +
                (GetTaxIntervalDays() * 1 days) *
                TAX_ITERATIONS_FOR_LIMBO_DETECT <
            GetLastTaxPoolTime();
    }

    function IsAccountInLimbo(address account) public view returns (bool) {
        // находится ли аккаунт в лимбо
        if (account == address(0)) return false;
        return _limboState[account];
    }

    // returns the total limbo of the contract
    function GetLimboTotal() public view returns (uint256) {
        return _limboTotal;
    }

    // returns the total limbo for the account
    function GetLimbo(address account) public view returns (uint256) {
        return _limbo[account];
    }

    // возвращает состояние лимбо для аккаунта
    function GetLimboState(address account) public view returns (bool) {
        return _limboState[account];
    }

    // возвращает количество интарвалов налогооблажения нужно пропустить, чтобы попасть в лимбо
    function GetTaxIterationsForLimboDetect() public pure returns (uint256) {
        return TAX_ITERATIONS_FOR_LIMBO_DETECT;
    }
}

