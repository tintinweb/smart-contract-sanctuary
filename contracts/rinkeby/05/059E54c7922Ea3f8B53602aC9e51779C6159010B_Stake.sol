    function transferFrom(address sender, address recipient, uint256 amount) external returns (bool);
    function transfer(address recipient, uint256 amount) external returns (bool);
    function balanceOf(address account) external view returns (uint256);
}

interface CLAIMTOKEN {
    function transfer(address recipient, uint256 amount) external returns (bool);
}

contract Stake is Ownable {
    using SafeMath for uint256;
    using BokkyPooBahsDateTimeLibrary for uint256;

    // uint BIGNUMBER = 10**18;
    uint DECIMAL = 10**10;
    
    address public mainWallet;
    mapping(address => uint) public totalStakedTokens;

    mapping(address => mapping(address => bool)) public HasStake;

    struct stakeInfo{
        uint256 StartDate;
        uint256 LastWithdrawDate;
        uint256 Claimed;
        uint256 Staked;
        uint level;
    }

    struct levelInfo {
        uint period;
        uint dailyApy;
    }

    mapping (address => mapping(address => uint)) public remainclaimAmount;
    mapping (address => mapping(address => uint)) public claimpertoken;
    mapping (address => mapping(address => uint)) public totalClaimAmount;
    mapping (address => mapping(address => stakeInfo[])) public StakingInfo;

    mapping (uint => uint) public stakeLev;
    mapping (uint => levelInfo[]) LevelInfo;
    // mapping (address => mapping(address => uint)) public userCummRewardPerStake; //tokenAddr to user to remaining claimable amount per stake
    mapping (address => uint) public tokenCummRewardPerStake; //tokenAddr to cummulative per token reward since the beginning or time
    
    uint public dailyEarningPercent  = 20;
    
    // uint public stakeDuration = 300;

    // uint256 public limitStakeTokens = 61970000 * (10 ** 10);
    
    constructor() public {
        stakeLev[0] = 60;
        stakeLev[1] = 90;
        stakeLev[2] = 365;
    }
    

    function setLevelDays(uint level, uint period) public 
    {
        stakeLev[level] = period;
    }

    function createStake(address token, uint256 _amount, uint256 level, address avior) public 
    {
        address sender = msg.sender;
        require(_amount != 0);
        require(STAKETOKEN(token).balanceOf(sender) >= _amount, "should be greater than xxxx");

        uint count = StakingInfo[sender][token].length;
        // date now
        uint dateNow = now;
        // if (staked amount history is no)
        uint remainingClaimableAmount = 0;
        StakingInfo[sender][token].push(stakeInfo(dateNow, 0, 0, _amount, level));
        
        if(count == 0) {
            totalStakedTokens[token] =  _amount;
        } else {
            totalStakedTokens[token] = totalStakedTokens[token].add(_amount);
            for(uint i = 0; i < count; i++) {
                if(StakingInfo[sender][token][i].level == level) {
                    remainingClaimableAmount = StakingInfo[sender][token][i].Staked;
                    uint diffdays = dateNow - StakingInfo[sender][token][i].StartDate;
                    uint duration = stakeLev[level] - diffdays;
                    remainclaimAmount[sender][token] += diffdays.mul(remainingClaimableAmount).mul(stakeLev[level]).mul(dailyEarningPercent).div(10000);
                    claimpertoken[sender][token] += duration.mul(remainingClaimableAmount).mul(dailyEarningPercent).div(10000);
                    // StakingInfo[sender][token][i].StartDate = dateNow;
                } else {
                    claimpertoken[sender][token] += stakeLev[level].mul(StakingInfo[sender][token][i].Staked).mul(dailyEarningPercent).div(10000);
                }
            }
            claim(remainclaimAmount[sender][token], sender, avior);
        }
        totalClaimAmount[sender][token] = remainclaimAmount[sender][token] + claimpertoken[sender][token];
        STAKETOKEN(token).transferFrom(sender, address(this), _amount);
        HasStake[sender][token] = true;
    }

