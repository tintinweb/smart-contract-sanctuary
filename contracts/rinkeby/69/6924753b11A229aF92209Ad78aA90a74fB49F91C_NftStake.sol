    uint256 public tokensPerBlock;

    struct stake {
        uint256 tokenId;
        uint256 stakedFromBlock;
        address owner;
    }

    // TokenID => Stake
    mapping(uint256 => stake) public receipt;
    // TokenID => Bonus Percent
    mapping(uint256 => uint256) public _bonusPercent;

    event StakeRewardUpdated(uint256 rewardPerBlock);
    event Staked(address indexed staker, uint256 tokenId);
    event UnStaked(address indexed staker, uint256 tokenId);
    event StakePayout(address indexed staker, uint256 tokenId, uint256 stakeAmount);

    modifier onlyStakeOwner(uint256 tokenId, address holder) {
        require(nftToken.ownerOf(tokenId) == address(this), "SLEEP: Contract is not owner of this NFT");
        require(receipt[tokenId].stakedFromBlock != 0, "SLEEP: Token is not staked");
        require(receipt[tokenId].owner == holder, "SLEEP: Caller is not NFT stake owner");
        _;
    }

    constructor(IERC721 _nftToken, uint256 _tokensPerBlock, uint256 _initialSupply) ERC20("Sleep", "SLEEP") {
        nftToken = _nftToken;
        tokensPerBlock = _tokensPerBlock;
        _mint(msg.sender, _initialSupply);
    }

    function getBonusAdjustedPayout(uint256 basePayout, uint256 tokenId) public view returns (uint256) {
        uint256 bonusPercent = _bonusPercent[tokenId];
        return basePayout + (basePayout * bonusPercent) / 100;
    }

    function getCurrentStakeEarned(uint256 tokenId) external view returns (uint256) {
        return _getTimeStaked(tokenId) * tokensPerBlock;
    }

    function stakeNFT(uint256[] calldata tokenIds) external nonReentrant() {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _stakeNFT(tokenIds[i], msg.sender);
        }
    }

    function unStakeNFT(uint256[] calldata tokenIds) external nonReentrant() {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _unStakeNFT(tokenIds[i], msg.sender);
        }
    }

    function burn(uint256 amount) external {
        _burn(msg.sender, amount);
    }

    function harvest(uint256[] calldata tokenIds) external nonReentrant() {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _payoutStake(tokenIds[i], msg.sender);
        }
    }

    function updateStakingReward(uint256 _tokensPerBlock) external onlyOwner() {
        tokensPerBlock = _tokensPerBlock;
        emit StakeRewardUpdated(tokensPerBlock);
    }

    function setBonusPayout(uint256[] calldata tokenIds, uint256 bonusPercent) external onlyOwner() {
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _bonusPercent[tokenIds[i]] = bonusPercent;
        }
    }

    function _stakeNFT(uint256 tokenId, address holder) internal {
        require(nftToken.ownerOf(tokenId) != address(this), "SLEEP: Token is already staked");

        nftToken.safeTransferFrom(holder, address(this), tokenId);
        require(nftToken.ownerOf(tokenId) == address(this), "SLEEP: Failed to own NFT");

        receipt[tokenId].tokenId = tokenId;
        receipt[tokenId].stakedFromBlock = block.number;
        receipt[tokenId].owner = holder;

        emit Staked(holder, tokenId);
    }

    function _unStakeNFT(uint256 tokenId, address holder) internal {
        _payoutStake(tokenId, holder);

        // delete stake record, effectively unstaking it
        delete receipt[tokenId];
        nftToken.safeTransferFrom(address(this), holder, tokenId);

        emit UnStaked(holder, tokenId);
    }

    function _payoutStake(uint256 tokenId, address holder) internal onlyStakeOwner(tokenId, holder) {
        require(receipt[tokenId].stakedFromBlock > 0, "SLEEP: Can not stake from block 0");

        uint256 basePayout = _getTimeStaked(tokenId) * tokensPerBlock;
        uint256 payout = getBonusAdjustedPayout(basePayout, tokenId);

        receipt[tokenId].stakedFromBlock = block.number;
        _mint(receipt[tokenId].owner, payout);

        emit StakePayout(holder, tokenId, payout);
    }

    function _getTimeStaked(uint256 tokenId) internal view returns (uint256) {
        if (receipt[tokenId].stakedFromBlock == 0) {
            return 0;
        }

        return block.number - receipt[tokenId].stakedFromBlock;
    }

    function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }
}