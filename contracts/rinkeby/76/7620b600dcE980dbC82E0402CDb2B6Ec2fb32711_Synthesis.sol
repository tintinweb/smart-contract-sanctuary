  struct metaBurnTransaction {
    address syntCaller;
    address dex;
    address[] path;
    uint256 amount;
    address chain2address;
    address receiveSide;
    address oppositeBridge;
    uint256 chainID;
    uint256 slippage;
    uint256 deadline;
    bytes swapCallData;
  }
}

