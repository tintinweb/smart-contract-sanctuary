import 'hardhat/console.sol';
import '@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol';
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import '@openzeppelin/contracts/utils/math/SafeMath.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/access/Ownable.sol';
import '@chainlink/contracts/src/v0.8/interfaces/AggregatorV3Interface.sol';

contract ICO is Ownable, ReentrancyGuard {
    using SafeERC20 for IERC20;
    using SafeMath for uint;

    struct Reward {
        uint256 amountTokens;
        uint256 price;
    }

    struct ICOStateSchema {
        uint256 currentIteration;
        uint256 currentPrice;
        uint256 userAllocatedTokensAmount;
        uint256 ICOAllocatedTokensAmount;
        uint256 tokensLeft;
    }

    struct Investor {
        uint256 amount;
        bool allowed;
    }

    uint256 public oneIterationTokenAmount = 10 * 10 ** 6 * 10 ** 10;
    uint256 private maxSellAmountInIteration = oneIterationTokenAmount.div(2);

    bool public icoCompleted;
    uint256 public icoStartTime;
    uint256 public icoEndTime;
    address public tokenAddress;
    uint256 public currentIterationOfICO;
    uint256 public current_allocatedTokens;
    uint256 public minLimit = 1 * 10 ** 10;
    uint256 public maxLimit = 1 * 10 ** 7 * 10 ** 10;
    uint256 public currentUsersTokensAmount;
    uint256 private referralBonus = 20;
    uint256 private commission = 8 * 10 ** 15;

    AggregatorV3Interface private priceFeed;
    uint256 public usdPrice;
    uint256 private startPriceInUSD = 5; // since this value is 0.0005 . Multiplying it by 10000
    uint256 private stepPriceInUSD;

    mapping(address => mapping(uint256 => Reward)) usersRewards;
    mapping(address => Investor) allowedInvestors;

    event Allocated(uint256 amount);
    event AllocatedReward(address user, uint256 amount, uint256 usdPrice);
    event WithdrawedTokens(address user, uint256 amount);
    event WithdrawedETH(address user, uint256 amount);
    event MinLimitUpdated(uint256 newLimit);
    event MaxLimitUpdated(uint256 newLimit);
    event Bought(address buyer, uint256 amount, uint256 usdPrice);
    event Sold(address rewardAddress, uint256 amount);

    modifier allowedInvestor {
        require(allowedInvestors[msg.sender].allowed, "Your address not allowed. Please contact with owner.");
        _;
    }

    modifier whenIcoStart {
        require(!icoCompleted, 'ICO completed');
        require(icoStartTime != 0, 'ICO has not started yet');
        _;
    }

    constructor(address _tokenAddress, address owner){
        require(_tokenAddress != address(0) && owner != address(0), "Incorrect addresses");
        tokenAddress = _tokenAddress;
        transferOwnership(owner);
        allowedInvestors[owner].allowed = true;
        priceFeed = AggregatorV3Interface(0x5f4eC3Df9cbd43714FE2740f5E3616155c5b8419); // ETH / USD mainnet
//        priceFeed = AggregatorV3Interface(0x9326BFA02ADD2366b30bacB125260Af641031331); // ETH / USD kovan
//        priceFeed = AggregatorV3Interface(0x8A753747A1Fa494EC906cE90E9f37563A8AF630e); // ETH / USD rinkeby
    }

    function startICO() public onlyOwner {
        require(icoStartTime == 0, "ICO was started before");
        _allocate();
        icoStartTime = block.timestamp;
    }

    function setMinLimit(uint256 newLimit) public onlyOwner {
        minLimit = newLimit;
        emit MinLimitUpdated(maxLimit);
    }

    function setMaxLimit(uint256 newLimit) public onlyOwner {
        maxLimit = newLimit;
        emit MaxLimitUpdated(maxLimit);
    }

    function setReferralBonus(uint256 _bonus) external onlyOwner {
        require(_bonus <= 100, 'Referal bonus should not be more than 100%');
        referralBonus = _bonus;
    }

    function getReferralBonus() external view returns (uint256) {
        return referralBonus;
    }

    function setCommission(uint256 _commission) external onlyOwner {
        commission = _commission;
    }

    function getCommission() external view returns (uint256) {
        return commission;
    }

    function addAddressToAllowed(address client) public onlyOwner {
        allowedInvestors[client].allowed = true;
    }

    function removeAddressFromAllowed(address client) public onlyOwner {
        allowedInvestors[client].allowed = false;
    }

    function _allocate() private {
        require(currentIterationOfICO <= 201, 'Funding cycle ended');
        if (icoStartTime == 0) {
            usdPrice = startPriceInUSD;
        } else if (currentIterationOfICO == 0) {
            oneIterationTokenAmount = 19 * 10 ** 5 * 10 ** 10;
            maxSellAmountInIteration = oneIterationTokenAmount.div(2);
            usdPrice = 10; // to have correct order over startPriceInUSD -> Multiplying 0.001 by 10000
            stepPriceInUSD = 1;
            currentIterationOfICO++;
        } else if (currentIterationOfICO > 0 && currentIterationOfICO < 100) {
            usdPrice = usdPrice.add(stepPriceInUSD);
            currentIterationOfICO++;
        } else if (currentIterationOfICO == 100) {
            oneIterationTokenAmount = 8 * 10 ** 6 * 10 ** 10;
            maxSellAmountInIteration = oneIterationTokenAmount.div(2);
            usdPrice = 110; // to have correct order over startPriceInUSD -> Multiplying 0.011 by 10000
            stepPriceInUSD = 10; // to have correct order over startPriceInUSD -> Multiplying 0.001 by 10000
            currentIterationOfICO++;
        } else if (currentIterationOfICO > 100 && currentIterationOfICO <= 201) {
            usdPrice = usdPrice.add(stepPriceInUSD);
            currentIterationOfICO++;
        }

        current_allocatedTokens = current_allocatedTokens.add(oneIterationTokenAmount);
        currentUsersTokensAmount = 0;
        usersRewards[owner()][currentIterationOfICO] = Reward({
            amountTokens: oneIterationTokenAmount,
            price: usdPrice
        });
        emit Allocated(current_allocatedTokens);
    }

    receive() external payable {
        buy();
    }

    function _getAmountForETH(uint amountETH) private view returns (uint256){
        (
            , 
            // rate is giving with precision to acutal price * 10 ** 8
            int rate,
            ,
            ,
            
        ) = priceFeed.latestRoundData();
        // So here we have to divide final amount
        uint256 usdAmount = amountETH.mul(uint256(rate)).div(10 ** 8);
//        console.log("usdAmount", usdAmount);
        // Since token price is less then 1 - we have to multiply the smallest value to 10 ** 4
        // So on every calculation of price it should be divided into 10 ** 4
        uint256 amountTokens = usdAmount.div(usdPrice).div(10 ** 4).div(10 ** 10).mul(10 ** 10);
        if (amountTokens > current_allocatedTokens) {
            uint256 tokenPrice = usdPrice;
            uint256 current_allocated = current_allocatedTokens;
            uint256 icoRound = currentIterationOfICO;
            amountTokens = 0;
            while (amountETH > 0) {
                uint256 amount = usdAmount.div(usdPrice).div(10 ** 4).div(10 ** 10).mul(10 ** 10);
                if (current_allocated > 0) {
                    amountTokens = amountTokens.add(current_allocated);
                    usdAmount = usdAmount.sub(current_allocated.div(10 ** 10).mul(tokenPrice)).div(10 ** 4);
                    current_allocated = 0;
                }
                if (amount > oneIterationTokenAmount) {
                    icoRound++;
                    amountTokens = amountTokens.add(oneIterationTokenAmount);
                    usdAmount = usdAmount.sub(oneIterationTokenAmount.div(10 ** 10).mul(tokenPrice)).div(10 ** 4);
                    (uint256 roundPrice, uint256 stepPrice) = getTokenPrice(icoRound);
                    tokenPrice = roundPrice.add(stepPrice);
                }
                uint256 iterationAmount = getTokensPerIteration(icoRound);
                if (amount <= iterationAmount) {
                    amountTokens = amountTokens.add(amount);
                    amountETH = 0;
                }
            }
        }
        return amountTokens;
    }

    // get cost is calculating price including switch to different price range
    function getCost(uint amount) public view returns (uint256){
        uint256 usdCost;
        uint256 ethCost;
        int exchangeRate = getLatestPrice();
        if (amount <= current_allocatedTokens) {
            usdCost = amount.mul(usdPrice).div(10 ** 4);
            ethCost = getPriceInETH(usdCost, exchangeRate);
        } else {
            uint256 price = usdPrice;
            uint256 stepPrice;
            uint256 current_allocated = current_allocatedTokens;
            uint256 icoRound = currentIterationOfICO;
            uint256 iterationAmount;
            while (amount > 0) {
//                console.log('amount from get cost',amount.div(10**10));
                if (current_allocated > 0) {
                    amount = amount.sub(current_allocated);
                    usdCost = current_allocated.div(10 ** 4).mul(price).add(usdCost);
                    current_allocated = 0;

                    icoRound++;
                    (price, stepPrice) = getTokenPrice(icoRound);
                    price = price.add(stepPrice);
                }
                // get amounts for the next round since it could be different
                iterationAmount = getTokensPerIteration(icoRound);
                if (amount > iterationAmount) {
                    amount = amount.sub(iterationAmount);
                    usdCost = iterationAmount.div(10 ** 4).mul(price).add(usdCost);
                    icoRound++;
                    (price, stepPrice) = getTokenPrice(icoRound);
                    price = price.add(stepPrice);
                }
                iterationAmount = getTokensPerIteration(icoRound);
                if (amount <= getTokensPerIteration(icoRound)) {
                    usdCost = amount.div(10 ** 4).mul(price).add(usdCost);
                    amount = 0;
                }
            }
            ethCost = getPriceInETH(usdCost, exchangeRate);
        }
        return ethCost;
    }

    function buy() public payable whenIcoStart allowedInvestor {
//        require(msg.value >= getCost(amount).add(commission), 'Incorrect amount of ETH');
        uint256 ethForTokens = msg.value.sub(commission);
        uint256 amount = _getAmountForETH(ethForTokens);
//        console.log('value', ethForTokens);
//        console.log(amount);
        require(amount >= minLimit, 'Amount for one purchase is too low');
        require(amount <= maxLimit, 'Limit for one purchase is reached');
        _changeCurrentAllocatedTokens(amount);
        _sendTokens(msg.sender, amount);
        _completeICO();
    }

    function _changeCurrentAllocatedTokens(uint256 amount) private {
//        console.log("amount", amount);
//        console.log('current_allocatedTokens', current_allocatedTokens);
        if (amount <= current_allocatedTokens) {
            current_allocatedTokens = current_allocatedTokens.sub(amount);
        } else {
            uint256 amountForLoop = amount;
            while (amountForLoop > 0) {
//                console.log('amountForLoop', amountForLoop.div(10**10));
//                console.log('current_allocatedTokens', current_allocatedTokens.div(10**10));
                if (current_allocatedTokens > 0) {
                    amountForLoop = amountForLoop.sub(current_allocatedTokens);
                    current_allocatedTokens = 0;
                    _allocate();
                }
//            console.log('oneIterationTokenAmount', oneIterationTokenAmount.div(10**10));
                if (amountForLoop > oneIterationTokenAmount) {
//                    console.log('amountForLoop2', amountForLoop.div(10**10));
//                    console.log('current_allocatedTokens2', current_allocatedTokens.div(10**10));
                    amountForLoop = amountForLoop.sub(current_allocatedTokens);
                    current_allocatedTokens = 0;
                    _allocate();
                }
                if (amountForLoop <= oneIterationTokenAmount) {
                    current_allocatedTokens = current_allocatedTokens.sub(amountForLoop);
                    amountForLoop = 0;
                }
            }
        }
    }

    function _completeICO() private {
        if (current_allocatedTokens == 0 && currentIterationOfICO == 201) {
            icoEndTime = block.timestamp;
            icoCompleted = true;
        } else if (current_allocatedTokens == 0) {
            _allocate();
        }
    }

    function _sendTokens(address client, uint256 amountToken) private nonReentrant {
        uint256 amount = allowedInvestors[client].amount.add(amountToken);
        allowedInvestors[client].amount = 0;
        IERC20(tokenAddress).transfer(client, amount);

        emit Bought(client, amountToken, usdPrice);
    }

    function batchSell(address[] calldata rewardAddresses, uint256[] calldata amounts) public payable
    whenIcoStart onlyOwner {
        require(
            rewardAddresses.length == amounts.length,
            'Count of items of rewardAddresses and amounts must be same.'
        );
        require(rewardAddresses.length != 0, 'Count of items must be not zero');
        for (uint256 i = 0; i < rewardAddresses.length; i++) {
            sell(rewardAddresses[i], amounts[i]);
        }
    }

    function sell(address rewardAddress, uint256 amount) private
    whenIcoStart nonReentrant {
        require(
            currentUsersTokensAmount.add(amount) <= maxSellAmountInIteration,
            'Sales capability reached for current stage'
        );
        require(
            IERC20(tokenAddress).allowance(msg.sender, address(this)) >= amount,
            'Not approved amount of tokens for sell'
        );
        currentUsersTokensAmount = currentUsersTokensAmount.add(amount);
        usersRewards[rewardAddress][currentIterationOfICO].amountTokens =
        usersRewards[rewardAddress][currentIterationOfICO].amountTokens.add(amount);
        usersRewards[rewardAddress][currentIterationOfICO].price = usdPrice;
        usersRewards[owner()][currentIterationOfICO].amountTokens =
        usersRewards[owner()][currentIterationOfICO].amountTokens.sub(amount);
        usersRewards[owner()][currentIterationOfICO].price = usdPrice;
        IERC20(tokenAddress).safeTransferFrom(msg.sender, address(this), amount);
        emit Sold(rewardAddress, amount);
    }

    function withdrawClientTokens(uint256 amount) public nonReentrant {
        require(usersRewards[msg.sender][currentIterationOfICO].amountTokens > 0, 'Not enough tokens');
        require(
            usersRewards[msg.sender][currentIterationOfICO].amountTokens >= amount,
            'Not enough tokens for this amount'
        );
        currentUsersTokensAmount = currentUsersTokensAmount.sub(amount);
        usersRewards[msg.sender][currentIterationOfICO].amountTokens =
        usersRewards[msg.sender][currentIterationOfICO].amountTokens.sub(amount);
        usersRewards[owner()][currentIterationOfICO].amountTokens =
        usersRewards[owner()][currentIterationOfICO].amountTokens.add(amount);

        IERC20(tokenAddress).safeTransfer(msg.sender, amount);
        emit WithdrawedTokens(msg.sender, amount);
    }

    function withdrawReward() public nonReentrant {
        uint256 amount = getRewardAmount();
        require(amount > 0, 'Not enough reward for withdraw');
        console.log(amount);
        (bool success,) = address(msg.sender).call{value : amount}("");
        require(success, 'Transfer failed');
        emit WithdrawedETH(msg.sender, amount);
    }

    function getRewardAmount() public view returns (uint256 amount) {
        for (uint256 i = 0; i < currentIterationOfICO; i++) {
            // iterating over all available rounds and calculate sum price in usd
            uint256 amountUSD =
            usersRewards[msg.sender][i].amountTokens.div(10 ** 14).mul(usersRewards[msg.sender][i].price);
            amount = amount.add(amountUSD);
        }

        amount = amount.mul(uint256(getLatestPrice()));
    }

    function getTokensAmount() public view returns (uint256 amount) {
        amount = usersRewards[msg.sender][currentIterationOfICO].amountTokens;
    }

    function getCurrentICOState() public view returns (ICOStateSchema memory currentState) {
        require(icoStartTime != 0, 'ICO was not started can not _allocate new tokens');
        currentState.currentIteration = currentIterationOfICO;
        currentState.currentPrice = usdPrice;
        currentState.userAllocatedTokensAmount = currentUsersTokensAmount;
        currentState.ICOAllocatedTokensAmount = oneIterationTokenAmount.sub(currentUsersTokensAmount);
        currentState.tokensLeft = current_allocatedTokens;
    }

    function buyWithReferral(address payable referral) external payable
        allowedInvestor whenIcoStart {
        require(referral != address(0), 'Referral address should not be empty');
        uint256 amount = _getAmountForETH(msg.value - commission);
        buy();
        uint256 bonusAmount = getCost(amount).mul(referralBonus).div(100);
        (bool success,) = referral.call{value: bonusAmount}("");
        require(success);
    }

    function buyFor(address buyer) public payable onlyOwner whenIcoStart {
        uint256 amount = _getAmountForETH(msg.value);
        require(amount >= minLimit, 'Amount for one purchase is too low');
        require(amount <= maxLimit, 'Limit for one purchase is reached');
        require(buyer != address(0), 'Buyer address should not be empty');
//        require(msg.value == getCost(amount), 'Incorrect amount of ETH');

        _changeCurrentAllocatedTokens(amount);
        IERC20(tokenAddress).transfer(buyer, amount);
        emit Bought(buyer, amount, usdPrice);
        _completeICO();
    }

    function buyForWithReferral(address buyer, address payable referral) external payable
    onlyOwner whenIcoStart {
        require(referral != address(0), 'Referral address should not be empty');
        uint256 amount = _getAmountForETH(msg.value);
        buyFor(buyer);
        uint256 bonusAmount = getCost(amount).mul(referralBonus).div(100);
        (bool success,) = referral.call{value : bonusAmount}("");
        require(success);
    }

    function getLatestPrice() public view returns (int) {
        (
        ,
        int price,
        ,
        ,
        ) = priceFeed.latestRoundData();
        return 1 ether / (price / (10 ** 8));
    }

    function getPriceInETH(uint256 amount, int exchangeRate) public pure returns (uint256) {
        return amount.mul(uint256(exchangeRate)).div(10 ** 10);
    }

    function getTokenPrice(uint256 icoIteration) public view returns (uint256, uint256) {
        require(icoIteration <= 201, 'Incorrect ICO round');
        uint256 price;
        uint256 stepPrice;
        if (icoIteration == 0) {
            price = startPriceInUSD;
        } else if (icoIteration == 1) {
            price = 10;
            stepPrice = 0;
        } else if (icoIteration > 1 && icoIteration <= 100) {
            price = 10;
            stepPrice = icoIteration.sub(1);
        } else if (icoIteration == 101) {
            price = 110;
            stepPrice = 0;
        } else {
            price = 110;
            stepPrice = icoIteration.sub(101).mul(10);
        }
//        console.log('icoIteration - ',icoIteration);
//        console.log('price - ', price);
//    console.log('stepPrice - ', stepPrice);
        return (price, stepPrice);
    }

    function getTokensPerIteration(uint256 icoIteration) public view returns (uint256) {
        require(icoIteration <= 201, 'Incorrect ICO round');
        uint256 amount;
        if (icoIteration == 0) {
            amount = oneIterationTokenAmount;
        } else if (icoIteration > 0 && icoIteration <= 100) {
            amount = 19 * 10 ** 5 * 10 ** 10;
        } else {
            amount = 8 * 10 ** 6 * 10 ** 10;
        }
//        console.log("jdsllsddsjjkdsdjklsjksd",amount, icoIteration);
        return amount;
    }
}

