pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "hardhat/console.sol";
import "./interfaces/IMintableCollection.sol";
import "./interfaces/staking/ISchnitzelStakingPool.sol";

contract SchnitzelStakingPool is ISchnitzelStakingPool, Initializable, OwnableUpgradeable {
    using SafeERC20 for IERC20;

    struct StakerInfo {
        uint256 nftId;
        uint256 amount;
        uint256 stakeStartTime;
        uint256 lockDays;
        uint256 rewardDebt;
        uint16 multiplier;
    }

    struct LockMultiplier {
        uint16 multiplier;
        bool exists;
    }

    IERC20 private token;
    IMintableCollection private nftCollection;
    uint256 public minStakeAmount;
    uint256 private nftStartId;

    uint256 public startDate;
    uint256 public endDate;

    uint256 public totalStakedAmount;
    uint256 public totalStakedAmountWithMultipliers;
    uint256 public totalRewardAmount;
    uint256 public accRewardPerShare;
    uint256 public lastRewardAmount;

    // NFT ID to staker info
    mapping(uint256 => StakerInfo) public stakes;

    // Mapping from days => multiplier for timelock
    mapping(uint256 => LockMultiplier) public lockMultipliers;

    uint256 private constant DIV_PRECISION = 1e18;

    event AddRewards(uint256 amount);
    event Staked(
        address indexed account,
        uint256 nftId,
        uint256 amount,
        uint256 lockDays
    );
    event Harvest(address indexed staker, uint256 nftId, uint256 amount);
    event Withdraw(address indexed staker, uint256 nftId, uint256 amount);
    event LogUpdateRewards(uint256 totalRewards, uint256 accRewardPerShare);

    modifier whenStarted {
        require(hasStarted(), "StakingPool: staking has not yet begun");
        _;
    }

    modifier notEnded {
        require(!hasEnded(), "StakingPool: staking has ended");
        _;
    }

    function initialize(
        IERC20 _token,
        IMintableCollection _nftCollection,
        uint256 _nftStartId,
        uint256 _minStakeAmount
    ) public initializer {
        token = _token;
        nftCollection = _nftCollection;
        nftStartId = _nftStartId;
        minStakeAmount = _minStakeAmount;
        OwnableUpgradeable.__Ownable_init();
    }

    function hasEnded() public view returns (bool) {
        return endDate != 0 && block.timestamp >= endDate;
    }

    function setEndDate(uint256 _endDate) external onlyOwner {
        require(
            _endDate > block.timestamp,
            "StakingPool: End date already reached"
        );
        endDate = _endDate;
    }

    function hasStarted() public view returns (bool) {
        return startDate != 0 && block.timestamp >= startDate;
    }

    function setStartDate(uint256 _startDate) external onlyOwner {
        require(
            startDate == 0,
            "StakingPool: Start date was already set"
        );
        startDate = _startDate;
    }

    // lockdays are passed as seconds, multiplier in percentage from 100 (e.g. 170 for 70% on top)
    function setLockMultiplier(uint256 lockDays, uint16 multiplier) external onlyOwner {
        lockMultipliers[lockDays] = LockMultiplier({
        multiplier: multiplier,
        exists: true
        });
    }

    function setMinStakeAmount(uint256 _minStakeAmount) external onlyOwner {
        minStakeAmount = _minStakeAmount;
    }

    function stake(uint256 amount, uint256 lockDays)
    external
    whenStarted
    notEnded
    {
        require(
            amount >= minStakeAmount,
            "StakingPool: Amount must be greater than or equal to min stake amount"
        );
        require(
            lockMultipliers[lockDays].exists,
            "StakingPool: Invalid number of lock days specified"
        );

        updateRewards();

        nftCollection.mint(msg.sender, nftStartId);

        StakerInfo storage staker = stakes[nftStartId];
        staker.stakeStartTime = block.timestamp;
        staker.amount = amount;

        staker.lockDays = lockDays;
        staker.multiplier = lockMultipliers[lockDays].multiplier;
        staker.nftId = nftStartId;

        // the amount with lock multiplier applied
        uint256 virtualAmount = virtualAmount(staker.amount, staker.multiplier);
        staker.rewardDebt = (virtualAmount * accRewardPerShare) / DIV_PRECISION;

        token.safeTransferFrom(msg.sender, address(this), amount);
        totalStakedAmount = totalStakedAmount + amount;

        totalStakedAmountWithMultipliers =
        totalStakedAmountWithMultipliers +
        virtualAmount;

        emit Staked(msg.sender, nftStartId, amount, lockDays);
        nftStartId = nftStartId + 1;
    }

    function withdraw(uint256 nftId) external whenStarted {
        StakerInfo storage staker = stakes[nftId];
        require(staker.nftId > 0, "StakingPool: No staker exists");
        require(
            nftCollection.ownerOf(nftId) == msg.sender,
            "StakingPool: Only the owner may withdraw"
        );
        require(
            (staker.stakeStartTime + staker.lockDays) < block.timestamp,
            "StakingPool: Lock time not expired"
        );

        updateRewards();

        uint256 virtualAmount = virtualAmount(staker.amount, staker.multiplier);

        uint256 accumulated =
        (virtualAmount * accRewardPerShare) / DIV_PRECISION;
        uint256 reward = accumulated - staker.rewardDebt;

        token.safeTransfer(msg.sender, reward + staker.amount);

        // also reset the global props
        totalStakedAmount = totalStakedAmount - staker.amount;
        totalStakedAmountWithMultipliers =
        totalStakedAmountWithMultipliers -
        virtualAmount;

        // reset all staker props
        staker.rewardDebt = 0;
        staker.amount = 0;
        staker.stakeStartTime = 0;
        staker.lockDays = 0;
        staker.nftId = 0;

        nftCollection.burn(nftId);

        emit Harvest(msg.sender, nftId, reward);
        emit Withdraw(msg.sender, nftId, staker.amount);
    }

    function updateRewards() private {
        if (totalRewardAmount > lastRewardAmount) {
            if (totalStakedAmountWithMultipliers > 0) {
                uint256 reward = totalRewardAmount - lastRewardAmount;
                accRewardPerShare =
                accRewardPerShare +
                (reward * DIV_PRECISION) /
                totalStakedAmountWithMultipliers;
            }
            lastRewardAmount = totalRewardAmount;
            emit LogUpdateRewards(lastRewardAmount, accRewardPerShare);
        }
    }

    function addRewards(uint256 amount) override external {
        require(amount > 0, "StakingPool: Amount must be greater than zero");
        token.safeTransferFrom(msg.sender, address(this), amount);
        totalRewardAmount = totalRewardAmount + amount;
        emit AddRewards(amount);
    }

    function harvest(uint256 nftId) external whenStarted {
        StakerInfo storage staker = stakes[nftId];
        require(staker.nftId > 0, "StakingPool: No staker exists");
        require(
            nftCollection.ownerOf(nftId) == msg.sender,
            "StakingPool: Only the owner may harvest"
        );

        updateRewards();

        uint256 accumulated =
        (virtualAmount(staker.amount, staker.multiplier) *
        accRewardPerShare) / DIV_PRECISION;
        uint256 reward = uint256(accumulated - staker.rewardDebt);
        staker.rewardDebt = accumulated;

        token.safeTransfer(msg.sender, reward);
        emit Harvest(msg.sender, nftId, reward);
    }

    function pendingReward(uint256 nftId) external view returns (uint256) {
        StakerInfo memory staker = stakes[nftId];
        require(staker.nftId > 0, "StakingPool: No staker exists");

        uint256 accumulated = (virtualAmount(staker.amount, staker.multiplier) * accRewardPerShare) / DIV_PRECISION;
        return uint256(accumulated - staker.rewardDebt);
    }

    // returns the virtual amount after having a multiplier applied
    function virtualAmount(uint256 amount, uint16 multiplier) private view returns (uint256) {
        return (amount * ((multiplier * DIV_PRECISION) / 100)) / DIV_PRECISION;
    }
}

