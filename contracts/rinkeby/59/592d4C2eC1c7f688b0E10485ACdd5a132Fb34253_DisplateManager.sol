pragma solidity ^0.8.7;
import "./utils/CloneFactory.sol";
import "./NFT.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "solidity-bytes-utils/contracts/BytesLib.sol";

contract DisplateManager is CloneFactory, Ownable {
    string public baseTokenUri;
    address accessManagerAddress;
    address public templateAddress;

    IAccessManager accessManager;

    event NFTDropped(
        address _NFTAddress,
        string _tokenName,
        string _tokenSymbol,
        uint32 _totalAmountOfEdition,
        uint32 _timeStart,
        uint32 _timeEnd,
        uint32 _nftId
    );

    modifier isOperationalAddress() {
        require(
            accessManager.isAllowed(msg.sender) == true,
            "You are not allowed to use this function"
        );
        _;
    }

    constructor(
        address _accessManangerAddress,
        address _templateAddress,
        string memory _baseTokenUri
    ) {
        setAccessManagerAddress(_accessManangerAddress);
        accessManager = IAccessManager(accessManagerAddress);
        templateAddress = _templateAddress;
        baseTokenUri = _baseTokenUri;
    }

    function NFTDrop(bytes memory _data) external isOperationalAddress {
        uint32 staticDataLength = BytesLib.toUint32(
            BytesLib.slice(_data, 0, 4),
            0
        );

        bytes memory staticData = BytesLib.slice(
            _data,
            4,
            staticDataLength / 2
        );

        (
            uint32 _timeStart,
            uint32 _timeEnd,
            uint32 _dropId,
            string memory _tokenName,
            string memory _tokenSymbol
        ) = abi.decode(staticData, (uint32, uint32, uint32, string, string));

        uint32 amountOfNFT = BytesLib.toUint32(
            BytesLib.slice(_data, staticDataLength / 2 + 3, 4),
            0
        );

        bytes memory dynamicData = BytesLib.slice(
            _data,
            staticDataLength / 2 + 7,
            _data.length - (staticDataLength / 2 + 7)
        );

        for (uint256 i = 0; i < amountOfNFT * 64; i += 64) {
            uint32 totalAmountOfEditions = convertUint256ToUint32(
                BytesLib.toUint256(BytesLib.slice(dynamicData, 0 + i, 32), 0)
            );

            uint32 nftId = convertUint256ToUint32(
                BytesLib.toUint256(BytesLib.slice(dynamicData, 32 + i, 32), 0)
            );

            NFT clonedNFT = NFT(createClone(templateAddress));

            clonedNFT.init(
                abi.encode(
                    _timeStart,
                    _timeEnd,
                    _dropId,
                    totalAmountOfEditions,
                    nftId,
                    _tokenName,
                    _tokenSymbol
                ),
                accessManagerAddress
            );

            emit NFTDropped(
                address(clonedNFT),
                _tokenName,
                _tokenSymbol,
                totalAmountOfEditions,
                _timeStart,
                _timeEnd,
                nftId
            );
        }
    }

    function convertUint256ToUint32(uint256 _param)
        private
        pure
        returns (uint32)
    {
        return uint32(_param);
    }

    function setAccessManagerAddress(address _newAccessManagerAddress)
        public
        onlyOwner
    {
        accessManagerAddress = _newAccessManagerAddress;
    }

    function setBaseTokenUri(string memory _newBaseTokenUri)
        public
        isOperationalAddress
    {
        baseTokenUri = _newBaseTokenUri;
    }

    function setTemplateAddress(address _newTemplateAddress)
        public
        isOperationalAddress
    {
        templateAddress = _newTemplateAddress;
    }
}

pragma solidity ^0.8.7;

contract CloneFactory {
    function createClone(address target) internal returns (address result) {
        bytes20 targetBytes = bytes20(target);
        assembly {
            let clone := mload(0x40)
            mstore(
                clone,
                0x3d602d80600a3d3981f3363d3d373d3d3d363d73000000000000000000000000
            )
            mstore(add(clone, 0x14), targetBytes)
            mstore(
                add(clone, 0x28),
                0x5af43d82803e903d91602b57fd5bf30000000000000000000000000000000000
            )
            result := create(0, clone, 0x37)
        }
    }
}

pragma solidity ^0.8.7;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "./utils/EIP712.sol";
import "../interfaces/IAccessManager.sol";
import "../interfaces/INFT.sol";
import "./DisplateManager.sol";

contract NFT is ERC721URIStorage, EIP712, INFT {
    uint32 totalAmountOfEdition;
    uint32 timeStart;
    uint32 timeEnd;
    uint32 dropId;
    uint32 public nftId;
    bool private initialized;
    string private constant delegatedOperationName = "Delegated Operation";
    string private constant version = "1";
    string _name;
    string _symbol;
    mapping(bytes32 => bool) private saltUsed;
    mapping(uint32 => bool) public isTokenBurned;

    bytes32 immutable TRANSFER_TYPEHASH =
        keccak256(
            "Transfer(address addressTo,uint32 tokenId,uint32 expirationTime,bytes32 salt)"
        );

    bytes32 immutable BURN_TYPEHASH =
        keccak256("Burn(uint32 tokenId,uint32 expirationTime,bytes32 salt)");

    IAccessManager accessManager;
    DisplateManager displateManager;

    event TokenBurned(
        address _user,
        string _tokenName,
        string _tokenSymbol,
        uint32 _tokenID
    );
    event TokenAwarded(
        address _user,
        string _tokenName,
        string _tokenSymbol,
        uint32 _tokenID,
        string _tokenURI
    );

    constructor()
        ERC721(_name, _symbol)
        EIP712(delegatedOperationName, version)
    {}

    function init(bytes memory _data, address _accessManangerAddress) external {
        require(!initialized, "Already initialized");
        initialized = true;

        (
            uint32 _timeStart,
            uint32 _timeEnd,
            uint32 _dropId,
            uint32 _totalAmountOfEdition,
            uint32 _nftId,
            string memory _tokenName,
            string memory _tokenSymbol
        ) = abi.decode(
                _data,
                (uint32, uint32, uint32, uint32, uint32, string, string)
            );

        _name = _tokenName;
        _symbol = _tokenSymbol;
        totalAmountOfEdition = _totalAmountOfEdition;
        timeStart = _timeStart;
        timeEnd = _timeEnd;
        dropId = _dropId;
        nftId = _nftId;

        accessManager = IAccessManager(_accessManangerAddress);
        displateManager = DisplateManager(msg.sender);
    }

    function totalSupply() public view returns (uint32) {
        return totalAmountOfEdition;
    }

    function name() public view override returns (string memory) {
        return _name;
    }

    function symbol() public view override returns (string memory) {
        return _symbol;
    }

    function awardToken(address _user, uint32 _tokenID) public override {
        require(timeStart <= block.timestamp, "Drop is not active yet");
        require(
            accessManager.isAllowed(msg.sender) == true,
            "You are not allowed to award tokens"
        );
        require(
            _tokenID <= totalAmountOfEdition,
            "Token ID is bigger then total amount of tokens in this drop"
        );
        require(
            _exists(_tokenID) == false,
            "Token with this ID was minted previously"
        );
        require(isTokenBurned[_tokenID] == false, "Token was burned");

        emit TokenAwarded(
            _user,
            _name,
            _symbol,
            _tokenID,
            tokenURI(uint256(_tokenID))
        );
        _safeMint(_user, _tokenID);
    }

    function tokenURI(uint256 _tokenID)
        public
        view
        override
        returns (string memory)
    {
        string memory baseTokenUri = displateManager.baseTokenUri();
        return
            string(
                abi.encodePacked(
                    baseTokenUri,
                    "/",
                    Strings.toString(dropId),
                    "/",
                    Strings.toString(nftId),
                    "/",
                    Strings.toString(_tokenID),
                    ".json"
                )
            );
    }

    function burnToken(uint32 _tokenID) public {
        require(
            ownerOf(_tokenID) == msg.sender ||
                accessManager.isAllowed(msg.sender) == true,
            "You are not a owner of this token"
        );

        _burn(_tokenID);
        isTokenBurned[_tokenID] = true;
        emit TokenBurned(msg.sender, _name, _symbol, _tokenID);
    }

    function delegatedTransfer(
        address _addressTo,
        uint32 _tokenId,
        uint32 _expirationDate,
        bytes32 _salt,
        bytes memory _signature
    ) public {
        require(_expirationDate > block.timestamp, "Time is out");
        require(saltUsed[_salt] == false, "This salt was used");

        bytes32 _hash = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    TRANSFER_TYPEHASH,
                    _addressTo,
                    _tokenId,
                    _expirationDate,
                    _salt
                )
            )
        );
        address owner = ECDSA.recover(_hash, _signature);

        saltUsed[_salt] = true;

        _transfer(owner, _addressTo, _tokenId);
    }

    function delegatedBurn(
        uint32 _tokenId,
        uint32 _expirationDate,
        bytes32 _salt,
        bytes memory _signature
    ) public {
        require(_expirationDate > block.timestamp, "Time is out");
        require(saltUsed[_salt] == false, "This salt was used");

        bytes32 _hash = _hashTypedDataV4(
            keccak256(
                abi.encode(BURN_TYPEHASH, _tokenId, _expirationDate, _salt)
            )
        );
        address owner = ECDSA.recover(_hash, _signature);

        require(owner == ownerOf(_tokenId), "Wrong sender");

        saltUsed[_salt] = true;

        _burn(_tokenId);

        isTokenBurned[_tokenId] = true;
    }
}

