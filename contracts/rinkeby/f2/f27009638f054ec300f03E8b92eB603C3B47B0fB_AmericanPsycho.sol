import '@openzeppelin/contracts/utils/Counters.sol';
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./BaseCard.sol";

abstract contract HashCalculations is BaseCard {

  using Counters for Counters.Counter;
  using Strings for *;
  using SafeMath for uint256;

  mapping(uint256 => Position) positions;

  struct Position{
      string title;
      uint maxCount;
      Counters.Counter currentCount;
      Thickness thickness;
  }
  struct Thickness{
      uint8 base;
      uint8 modulo;
      uint8 bitsNeeded;
  }
	constructor() BaseCard() {
      positions[0] = Position("Chairman", 25, Counters.Counter(0), Thickness(49, 2, 1));           // chairman
      positions[1] = Position("President", 25, Counters.Counter(0), Thickness(47, 4, 2));          // president
      positions[2] = Position("Vice President", 100, Counters.Counter(0), Thickness(40, 8, 3));    // vice
      positions[3] = Position("Director", 200, Counters.Counter(0), Thickness(32,10, 4));          // director
      positions[4] = Position("Regional Manager", 313, Counters.Counter(0), Thickness(21,12, 4));  // regional manager
      positions[5] = Position("Strategic Analyst", 500, Counters.Counter(0), Thickness(15, 8, 3)); // stategic analyst
      positions[6] = Position("Manager", 800, Counters.Counter(0), Thickness( 2,16, 4));           // manager
      positions[7] = Position("Intern", 20, Counters.Counter(0), Thickness( 0, 2, 1));             // intern
	}

   function generateWithRandom(address sender, uint random) internal returns (uint) {
      Card[] storage cards = addressToCards[sender];
      uint hash = 0;
      (random, hash) = createTitle(random, hash);
      (random, hash) = create(random, hash, 9, 5);
      (random, hash) = create(random, hash, 14, 5);
      (random, hash) = create(random, hash, 18, 1);
      (random, hash) = create(random, hash, 19, 1);
      (random, hash) = create(random, hash, 21, 3);
      (random, hash) = create(random, hash, 24, 3);
      (random, hash) = create(random, hash, 27, 3);
      (random, hash) = create(random, hash, 30, 3);
      (random, hash) = create(random, hash, 33, 5);
      (random, hash) = createThickness(random, hash);
      (random, hash) = create(random, hash, 44, 4);
      (random, hash) = create(random, hash, 48, 2);
      (random, hash) = create(random, hash, 50, 1);
      (random, hash) = create(random, hash, 51, 2);
      (random, hash) = create(random, hash, 53, 6);
      (random, hash) = create(random, hash, 59, 5);

      // Beastco will work on this
      Card memory card = Card("", "", hash);
      // Card memory card = Card(getName(hash), getContactName(hash),hash);

      cards.push(card);
      hashToCard[hash] = card;

      return cards.length;
  }

	function createTitle(uint random, uint hash) internal returns (uint, uint) {
		uint titleRandom;
		(random, titleRandom) = getRandom(random, 20);
		titleRandom = titleRandom % (maxSupply - _tokenIdTracker.current());

		uint selectedPosition = 0;
		for (uint i = 0; i < 8; ++i) {
			uint remaining = positions[i].maxCount - positions[i].currentCount.current();
			if (titleRandom < remaining) {
				selectedPosition = i;
				break;
			}
			titleRandom -= remaining;
		}

		_tokenIdTracker.increment();
    positions[selectedPosition].currentCount.increment();

		uint32 prefix;
		uint32 suffix;
		uint32 temp;

		if (selectedPosition == 6) {
			// chance you could just be "Manager."
			// Very rare role
			(random, prefix) = getRandom(random, 2);
			(random, suffix) = getRandom(random, 3);
		} else if (selectedPosition >= 2 && selectedPosition <= 5) {
			(random, temp) = getRandom(random, 3);
			if (temp == 1) { // 12.5% chance of mutation
				(random, prefix) = getRandom(random, 2);
			}
			(random, temp) = getRandom(random, 1);
			if (temp == 1) { // 12.5% chance of mutation
				(random, suffix) = getRandom(random, 3);
			}
		}

		if (selectedPosition == 3 || selectedPosition == 2) {
			(random, temp) = getRandom(random, 4);
			if (temp == 1) { // ~6% chance of mutation
				selectedPosition--;
				if (prefix == 0) {
					(random, prefix) = getRandom(random, 2);
					if (prefix == 0) {
						prefix++;
					}
				}
				if (suffix == 0) {
					(random, suffix) = getRandom(random, 3);
					if (suffix == 0) {
						suffix++;
					}
				}
			}
		}

		hash = hash | ((suffix & 0x7) << 6) | ((selectedPosition) << 2) | (prefix & 0x3);
		return (random, hash);
	}

	function create(uint random, uint hash, uint8 offset, uint8 size) internal pure returns (uint, uint) {
		uint32 value;
		(random, value) = getRandom(random, size);
		hash = hash | (value << offset);
		return (random, hash);
	}

	function createThickness(uint random, uint hash) public view returns (uint, uint) {
    uint8 title = pluck(hash, 2, 0x7);
		uint8 base = positions[title].thickness.base;
		uint8 modulo = positions[title].thickness.modulo;
		uint8 bitsNeeded = positions[title].thickness.bitsNeeded;

		uint32 t;
		(random, t) = getRandom(random, bitsNeeded);

		uint256 thickness = base + uint32(t % modulo);

		hash = hash | (thickness << 38);
    	return (random, hash);
	  }

    function pluck(uint hash, uint8 offset, uint8 size) public pure returns (uint8) {
        return uint8((hash >> offset) & size);
    }

	// Gets a random number and returns the remainder of the random number
	// plus the random number within those bits.
	// Random number capped to 32 bit number
	function getRandom(uint random, uint8 bitLength) internal pure returns (uint, uint32) {
		uint32 part = uint32((random & ((0x1 << bitLength) - 1)));
		return (random >> bitLength, part);
	}

}

