pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
import "./HashCalculations.sol";
import "./SVGCardGenerator.sol";


 contract AmericanPsycho is HashCalculations, ERC721, Ownable, VRFConsumerBase, IERC2981, AccessControl {
    using Counters for Counters.Counter;
    using Strings for *;
    using SafeMath for uint256;
    mapping(bytes4 => bool) internal supportedInterfaces;

    // TODO: Remove this
    bytes32 internal _keyHash = 0x2ed0feb3e7fd2022120aa84fab1945545a9f2ffc9076fd6156fa96eaff4c1311;
    uint256 internal _fee = 0.1 * 10**18; // 0.1 LINK

    // We need set an artist
    // or we need to change this to multiple people
    address private _artist;

    address public cardGeneratorContract;

    // This is 10%
    uint256 private _royaltyAmount = 1000;

    // Should we allow the cardGenerator address to change
    constructor(string memory name, string memory symbol, address cardContract)

      ERC721(name, symbol) HashCalculations() VRFConsumerBase(
          0xdD3782915140c8f3b190B5D67eAc6dc5760C46E9, // VRF Coordinator
          0xa36085F69e2889c224210F603D836748e7dC0088  // LINK Token
      ) {

      cardGeneratorContract = cardContract;
    }

    function externalContractTest() public view returns (string memory) {
        SVGCardGenerator callee = SVGCardGenerator(cardGeneratorContract);
        return callee.generateSVG(0);
    }

    function GimmieAnSVG(uint hash) public view returns (string memory) {
        SVGCardGenerator callee = SVGCardGenerator(cardGeneratorContract);
        return callee.generateSVG(hash);
    }

    function getTotalSupply() public view returns (uint) {
        return maxSupply;
    }

    // This is wrong
    // Learn how this works
    function supportsInterface(bytes4 interfaceID) public view virtual override(IERC165, ERC721, AccessControl) returns (bool) {
        return supportedInterfaces[interfaceID];
    }

    function getLength() public view returns (uint) {
        Card[] storage cards = addressToCards[msg.sender];
        return cards.length;
    }

    function getHash(uint idx) public view returns (uint256) {
        require(getLength() > idx, "You don't have that many cards");
        Card storage card = addressToCards[msg.sender][idx];
        return card.hash;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomNumber) internal virtual override {
        address sender = awaitingRandom[requestId];
        delete awaitingRandom[requestId];
        generateWithRandom(sender, randomNumber);
    }

    function generate() external virtual {
        // require(LINK.balanceOf(address(this)) >= _fee, "Not enough LINK - fill contract with faucet");
        require(_tokenIdTracker.current() < maxSupply, "Business hours are over.");

        bytes32 requestId = requestRandomness(_keyHash, _fee);
        awaitingRandom[requestId] = msg.sender;

        // When on link network, we don't call this function
        // fulfillRandomness(requestId, _getTempRandom());
    }

    function updateRoyalty(uint256 amount) public onlyOwner {
        _royaltyAmount = amount;
    }

    function royaltyInfo(
        uint256 _tokenId,
        uint256 _salePrice
    ) external view override(IERC2981) returns (
        address receiver,
        uint256 royaltyAmount
    ) {
        return (_artist, _royaltyAmount);
    }
}

