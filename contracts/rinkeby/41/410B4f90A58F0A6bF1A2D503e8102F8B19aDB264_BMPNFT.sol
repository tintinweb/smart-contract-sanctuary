struct Pixel {
    bytes1 blue;
    bytes1 green;
    bytes1 red;
}

library PixelLib {
    function complementary(Pixel memory pixel)
        internal
        pure
        returns (Pixel memory)
    {
        uint16 blue = uint16(uint8(pixel.blue));
        uint16 green = uint16(uint8(pixel.green));
        uint16 red = uint16(uint8(pixel.red));
        uint16 max = blue;
        uint16 min = blue;
        if (green > max) {
            max = green;
        }
        if (green < min) {
            min = green;
        }
        if (red > max) {
            max = red;
        }
        if (red < min) {
            min = red;
        }
        return
            Pixel(
                bytes1(uint8(max + min - blue)),
                bytes1(uint8(max + min - green)),
                bytes1(uint8(max + min - red))
            );
    }
}

struct PixelFactory {
    bytes32 data;
    bool random;
    int16 step;
    Pixel from;
    Pixel to;
}

library PixelFactoryLib {
    using PixelLib for Pixel;

    function init(
        PixelFactory memory factory,
        bytes32 data,
        int16 step
    ) internal pure {
        factory.data = data;
        factory.step = step;
        factory.random = uint256(data) % 3 == 0;
        if (factory.random) {
            return;
        }
        factory.from = Pixel(data[0], data[1], data[2]);
        uint248 toPattern = uint248(bytes31(data)) % 4;
        if (toPattern == 0) {
            factory.to = Pixel(0, 0, 0);
        } else if (toPattern == 1) {
            factory.to = Pixel(0xff, 0xff, 0xff);
        } else if (toPattern == 2) {
            factory.to = factory.from.complementary();
        } else {
            factory.to = Pixel(data[3], data[4], data[5]);
        }
        if (uint240(bytes30(data)) % 2 == 0) {
            Pixel memory tmp = factory.from;
            factory.from = factory.to;
            factory.to = tmp;
        }
    }

    function create(PixelFactory memory factory, int16 index)
        internal
        pure
        returns (Pixel memory)
    {
        Pixel memory pixel;
        if (factory.random) {
            pixel.blue = factory.data[(uint16(index) * 3) % 32];
            pixel.green = factory.data[(uint16(index) * 3 + 1) % 32];
            pixel.red = factory.data[(uint16(index) * 3 + 2) % 32];
            return pixel;
        }
        int16 blueDiff = ((int16(uint16(uint8(factory.to.blue))) -
            int16(uint16(uint8(factory.from.blue)))) * index) / factory.step;
        pixel.blue = bytes1(
            uint8(int8(int16(uint16(uint8(factory.from.blue))) + blueDiff))
        );
        int16 greenDiff = ((int16(uint16(uint8(factory.to.green))) -
            int16(uint16(uint8(factory.from.green)))) * index) / factory.step;
        pixel.green = bytes1(
            uint8(int8(int16(uint16(uint8(factory.from.green))) + greenDiff))
        );
        int16 redDiff = ((int16(uint16(uint8(factory.to.red))) -
            int16(uint16(uint8(factory.from.red)))) * index) / factory.step;
        pixel.red = bytes1(
            uint8(int8(int16(uint16(uint8(factory.from.red))) + redDiff))
        );
        return pixel;
    }
}

struct Position {
    int16 x;
    int16 y;
}

library PositionLib {
    function add(
        Position memory position,
        int16 x,
        int16 y
    ) internal pure returns (Position memory) {
        return Position(position.x + x, position.y + y);
    }
}

library BitmapLib {
    using PixelFactoryLib for PixelFactory;
    using PositionLib for Position;
    using BitmapLib for bytes;

    function setPixel(
        bytes memory bmpBody,
        Position memory position,
        Pixel memory pixel
    ) internal pure {
        uint16 index = uint16(position.y) * 96 + uint16(position.x) * 3;
        bmpBody[index] = pixel.blue;
        bmpBody[index + 1] = pixel.green;
        bmpBody[index + 2] = pixel.red;
    }

    function vertical(bytes memory bmpBody, bytes32 data) internal pure {
        PixelFactory memory factory;
        factory.init(data, 31);
        for (int16 i = 0; i < 31; i++) {
            Pixel memory pixel = factory.create(i);
            for (int16 j = 0; j < 31; j++) {
                bmpBody.setPixel(Position(i, j), pixel);
            }
        }
    }

    function horizontal(bytes memory bmpBody, bytes32 data) internal pure {
        PixelFactory memory factory;
        factory.init(data, 31);
        for (int16 i = 0; i < 31; i++) {
            Pixel memory pixel = factory.create(i);
            for (int16 j = 0; j < 31; j++) {
                bmpBody.setPixel(Position(j, i), pixel);
            }
        }
    }

    function cross(bytes memory bmpBody, bytes32 data) internal pure {
        PixelFactory memory factory;
        factory.init(data, 16);
        for (int16 i = 0; i <= 15; i++) {
            Pixel memory pixel = factory.create(i);
            for (int16 j = 0; j < 31; j++) {
                bmpBody.setPixel(Position(i, j), pixel);
                bmpBody.setPixel(Position(30 - i, j), pixel);
                bmpBody.setPixel(Position(j, i), pixel);
                bmpBody.setPixel(Position(j, 30 - i), pixel);
            }
        }
    }

    function square(bytes memory bmpBody, bytes32 data) internal pure {
        PixelFactory memory factory;
        factory.init(data, 16);
        for (int16 i = 15; i >= 0; i--) {
            Pixel memory pixel = factory.create(i);
            for (int16 j = 0; j < 31; j++) {
                bmpBody.setPixel(Position(i, j), pixel);
                bmpBody.setPixel(Position(30 - i, j), pixel);
                bmpBody.setPixel(Position(j, i), pixel);
                bmpBody.setPixel(Position(j, 30 - i), pixel);
            }
        }
    }

    function diagonal(bytes memory bmpBody, bytes32 data) internal pure {
        PixelFactory memory factory;
        factory.init(data, 16);
        for (int16 i = 15; i >= 0; i--) {
            Pixel memory pixel = factory.create(i);
            for (int16 j = 0; j < 31 - i; j++) {
                bmpBody.setPixel(Position(i, 0).add(j, j), pixel);
                bmpBody.setPixel(Position(0, i).add(j, j), pixel);
                bmpBody.setPixel(Position(i, 30).add(j, -j), pixel);
                bmpBody.setPixel(Position(0, 30 - i).add(j, -j), pixel);
            }
        }
    }

    function diagonal2(bytes memory bmpBody, bytes32 data) internal pure {
        PixelFactory memory factory;
        factory.init(data, 16);
        for (int16 i = 0; i <= 15; i++) {
            Pixel memory pixel = factory.create(i);
            for (int16 j = 0; j < 31 - i; j++) {
                bmpBody.setPixel(Position(i, 0).add(j, j), pixel);
                bmpBody.setPixel(Position(0, i).add(j, j), pixel);
                bmpBody.setPixel(Position(i, 30).add(j, -j), pixel);
                bmpBody.setPixel(Position(0, 30 - i).add(j, -j), pixel);
            }
        }
    }
}

contract BMPNFT is ERC721, Ownable {
    using BitmapLib for bytes;

    constructor() ERC721("Hoge", "HOGE") {}

    function mint(uint256 tokenId) public onlyOwner {
        require(tokenId <= 10000, "invalid tokenId");
        _mint(_msgSender(), tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function getBMPBody(uint256 tokenId) public pure returns (bytes memory) {
        bytes32 data = keccak256(abi.encodePacked("BMPNFT Loot", tokenId));
        bytes memory bmpData = new bytes(2976);
        uint256 bmpPattern = uint256(data) % 6;
        if (bmpPattern == 0) {
            bmpData.vertical(data);
        } else if (bmpPattern == 1) {
            bmpData.horizontal(data);
        } else if (bmpPattern == 2) {
            bmpData.square(data);
        } else if (bmpPattern == 3) {
            bmpData.cross(data);
        } else if (bmpPattern == 4) {
            bmpData.diagonal(data);
        } else {
            bmpData.diagonal2(data);
        }
        return bmpData;
    }

    function getBMP(uint256 tokenId) public pure returns (string memory) {
        return
            string(
                abi.encodePacked(
                    "data:image/bmp;base64,",
                    "Qk15CwAAAAAAADYAAAAoAAAAHwAAAB8AAAABABgAAAAAAEMLAAAAAAAAAAAAAAAAAAAAAAAA",
                    Base64.encode(getBMPBody(tokenId))
                )
            );
    }

    function getSVG(uint256 tokenId) public pure returns (string memory) {
        string memory svg = string(
            abi.encodePacked(
                '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 350 350">',
                '<image x="0" y="0" height="32px" width="32px" style="image-rendering: pixelated; width:100%; height: 100%" preserveAspectRatio="xMinYMin meet" href="',
                getBMP(tokenId),
                '"/></svg>'
            )
        );
        return
            string(
                abi.encodePacked(
                    "data:image/svg+xml;base64,",
                    Base64.encode(bytes(svg))
                )
            );
    }

    function tokenURI(uint256 tokenId)
        public
        pure
        override
        returns (string memory)
    {
        string memory metadata = string(
            abi.encodePacked(
                '{"name": "BMPNFT #',
                toString(tokenId),
                '",',
                '"description": "BMPNFT",',
                '"image": "data:image/svg+xml;base64,',
                Base64.encode(bytes(getSVG(tokenId))),
                '"}'
            )
        );
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(bytes(metadata))
                )
            );
    }

    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT license
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
}

