// Latest stable version of solidity
pragma solidity ^0.8.5;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/Strings.sol";

contract Collection is ERC721, AccessControl {
    event Minted(
        address indexed operator,
        address indexed to,
        uint256 indexed id,
        uint256 amount
    );
    event MintedBatch(
        address indexed operator,
        address indexed to,
        uint256[] indexed ids
    );
    event Sold(
        address indexed operator,
        address indexed to,
        uint256 indexed id,
        uint256 price
    );
    event PaymentShared(address account, uint256 amount);
    event PaymentTreasure(address account, uint256 amount);
    event ExternalAddressesAdded(address token, address treasury);
    event NewUsdAmount(uint256 amount);
    event NewStartTime(uint256 startTime);
    event NewEndTime(uint256 endTime);

    using EnumerableSet for EnumerableSet.UintSet;
    using Strings for uint256;

    EnumerableSet.UintSet soldCards;

    IERC20 private token;

    string public baseUri;
    uint256 public amount;
    uint256 public available;
    uint256 public sold;
    uint256 public total;
    uint256 public lotBonus;
    uint256 public mintBonus;

    address public facAddress;
    address public jstTreasury;

    uint256 public startTime;
    uint256 public endTime;

    constructor(
        string memory name,
        string memory symbol,
        string memory _baseUri,
        uint256 _total,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _amount,
        uint256 _lotBonus,
        uint256 _mintBonus,
        address admin,
        address _facAddress
    ) ERC721(name, symbol) {
        amount = _amount;
        available = _total;
        lotBonus = _lotBonus;
        mintBonus = _mintBonus;
        total = _total;
        startTime = _startTime;
        endTime = _endTime;
        facAddress = _facAddress;
        baseUri = _baseUri;
        _setupRole(DEFAULT_ADMIN_ROLE, admin);
    }

    function _baseURI() internal view override returns (string memory) {
        return baseUri;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        string memory baseURI = _baseURI();
        return
            bytes(baseURI).length > 0
                ? string(abi.encodePacked(baseURI, tokenId.toString(), ".json"))
                : "";
    }

    function addExternalAddresses(address _token, address _treasury)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        token = IERC20(_token);
        jstTreasury = _treasury;
        emit ExternalAddressesAdded(_token, _treasury);
    }

    function recoverToken(address _token)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        uint256 _amount = IERC20(_token).balanceOf(address(this));
        IERC20(_token).transfer(msg.sender, _amount);
    }

    function buy(address buyer, uint256 _id) external {
        require(
            msg.sender == facAddress,
            "This function can only be called by factory contract"
        );
        require(!(soldCards.contains(_id)), "This card already sold");
        require(available > 0, "Sold Out");
        require(
            startTime <= block.timestamp && endTime > block.timestamp,
            "Sale did not start yet"
        );

        _withToken(buyer);

        _mint(buyer, _id);
        tokenURI(_id);

        available -= 1;
        sold += 1;
        soldCards.add(_id);

        emit Sold(address(this), buyer, _id, amount);
    }

    function mint(address to, uint256 _id)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(!(soldCards.contains(_id)), "This card already sold");
        require(available > 0, "Sold Out");

        _mint(to, _id);
        tokenURI(_id);

        available -= 1;
        sold += 1;
        soldCards.add(_id);

        emit Minted(address(this), to, _id, amount);
    }

    function mintBatch(address to, uint256[] memory ids)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        require(available > ids.length, "Sold Out");

        for (uint256 i = 0; i < ids.length; i++) {
            require(!(soldCards.contains(ids[i])), "This card already sold");
        }

        for (uint256 i = 0; i < ids.length; i++) {
            _mint(to, ids[i]);
            soldCards.add(ids[i]);
        }

        available -= ids.length;
        sold += ids.length;

        //emit MintedBatch(address(this), to, ids);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(ERC721, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function setStarTime(uint256 _starTime)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        startTime = _starTime;
        emit NewStartTime(startTime);
    }

    function setEndTime(uint256 _endTime)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        endTime = _endTime;
        emit NewEndTime(endTime);
    }

    function setAmount(uint256 _newAmount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        amount = _newAmount;

        emit NewUsdAmount(amount);
    }

    function getSold() external view returns (uint256) {
        return sold;
    }

    function getLotBon() external view returns (uint256) {
        return lotBonus;
    }

    function getMintBon() external view returns (uint256) {
        return mintBonus;
    }

    function _withToken(address buyer) private {
        require(
            token.balanceOf(buyer) >= amount,
            "Insufficient funds: Cannot buy this NFT"
        );

        token.transferFrom(buyer, address(this), amount);
        token.transfer(jstTreasury, amount);

