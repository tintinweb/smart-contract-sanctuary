pragma solidity ^0.8.8;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "./ICox.sol";
import "./IGame.sol";
import "./Eggs.sol";


contract Cox is ICox, ERC721, VRFConsumerBase, Ownable {

    address public constant VRF_COORDINATOR = 0xb3dCcb4Cf7a26f6cf6B120Cf5A73875B7BBc655B;
    address public constant LINK_TOKEN_ADDRESS = 0x01BE23585060835E02B77ef475b0Cc51aA1e0709;
    bytes32 public constant KEY_HASH = 0x2ed0feb3e7fd2022120aa84fab1945545a9f2ffc9076fd6156fa96eaff4c1311;
    IUniswapV2Router02 public uniswapRouter = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);

    event GasInfo (uint left);

    struct VRFRequestData {
        uint16[] tokenIds;
        bool stake;
    }

    mapping(bytes32 => VRFRequestData) public vrfRequestIds;

    uint public constant EMPTY_DNA = 0x1000;
    mapping(uint => uint) public tokenDNA;


    uint public mintsPerVRFRequest = 2;
    uint public linkFee = 2 * 10 ** 18;

    uint public foxAndHensMaxSupply = 20000;
    uint public foxAndHensSupply = 0;
    uint public huntersAndCoxMaxSupply = 3000;
    uint public huntersAndCoxSupply = 0;

    uint public huntersUnlockThreshold = 10000;

    uint public foxProbability = 10;
    uint public hunterProbability = 20;

    uint public mintPriceStep = 500;

    uint public minEthMintPrice = 0.02 ether;
    uint public ethMintStep = 0.005 ether;

    uint public minEggsMintPrice = 20000 ether;
    uint public eggsMintStep = 20000 ether;

    uint public hunterOrCoxStealChance = 30;

    uint public saleStartTimestamp = 1638569088;

    IGame public iGame;
    Eggs public eggs;

    mapping(address => bool) public operators;

    event TokenDNAUpdate(uint tokenId, uint dna);

    uint8[3][][][4] public rarities;

    constructor(
        Eggs _eggs
    ) ERC721("Cox Game", 'WCOX') VRFConsumerBase(VRF_COORDINATOR, LINK_TOKEN_ADDRESS) payable {
        eggs = _eggs;
        rarities[0] = [
            // alpha 1 - gold chicken, 1 - common chicken
            [[5,1,2],[100,1,1],[100,1,1]],
            // head
            [[5,4,3],[20,5,2],[100,11,1]],
            // wings
            [[5,3,3],[20,3,2],[100,4,1]],
            // eyes
            [[5,7,3],[20,8,2],[100,15,1]],
            // feet
            [[5,4,3],[20,5,2],[100,6,1]],
            // beak
            [[5,3,3],[20,4,2],[100,3,1]],
            // plumage
            [[5,4,3],[20,5,2],[100,6,1]]
        ];
        rarities[1] = [
            // alpha 2 -- 10-2 alpha, 3 -- 10-3 alpha, 4 -- 10-4 alpha
            [[5,1,2],[20,1,3],[100,1,4]],
            // head
            [[5,4,3],[20,5,2],[100,6,1]],
            // eyes
            [[5,5,3],[20,10,2],[100,15,1]],
            // nose
            [[5,2,3],[20,1,2],[100,2,1]],
            // neck
            [[5,6,3],[20,6,2],[100,8,1]],
            // fur
            [[5,4,3],[20,6,2],[100,5,1]]
        ];
        rarities[2] = [
        // head
            [[5,4,3],[20,4,2],[100,7,1]],
            // mouth
            [[5,3,3],[20,5,2],[100,7,1]],
            // eyes
            [[5,4,3],[20,5,2],[100,16,1]],
            // weapon
            [[5,3,3],[20,3,2],[100,4,1]],
            // shirt
            [[5,2,3],[20,4,2],[100,4,1]],
            // pants
            [[5,2,3],[20,3,2],[100,5,1]],
            // boots
            [[5,2,3],[20,4,2],[100,4,1]]
        ];
        rarities[3] = [
        // eyes
            [[5,6,3],[20,11,2],[100,13,1]],
            // beak
            [[5,2,3],[20,3,2],[100,5,1]],
            // feet
            [[5,4,3],[20,6,2],[100,10,1]],
            // head
            [[5,3,3],[20,5,2],[100,7,1]],
            // tail
            [[5,3,3],[20,6,2],[100,6,1]],
            // plumage
            [[5,4,3],[20,7,2],[100,9,1]]
        ];
    }

    function totalSupply() public view returns (uint) {
        return foxAndHensSupply + huntersAndCoxSupply;
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal virtual override {
        if (from != address(0)) {
            getType(uint16(tokenId));
        }
    }

    function isApprovedForAll(address owner, address operator) public view override(IERC721, ERC721) returns (bool) {
        if (operators[operator]) {
            return true;
        }
        return super.isApprovedForAll(owner, operator);
    }

    function mintPrice(uint amount) public view returns (uint) {
        return amount * (minEthMintPrice + (foxAndHensSupply / mintPriceStep) * ethMintStep);
    }

    function eggsPrice(uint amount) public view returns (uint) {
        return amount * (minEggsMintPrice + (huntersAndCoxSupply / mintPriceStep) * eggsMintStep);
    }

    function mintFoxOrHen(uint amount, bool stake) external payable {
        require(foxAndHensSupply + amount <= foxAndHensMaxSupply, "All tokens minted");
        require(block.timestamp >= saleStartTimestamp, "Only from");
        uint price = mintPrice(amount);
        require(price <= msg.value, "Not enough ethers");
        uint startTokenId = foxAndHensSupply + 1;
        foxAndHensSupply += amount;
        mintNFTs(startTokenId, amount, stake);
    }

    function mintHunterOrCok(uint amount, bool stake) external payable {
        require(huntersAndCoxSupply + amount <= huntersAndCoxMaxSupply, "All tokens minted");
        require(foxAndHensSupply >= huntersUnlockThreshold, "Threshold is not passed");
        eggs.burn(msg.sender, eggsPrice(amount));
        uint startTokenId = huntersAndCoxSupply + 1;
        huntersAndCoxSupply += amount;
        mintNFTs(startTokenId, amount, stake);
    }


    function mintNFTs(uint startTokenId, uint amount, bool stake) internal {
        require(tx.origin == _msgSender(), "Only EOA");
        require(amount > 0 && amount <= 10, "Invalid mint amount");
        uint256 vrfRequestsNeeded = (amount + mintsPerVRFRequest - 1) / mintsPerVRFRequest;

        address[] memory path = new address[](2);
        path[0] = uniswapRouter.WETH();
        path[1] = LINK_TOKEN_ADDRESS;
        if (LINK.balanceOf(address(this)) < linkFee * vrfRequestsNeeded) {
            uniswapRouter.swapETHForExactTokens {value : 0.2 ether}(linkFee * vrfRequestsNeeded, path, address(this), block.timestamp + 1 days);
            require(LINK.balanceOf(address(this)) >= linkFee * vrfRequestsNeeded, "Not enough LINK");
        }

        bytes32 requestId;

        requestId = requestRandomness(KEY_HASH, linkFee);
        VRFRequestData storage vrfRequest = vrfRequestIds[requestId];
        vrfRequest.stake = stake;

        for (uint8 i = 0; i < amount; i++) {
            // Call Chainlink every mintsPerVRFRequest mints
            if (i != 0 && i % mintsPerVRFRequest == 0) {
                requestId = requestRandomness(KEY_HASH, linkFee);
                vrfRequest = vrfRequestIds[requestId];
                vrfRequest.stake = stake;
            }

            uint tokenId = startTokenId + i;
            tokenDNA[tokenId] = EMPTY_DNA;
            vrfRequest.tokenIds.push(uint16(tokenId));
            _safeMint(msg.sender, tokenId);
        }
    }



    function getAlpha(uint16 tokenId) public view override returns (uint16) {
        Type tokenType = getType(tokenId);
        if (tokenType == Type.CHICKEN) {
            if (selectGene(uint8(tokenType), tokenId, 0) / 100 == 1) {
                return 1;
            } else {
                return 3;
            }
        } else if (tokenType == Type.FOX) {
            return 10 - selectGene(uint8(tokenType), tokenId, 0) / 100;
        } else {
            revert("Alpha is not exist for this token type");
        }
    }

    function getTraitValue(uint16 tokenId, uint16 trait) public view override returns (uint16) {
        return selectGene(uint8(getType(tokenId)), tokenId, trait);
    }

    function getType(uint16 tokenId) public view override returns (Type) {
        require(isTokenHasMeta(tokenId), "Token metadata is not initialized");
        uint16 seed = uint16(tokenDNA[tokenId]);
        uint8 tokenType;
        if (tokenId <= foxAndHensMaxSupply) {
            tokenType = seed % 100 < foxProbability ? 2 : 1;
        } else {
            tokenType = seed % 100 < hunterProbability ? 3 : 4;
        }
        return Type(tokenType - 1);
    }

    function selectGene(uint8 tokenType, uint16 tokenId, uint16 trait) view public returns (uint16) {
        uint16 seed = uint16(tokenDNA[tokenId] >> (16 * (trait + 1)));
        uint8 groupF = uint8(seed) % 100;
        uint8 selectedGroup = 0xFF;
        for(uint8 i = 0; i < rarities[tokenType][trait].length; i++) {
           if (groupF < rarities[tokenType][trait][i][0]) {
               selectedGroup = i;
               break;
           }
        }
        require(selectedGroup != 0xFF, "Wrong group configuration");
        return rarities[tokenType][trait][selectedGroup][2] * 100
            + uint8(seed >> 8) % rarities[tokenType][trait][selectedGroup][1] + 1;
    }

    function isTokenHasMeta(uint16 tokenId) public view returns(bool) {
        return tokenDNA[tokenId] != EMPTY_DNA && tokenDNA[tokenId] != 0;
    }

    function fulfillRandomness(bytes32 requestId, uint256 randomNumber) internal override {
        emit GasInfo(gasleft());
        uint16[] memory tokenIds = vrfRequestIds[requestId].tokenIds;
        bool stake = vrfRequestIds[requestId].stake;
        emit GasInfo(gasleft());
        delete vrfRequestIds[requestId];
        emit GasInfo(gasleft());

        for (uint8 i = 0; i < tokenIds.length; i++) {
            uint16 tokenId = tokenIds[i];
            require(!isTokenHasMeta(tokenId), "DNA already set");
            uint256 seed;
            if (i == 0) {
                seed = randomNumber;
            } else {
                seed = uint256(keccak256(abi.encode(randomNumber, 100 * i)));
            }
            tokenDNA[tokenId] = seed;
            emit TokenDNAUpdate(tokenId, seed);
        }
        emit GasInfo(gasleft());
        if (stake) {
            address acc = ownerOf(tokenIds[0]);
            try iGame.stake(acc, tokenIds) {
                for (uint16 i = 0; i < tokenIds.length; i++) {
                    _transfer(acc, address(iGame), tokenIds[i]);
                }
            } catch(bytes memory lowLevelData) {
            }
        }
        emit GasInfo(gasleft());
    }

    function fullfillRandomnessManually(bytes32 requestId, uint256 randomNumber) external onlyOwner {
        fulfillRandomness(requestId, randomNumber);
    }


    function retryChainlinkRequest(uint16 tokenId) external onlyOwner {
        require(_exists(tokenId), "Token does not exist");
        require(!isTokenHasMeta(tokenId), "Already has DNA set - cannot be changed");
        require(LINK.balanceOf(address(this)) >= linkFee, "Not enough LINK");

        bytes32 requestId = requestRandomness(KEY_HASH, linkFee);
        uint16[] memory tokenIds = new uint16[](1);
        tokenIds[0] = tokenId;
        vrfRequestIds[requestId] = VRFRequestData({
            stake: false,
            tokenIds: tokenIds
        });
    }

    receive() external payable {

    }

    function withdraw() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    function withdrawLINK() external onlyOwner {
        LINK.transfer(owner(), LINK.balanceOf(address(this)));
    }

    // SETTEERS AND CONFIGURE REGION
    function addOperator(address operator) external onlyOwner {
        operators[operator] = true;
    }

    function removeOperator(address operator) external onlyOwner {
        operators[operator] = false;
    }

    function setMintsPerVRFRequest(uint _mintsPerVRFRequest) external onlyOwner {
        mintsPerVRFRequest = _mintsPerVRFRequest;
    }

    function setLinkFee(uint _linkFee) external onlyOwner {
        linkFee = _linkFee;
    }

    function setFoxAndHensMaxSupply(uint _foxAndHensMaxSupply) external onlyOwner {
        foxAndHensMaxSupply = _foxAndHensMaxSupply;
    }

    function setHuntersAndCoxMaxSupply(uint _huntersAndCoxMaxSupply) external onlyOwner {
        huntersAndCoxMaxSupply = _huntersAndCoxMaxSupply;
    }

    function setHuntersUnlockThreshold(uint _huntersUnlockThreshold) external onlyOwner {
        huntersUnlockThreshold = _huntersUnlockThreshold;
    }

    function setFoxProbability(uint _foxProbability) external onlyOwner {
        foxProbability = _foxProbability;
    }

    function setHunterProbability(uint _hunterProbability) external onlyOwner {
        hunterProbability = _hunterProbability;
    }

    function setMintPriceStep(uint _mintPriceStep) external onlyOwner {
        mintPriceStep = _mintPriceStep;
    }

    function setMinEthMintPrice(uint _minEthMintPrice) external onlyOwner {
        minEthMintPrice = _minEthMintPrice;
    }

    function setEthMintStep(uint _ethMintStep) external onlyOwner {
        ethMintStep = _ethMintStep;
    }

    function setMinEggsMintPrice(uint _minEggsMintPrice) external onlyOwner {
        minEggsMintPrice = _minEggsMintPrice;
    }

    function setEggsMintStep(uint _eggsMintStep) external onlyOwner {
        eggsMintStep = _eggsMintStep;
    }

    function setHunterOrCoxStealChance(uint _hunterOrCoxStealChance) external onlyOwner {
        hunterOrCoxStealChance = _hunterOrCoxStealChance;
    }

    function setSaleStartTimestamp(uint _saleStartTimestamp) external onlyOwner {
        saleStartTimestamp = _saleStartTimestamp;
    }

    function setIGame(IGame _iGame) external onlyOwner {
        iGame = _iGame;
    }

    function setEggs(Eggs _eggs) external onlyOwner {
        eggs = _eggs;
    }


    function configure(
        uint _foxAndHensMaxSupply,
        uint _huntersAndCoxMaxSupply,
        uint _huntersUnlockThreshold,
        uint _foxProbability,
        uint _hunterProbability,
        uint _mintPriceStep,
        uint _minEthMintPrice,
        uint _ethMintStep,
        uint _minEggsMintPrice,
        uint _eggsMintStep,
        uint _hunterOrCoxStealChance
    ) external onlyOwner {
        foxAndHensMaxSupply = _foxAndHensMaxSupply;
        huntersAndCoxMaxSupply = _huntersAndCoxMaxSupply;
        huntersUnlockThreshold = _huntersUnlockThreshold;
        foxProbability = _foxProbability;
        hunterProbability = _hunterProbability;
        mintPriceStep = _mintPriceStep;
        minEthMintPrice = _minEthMintPrice;
        ethMintStep = _ethMintStep;
        minEggsMintPrice = _minEggsMintPrice;
        eggsMintStep = _eggsMintStep;
        hunterOrCoxStealChance = _hunterOrCoxStealChance;
    }
}

