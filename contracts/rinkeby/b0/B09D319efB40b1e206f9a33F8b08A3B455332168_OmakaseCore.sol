pragma solidity ^0.8.4;

import {InsurancePackage, LibInsurancePackage} from "./InsurancePackage.sol";
import {Errors} from "../libraries/Errors.sol";

struct Policy {
    bytes32 flightId;
    bytes32 userHash;
    bytes32 packageId;
}

struct PolicyInitArgs {
    bytes32 userHash;
    bytes32 packageId;
}

library LibPolicy {
    using LibInsurancePackage for InsurancePackage;

    function initialize(
        Policy storage self,
        mapping(bytes32 => InsurancePackage) storage insurancePackages,
        PolicyInitArgs calldata args
    ) internal {
        require(isEmpty(self), Errors.POL_ALREADY_REGISTERED);

        self.userHash = args.userHash;
        self.packageId = args.packageId;

        require(
            !insurancePackages[self.packageId].isEmpty(),
            Errors.POL_INVALID_INSURANCE_PACKAGE
        );
    }

    function isEmpty(Policy storage self) internal view returns (bool) {
        return self.userHash == 0;
    }
}

