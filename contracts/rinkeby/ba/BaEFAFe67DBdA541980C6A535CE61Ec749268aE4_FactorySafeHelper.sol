pragma solidity ^0.8.0;

import {
    DaoModule,
    Executor,
    Realitio
} from "@gnosis/dao-module/contracts/DaoModule.sol";
import "hardhat/console.sol";

import {IFactorySafeHelper} from "./interfaces/IFactorySafeHelper.sol";
import {
    IGnosisSafeProxyFactory
} from "./interfaces/IGnosisSafeProxyFactory.sol";
import {IGnosisSafe} from "./interfaces/IGnosisSafe.sol";

contract FactorySafeHelper is IFactorySafeHelper {
    IGnosisSafeProxyFactory public immutable GNOSIS_SAFE_PROXY_FACTORY;
    address public immutable GNOSIS_SAFE_TEMPLATE_ADDRESS;
    uint256 public immutable REALITIO_TEMPLATE_ID;

    constructor(
        address proxyFactoryAddress,
        address safeTemplateAddress,
        uint256 realitioTemplateId
    ) {
        GNOSIS_SAFE_PROXY_FACTORY = IGnosisSafeProxyFactory(
            proxyFactoryAddress
        );
        GNOSIS_SAFE_TEMPLATE_ADDRESS = safeTemplateAddress;
        REALITIO_TEMPLATE_ID = realitioTemplateId;
    }

    function createAndSetupSafe(bytes32 salt)
        external
        override
        returns (address safeAddress, address)
    {
        salt = keccak256(abi.encodePacked(salt, msg.sender, address(this)));
        console.log("Got safe salt", uint256(salt));
        // Deploy and initialize safe
        IGnosisSafe safe =
            IGnosisSafe(
                payable(
                    address(
                        GNOSIS_SAFE_PROXY_FACTORY.createProxyWithNonce(
                            GNOSIS_SAFE_TEMPLATE_ADDRESS,
                            "",
                            uint256(salt)
                        )
                    )
                )
            );
        address[] memory owners = new address[](1);
        owners[0] = msg.sender;
        safe.setup(
            owners, // owners
            1, // threshold
            address(0), // to
            "", // data
            address(0), // fallbackHandler
            address(0), // paymentToken
            0, // payment
            payable(0) // paymentReceiver
        );
        safeAddress = address(safe);
        DaoModule daoModule =
            new DaoModule{salt: ""}(
                Executor(safeAddress),
                Realitio(address(0)), // TODO realitio address
                10**6, // TODO timeout
                0, // TODO cd
                0, // TODO expiration
                0, // TODO bond
                REALITIO_TEMPLATE_ID
            );
        // TODO enable dao module
        // TODO [optional] remove address(this) from safe owners

        return (safeAddress, address(daoModule));
    }
}

