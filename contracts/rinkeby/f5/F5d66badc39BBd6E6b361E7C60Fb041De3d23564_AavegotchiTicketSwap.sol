import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./AavegotchiTicketERC20Token.sol";
import "./AavegotchiTicketLpToken.sol";
import "./AavegotchiTicketPool.sol";
import "./String.sol";

contract AavegotchiTicketSwap is Context, Ownable, ERC165, IERC1155Receiver, ReentrancyGuard {
  struct Ticket {
    bool supported;
    address tokenAddress;
    address poolAddress;
  } 

  IERC1155 public ticketToken;
  IERC20 public daiToken;
  AavegotchiTicketLpToken public lpToken;
  address public treasury;

  mapping(uint256 => Ticket) public tickets;

  event TicketSetup(uint256 indexed id, address pool, address token);
  event Swapped(address indexed sender, uint256[] fromIds, uint256[] fromValues, uint256[] toIds, uint256[] toValues, int256 daiDelta);
  event TokenSwapped(address indexed sender, uint256 fromId, uint256 amountIn, uint256 toId, uint256 amountOut);
  event TicketWithdrawn(uint256 id, uint256 amount);
  event DaiWithdrawn(uint256 amount);

  event TicketConverted(address indexed sender, uint256 id, uint256 amount);

  constructor(address ticket_, address dai_, address lp_, address treasury_) {
    ticketToken = IERC1155(ticket_);
    daiToken = IERC20(dai_);
    lpToken = AavegotchiTicketLpToken(lp_);
    treasury = treasury_;
  }
  /* max code size exceeded
  function estimateSwapFee(
    uint256[] calldata fromIds_,
    uint256[] calldata fromValues_,
    uint256[] calldata toIds_,
    uint256[] calldata toValues_) 
  public view returns(uint256) {
    uint256 fee;
    for(uint256 i; i < fromIds_.length; i++) {
      fee += AavegotchiTicketPool(tickets[fromIds_[i]].poolAddress).fee() * fromValues_[i];
    }

    for(uint256 i; i < toIds_.length; i++) {
      fee += AavegotchiTicketPool(tickets[toIds_[i]].poolAddress).fee() * toValues_[i];
    }

    return fee;
  }

  function estimateSwapDai(
    uint256[] calldata fromIds_,
    uint256[] calldata fromValues_,
    uint256[] calldata toIds_,
    uint256[] calldata toValues_) 
  public view returns(int256) {
    int256 daiBalance;
    for(uint256 i; i < fromIds_.length; i++) {
      daiBalance += int256(AavegotchiTicketPool(tickets[fromIds_[i]].poolAddress).estimateSwapToDaiByToken(fromValues_[i]));
    }

    for(uint256 i; i < toIds_.length; i++) {
      daiBalance -= int256(AavegotchiTicketPool(tickets[fromIds_[i]].poolAddress).estimateSwapFromDaiByToken(toValues_[i]));
    }

    return daiBalance;
  }

  */

  function setupTickets(uint256[] calldata ids_, uint256[] calldata fees_) external onlyOwner {
    require(ids_.length == fees_.length, "AavegotchiTicketSwap: Mismatch in ids and fees length");
    for(uint256 i = 0; i < ids_.length; i++) {
      AavegotchiTicketERC20Token token = new AavegotchiTicketERC20Token(String.concat("AavegotchiTicketToken_", ids_[i]), String.concat("ATT_", ids_[i]));
      AavegotchiTicketPool pool = new AavegotchiTicketPool(ids_[i], address(daiToken), address(token), treasury, address(lpToken), fees_[i]);
      lpToken.grantRole(lpToken.MANAGER_ROLE(), address(pool));

      tickets[ids_[i]] = Ticket({
        supported: true,
        poolAddress: address(pool),
        tokenAddress: address(token)
      });

      emit TicketSetup(ids_[i], address(pool), address(token));
    }
  }

  function convertNftToToken(uint256 id_, uint256 amount_) external {
    require(tickets[id_].supported, "AavegotchiTicketSwap: Ticket not supported");
    ticketToken.safeTransferFrom(_msgSender(), address(this), id_, amount_, "");
    AavegotchiTicketERC20Token(tickets[id_].tokenAddress).mint(_msgSender(), amount_ * 1 ether);
    emit TicketConverted(_msgSender(), id_, amount_);
  }

  function convertTokenToNft(uint256 id_, uint256 amount_) external {
    require(tickets[id_].supported, "AavegotchiTicketSwap: Ticket not supported");
    AavegotchiTicketERC20Token(tickets[id_].tokenAddress).burn(_msgSender(), amount_ * 1 ether);
    ticketToken.safeTransferFrom(address(this), _msgSender(), id_, amount_, "");    
    emit TicketConverted(_msgSender(), id_, amount_);
  }

  function estimateTokenSwapOut(uint256 tokenId0_, uint256 tokenId1_, uint256 amount_) public view returns(uint256) {
    return AavegotchiTicketPool(tickets[tokenId1_].poolAddress).estimateSwapFromDaiByDai(
      AavegotchiTicketPool(tickets[tokenId0_].poolAddress).estimateSwapToDaiByToken(amount_)
    );
  }

  function swapToken(uint256 tokenId0_, uint256 amount0_, uint256 tokenId1_, uint256 amount1Min_) external nonReentrant {
    require(tokenId0_ != tokenId1_, "AavegotchiTicketSwap: token ids is equal");
    require(tickets[tokenId0_].supported, "AavegotchiTicketSwap: token 0 is not supported");
    require(tickets[tokenId1_].supported, "AavegotchiTicketSwap: token 1 is not supported");

    uint256 token1Out = estimateTokenSwapOut(tokenId0_, tokenId1_, amount0_);
    require(token1Out >= amount1Min_, "AavegotchiTicketSwap: min swap amount reached");

    AavegotchiTicketERC20Token(tickets[tokenId0_].tokenAddress).transferFrom(_msgSender(), address(this), amount0_);
    uint256 daiAmount = AavegotchiTicketPool(tickets[tokenId0_].poolAddress).swapToDaiByToken(amount0_);
    uint256 out = AavegotchiTicketPool(tickets[tokenId1_].poolAddress).swapFromDaiByDai(daiAmount);
    AavegotchiTicketERC20Token(tickets[tokenId1_].tokenAddress).transferFrom(address(this), _msgSender(), out);

    emit TokenSwapped(_msgSender(), tokenId0_, amount0_, tokenId1_, token1Out);
  }

  function swap(
    uint256[] calldata fromIds_,
    uint256[] calldata fromValues_,
    uint256[] calldata toIds_,
    uint256[] calldata toValues_)
  external nonReentrant {
    require(fromIds_.length == fromValues_.length, "AavegotchiTicketSwap: Mismatch in fromIds and fromValues length");
    require(toIds_.length == toValues_.length, "AavegotchiTicketSwap: Mismatch in toIds and toValues length");

    uint256 beforeBalance = daiToken.balanceOf(address(this));
 
    for(uint256 i; i < fromIds_.length; i++) {
      require(tickets[fromIds_[i]].supported, "AavegotchiTicketSwap: From ticket not supported");
      AavegotchiTicketERC20Token(tickets[fromIds_[i]].tokenAddress).transferFrom(_msgSender(), address(this), fromValues_[i]);
      AavegotchiTicketPool(tickets[fromIds_[i]].poolAddress).swapToDaiByToken(fromValues_[i]);
    }

    for(uint256 i; i < toIds_.length; i++) {
      require(tickets[toIds_[i]].supported, "AavegotchiTicketSwap: To ticket not supported");
      AavegotchiTicketPool(tickets[toIds_[i]].poolAddress).swapFromDaiByToken(toValues_[i]);
      AavegotchiTicketERC20Token(tickets[toIds_[i]].tokenAddress).transfer(_msgSender(), toValues_[i]);
    }

    int256 delta = int256(daiToken.balanceOf(address(this))) - int256(beforeBalance);
    if (delta > 0) {
      daiToken.transfer(_msgSender(), uint256(delta));
    } else {
      daiToken.transferFrom(address(this), _msgSender(), uint256(delta * -1));
    }

    emit Swapped(_msgSender(), fromIds_, fromValues_, toIds_, toValues_, delta);
  }

  function withdrawTickets(uint256 id_, uint256 amount_) external onlyOwner {
    ticketToken.safeTransferFrom(address(this), _msgSender(), id_, amount_, "0x01");

    emit TicketWithdrawn(id_, amount_);
  }

  function withdrawDai(uint256 amount_) external onlyOwner {
    daiToken.transferFrom(address(this), _msgSender(), amount_);

    emit DaiWithdrawn(amount_);
  }
  
  function onERC1155Received(address, address, uint256, uint256, bytes calldata) 
  public virtual override returns (bytes4) {
    require(_msgSender() == address(ticketToken), "AavegotchiTicketSwap: Only ticket tokens allowed");
    return this.onERC1155Received.selector;
  }

  function onERC1155BatchReceived(address, address, uint256[] calldata, uint256[] calldata, bytes calldata)
  public virtual override returns (bytes4) {
    require(_msgSender() == address(ticketToken), "AavegotchiTicketSwap: Only ticket tokens allowed");
    return this.onERC1155BatchReceived.selector;
  }
}

