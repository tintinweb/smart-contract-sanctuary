pragma solidity ^0.8.7;

import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "./IAavegotchiTicketLpToken.sol";

contract AavegotchiTicketLpToken is IAavegotchiTicketLpToken, ERC1155, AccessControl {
  constructor() ERC1155("") {
    _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
  }

  modifier onlyManager() {
    require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()) || hasRole(MANAGER_ROLE, _msgSender()), "AavegotchiTicketLpToken: caller is not a manager");
    _;
  }

  modifier onlyOwner() {
    require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "AavegotchiTicketLpToken: caller is not a manager");
    _;
  }

  function setConfig(uint256[] calldata _ids, bool[] calldata _supported) external override onlyOwner {
    require(_ids.length == _supported.length, "AavegotchiTicketLpToken: invalid arrays len");

    for (uint256 i; i < _ids.length; i++) {
      supportedIds[_ids[i]] = _supported[i];
    }
  }

  function mint(address _to, uint256 _id, uint256 _amount) external override onlyManager {
    require(supportedIds[_id], "AavegotchiTicketLpToken: id not allowed");
    
    _mint(_to, _id, _amount, "");
    totalSupply[_id] += _amount;
  }

  function burn(address _from, uint256 _id, uint256 _amount) external override onlyManager {
    require(supportedIds[_id], "AavegotchiTicketLpToken: id not allowed");
    
    totalSupply[_id] -= _amount;
    _burn(_from, _id, _amount);
  }

  function supportsInterface(bytes4 interfaceId) 
    public virtual override(ERC1155, AccessControl, IERC165) view returns (bool) {
      return interfaceId == this.supportsInterface.selector;
  }
}

