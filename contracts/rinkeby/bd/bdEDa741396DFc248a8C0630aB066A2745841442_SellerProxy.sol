pragma solidity ^0.8.7;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "../structs/proxy/ProxyData.sol";
import "../library/UintSet.sol";
import "hardhat/console.sol";
enum PaymentType {
    TOKEN,
    ETHER,
    NONE
}

contract SellerProxy is AccessControl {
    using UintSet for Set;

    event PaymentShared(
        address buyer,
        address collection,
        uint256 amount,
        uint8 payType
    );
    event PaymentTreasure(
        address buyer,
        address collection,
        uint256 amount,
        uint8 payType
    );
    event ExternalAddressesAdded(address token, address treasury);
    event Sold(
        address indexed buyer,
        address indexed collection,
        uint256 indexed tokenId,
        uint256 price,
        uint8 payType,
        string username
    );
    event NewStartTime(uint256 startTime);
    event NewEndTime(uint256 endTime);
    event NewPrice(address collection, uint256 price);
    event NewArtist(address collection, address artist);
    event NewPercent(uint256 percent);
    event Initialize(bool isInit);

    // Polygon collection => User => token IDs
    mapping(address => mapping(address => Set)) private addressToTokenId;

    // Polygon collection => Artist
    mapping(address => address) collectionToArtist;

    // Polygon collection => Card Price
    mapping(address => uint256) collectionPrice;

    // Polygon collection => Sold Card IDs
    mapping(address => Set) soldCards;

    IERC20 private token;
    // Total sold card amount for all collection
    uint256 public totalSold;
    address public treasury;

    uint256 private startTime_;
    uint256 private endTime_;
    uint256 private percent_;

    constructor(ProxyData memory proxyData) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        startTime_ = proxyData.startTime;
        endTime_ = proxyData.endTime;
        percent_ = proxyData.percent;

        setExternalAddresses(proxyData.token, proxyData.treasury);
        emit Initialize(true);
    }

    function tokenEmergency(address _token, uint256 _amount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        IERC20(_token).transfer(msg.sender, _amount);
    }

    function etherEmergency(uint256 _amount)
        external
        payable
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        (bool success, ) = _msgSender().call{value: _amount}("");
        require(success, "Revert: There was a problem sending ether to admin");
    }

    function buy(
        address _collection,
        uint256 _id,
        string memory _username,
        PaymentType _payType
    ) external payable {
        require(
            !(soldCards[_collection].contains(_id)),
            "This card already sold"
        );
        require(
            startTime_ <= block.timestamp && endTime_ > block.timestamp,
            "Sale did not start yet"
        );

        if (_payType == PaymentType.TOKEN) {
            _withToken(msg.sender, _collection);
        } else {
            _withEther(msg.sender, _collection);
        }

        addressToTokenId[_collection][msg.sender].add(_id);

        soldCards[_collection].add(_id);
        totalSold += 1;

        emit Sold(
            msg.sender,
            _collection,
            _id,
            collectionPrice[_collection],
            uint8(_payType),
            _username
        );
    }

    function reserveIdPrivate(
        string memory _username,
        address _collection,
        address _to,
        uint256 _id
    ) external onlyRole(DEFAULT_ADMIN_ROLE) {
        require(
            !(soldCards[_collection].contains(_id)),
            "This card already sold"
        );

        addressToTokenId[_collection][msg.sender].add(_id);
        soldCards[_collection].add(_id);
        totalSold += 1;

        emit Sold(
            _to,
            _collection,
            _id,
            collectionPrice[_collection],
            uint8(PaymentType.NONE),
            _username
        );
    }

    function setStarTime(uint256 _starTime)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        startTime_ = _starTime;
        emit NewStartTime(startTime_);
    }

    function getStartTime() public view returns (uint256) {
        return startTime_;
    }

    function setEndTime(uint256 _endTime)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        endTime_ = _endTime;
        emit NewEndTime(endTime_);
    }

    function getEndTime() public view returns (uint256) {
        return endTime_;
    }

    function setCollectionPrice(address _collection, uint256 _newAmount)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        collectionPrice[_collection] = _newAmount;
        emit NewPrice(_collection, _newAmount);
    }

    function getCollectionPrice(address _collection)
        public
        view
        returns (uint256)
    {
        return collectionPrice[_collection];
    }

    function setCollectionArtist(address _collection, address _artist)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        collectionToArtist[_collection] = _artist;

        emit NewArtist(_collection, _artist);
    }

    function getCollectionArtist(address _collection)
        public
        view
        returns (address)
    {
        return collectionToArtist[_collection];
    }

    function setPercent(uint256 _percent)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        percent_ = _percent;
        emit NewPercent(_percent);
    }

    function getPercent() external view returns (uint256) {
        return percent_;
    }

    function setExternalAddresses(address _token, address _treasury)
        public
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        token = IERC20(_token);
        treasury = _treasury;

        emit ExternalAddressesAdded(_token, _treasury);
    }

    function getExternalAddresses() public view returns (address, address) {
        return (address(token), treasury);
    }

    function getSold(address _collection) external view returns (uint256) {
        return soldCards[_collection].getArray().length;
    }

    function getAddressToTokenId(address collection, address buyer)
        external
        view
        returns (uint256[] memory)
    {
        return addressToTokenId[collection][buyer].getArray();
    }

    function _calcPerc(uint256 _amount) private view returns (uint256) {
        uint256 sellmul = _amount * percent_;
        uint256 sellAmount = sellmul / 1e18;
        return sellAmount;
    }

    function _withToken(address _buyer, address _collection) private {
        uint256 amount = getCollectionPrice(_collection);
        address artist = getCollectionArtist(_collection);
        require(
            token.balanceOf(_buyer) >= amount,
            "Insufficient funds: Cannot buy this NFT"
        );

        uint256 treasAmount = _calcPerc(amount);
        uint256 shareAmount = amount - treasAmount;

        token.transferFrom(_buyer, treasury, treasAmount);
        token.transferFrom(_buyer, artist, shareAmount);

        emit PaymentTreasure(_buyer, _collection, treasAmount, 0);
        emit PaymentShared(_buyer, _collection, shareAmount, 0);
    }

    function _withEther(address _buyer, address _collection) private {
        uint256 amount = getCollectionPrice(_collection);
        address artist = getCollectionArtist(_collection);
        require(msg.value >= amount, "Insufficient funds: Cannot buy this NFT");

        uint256 treasAmount = _calcPerc(amount);
        uint256 shareAmount = amount - treasAmount;

        (bool success, ) = payable(treasury).call{value: treasAmount}("");
        require(
            success,
            "Revert: There was a problem sending ether to treasury"
        );

        (success, ) = payable(artist).call{value: shareAmount}("");
        require(success, "Revert: There was a problem sending ether to artist");

        emit PaymentTreasure(_buyer, _collection, treasAmount, 1);
        emit PaymentShared(_buyer, _collection, shareAmount, 1);
    }

    fallback() external payable {}
}

