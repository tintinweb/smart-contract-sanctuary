pragma solidity 0.6.12;

// Contracts
import "@openzeppelin/contracts/presets/ERC721PresetMinterPauserAutoId.sol";

// Interfaces
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "../tokens/IMintableERC20.sol";
import "hardhat/console.sol"; // TODO remove

// Libraries
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/EnumerableSet.sol";
import "../utils/VaultsCommon.sol";
import "../libs/AddressesLib.sol";

library VaultMetadataLib {
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    using Address for address;
    using AddressesLib for address[];

    struct VaultMetadata {
        bytes32 vaultId;
        uint256 seasonId;
        uint256 startBlockNumber;
        uint256 endBlockNumber;
        uint256 endClaimBlockNumber;
        address creator;
        address[] tokens;
        address[] rewards;
        address[] boosts;
        mapping(address => uint256) rewardAmounts;
        /*
            It defines whether an address is supported in the vault.

            It includes:
                - Input tokens.
                - Output tokens.
                - Boost tokens.
                
            Boost tokens must must implement the IBoostWrapper interface.
         */
        mapping(address => bool) tokensSupported;
        /*
            It maps the max capacity for each stakeable/input token (ERC20 and ERC721).

            As we might have the same token as input and output, this max capacity mapping ONLY considers the input tokens.

            To test whether a token (input/output) is supported or not, use the tokensSupported mapping.
        */
        mapping(address => uint256) maxCapacity;
        /*
            The vault creator is only added as owner in the creation process.
            There is an 'add/removeOwner' function to add/remove accounts.
        */
        mapping(address => bool) owners;
        /*
            It maps the mode to transfer the reward tokens to the user.
            Example:
                RewardToken(0x123123...123123) => TransferMode.Mint
         */
        mapping(address => VaultsCommon.TransferMode) mode;
        /*
            It maps whether an address is an NFT or not.
            It includes:
                - Input tokens.
         */
        mapping(address => bool) nftsSupported;
    }

    function create(
        VaultMetadata storage self,
        bytes32 vaultId,
        uint256 seasonId,
        uint256 startBlockNumber,
        uint256 endBlockNumber,
        uint256 endClaimBlockNumber,
        address[] memory tokens,
        address[] memory rewards,
        uint256[] memory rewardAmounts,
        uint256[] memory tokensMaxCap
    ) internal {
        requireNotExists(self);
        require(vaultId != "", "VAULT_ID_REQUIRED");
        require(block.number <= startBlockNumber, "START_BLOCK_NUMBER_INVALID");
        require(startBlockNumber < endBlockNumber, "BLOCK_NUMBERS_INVALID");
        require(tokens.length > 0, "TOKENS_ARE_REQUIRED");
        require(rewards.length > 0, "REWARDS_ARE_REQUIRED");
        require(rewards.length == rewardAmounts.length, "TOTAL_REWARDS_LENGTH_INVALID");
        require(tokens.length == tokensMaxCap.length, "TOKENS_MAX_CAP_LENGTH_INVALID");

        self.owners[msg.sender] = true;
        self.tokens = tokens;
        self.rewards = rewards;
        self.creator = msg.sender;
        for (uint256 index = 0; index < tokens.length; index++) {
            require(tokens[index].isContract(), "TOKEN_MUST_BE_CONTRACT");
            self.maxCapacity[tokens[index]] = tokensMaxCap[index];
            self.tokensSupported[tokens[index]] = true;
        }
        for (uint256 index = 0; index < rewards.length; index++) {
            require(rewards[index].isContract(), "REWARD_MUST_BE_CONTRACT");
            self.rewardAmounts[rewards[index]] = rewardAmounts[index];
            self.mode[rewards[index]] = VaultsCommon.TransferMode.Mint;
            self.tokensSupported[rewards[index]] = true;
        }
        self.vaultId = vaultId;
        self.seasonId = seasonId;
        self.startBlockNumber = startBlockNumber;
        self.endBlockNumber = endBlockNumber;
        self.endClaimBlockNumber = endClaimBlockNumber;
    }

    function addSupportedBoost(VaultMetadata storage self, address boost) internal {
        require(exists(self), "VAULT_DOESNT_EXIST");
        addSupportedToken(self, boost);
        self.boosts.add(boost);
    }

    function removeSupportedBoost(VaultMetadata storage self, address boost) internal {
        require(exists(self), "VAULT_DOESNT_EXIST");
        removeSupportedToken(self, boost);
        self.boosts.remove(boost);
    }

    function addAsNFT(VaultMetadata storage self, address token) internal {
        require(exists(self), "VAULT_DOESNT_EXIST");
        requireIsTokenSupported(self, token);
        self.nftsSupported[token] = true;
    }

    function removeAsNFT(VaultMetadata storage self, address token) internal {
        require(exists(self), "VAULT_DOESNT_EXIST");
        self.nftsSupported[token] = false;
    }

    function setTransferRewardsMode(
        VaultMetadata storage self,
        address reward,
        VaultsCommon.TransferMode mode
    ) internal {
        require(reward.isContract(), "REWARD_MUST_BE_CONTRACT");
        self.mode[reward] = mode;
    }

    function addSupportedToken(VaultMetadata storage self, address token) internal {
        require(token.isContract(), "TOKEN_MUST_BE_CONTRACT");
        require(!self.tokensSupported[token], "TOKEN_IS_ALREADY_SUPPORTED");
        self.tokensSupported[token] = true;
    }

    function removeSupportedToken(VaultMetadata storage self, address token) internal {
        require(self.tokensSupported[token], "TOKEN_ISNT_SUPPORTED");
        self.tokensSupported[token] = false;
    }

    function addOwner(VaultMetadata storage self, address account) internal {
        requireIsCreator(self, msg.sender);
        require(!self.owners[account], "ACCOUNT_ALREADY_IS_OWNER");
        self.owners[account] = true;
    }

    function removeOwner(VaultMetadata storage self, address account) internal {
        requireIsCreator(self, msg.sender);
        require(self.owners[account], "ACCOUNT_ISNT_OWNER");
        self.owners[account] = false;
    }

    /** View Functions */

    function requireIsOwner(VaultMetadata storage self, address account) internal view {
        require(isOwner(self, account), "ACCOUNT_ISNT_VAULT_OWNER");
    }

    function isOwner(VaultMetadata storage self, address account) internal view returns (bool) {
        return self.owners[account];
    }

    function isNFT(VaultMetadata storage self, address token) internal view returns (bool) {
        return self.nftsSupported[token];
    }

    function exists(VaultMetadata storage self) internal view returns (bool) {
        return self.creator != address(0x0);
    }

    function getRewardAmountsFor(VaultMetadata storage self, address rewardToken)
        internal
        view
        returns (uint256)
    {
        return self.rewardAmounts[rewardToken];
    }

    function getMaxCapacity(VaultMetadata storage self, address token)
        internal
        view
        returns (uint256)
    {
        return self.maxCapacity[token];
    }

    function isTokenSupported(VaultMetadata storage self, address token)
        internal
        view
        returns (bool)
    {
        return self.tokensSupported[token];
    }

    function requireIsTokenSupported(VaultMetadata storage self, address token) internal view {
        require(isTokenSupported(self, token), "VAULT_DOESNT_SUPPORT_TOKEN");
    }

    function requireIsCreator(VaultMetadata storage self, address account) internal view {
        require(isCreator(self, account), "ACCOUNT_ISNT_CREATOR");
    }

    function isCreator(VaultMetadata storage self, address account) internal view returns (bool) {
        return self.creator == account;
    }

    function isInProgress(VaultMetadata storage self) internal view returns (bool) {
        uint256 currentBlockNumber = block.number;
        return
            exists(self) &&
            self.startBlockNumber <= currentBlockNumber &&
            currentBlockNumber <= self.endBlockNumber;
    }

    function isInRedemption(VaultMetadata storage self) internal view returns (bool) {
        return isStakingFinished(self) && self.endClaimBlockNumber > block.number;
    }

    function isStakingFinished(VaultMetadata storage self) internal view returns (bool) {
        return exists(self) && self.endBlockNumber < block.number;
    }

    function isPending(VaultMetadata storage self) internal view returns (bool) {
        return exists(self) && self.startBlockNumber > block.number;
    }

    function requireNotExists(VaultMetadata storage self) internal view {
