    function getAmountForToken(
        bytes32 vaultId,
        uint256 seasonId,
        address tokenAddress,
        uint256 amountOrId
    ) external view returns (uint256 amount, bool isNft);

    function getVaultMetadataForReward(
        bytes32 vaultId,
        uint256 seasonId,
        address reward
    ) external view returns (VaultsCommon.TransferMode mode);

    function getVaultMetadata(bytes32 vaultId, uint256 seasonId)
        external
        view
        returns (
            uint256 startBlockNumber,
            uint256 endBlockNumber,
            uint256 endClaimBlockNumber,
            address[] memory tokens,
            address[] memory rewards,
            address[] memory boosts
        );

    function getVaultMetadataForToken(
        bytes32 vaultId,
        uint256 seasonId,
        address token
    ) external view returns (uint256 maxCapacity, uint256 rewardAmounts);

    function getMaxCapacity(
        bytes32 vaultId,
        uint256 seasonId,
        address token
    ) external view returns (uint256);

    function getRewardAmountsFor(
