    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external override returns (bytes4) {
        // It is implemented to support ERC721 transfers.
        return IERC721Receiver.onERC721Received.selector;
    }

    /** View Functions */

    function getRewardsFor(
        uint256 seasonId,
        bytes32 vaultId,
        address rewardToken,
        address userWallet
    ) external view override returns (uint256 rewards, uint256 boostRewards) {
        return _getRewardsFor(seasonId, vaultId, rewardToken, userWallet);
    }

    function getVaultInfoFor(
        uint256 seasonId,
        bytes32 vaultId,
        address token
    ) external view override returns (uint256 totalDeposit) {
        VaultInfoLib.VaultInfo storage pool = vaultInfo[seasonId][vaultId];
        return (pool.totalDeposit[token]);
    }

    function getUserTokenBalancesFor(
        uint256 seasonId,
        bytes32 vaultId,
        address account
    ) external view override returns (address[] memory tokens, uint256[] memory balances) {
        (, , , address[] memory tokensList, , ) =
            _vaultsMetadataRegistry().getVaultMetadata(vaultId, seasonId);

        UserInfoLib.UserInfo storage user = userInfo[seasonId][vaultId][account];
        for (uint256 index = 0; index < tokensList.length; index++) {
            tokens[index] = tokensList[index];
            balances[index] = user.getBalance(tokensList[index]);
        }
    }

    function getUserRewardsInfoFor(
        uint256 seasonId,
        bytes32 vaultId,
        address account
    )
        external
        view
        override
        returns (
            address[] memory rewards,
            uint256[] memory paidRewards,
            uint256[] memory paidBoostedRewards,
            uint256[] memory availableRewards
        )
    {
        (, , , , address[] memory rewardsList, ) =
            _vaultsMetadataRegistry().getVaultMetadata(vaultId, seasonId);
        rewards = new address[](rewardsList.length);
        paidRewards = new uint256[](rewardsList.length);
        availableRewards = new uint256[](rewardsList.length);
        paidBoostedRewards = new uint256[](rewardsList.length);
        UserInfoLib.UserInfo storage user = userInfo[seasonId][vaultId][account];
        for (uint256 index = 0; index < rewardsList.length; index++) {
            rewards[index] = rewardsList[index];
            (uint256 currentPaidRewards, uint256 currentPaidBoostedRewards) =
                user.getPaidRewards(rewardsList[index]);
            paidRewards[index] = currentPaidRewards;
            paidBoostedRewards[index] = currentPaidBoostedRewards;
            (uint256 rewardsFor, ) = _getRewardsFor(seasonId, vaultId, rewardsList[index], account);
            availableRewards[index] = rewardsFor;
        }
    }

    function getUserBoostsInfoFor(
        uint256 seasonId,
        bytes32 vaultId,
        address account,
        address boostAddress
    ) external view override returns (uint256[] memory ids) {
        UserInfoLib.UserInfo storage user = userInfo[seasonId][vaultId][account];
        ids = user.getBoostIdsFor(boostAddress);
    }

    function getUserGeneralInfoFor(
        uint256 seasonId,
        bytes32 vaultId,
        address account
    )
        external
        view
        override
        returns (
            uint256 usedBoosts,
            uint256 availableBoosts,
            uint256 claimedRewards
        )
    {
        UserInfoLib.UserInfo memory user = userInfo[seasonId][vaultId][account];
        return (
            user.usedBoosts,
            _getMaxBoostsPerUser(vaultId).sub(user.usedBoosts),
            user.claimedRewards
        );
    }

    function getMaxBoostsPerUser(bytes32 vaultId) external view override returns (uint256) {
        return _getMaxBoostsPerUser(vaultId);
    }

    // TODO Verify if we need the season.
    function getMaxTotalBoosts(bytes32 vaultId) external view override returns (uint256) {
        return _getMaxTotalBoosts(vaultId);
    }

    /** Internal Functions */

    function _claim(
        bytes32 vaultId,
        uint256 seasonId,
        address rewardToken,
        address userWallet
    ) internal {
        require(
            _vaultsMetadataRegistry().isVaultInRedemption(seasonId, vaultId),
            "VAULT_ISNT_IN_REDEMPTION"
        );
        (uint256 availableRewards, uint256 boostRewards) =
            _getRewardsFor(seasonId, vaultId, rewardToken, userWallet);
        require(availableRewards > 0, "USER_HASNT_BALANCE_TO_CLAIM");

        UserInfoLib.UserInfo storage vaultUserInfo = userInfo[seasonId][vaultId][userWallet];

        vaultUserInfo.increasePaidRewards(rewardToken, availableRewards, boostRewards);

        VaultsCommon.TransferMode mode =
            _vaultsMetadataRegistry().getVaultMetadataForReward(vaultId, seasonId, rewardToken);

        _transferERC20(mode, userWallet, rewardToken, availableRewards.add(boostRewards));

        emit TokensClaimed(
            userWallet,
            seasonId,
            vaultId,
            rewardToken,
            availableRewards,
            boostRewards
        );
    }

    function _getRewardsFor(
        uint256 seasonId,
        bytes32 vaultId,
        address rewardToken,
        address userWallet
    ) internal view returns (uint256 rewards, uint256 boostRewards) {
        uint256 currentSeasonId = _getCurrentSeasonIfNeeded(seasonId, vaultId);
        VaultInfoLib.VaultInfo storage pool = vaultInfo[currentSeasonId][vaultId];
        UserInfoLib.UserInfo storage vaultUserInfo = userInfo[currentSeasonId][vaultId][userWallet];

        (, , , address[] memory tokens, , address[] memory boosts) =
            _vaultsMetadataRegistry().getVaultMetadata(vaultId, currentSeasonId);

        if (tokens.length == 0) {
            return (0, 0);
        }

        uint256 percentageAverage = _getAveragePercentage(tokens, pool, vaultUserInfo);
        uint256 sumBoostMultipliers = _getSumBoostMultipliers(boosts, userWallet, vaultUserInfo);

        uint256 totalTokensRewardsAmount =
            _vaultsMetadataRegistry().getRewardAmountsFor(vaultId, currentSeasonId, rewardToken);
        console.log("Total Rewards: ", totalTokensRewardsAmount);
        console.log("Percentage Avg.: ", percentageAverage);
        uint256 totalRewards = totalTokensRewardsAmount.mul(percentageAverage).div(AVOID_DECIMALS);
        console.log("Total Rewards: ", totalRewards);
        (uint256 paidRewards, uint256 paidBoostedRewards) =
            vaultUserInfo.getPaidRewards(rewardToken);
        console.log("Paid Rewards: ", paidRewards);
        console.log("Paid Boosted Rewards: ", paidBoostedRewards);
        uint256 availableRewards = totalRewards.sub(paidRewards);

        if (sumBoostMultipliers == 0) {
            return (availableRewards, 0);
        }

        uint256 totalBoostedRewards =
            availableRewards.mul(sumBoostMultipliers).div(PERCENTAGE_SCALE);
        return (availableRewards, totalBoostedRewards.sub(availableRewards));
    }

    function _getMaxBoostsPerUser(bytes32 vaultId) internal view returns (uint256) {
        //TODO Extract to PlatformSettingsConsts.sol
        return
            _vaultsSettings().getVaultSettingOrDefaultValue(vaultId, MAX_BOOSTS_PER_USER_SETTING);
    }

    function _getMaxTotalBoosts(bytes32 vaultId) internal view returns (uint256) {
        //TODO Extract to PlatformSettingsConsts.sol
        return _vaultsSettings().getVaultSettingOrDefaultValue(vaultId, MAX_TOTAL_BOOSTS_SETTING);
    }

    function _vaultsSettings() internal view returns (IVaultsSettings) {
        return IVaultsSettings(vaultsSettings);
    }

    function _validateContractAsSenderIfNeeded(address account) internal view {
        //TODO Extract to PlatformSettingsConsts.sol
        uint256 allowContractsValue = _settings().getSettingValue("ALLOW_CONTRACTS_IN_VAULTS");
        if (allowContractsValue == 1) {
            require(account == tx.origin, "CONTRACTS_ARENT_ALLOWED");
        }
    }

    function _getCurrentSeasonIfNeeded(uint256 seasonId, bytes32 vaultId)
        internal
        view
        returns (uint256)
    {
        if (seasonId != 0) {
            return seasonId;
        }
        return _vaultsMetadataRegistry().getCurrentSeason(vaultId);
    }

    function _getCurrentSeason(bytes32 vaultId) internal view returns (uint256) {
        return _vaultsMetadataRegistry().getCurrentSeason(vaultId);
    }

    function _vaultsMetadataRegistry() internal view returns (IVaultsMetadataRegistry) {
        return IVaultsMetadataRegistry(vaultMetadataRegistry);
    }

    /* Private Functions */

    function _transferERC20(
        VaultsCommon.TransferMode mode,
        address to,
        address token,
        uint256 amount
    ) private {
        require(mode > VaultsCommon.TransferMode.Undefined, "TRANSFER_MODE_NOT_DEFINED");
        if (mode == VaultsCommon.TransferMode.Mint) {
            IMintableERC20(token).mint(to, amount);
        }
        if (mode == VaultsCommon.TransferMode.Transfer) {
            IERC20(token).safeTransfer(to, amount);
        }
    }

    function _getSumBoostMultipliers(
        address[] memory boosts,
        address userWallet,
        UserInfoLib.UserInfo storage vaultUserInfo
    ) private view returns (uint256) {
        uint256 sumBoostMultipliers;
        for (uint256 index = 0; index < boosts.length; index++) {
            sumBoostMultipliers = sumBoostMultipliers.add(
                vaultUserInfo.getAccumulatedMultipliersFor(userWallet, boosts[index])
            );
        }
        return sumBoostMultipliers;
    }

    function _getAveragePercentage(
        address[] memory tokens,
        VaultInfoLib.VaultInfo storage pool,
        UserInfoLib.UserInfo storage vaultUserInfo
    ) private view returns (uint256) {
        uint256 totalPercentage;
        for (uint256 index = 0; index < tokens.length; index++) {
            uint256 tokenTotalDeposit = pool.getTotalDeposit(tokens[index]);
            totalPercentage = totalPercentage.add(
                vaultUserInfo.getBalancePercentage(tokens[index], tokenTotalDeposit)
            );
        }
        return totalPercentage.div(tokens.length);
    }
}

