pragma solidity 0.6.12;

// Interfaces
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

// Libraries
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";

library VaultInfoLib {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    uint256 public constant AVOID_DECIMALS = 1e18;

    struct VaultInfo {
        uint256 usedBoosts;
        mapping(address => uint256) totalDeposit; // Accumulated deposit tokens.
    }

    function increaseTotalDeposit(
        VaultInfo storage self,
        address tokenAddress,
        uint256 amount
    ) internal {
        self.totalDeposit[tokenAddress] = self.totalDeposit[tokenAddress].add(amount);
    }

    function decreaseTotalDeposit(
        VaultInfo storage self,
        address tokenAddress,
        uint256 amount
    ) internal {
        self.totalDeposit[tokenAddress] = self.totalDeposit[tokenAddress].sub(amount);
    }

    function getTotalDeposit(VaultInfo storage self, address tokenAddress)
        internal
        view
        returns (uint256)
    {
        return self.totalDeposit[tokenAddress];
    }

    function hasTotalDeposit(VaultInfo storage self, address tokenAddress)
        internal
        view
        returns (bool)
    {
        return self.totalDeposit[tokenAddress] > 0;
    }

    function increaseUsedBoots(VaultInfo storage self, uint256 amount) internal {
        self.usedBoosts = self.usedBoosts.add(amount);
    }

    function decreaseUsedBoots(VaultInfo storage self, uint256 amount) internal {
        self.usedBoosts = self.usedBoosts.sub(amount);
    }
}

