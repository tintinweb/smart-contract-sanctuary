pragma solidity 0.6.12;

import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";

// Libraries
import "@openzeppelin/contracts/utils/EnumerableSet.sol";

// Interface
import "../boost/IBoostWrapper.sol";

import "hardhat/console.sol";

// TODO Remove it.

library UserInfoLib {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    uint256 internal constant AVOID_DECIMALS = 1e18;

    struct UserInfo {
        uint256 usedBoosts;
        uint256 claimedRewards;
        mapping(address => uint256) balances; // Amounts per token (ERC20/ERC721).
        /*
            Key: Reward token => Rewards paid.
         */
        mapping(address => uint256) paidRewards;
        mapping(address => uint256) paidBoostedRewards;
        /*
            It maps boost address to boost ids.
        */
        mapping(address => EnumerableSet.UintSet) boosts;
    }

    function safeERC20TransferFrom(
        UserInfo storage self,
        address user,
        address tokenAddress,
        uint256 amount
    ) internal {
        require(
            IERC20(tokenAddress).allowance(user, address(this)) >= amount,
            "TOKEN_ALLOWANCE_LT_EXPECTED"
        );
        self.balances[tokenAddress] = self.balances[tokenAddress].add(amount);
        IERC20(tokenAddress).safeTransferFrom(user, address(this), amount);
    }

    function safeERC721TransferFrom(
        UserInfo storage self,
        address user,
        address tokenAddress,
        uint256 id
    ) internal {
        //TODO Add exists?
        self.balances[tokenAddress] = self.balances[tokenAddress].add(1);
        _safeERC721TransferFrom(self, user, tokenAddress, id);
    }

    function safeTransferBoostFrom(
        UserInfo storage self,
        address user,
        address boostAddress,
        uint256 boostId
    ) internal {
        //TODO Add exists?
        self.boosts[boostAddress].add(boostId);
        self.usedBoosts = self.usedBoosts.add(1);
        _safeERC721TransferFrom(
            self,
            user,
            IBoostWrapper(boostAddress).getUnderlyingBoost(),
            boostId
        );
    }

    function safeTransferBoost(
        UserInfo storage self,
        address user,
        address boostAddress,
        uint256 boostId
    ) internal {
        IERC721 token = IERC721(IBoostWrapper(boostAddress).getUnderlyingBoost());
        require(self.boosts[boostAddress].contains(boostId), "USER_DIDNT_ADD_BOOST_ID");
        require(token.ownerOf(boostId) == address(this), "TOKEN_ISNT_OWNED_BY_VAULT");
        self.boosts[boostAddress].remove(boostId);
        self.usedBoosts = self.usedBoosts.sub(1);
        token.safeTransferFrom(address(this), user, boostId, "");
    }

    function safeERC20Transfer(
        UserInfo storage self,
        address user,
        address tokenAddress,
        uint256 amount
    ) internal {
        self.balances[tokenAddress] = self.balances[tokenAddress].sub(amount);
        IERC20(tokenAddress).safeTransfer(user, amount);
    }

    function safeERC721Transfer(
        UserInfo storage self,
        address user,
        address tokenAddress,
        uint256 id
    ) internal {
        self.balances[tokenAddress] = self.balances[tokenAddress].sub(1);
        require(IERC721(tokenAddress).ownerOf(id) == address(this), "TOKEN_ID_ISNT_OWNED_BY_VAULT");
        IERC721(tokenAddress).safeTransferFrom(address(this), user, id, "");
    }

    /* View Functions */

    function getAccumulatedMultipliersFor(
        UserInfo storage self,
        address userAccount,
        address boostAddress
    ) internal view returns (uint256) {
        EnumerableSet.UintSet storage boostsList = self.boosts[boostAddress];
        uint256 totalMultiplier;
        for (uint256 index = 0; index < boostsList.length(); index++) {
            uint256 multiplier =
                IBoostWrapper(boostAddress).getMultiplierValue(
                    userAccount,
                    uint256(boostsList.at(index))
                );
            totalMultiplier = totalMultiplier.add(multiplier);
        }
        return totalMultiplier;
    }

    function hasBalance(UserInfo storage self, address tokenAddress) internal view returns (bool) {
        return self.balances[tokenAddress] > 0;
    }

    function getBalance(UserInfo storage self, address tokenAddress)
        internal
        view
        returns (uint256)
    {
        return self.balances[tokenAddress];
    }

    function isBalanceGte(
        UserInfo storage self,
        address tokenAddress,
        uint256 balance
    ) internal view returns (bool) {
        return self.balances[tokenAddress] >= balance;
    }

    function getBalancePercentage(
        UserInfo storage self,
        address tokenAddress,
        uint256 totalDeposit
    ) internal view returns (uint256) {
        if (totalDeposit == 0) {
            return 0;
        }
        uint256 balance = getBalance(self, tokenAddress);
        return balance.mul(AVOID_DECIMALS).div(totalDeposit);
    }

    function getPaidRewards(UserInfo storage self, address tokenAddress)
        internal
        view
        returns (uint256 paidRewards, uint256 paidBoostedRewards)
    {
        return (self.paidRewards[tokenAddress], self.paidBoostedRewards[tokenAddress]);
    }

    function increasePaidRewards(
        UserInfo storage self,
        address rewardAddress,
        uint256 amount,
        uint256 boostedAmount
    ) internal {
        self.paidRewards[rewardAddress] = self.paidRewards[rewardAddress].add(amount);
        self.paidBoostedRewards[rewardAddress] = self.paidBoostedRewards[rewardAddress].add(
            boostedAmount
        );
        self.claimedRewards = self.claimedRewards.add(1);
    }

    function hasClaimedAll(UserInfo storage self, uint256 totalVaultRewardsTokens)
        internal
        view
        returns (bool)
    {
        return totalVaultRewardsTokens == self.claimedRewards;
    }

    function getBoostIdsFor(UserInfo storage self, address boostAddress)
        internal
        view
        returns (uint256[] memory ids)
    {
        EnumerableSet.UintSet storage boostsList = self.boosts[boostAddress];
        for (uint256 index = 0; index < boostsList.length(); index++) {
            ids[index] = uint256(boostsList.at(index));
        }
    }

    /* Private Functions */

    function _safeERC721TransferFrom(
        UserInfo storage,
        address from,
        address tokenAddress,
        uint256 id
    ) private {
        IERC721 token = IERC721(tokenAddress);
        require(token.getApproved(id) == address(this), "TOKEN_ISNT_APPROVED_BY_OWNER");
        token.safeTransferFrom(from, address(this), id, "");
    }
}

