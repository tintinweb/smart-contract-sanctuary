        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address[] rewards,
        VaultsCommon.TransferMode mode
    );

    function createVault(VaultsCommon.VaultCreation calldata vaultCreation) external;

    function addOwners(bytes32 vaultId, address[] calldata accounts) external;

    function removeOwners(bytes32 vaultId, address[] calldata accounts) external;

    function addSupportedTokens(bytes32 vaultId, address[] calldata tokens) external;

    function removeSupportedTokens(bytes32 vaultId, address[] calldata tokens) external;

    function addSupportedBoosts(bytes32 vaultId, address[] calldata boosts) external;

    function removeSupportedBoosts(bytes32 vaultId, address[] calldata boosts) external;

    function addAsNFTs(
        bytes32 vaultId,
        uint256 seasonId,
        address[] calldata tokens
    ) external;

    function removeAsNFTs(
        bytes32 vaultId,
        uint256 seasonId,
        address[] calldata tokens
