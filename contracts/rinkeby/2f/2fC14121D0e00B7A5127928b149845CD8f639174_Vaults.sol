contract Vaults is MigratorBase, IVaults, IERC721Receiver {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using VaultInfoLib for VaultInfoLib.VaultInfo;
    using UserInfoLib for UserInfoLib.UserInfo;
    using VaultMetadataLib for VaultMetadataLib.VaultMetadata;

    uint256 public constant AVOID_DECIMALS = 1e18;
    uint256 public constant PERCENTAGE_SCALE = 10000;
    bytes32 public constant MAX_BOOSTS_PER_USER_SETTING = "MAX_BOOSTS_PER_USER";
    bytes32 public constant MAX_TOTAL_BOOSTS_SETTING = "MAX_TOTAL_BOOSTS";

    /**
        @dev Maps Season ID => Vault ID => Vault Info
     */
    mapping(uint256 => mapping(bytes32 => VaultInfoLib.VaultInfo)) internal vaultInfo;

    /**
        @dev Maps Season ID => Vault ID => User address => User Infos
     */
    // TODO Use hash?
    mapping(uint256 => mapping(bytes32 => mapping(address => UserInfoLib.UserInfo)))
        internal userInfo;

    address public override vaultMetadataRegistry;

    address public override tokensRegistry;

    address public override vaultsSettings;

    constructor(
        address settingsAddress,
        address tokensRegistryAddress,
        address vaultMetadataRegistryAddress,
        address vaultsSettingsAddress
    ) public MigratorBase(settingsAddress) {
        require(vaultMetadataRegistryAddress.isContract(), "VAULT_MD_REG_MUST_BE_CONTRACT");
        require(tokensRegistryAddress.isContract(), "TOKENS_REGISTRY_MUST_BE_CONTRACT");
        require(vaultsSettingsAddress.isContract(), "VAULTS_SETTINGS_MUST_BE_CONTRACT");

        vaultMetadataRegistry = vaultMetadataRegistryAddress;
        tokensRegistry = tokensRegistryAddress;
        vaultsSettings = vaultsSettingsAddress;
    }

    function addBoosts(
        bytes32 vaultId,
        address boostAddress,
        uint256[] calldata boostIds
    ) external override {
        require(boostIds.length > 0, "BOOSTS_ARE_REQUIRED");
        require(boostAddress.isContract(), "BOOST_MUST_BE_CONTRACT");
        // TODO Maybe it is a double verification?
        uint256 currentSeasonId = _vaultsMetadataRegistry().getCurrentSeason(vaultId);
        _vaultsMetadataRegistry().requireIsTokenSupported(currentSeasonId, vaultId, boostAddress);

        address userWallet = msg.sender;
        _validateContractAsSenderIfNeeded(userWallet);
        require(
            // TODO Verify whether period is ok or not.
            _vaultsMetadataRegistry().isVaultPending(currentSeasonId, vaultId),
            "VAULT_SEASON_ISNT_PENDING"
        );

        VaultInfoLib.VaultInfo storage pool = vaultInfo[currentSeasonId][vaultId];
        UserInfoLib.UserInfo storage vaultUserInfo = userInfo[currentSeasonId][vaultId][userWallet];

        require(
            _getMaxTotalBoosts(vaultId) >= pool.usedBoosts.add(boostIds.length),
            "MAX_TOTAL_BOOSTS_EXCEEDED"
        );
        require(
            _getMaxBoostsPerUser(vaultId) >= vaultUserInfo.usedBoosts.add(boostIds.length),
            "MAX_BOOSTS_PER_USER_EXCEEDED"
        );

        // TODO I think it is not needed
        // _vaultsMetadataRegistry().requireHasBoostMultiplier(vaultId, seasonId, boostAddress);
        for (uint256 index = 0; index < boostIds.length; index++) {
            pool.increaseUsedBoots(1);
            vaultUserInfo.safeTransferBoostFrom(userWallet, boostAddress, boostIds[index]);
        }
        emit BoostIdsAdded(userWallet, currentSeasonId, vaultId, boostAddress, boostIds);
    }

    function removeBoosts(
        bytes32 vaultId,
        address boostAddress,
        uint256[] calldata boostIds
    ) external override {
        require(boostIds.length > 0, "BOOSTS_ARE_REQUIRED");
        require(boostAddress.isContract(), "BOOST_MUST_BE_CONTRACT");
        address userWallet = msg.sender;
        _validateContractAsSenderIfNeeded(userWallet);

        uint256 currentSeasonId = _vaultsMetadataRegistry().getCurrentSeason(vaultId);
        require(
            // TODO Verify user can remove the boosts after the period ends.
            _vaultsMetadataRegistry().isVaultStakingFinished(currentSeasonId, vaultId),
            "STAKING_PERIOD_NOT_FINISHED"
        );
        _vaultsMetadataRegistry().requireIsTokenSupported(currentSeasonId, vaultId, boostAddress);
        // TODO Verify whether period is ok or not.
        VaultInfoLib.VaultInfo storage pool = vaultInfo[currentSeasonId][vaultId];
        UserInfoLib.UserInfo storage vaultUserInfo = userInfo[currentSeasonId][vaultId][userWallet];

        for (uint256 index = 0; index < boostIds.length; index++) {
            pool.decreaseUsedBoots(1);
            // TODO Verify is the boost id is from senders
            vaultUserInfo.safeTransferBoost(userWallet, boostAddress, boostIds[index]);
        }

        emit BoostIdsRemoved(userWallet, currentSeasonId, vaultId, boostAddress, boostIds);
    }

    function stake(
        address tokenAddress,
        bytes32 vaultId,
        uint256 amountOrId
    ) external override {
        // TODO Add SafeGuard for reentrancy
        require(amountOrId > 0, "AMOUNT_OR_ID_MUST_BE_GT_ZERO");
        address userWallet = msg.sender;
        _validateContractAsSenderIfNeeded(userWallet);

        uint256 currentSeasonId = _vaultsMetadataRegistry().getCurrentSeason(vaultId);
        require(
            _vaultsMetadataRegistry().isVaultPending(currentSeasonId, vaultId),
            "VAULT_SEASON_ISNT_PENDING"
        );
        _vaultsMetadataRegistry().requireIsTokenSupported(currentSeasonId, vaultId, tokenAddress);

        /*
            TODO Verify if it is possible to stake when the period/season is already in progress.
         */
        VaultInfoLib.VaultInfo storage pool = vaultInfo[currentSeasonId][vaultId];
        UserInfoLib.UserInfo storage vaultUserInfo = userInfo[currentSeasonId][vaultId][userWallet];

        (uint256 amount, bool isNft) =
            _vaultsMetadataRegistry().getAmountForToken(
                vaultId,
                currentSeasonId,
                tokenAddress,
                amountOrId
            );
        uint256 maxCapacity =
            _vaultsMetadataRegistry().getMaxCapacity(vaultId, currentSeasonId, tokenAddress);
        require(
            maxCapacity >= vaultUserInfo.getBalance(tokenAddress).add(amount),
            "AMOUNT_EXCEEDS_MAX_STAKE_CAP"
        );

        pool.increaseTotalDeposit(tokenAddress, amount);

        if (isNft) {
            // ERC721
            vaultUserInfo.safeERC721TransferFrom(userWallet, tokenAddress, amountOrId);
        } else {
            // ERC20
            vaultUserInfo.safeERC20TransferFrom(userWallet, tokenAddress, amountOrId);
        }

        emit TokensStaked(userWallet, currentSeasonId, vaultId, tokenAddress, amountOrId);
    }

    function unstake(
        bytes32 vaultId,
        address tokenAddress,
        uint256 amountOrId
    ) external override {
        // TODO Add safe reentrancy guard
        require(amountOrId > 0, "AMOUNT_OR_ID_MUST_BE_GT_ZERO");
        address userWallet = msg.sender;
        _validateContractAsSenderIfNeeded(userWallet);

        uint256 currentSeasonId = _vaultsMetadataRegistry().getCurrentSeason(vaultId);
        _vaultsMetadataRegistry().requireIsTokenSupported(currentSeasonId, vaultId, tokenAddress);
        require(
            // TODO Verify user can unstake the tokens after the period ends.
            _vaultsMetadataRegistry().isVaultStakingFinished(currentSeasonId, vaultId),
            "STAKING_PERIOD_NOT_FINISHED"
        );

        /**
            TODO Verify if it is possible to unstake when the period/season is still in progress.
         */
        VaultInfoLib.VaultInfo storage pool = vaultInfo[currentSeasonId][vaultId];
        UserInfoLib.UserInfo storage vaultUserInfo = userInfo[currentSeasonId][vaultId][userWallet];

        (, , , , address[] memory rewards, ) =
            _vaultsMetadataRegistry().getVaultMetadata(vaultId, currentSeasonId);
        require(vaultUserInfo.hasClaimedAll(rewards.length), "STILL_HAS_RECLAIMABLE_BALANCE");

        (uint256 amount, bool isNft) =
            _vaultsMetadataRegistry().getAmountForToken(
                vaultId,
                currentSeasonId,
                tokenAddress,
                amountOrId
            );

        require(
            amount > 0 && vaultUserInfo.isBalanceGte(tokenAddress, amount),
            "AMOUNT_EXCEEDS_STAKED_BALANCE"
        );
        pool.decreaseTotalDeposit(tokenAddress, amount);
        if (isNft) {
            vaultUserInfo.safeERC721Transfer(
                userWallet,
                tokenAddress,
                amountOrId // Token ID
            );
        } else {
            vaultUserInfo.safeERC20Transfer(
                userWallet,
                tokenAddress,
                amountOrId // Token Amount
            );
        }

        emit TokensUnstaked(userWallet, currentSeasonId, vaultId, tokenAddress, amountOrId);
    }

    function claim(bytes32 vaultId, address rewardToken) external override {
        address userWallet = msg.sender;
        _validateContractAsSenderIfNeeded(userWallet);

        uint256 currentSeasonId = _vaultsMetadataRegistry().getCurrentSeason(vaultId);

        _claim(vaultId, currentSeasonId, rewardToken, userWallet);
    }

    function claimAll(bytes32 vaultId) external override {
        address userWallet = msg.sender;
        _validateContractAsSenderIfNeeded(userWallet);

        uint256 currentSeasonId = _vaultsMetadataRegistry().getCurrentSeason(vaultId);

        (, , , , address[] memory rewards, ) =
            _vaultsMetadataRegistry().getVaultMetadata(vaultId, currentSeasonId);

        for (uint256 indexAt = 0; indexAt < rewards.length; indexAt++) {
            _claim(vaultId, currentSeasonId, rewards[indexAt], userWallet);
        }
    }

