pragma solidity 0.6.12;
pragma experimental ABIEncoderV2;

// Contracts
import "../utils/VaultsCommon.sol";

// Libraries

// Interfaces

interface IVaultsMetadataRegistry {
    event VaultMetadataCreated(
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        uint256 startBlockNumber,
        uint256 endBlockNumber,
        uint256 endClaimBlockNumber
    );

    event VaultOwnerAdded(
        address indexed adder,
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address account
    );

    event VaultOwnerRemoved(
        address indexed remover,
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address account
    );

    event NFTIDsAdded(
        address indexed adder,
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address token,
        uint256[] ids
    );

    event SupportedTokensAdded(
        address indexed adder,
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address[] tokens
    );

    event SupportedTokensRemoved(
        address indexed remover,
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address[] tokens
    );

    event SupportedBoostsAdded(
        address indexed adder,
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address[] boosts
    );

    event SupportedBoostsRemoved(
        address indexed remover,
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address[] boosts
    );

    event NFTTokensAdded(
        address indexed adder,
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address[] tokens
    );

    event NFTTokensRemoved(
        address indexed remover,
        bytes32 indexed vaultId,
        uint256 indexed seasonId,
        address[] tokens
    );

