pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";
import "./IBot.sol";
import "./Funds.sol";
import "./Participant.sol";

contract BotContract is EIP712, IBot, Ownable, ERC721, Funds, Participant {

    string private baseURI;
    address private protocolTreasure;
    uint8 public protocolFee;

    using SafeERC20 for IERC20;

    modifier botExists(uint256 _botId) {
        require(_exists(_botId));
        _;
    }

    modifier amountMoreZero(uint256 _amount) {
        require(_amount > 0);
        _;
    }

    function checkVoucher(bytes32 data, bytes calldata _signature) internal view returns (bool){
        return (ECDSA.recover(_hashTypedDataV4(data), _signature) == owner());
    }

    mapping(uint256 => Bot) private bots;

    constructor(address _protocolTreasure, uint8 _protocolFee) ERC721("BotContract", "STB") EIP712('Test', '1')
    {
        protocolTreasure = _protocolTreasure;
        protocolFee = _protocolFee;
        baseURI = 'https://dex.starbot.cc/storage/metadata/';
    }

    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT licence
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }

    function tokenURI(uint256 _botId) public virtual botExists(_botId) view override returns (string memory)
    {
        return string(abi.encodePacked(baseURI, toString(_botId), ".json"));
    }

    function createBot(uint256 _botId, address _token, address _deposit, uint256 _minAmount, uint8 _loops, uint8 _fee, bytes calldata _signature) external
    {
        require(_fee < 100 - protocolFee);
        require(checkVoucher(keccak256(abi.encode(
                keccak256("createBot(uint256 botId,address token,address deposit)"),
                _botId,
                _token,
                _deposit
            )), _signature));
        bots[_botId] = Bot(
            _botId,
            _token,
            _deposit,
            address(0),
            0,
            _minAmount,
            0,
            0,
            0,
            _fee,
            _loops,
            0
        );
        _mint(_msgSender(), _botId);
    }

    function deposit(uint256 _botId, uint256 _amount) external botExists(_botId) amountMoreZero(_amount)
    {
        Bot storage _bot = bots[_botId];
        IERC20(_bot.token).safeTransferFrom(_msgSender(), address(this), _amount);
        addFund(_botId, _msgSender(), _amount);
        addParticipant(_botId, _msgSender());

        _bot.totalAmount+= _amount;
        emit Deposit(_botId, _msgSender(), _amount);
    }

    function withdraw(uint256 _botId, uint256 _amount) external botExists(_botId) amountMoreZero(_amount)
    {
        require(getFreeFund(_botId, _msgSender()) >= _amount);
        IERC20(bots[_botId].token).safeTransfer(_msgSender(), _amount);
        bots[_botId].totalAmount -= _amount;
        subFund(_botId, _msgSender(), _amount);
        if(getTotalFund(_botId, _msgSender()) == uint256(0))
            removeParticipant(_botId, _msgSender());
        emit Withdraw(_botId, _msgSender(), _amount);
    }

    function getBot(uint256 _botId) external view botExists(_botId) returns(Bot memory)
    {
        return bots[_botId];
    }

    function startBot(uint256 _botId, address _wallet, bytes calldata _signature) external botExists(_botId)
    {
        require(checkVoucher(keccak256(abi.encode(
                keccak256("startBot(uint256 botId,address wallet)"),
                _botId,
                _wallet
            )), _signature));
        bots[_botId].wallet = _wallet;
        bots[_botId].current_loop = 1;
        _start(_botId);

        emit Start(_botId, bots[_botId].totalAmount);
    }

    function _start(uint256 _botId) internal
    {
        require(bots[_botId].totalAmount >= bots[_botId].minAmount);
        require(bots[_botId].startTime == uint64(0));
        bots[_botId].startAmount = bots[_botId].totalAmount;
        IERC20(bots[_botId].token).safeTransfer(bots[_botId].wallet, bots[_botId].totalAmount);
        bots[_botId].totalUseAmount = takeForLoop(_botId);
        bots[_botId].startTime = uint64(block.timestamp);
    }

    function fixProfit(uint256 _botId) external onlyOwner botExists(_botId)
    {
        uint256 _balance = IERC20(bots[_botId].token).balanceOf(bots[_botId].deposit);
        uint256 _startTotalAmount = bots[_botId].totalAmount;
        uint256 _incomeAmount = _balance - _startTotalAmount;
        require(_incomeAmount>0);
        uint256 _protocolFeeAmount = 0;
        if (protocolFee > 0) {
            _protocolFeeAmount = _incomeAmount * protocolFee / 100;
        }

        IERC20(bots[_botId].token).safeTransferFrom(bots[_botId].deposit, address(this), _balance);
        uint256 ownerFee = 0;
        if (bots[_botId].ownerFee > 0) {
            ownerFee = _incomeAmount * bots[_botId].ownerFee / 100;
            IERC20(bots[_botId].token).safeTransfer(ownerOf(_botId), ownerFee);
        }

        uint256 _profitAmount = _incomeAmount - _protocolFeeAmount - ownerFee;
        uint256 _totalProfitAmount = addProfit(_botId, _profitAmount, bots[_botId].totalUseAmount);
        bots[_botId].totalAmount+= _totalProfitAmount;

        _protocolFeeAmount+= _profitAmount - _totalProfitAmount;
        if(_protocolFeeAmount > 0){
            IERC20(bots[_botId].token).safeTransfer(protocolTreasure, _protocolFeeAmount);
        }

        bots[_botId].startTime = 0;
        bots[_botId].totalUseAmount = 0;
        if (bots[_botId].current_loop != bots[_botId].loops) {
            bots[_botId].current_loop++;
            _start(_botId);
        }

        emit Profit(_botId, _startTotalAmount, _profitAmount);
    }
}

