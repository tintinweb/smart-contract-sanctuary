pragma solidity ^0.8.4;

import "./IBot.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

contract Funds is IBot {

    mapping(uint256 => mapping(address => Fund)) private funds;

    struct Fund {
        uint256 total;
        //uint256 out;
        uint256 use;
    }

    function addFund(uint256 _botId, address _user, uint256 _amount)
        internal
    {
        funds[_botId][_user].total+= _amount;
    }

    function subFund(uint256 _botId, address _user, uint256 _amount)
    internal
    {
        funds[_botId][_user].total-= _amount;
    }

    function getFreeFund(uint256 _botId, address _user) internal view returns (uint256)
    {
        return  funds[_botId][_user].total - funds[_botId][_user].use;
    }

    function getFund(uint256 _botId, address _user) public view returns (Fund memory)
    {
        return  funds[_botId][_user];
    }

    function getTotalFund(uint256 _botId, address _user) internal view returns (uint256)
    {
        return  funds[_botId][_user].total;
    }

    function blockFund(uint256 _botId, address _user) internal returns (uint256)
    {
        funds[_botId][_user].use = funds[_botId][_user].total;
        return funds[_botId][_user].use;
    }

    function addProfitFund(uint256 _botId, address _user, uint256 _profitAmount, uint256 _startAmount) internal returns (uint256)
    {
        uint _amount = 0;
        if (funds[_botId][_user].use > 0) {
            _amount = funds[_botId][_user].use * _profitAmount / _startAmount;
            if (_amount > 0) {
                funds[_botId][_user].total+= _amount;
            }
            funds[_botId][_user].use = 0;

        }
        return _amount;
    }
}

