pragma solidity ^0.8.4;

import "./IBot.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "./Funds.sol";

contract Participant is IBot, Funds {

    using EnumerableSet for EnumerableSet.AddressSet;
    mapping (uint256 => EnumerableSet.AddressSet) private participants;

    function addParticipant(uint256 _botId, address _user) internal{
        //if (!participants[_botId].contains(_user)) {
            participants[_botId].add(_user);
        //}
    }

    function getParticipant(uint256 _botId) external view returns (bytes32[] memory)
    {
        return participants[_botId]._inner._values;
    }

    function removeParticipant(uint256 _botId, address _user) internal
    {
        participants[_botId].remove(_user);
    }

    function takeForLoop(uint256 _botId) internal returns(uint256)
    {
        uint256 _totalUseAmount = 0;
        for (uint256 index = 0; index < participants[_botId].length(); index++) {
            _totalUseAmount+= blockFund(_botId, participants[_botId].at(index));
        }
        return _totalUseAmount;
    }

    function addProfit(uint256 _botId, uint256 _profitAmount, uint256 _useAmount) internal returns(uint256)
    {
        uint256 _totalProfitAmount = 0;
        for (uint256 index = 0; index < participants[_botId].length(); index++) {
            _totalProfitAmount+= addProfitFund(_botId, participants[_botId].at(index), _profitAmount, _useAmount);
        }
        return _totalProfitAmount;
    }
}

