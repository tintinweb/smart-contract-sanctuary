interface IDistributor {
  function distribute() external returns (bool);
}

contract PegaStaking is Ownable, IERC721Receiver, ReentrancyGuard, Pausable {
  using SafeMath for uint256;
  using SafeERC20 for IERC20;

  struct Claim {
    uint256 deposit;
    uint256 gons;
    uint256 expiry;
    uint256 tokenLevel;
    uint16 tokenId;
    bool lock; // prevents malicious delays
    bool initialized;
  }

  struct Epoch {
    uint256 length;
    uint256 number;
    uint256 endBlock;
    uint256 distribute;
  }
  Epoch public epoch;

  struct Rebase {
    uint256 epoch;
    uint256 rebase; // 18 decimals
    uint256 totalStakedBefore;
    uint256 totalStakedAfter;
    uint256 amountRebased;
    uint256 index;
    uint256 blockNumberOccured;
  }
  Rebase[] public rebases;

  address public immutable PEGA;
  address public immutable pegaPal;
  address public initializer;
  address public distributor;

  uint256 private constant MAX_UINT256 = ~uint256(0);
  uint256 private constant INITIAL_FRAGMENTS_SUPPLY = 5000000 * 10**9;

  // TOTAL_GONS is a multiple of INITIAL_FRAGMENTS_SUPPLY so that _gonsPerFragment is an integer.
  // Use the highest value that fits in a uint256 for max granularity.
  uint256 private constant TOTAL_GONS =
    MAX_UINT256 - (MAX_UINT256 % INITIAL_FRAGMENTS_SUPPLY);

  // MAX_SUPPLY = maximum integer < (sqrt(4*TOTAL_GONS + 1) - 1) / 2
  uint256 private constant MAX_SUPPLY = ~uint128(0); // (2^128) - 1

  uint256 private _gonsPerFragment = TOTAL_GONS.div(INITIAL_FRAGMENTS_SUPPLY);
  uint256 public warmupPeriod;

  mapping(address => uint256) private _gonBalances;

  //addresses
  uint256 _totalSupply;
  uint256 public INDEX;

  //events
  event LogSupply(
    uint256 indexed epoch,
    uint256 timestamp,
    uint256 totalSupply
  );
  event LogStaked(uint256 tokenId, uint256 amount, address indexed user);
  event LogRebase(uint256 indexed epoch, uint256 rebase, uint256 index);
  event RebaseEvent(uint256 indexed epoch, uint256 profit);
  event LogStakingContractUpdated(address stakingContract);
  event LogUnstakeTokens(uint256 id, uint256 amount, address indexed user);
  event LogUnstakeAll(uint256 id, uint256 amount, address indexed user);

  // mappings
  mapping(address => Claim) public stakingInfo;

  constructor(
    address _PEGA,
    address _pegaPal,
    uint256 _epochLength,
    uint256 _firstEpochNumber,
    uint256 _firstEpochBlock
  ) {
    initializer = msg.sender;
    PEGA = _PEGA;
    pegaPal = _pegaPal;
    _totalSupply = INITIAL_FRAGMENTS_SUPPLY;
    epoch = Epoch({
      length: _epochLength,
      number: _firstEpochNumber,
      endBlock: _firstEpochBlock,
      distribute: 0
    });
    _pause();
  }

  function initialize() external returns (bool) {
    require(msg.sender == initializer);
    _gonBalances[address(this)] = TOTAL_GONS;
    initializer = address(0);
    return true;
  }

  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  // Staking contract holds excess sFUEL since when you stake it gives you sFUEL and rebases on top of it.
  function circulatingSupply() public view returns (uint256) {
    return _totalSupply.sub(balanceOf(address(this)));
  }

  function balanceOf(address who) public view returns (uint256) {
    return _gonBalances[who].div(_gonsPerFragment);
  }

  function gonsForBalance(uint256 amount) public view returns (uint256) {
    return amount.mul(_gonsPerFragment);
  }

  function balanceForGons(uint256 gons) public view returns (uint256) {
    return gons.div(_gonsPerFragment);
  }

  function index() public view returns (uint256) {
    return balanceForGons(INDEX);
  }

  function setIndex(uint256 _INDEX) external onlyOwner returns (bool) {
    require(INDEX == 0);
    INDEX = gonsForBalance(_INDEX);
    return true;
  }

  function setDistributor(address _address) external onlyOwner {
    distributor = _address;
  }

  /**
   * @notice set warmup period for new stakers
   * @param _warmupPeriod uint
   */
  function setWarmup(uint256 _warmupPeriod) external onlyOwner {
    warmupPeriod = _warmupPeriod;
  }

  // Earnings = Balance - Deposite
  function rewardBalance(address who) public view returns (uint256) {
    Claim memory info = stakingInfo[who];
    uint256 totalUserBalance = balanceOf(who).sub(info.deposit);
    return totalUserBalance;
  }

  //deposit function. - Tested
  function stake(uint16 _tokenId)
    external
    whenNotPaused
    nonReentrant
    returns (bool)
  {
    rebase();

    Claim memory info = stakingInfo[msg.sender];
    require(!info.initialized, "User can only stake one NFT per Address");

    PegaPal(pegaPal).safeTransferFrom(msg.sender, address(this), _tokenId);

    uint256 tokenLevel = PegaPal(pegaPal).getLevel(_tokenId);
    uint256 tokenPsuedoValue = tokenLevel *
      500 *
      10**uint256(ERC20(PEGA).decimals());

    stakingInfo[msg.sender] = Claim({
      deposit: info.deposit.add(tokenPsuedoValue),
      gons: info.gons.add(gonsForBalance(tokenPsuedoValue)),
      expiry: epoch.number.add(warmupPeriod),
      tokenLevel: tokenLevel,
      tokenId: _tokenId,
      lock: false,
      initialized: true
    });

    PegaERC20(PEGA).createsPega(tokenPsuedoValue);

    uint256 gonValue = gonsForBalance(tokenPsuedoValue);
    _gonBalances[address(this)] = _gonBalances[address(this)].sub(gonValue);
    _gonBalances[msg.sender] = _gonBalances[msg.sender].add(gonValue);
    emit LogStaked(_tokenId, tokenPsuedoValue, msg.sender);
    return true;
  }

  function rebase() public {
    if (epoch.endBlock <= block.number) {
      _rebasePsuedo(epoch.distribute, epoch.number);

      epoch.endBlock = epoch.endBlock.add(epoch.length);
      epoch.number++;

      if (distributor != address(0)) {
        IDistributor(distributor).distribute(); // mints more OHM to the contract at a fixed rate
      }
      // contract balance of ohm -> What's been staked + minted by distributor
      uint256 balance = IERC20(PEGA).balanceOf(address(this)).add(
        PegaERC20(PEGA).getStakedSupply()
      );
      uint256 staked = circulatingSupply(); // staked ohm supply

      if (balance <= staked) {
        epoch.distribute = 0;
      } else {
        epoch.distribute = balance.sub(staked);
      }
    }
  }

  function _rebasePsuedo(uint256 profit_, uint256 epoch_)
    internal
    returns (uint256)
  {
    uint256 rebaseAmount;
    uint256 circulatingSupply_ = circulatingSupply(); // set this to staked amount maybe ends compounding

    if (profit_ == 0) {
      emit LogSupply(epoch_, block.timestamp, _totalSupply);
      emit LogRebase(epoch_, 0, index());
      return 0;
    } else if (circulatingSupply_ > 0) {
      rebaseAmount = profit_.mul(_totalSupply).div(circulatingSupply_);
    } else {
      rebaseAmount = profit_;
    }

    _totalSupply = _totalSupply.add(rebaseAmount);

    if (_totalSupply > MAX_SUPPLY) {
      _totalSupply = MAX_SUPPLY;
    }

    _gonsPerFragment = TOTAL_GONS.div(_totalSupply);

    _storeRebase(circulatingSupply_, profit_, epoch_);
    emit RebaseEvent(profit_, epoch_);
    return _totalSupply;
  }

  /**
    @notice emits event with data about rebase
    @param previousCirculating_ uint
    @param profit_ uint
    @param epoch_ uint
    @return bool
    */
  function _storeRebase(
    uint256 previousCirculating_,
    uint256 profit_,
    uint256 epoch_
  ) internal returns (bool) {
    uint256 rebasePercent = profit_.mul(1e18).div(previousCirculating_);

    rebases.push(
      Rebase({
        epoch: epoch_,
        rebase: rebasePercent, // 18 decimals
        totalStakedBefore: previousCirculating_,
        totalStakedAfter: circulatingSupply(),
        amountRebased: profit_,
        index: index(),
        blockNumberOccured: block.number
      })
    );

    emit LogSupply(epoch_, block.timestamp, _totalSupply);
    emit LogRebase(epoch_, rebasePercent, index());

    return true;
  }

  function unstakeTokens(uint256 _amount, bool _trigger) external nonReentrant {
    if (_trigger) {
      rebase();
    }

    Claim memory info = stakingInfo[msg.sender];
    require(info.initialized, "No Staked Assets");

    if (epoch.number >= info.expiry && info.expiry != 0) {
      uint256 maxRewards = rewardBalance(msg.sender);
      uint256 gonValue = gonsForBalance(_amount);
      require(
        gonsForBalance(_amount) <= gonsForBalance(maxRewards),
        "Insufficient Balance."
      );

      _gonBalances[address(this)] = _gonBalances[address(this)].add(gonValue);
      _gonBalances[msg.sender] = _gonBalances[msg.sender].sub(gonValue);

      emit LogUnstakeTokens(info.tokenId, _amount, msg.sender);
      IERC20(PEGA).safeTransfer(msg.sender, _amount);
    }
  }

  function unstakeAll(bool _trigger) external nonReentrant {
    if (_trigger) {
      rebase();
    }
    Claim memory info = stakingInfo[msg.sender];
    require(info.initialized, "No Staked Assets");

    if (epoch.number >= info.expiry && info.expiry != 0) {
      uint256 maxRewards = rewardBalance(msg.sender);
      uint256 gonValue = _gonBalances[msg.sender];

      _gonBalances[address(this)] = _gonBalances[address(this)].add(gonValue);
      _gonBalances[msg.sender] = _gonBalances[msg.sender].sub(gonValue);

      uint256 tokenPsuedoValue = info.tokenLevel *
        500 *
        10**uint256(ERC20(PEGA).decimals());

      delete stakingInfo[msg.sender];

      emit LogUnstakeAll(
        info.tokenId,
        tokenPsuedoValue.add(maxRewards),
        msg.sender
      );
      PegaERC20(PEGA).burnsPega(tokenPsuedoValue);
      PegaPal(pegaPal).safeTransferFrom( // unstakes both
        address(this),
        msg.sender,
        info.tokenId
      );
      IERC20(PEGA).safeTransfer(msg.sender, maxRewards);
    }
  }

  function onERC721Received(
    address,
    address,
    uint256,
    bytes calldata
  ) external pure override returns (bytes4) {
    return IERC721Receiver.onERC721Received.selector;
  }
}

