pragma solidity ^0.8.7;

import {ERC721} from '../openzeppelin/ERC721.sol';
import {IERC20} from '../openzeppelin/IERC20.sol';
import {IDecentralistsDescriptor} from './IDecentralistsDescriptor.sol';
import {IDecentralists} from './IDecentralists.sol';

import 'hardhat/console.sol';

contract Decentralists is IDecentralists, ERC721 {
  // Costs
  // uint256 private constant HUMAN_COST = 0 ether;
  uint256 private constant VAMPIRE_COST = 0.08 ether;
  uint256 private constant METAHUMAN_COST = 0.08 ether;
  uint256 private constant APE_COST = 0.5 ether;

  // Caps
  uint16 public immutable MAXIMUM_NUMBER_OF_MINTS;
  uint16 public immutable MAXIMUM_NUMBER_OF_FEMALE_HUMANS;
  uint16 public immutable MAXIMUM_NUMBER_OF_MALE_HUMANS;
  uint16 public immutable MAXIMUM_NUMBER_OF_FEMALE_VAMPIRES;
  uint16 public immutable MAXIMUM_NUMBER_OF_MALE_VAMPIRES;
  uint16 public immutable MAXIMUM_NUMBER_OF_FEMALE_METAHUMANS;
  uint16 public immutable MAXIMUM_NUMBER_OF_MALE_METAHUMANS;
  uint16 public immutable MAXIMUM_NUMBER_OF_APES;

  // Decentralists Governance address
  address public governance;

  // Token URI descriptor
  IDecentralistsDescriptor public descriptor;
  bool public isDescriptorLocked;

  // Private Phase
  bool public isPublicPhaseActive;
  // Whitelisted
  mapping(address => bool) private _privateSaleWhitelisted;

  // Decremental counters: max -> 0
  uint16 count;
  uint16 femaleHumans;
  uint16 maleHumans;
  uint16 femaleVampires;
  uint16 maleVampires;
  uint16 femaleMetahumans;
  uint16 maleMetahumans;
  uint16 apes;

  // Combination of attributes
  struct Combination {
    uint8 base; // base
    uint8 neck; // neck
    uint8 facial; // facial
    uint8 earrings; // earrings
    uint8 head; // head
    uint8 glasses; // glasses
    uint8 lipstick; // lipstick
    uint8 smoking; // smoking
  }

  // Combinations: attributes_hash => tokenId
  mapping(bytes32 => uint256) private _combinationToId;

  // Combinations: tokenId => attributes
  mapping(uint256 => Combination) private _idToCombination;

  // Humans
  mapping(address => bool) private _isHuman;

  modifier whenDescriptorNotLocked() {
    require(!isDescriptorLocked, 'DESCRIPTOR_LOCKED');
    _;
  }

  modifier onlyGovernance() {
    require(msg.sender == governance, 'ONLY_BY_GOVERNANCE');
    _;
  }

  constructor(
    address tokenGovernance,
    address tokenDescriptor,
    address[] memory whitelisted,
    uint256[] memory caps
  ) ERC721('Decentralists', 'DCN') {
    // Decremental counters
    require(caps.length == 8);
    count = MAXIMUM_NUMBER_OF_MINTS = uint16(caps[0]);
    femaleHumans = MAXIMUM_NUMBER_OF_FEMALE_HUMANS = uint16(caps[1]);
    maleHumans = MAXIMUM_NUMBER_OF_MALE_HUMANS = uint16(caps[2]);
    femaleVampires = MAXIMUM_NUMBER_OF_FEMALE_VAMPIRES = uint16(caps[3]);
    maleVampires = MAXIMUM_NUMBER_OF_MALE_VAMPIRES = uint16(caps[4]);
    femaleMetahumans = MAXIMUM_NUMBER_OF_FEMALE_METAHUMANS = uint16(caps[5]);
    maleMetahumans = MAXIMUM_NUMBER_OF_MALE_METAHUMANS = uint16(caps[6]);
    apes = MAXIMUM_NUMBER_OF_APES = uint16(caps[7]);

    governance = tokenGovernance;
    descriptor = IDecentralistsDescriptor(tokenDescriptor);

    // Private Sale
    for (uint256 i; i < whitelisted.length; i++) {
      _privateSaleWhitelisted[whitelisted[i]] = true;
      emit PrivateSaleWhitelistedAddition(whitelisted[i]);
    }
  }

  function claim(uint256[] memory attributes) external payable override {
    require(isPublicPhaseActive, 'PRIVATE_SALE_ACTIVE');
    require(_checkValue(attributes[0]), 'INCORRECT_VALUE');
    _claim(attributes);
  }

  function privateSaleClaim(uint256[] memory attributes) external override {
    require(!isPublicPhaseActive, 'PRIVATE_SALE_NOT_ACTIVE');
    require(_privateSaleWhitelisted[msg.sender], 'ONLY_WHITELISTED');
    _claim(attributes);
  }

  function _checkAvailability(uint256 _base) internal view returns (bool) {
    if (_base == 0 && femaleHumans == 0) {
      return false;
    } else if (_base == 1 && maleHumans == 0) {
      return false;
    } else if (_base == 2 && femaleVampires == 0) {
      return false;
    } else if (_base == 3 && maleVampires == 0) {
      return false;
    } else if (_base == 4 && femaleMetahumans == 0) {
      return false;
    } else if (_base == 5 && maleMetahumans == 0) {
      return false;
    } else if (_base == 6 && apes == 0) {
      return false;
    } else {
      return true;
    }
  }

  function _checkValue(uint256 _base) internal view returns (bool) {
    if (_base / 2 == 0) {
      return true;
    } else if (_base / 2 == 1 && msg.value >= VAMPIRE_COST) {
      return true;
    } else if (_base / 2 == 2 && msg.value >= METAHUMAN_COST) {
      return true;
    } else if (_base / 2 == 3 && msg.value >= APE_COST) {
      return true;
    } else {
      return false;
    }
  }

  function _claim(uint256[] memory attributes) internal {
    require(msg.sender == tx.origin, 'ONLY_HUMAN');
    require(count > 0, 'NO_MORE_CLAIMS');
    require(!existsWithAttributes(attributes), 'ALREADY_EXISTS');

    uint256 base = attributes[0];
    require(base / 2 > 0 || !_isHuman[msg.sender], 'ALREADY_HUMAN');
    require(_checkAvailability(base), 'NO_MORE_CLAIMS_IN_TRAIT');

    // TokenID
    // TokenId 0 is reserved
    uint256 tokenId = MAXIMUM_NUMBER_OF_MINTS - count + 1;
    count--;

    // Update trait counter
    if (base == 0) {
      femaleHumans--;
    } else if (base == 1) {
      maleHumans--;
    } else if (base == 2) {
      femaleVampires--;
    } else if (base == 3) {
      maleVampires--;
    } else if (base == 4) {
      femaleMetahumans--;
    } else if (base == 5) {
      maleMetahumans--;
    } else if (base == 6) {
      apes--;
    }

    // Attributes
    _combinationToId[
      keccak256(
        abi.encodePacked(
          attributes[0], // base
          attributes[1], // neck
          attributes[2], // facial
          attributes[3], // earrings
          attributes[4], // head
          attributes[5], // glasses
          attributes[6], // lipstick
          attributes[7] // smoking
        )
      )
    ] = tokenId;
    _idToCombination[tokenId] = Combination(
      uint8(attributes[0]), // base
      uint8(attributes[1]), // neck
      uint8(attributes[2]), // facial
      uint8(attributes[3]), // earrings
      uint8(attributes[4]), // head
      uint8(attributes[5]), // glasses
      uint8(attributes[6]), // lipstick
      uint8(attributes[7]) // smoking
    );

    // Initiate minting
    _safeMint(msg.sender, tokenId);

    emit DecentralistMint(tokenId, msg.sender, attributes);
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 tokenId
  ) internal override(ERC721) {
    // Human
    if (_idToCombination[tokenId].base / 2 == 0) {
      require(!_isHuman[to], 'ALREADY_HUMAN');

      _isHuman[from] = false;
      _isHuman[to] = true;
    }
  }

  function existsWithAttributes(uint256[] memory attributes) internal view returns (bool) {
    return
      _combinationToId[
        keccak256(
          abi.encodePacked(
            attributes[0], // base
            attributes[1], // neck
            attributes[2], // facial
            attributes[3], // earrings
            attributes[4], // head
            attributes[5], // glasses
            attributes[6], // lipstick
            attributes[7] // smoking
          )
        )
      ] != 0;
  }

  function getTokenId(uint256[] memory attributes) external view override returns (uint256) {
    require(existsWithAttributes(attributes), 'NOT_EXISTS');
    return
      _combinationToId[
        keccak256(
          abi.encodePacked(
            attributes[0], // base
            attributes[1], // neck
            attributes[2], // facial
            attributes[3], // earrings
            attributes[4], // head
            attributes[5], // glasses
            attributes[6], // lipstick
            attributes[7] // smoking
          )
        )
      ];
  }

  function getAttributes(uint256 tokenId) external view override returns (uint256[] memory) {
    return _getAttributes(tokenId);
  }

  function _getAttributes(uint256 tokenId) internal view returns (uint256[] memory) {
    uint256[] memory result = new uint256[](8);
    result[0] = _idToCombination[tokenId].base;
    result[1] = _idToCombination[tokenId].neck;
    result[2] = _idToCombination[tokenId].facial;
    result[3] = _idToCombination[tokenId].earrings;
    result[4] = _idToCombination[tokenId].head;
    result[5] = _idToCombination[tokenId].glasses;
    result[6] = _idToCombination[tokenId].lipstick;
    result[7] = _idToCombination[tokenId].smoking;
    return result;
  }

  // TODO: Review
  function formatTokenURI(string memory _imageSVG) public pure returns (string memory) {
    return
      string(
        abi.encodePacked(
          '{"name": "A decentralist", "description": "An awesome NFT for you", "attributes": "list attributes here", "image": "',
          _imageSVG,
          '"}'
        )
      );
  }

  function tokenURI(uint256 tokenId)
    public
    view
    override(ERC721, IDecentralists)
    returns (string memory)
  {
    require(_exists(tokenId), 'TOKEN_NOT_EXISTS');
    return descriptor.tokenURI(tokenId, _getAttributes(tokenId));
  }

  function isHuman(address user) external view override returns (bool) {
    return _isHuman[user];
  }

  function totalSupply() external view override returns (uint256) {
    return MAXIMUM_NUMBER_OF_MINTS - count;
  }

  function totalFemaleHumansSupply() external view override returns (uint256) {
    return MAXIMUM_NUMBER_OF_FEMALE_HUMANS - femaleHumans;
  }

  function totalMaleHumansSupply() external view override returns (uint256) {
    return MAXIMUM_NUMBER_OF_MALE_HUMANS - maleHumans;
  }

  function totalFemaleVampiresSupply() external view override returns (uint256) {
    return MAXIMUM_NUMBER_OF_FEMALE_VAMPIRES - femaleVampires;
  }

  function totalMaleVampiresSupply() external view override returns (uint256) {
    return MAXIMUM_NUMBER_OF_MALE_VAMPIRES - maleVampires;
  }

  function totalFemaleMetahumansSupply() external view override returns (uint256) {
    return MAXIMUM_NUMBER_OF_FEMALE_METAHUMANS - femaleMetahumans;
  }

  function totalMaleMetahumansSupply() external view override returns (uint256) {
    return MAXIMUM_NUMBER_OF_MALE_METAHUMANS - maleMetahumans;
  }

  function totalApesSupply() external view override returns (uint256) {
    return MAXIMUM_NUMBER_OF_APES - apes;
  }

  function isPrivateSaleWhitelisted(address user) external view override returns (bool) {
    return _privateSaleWhitelisted[user];
  }

  // /////////////////////////
  // Governance
  // /////////////////////////

  function pullFunds(address recipient, uint256 amount) external override onlyGovernance {
    (bool success, ) = recipient.call{value: amount}('');
    require(success, 'Address: unable to send value');
    emit FundsWithdrawal(recipient, amount);
  }

  function pullTokens(
    address asset,
    address recipient,
    uint256 amount
  ) external override onlyGovernance {
    IERC20(asset).transfer(recipient, amount);
  }

  function addPrivateSaleWhitelisted(address whitelisted) public override onlyGovernance {
    _privateSaleWhitelisted[whitelisted] = true;

    emit PrivateSaleWhitelistedAddition(whitelisted);
  }

  function removePrivateSaleWhitelisted(address whitelisted) public override onlyGovernance {
    _privateSaleWhitelisted[whitelisted] = false;

    emit PrivateSaleWhitelistedRemoval(whitelisted);
  }

  function setGovernance(address newGovernance) external override onlyGovernance {
    governance = newGovernance;

    emit GovernanceUpdate(newGovernance);
  }

  function setDescriptor(address newDescriptor)
    external
    override
    onlyGovernance
    whenDescriptorNotLocked
  {
    descriptor = IDecentralistsDescriptor(newDescriptor);

    emit DescriptorUpdate(newDescriptor);
  }

  function lockDescriptor() external override onlyGovernance whenDescriptorNotLocked {
    isDescriptorLocked = true;

    emit DescriptorLock();
  }

  function deactivatePrivateSale() external override onlyGovernance {
    isPublicPhaseActive = true;

    emit PrivateSaleEnd();
  }
}

