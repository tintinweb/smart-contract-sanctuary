        require(!mintGuardianPaused[vToken], "mint is paused");


        // initialize lock when first entering the market. If userUnlock time is 0, then set unlock time = current time + lock duration
        if(userUnlockTime[minter] == 0) {
            userUnlockTime[minter] = add_(block.timestamp, lockDuration);
        }

        // Shh - currently unused
        mintAmount;

        if (!markets[vToken].isListed) {
            return uint(Error.MARKET_NOT_LISTED);
