interface ComptrollerLensInterface {
    function markets(address) external view returns (bool, uint);
    function oracle() external view returns (PriceOracle);
    function getAccountLiquidity(address) external view returns (uint, uint, uint);
    function getAssetsIn(address) external view returns (VToken[] memory);
    function claimSupreme(address) external;
    function venusAccrued(address) external view returns (uint);
    function getAllMarkets() external view returns (VToken[] memory);
    function venusSpeedsSupply(address) external view returns (uint);
    function venusSpeedsBorrow(address) external view returns (uint);
}

contract VenusLens is Exponential {
    
    function getTotalVenusAccruedAll(VToken[] calldata vTokens, address user) external view returns (uint256) {
        uint256 vTokenCount = vTokens.length;
        Comptroller comptroller = Comptroller(address(vTokens[0].comptroller()));
        uint256 totalVenusAccrued = comptroller.venusAccrued(user);
        for (uint i = 0; i < vTokenCount; i++) {
            totalVenusAccrued = add_(totalVenusAccrued, getTotalVenusAccrued(vTokens[i], user));
        }
        
        return totalVenusAccrued;
    }
    
    function getTotalVenusAccrued(VToken vToken, address user) public view returns (uint256) {
        Comptroller comptroller = Comptroller(address(vToken.comptroller()));
        uint256 supplyVenusDelta = getSupplierVenusDelta(comptroller, vToken, user);
        uint256 borrowVenusDelta = getBorrowerVenusDelta(comptroller, vToken, user);
        uint256 venusDelta = add_(supplyVenusDelta, borrowVenusDelta);
        //uint256 totalVenusAccrued = add_(comptroller.venusAccrued(user), venusDelta);
        
        return venusDelta;
    }
    
    function getSupplierVenusDelta(Comptroller comptroller, VToken vToken, address supplier) internal view returns (uint256) {
        (uint224 supplyStateIndex, ) = comptroller.venusSupplyState(address(vToken));
        Double memory supplyIndex = Double({mantissa: supplyStateIndex});
        Double memory supplierIndex = Double({mantissa: comptroller.venusSupplierIndex(address(vToken), supplier)});
        
        if (supplierIndex.mantissa == 0 && supplyIndex.mantissa > 0) {
            supplierIndex.mantissa = comptroller.venusInitialIndex();
        }
    
        Double memory deltaIndex = sub_(supplyIndex, supplierIndex);
        uint256 supplierTokens = vToken.balanceOf(supplier);
        uint256 supplierDelta = mul_(supplierTokens, deltaIndex);
        return supplierDelta;
    }

    function getBorrowerVenusDelta(Comptroller comptroller, VToken vToken, address borrower) internal view returns (uint256) {
        Exp memory marketBorrowIndex = Exp({mantissa: vToken.borrowIndex()});
        (uint224 borrowStateIndex, ) = comptroller.venusBorrowState(address(vToken));
        Double memory borrowIndex = Double({mantissa: borrowStateIndex});
        Double memory borrowerIndex = Double({mantissa: comptroller.venusBorrowerIndex(address(vToken), borrower)});

        if (borrowerIndex.mantissa > 0) {
            Double memory deltaIndex = sub_(borrowIndex, borrowerIndex);
            uint256 borrowerAmount = div_(vToken.borrowBalanceStored(borrower), marketBorrowIndex);
            uint256 borrowerDelta = mul_(borrowerAmount, deltaIndex);
            return borrowerDelta;
        }
        return 0;
    }
    
    struct VTokenMetadata {
        address vToken;
        uint exchangeRateCurrent;
        uint supplyRatePerBlock;
        uint borrowRatePerBlock;
        uint reserveFactorMantissa;
        uint totalBorrows;
        uint totalReserves;
        uint totalSupply;
        uint totalCash;
        bool isListed;
        uint collateralFactorMantissa;
        address underlyingAssetAddress;
        uint vTokenDecimals;
        uint underlyingDecimals;
        uint venusSpeedsSupply;
        uint venusSpeedsBorrow;
    }

    function vTokenMetadata(VToken vToken) public view returns (VTokenMetadata memory) {
        uint exchangeRateCurrent = vToken.exchangeRateStored();
        ComptrollerLensInterface comptroller = ComptrollerLensInterface(address(vToken.comptroller()));
        (bool isListed, uint collateralFactorMantissa) = comptroller.markets(address(vToken));
        address underlyingAssetAddress;
        uint underlyingDecimals;

        if (compareStrings(vToken.symbol(), "xGM")) {
            underlyingAssetAddress = address(0);
            underlyingDecimals = 18;
        } else {
            VBep20 vBep20 = VBep20(address(vToken));
            underlyingAssetAddress = vBep20.underlying();
            underlyingDecimals = EIP20Interface(vBep20.underlying()).decimals();
        }

        return VTokenMetadata({
            vToken: address(vToken),
            exchangeRateCurrent: exchangeRateCurrent,
            supplyRatePerBlock: vToken.supplyRatePerBlock(),
            borrowRatePerBlock: vToken.borrowRatePerBlock(),
            reserveFactorMantissa: vToken.reserveFactorMantissa(),
            totalBorrows: vToken.totalBorrows(),
            totalReserves: vToken.totalReserves(),
            totalSupply: vToken.totalSupply(),
            totalCash: vToken.getCash(),
            isListed: isListed,
            collateralFactorMantissa: collateralFactorMantissa,
            underlyingAssetAddress: underlyingAssetAddress,
            vTokenDecimals: vToken.decimals(),
            underlyingDecimals: underlyingDecimals,
            venusSpeedsSupply: comptroller.venusSpeedsSupply(address(vToken)),
            venusSpeedsBorrow: comptroller.venusSpeedsBorrow(address(vToken))
        });
    }

    struct VTokenMetaDataView {
        address poolAddress;
        string tokenSymbol;
        address tokenAddress;
        uint price;
        uint decimals;
        uint totalSupply;
        uint totalBorrow;
        uint lockedAmount;
        uint liquidateAmount;
        uint borrowRate;
        uint supplyRate;
    }
    
    function mul(uint256 a, uint256 b) internal pure returns (uint256) {
        if (a == 0) return 0;
        uint256 c = a * b;
        require(c / a == b, "SafeMath: multiplication overflow");
        return c;
    }
    
    /**
     * @dev Returns the integer division of two unsigned integers, reverting on
     * division by zero. The result is rounded towards zero.
     *
     * Counterpart to Solidity's `/` operator. Note: this function uses a
     * `revert` opcode (which leaves remaining gas untouched) while Solidity
     * uses an invalid opcode to revert (consuming all remaining gas).
     *
     * Requirements:
     *
     * - The divisor cannot be zero.
     */
    function div(uint256 a, uint256 b) internal pure returns (uint256) {
        require(b > 0, "SafeMath: division by zero");
        return a / b;
    }
    
    function getVTokenMetaDataView(VToken vToken) public view returns (VTokenMetaDataView memory) {
        uint price = vTokenUnderlyingPrice(vToken).underlyingPrice;
        uint decimals = vToken.decimals();
        uint exchangeR = div(vToken.exchangeRateStored(), 1e18);
        uint totalSupply =  div(mul(mul(vToken.totalSupply(), price), exchangeR), 1e20);
        uint totalBorrow = div(mul(vToken.totalBorrows(), price), 1e20);

        uint TVL = add(vToken.totalSupply(), vToken.totalBorrows(), "add");
        address tokenAddress = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;
        if (compareStrings(vToken.symbol(), "xGM")) {
            tokenAddress = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;
        } else {
            VBep20 vBep20 = VBep20(address(vToken));
            tokenAddress = vBep20.underlying();
        }
        return VTokenMetaDataView(
            {
                poolAddress: address(vToken),
                tokenSymbol: vToken.symbol(),
                tokenAddress: tokenAddress,
                price: price,
                decimals: decimals,
                totalSupply: totalSupply,
                totalBorrow: totalBorrow,
                lockedAmount: TVL,
                liquidateAmount: 0,
                borrowRate: vToken.borrowRatePerBlock(),
                supplyRate: vToken.supplyRatePerBlock()
                   
                
            });
        
    }
    
    function getTokenInfo(ComptrollerLensInterface comptroller) public view returns (VTokenMetaDataView[] memory){
        
        VToken[] memory vTokens = comptroller.getAllMarkets();
        uint vTokenCount = vTokens.length;
        VTokenMetaDataView[] memory res = new VTokenMetaDataView[](vTokenCount);
        for (uint i = 0; i < vTokenCount; i++) {
            res[i] = getVTokenMetaDataView(vTokens[i]);
        }
        
        return res;
    }


    struct totalInfo {
        uint totalSupply;
        uint totalBorrow;
        uint TVL;
        uint liquidated24h;
        
    }
    
    function getTotalInfo(ComptrollerLensInterface comptroller) external view returns (totalInfo memory) {
        
        VTokenMetaDataView[] memory res = getTokenInfo(comptroller);
        uint vTokenCount = res.length;
        uint totalSupply = 0;
        uint totalBorrow = 0;
        
        for (uint i = 0; i < vTokenCount; i++) {
            totalSupply += res[i].totalSupply;
            totalBorrow += res[i].totalBorrow;
        }
        
        return totalInfo({
            totalSupply: totalSupply,
            totalBorrow: totalBorrow,
            TVL: totalSupply + totalBorrow,
            liquidated24h: 0
        });
    }
    function vTokenMetadataAll(VToken[] calldata vTokens) external view returns (VTokenMetadata[] memory) {
        uint vTokenCount = vTokens.length;
        VTokenMetadata[] memory res = new VTokenMetadata[](vTokenCount);
        for (uint i = 0; i < vTokenCount; i++) {
            res[i] = vTokenMetadata(vTokens[i]);
        }
        return res;
    }

    struct VTokenBalances {
        address vToken;
        uint balanceOf;
        uint borrowBalanceCurrent;
        uint supplyBalanceCurrent;
        uint balanceOfUnderlying;
        uint tokenBalance;
        uint tokenAllowance;
        bool isCollateral;
    }

    function vTokenBalances(VToken vToken, address payable account) public view returns (VTokenBalances memory) {
        uint balanceOf = vToken.balanceOf(account);
        uint borrowBalanceCurrent = vToken.borrowBalanceStored(account);
        uint exchangeRateCurrent = vToken.exchangeRateStored();
        Exp memory exchangeRate = Exp({mantissa: exchangeRateCurrent});
        (MathError mErr, uint balanceOfUnderlying) = mulScalarTruncate(exchangeRate, balanceOf);
        require(mErr == MathError.NO_ERROR, "balanceOfUnderlying could not be calculated");
        
        uint tokenBalance;
        uint tokenAllowance;

        if (compareStrings(vToken.symbol(), "xGM")) {
            tokenBalance = account.balance;
            tokenAllowance = account.balance;
        } else {
            VBep20 vBep20 = VBep20(address(vToken));
            EIP20Interface underlying = EIP20Interface(vBep20.underlying());
            tokenBalance = underlying.balanceOf(account);
            tokenAllowance = underlying.allowance(account, address(vToken));
        }

        
        return VTokenBalances({
            vToken: address(vToken),
            balanceOf: balanceOf,
            borrowBalanceCurrent: borrowBalanceCurrent,
            supplyBalanceCurrent: vToken.balanceOf(account),
            balanceOfUnderlying: balanceOfUnderlying,
            tokenBalance: tokenBalance,
            tokenAllowance: tokenAllowance,
            isCollateral: false
        });
    }

    function vTokenBalancesAll(VToken[] calldata vTokens, address payable account) external view returns (VTokenBalances[] memory) {
        uint vTokenCount = vTokens.length;
        VTokenBalances[] memory res = new VTokenBalances[](vTokenCount);
        ComptrollerLensInterface comptroller = ComptrollerLensInterface(address(vTokens[0].comptroller()));
        VToken[] memory assets = comptroller.getAssetsIn(account);
        uint assetCount = assets.length;
        

        for (uint i = 0; i < vTokenCount; i++) {
            res[i] = vTokenBalances(vTokens[i], account);
            for(uint j = 0; j < assetCount; j++) {
                if(address(assets[j]) == address(vTokens[i])) {
                    res[i].isCollateral = true;
                }
            }
        }
        return res;
    }

    struct VTokenUnderlyingPrice {
        address vToken;
        uint underlyingPrice;
    }

    function vTokenUnderlyingPrice(VToken vToken) public view returns (VTokenUnderlyingPrice memory) {
        ComptrollerLensInterface comptroller = ComptrollerLensInterface(address(vToken.comptroller()));
        PriceOracle priceOracle = comptroller.oracle();

        return VTokenUnderlyingPrice({
            vToken: address(vToken),
            underlyingPrice: priceOracle.getUnderlyingPrice(vToken)
        });
    }

    function vTokenUnderlyingPriceAll(VToken[] calldata vTokens) external view returns (VTokenUnderlyingPrice[] memory) {
