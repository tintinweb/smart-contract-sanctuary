                blockNumber: uint256(blockNumbers[i]),
                votes: uint256(sxc.getPriorVotes(account, blockNumbers[i]))
            });
        }
        return res;
    }


    function getSXCBalanceMetadataExt(SXC sxc, ComptrollerLensInterface comptroller, address account) external returns (SXCBalanceMetadataExt memory) {
            uint balance = sxc.balanceOf(account);
            comptroller.claimSupreme(account);
            uint newBalance = sxc.balanceOf(account);
            uint accrued = comptroller.venusAccrued(account);
            uint total = add(accrued, newBalance, "sum sxc total");
            uint allocated = sub(total, balance, "sub allocated");
    
            return SXCBalanceMetadataExt({
                balance: balance,
                votes: uint256(sxc.getCurrentVotes(account)),
                delegate: sxc.delegates(account),
                allocated: allocated
            });
        }
    function compareStrings(string memory a, string memory b) internal pure returns (bool) {
        return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
    }

    function add(uint a, uint b, string memory errorMessage) internal pure returns (uint) {
        uint c = a + b;
        require(c >= a, errorMessage);
        return c;
    }

    function sub(uint a, uint b, string memory errorMessage) internal pure returns (uint) {
        require(b <= a, errorMessage);
        uint c = a - b;
        return c;
    }
}


