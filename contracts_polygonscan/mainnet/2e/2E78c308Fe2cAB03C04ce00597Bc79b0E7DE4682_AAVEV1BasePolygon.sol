/**
    AAVEV1Base is the IV implementation that targets AAVE.
    It takes the base asset and stakes into minichef
*/
abstract contract AAVEV1Base is InvestmentVehicleSingleAssetBaseV1Upgradeable {

    using SafeMathUpgradeable for uint256;
    using SafeERC20Upgradeable for IERC20Upgradeable;
   
    address aToken;
    uint16 constant referalCode = 0;

    /// initialize the iv 
    /// @param _store the address of system storage
    /// @param _baseAsset the address of base asset
    function initialize(
        address _store,
        address _baseAsset
    ) public override initializer {
        super.initialize(_store, _baseAsset);
        ILendingPool.ReserveData memory reserveData = ILendingPool(_lendingPool()).getReserveData(
            _baseAsset
        );
        aToken = reserveData.aTokenAddress;
    }

    // Override these function to provide the corresponding address.
    function _lendingPool() internal virtual pure returns(address);
    function _incentivesController() internal virtual pure returns(address);
    function _rewardToken() internal virtual pure returns(address);
    function _sushiRouter() internal virtual pure returns(address);
    
    function _investAll() internal override {
        address _baseAsset = baseAsset;
        uint256 baseAssetAmountInVehicle = IERC20Upgradeable(_baseAsset).balanceOf(address(this));
        if(baseAssetAmountInVehicle > 0){
            // Approve lendingPool
            IERC20Upgradeable(_baseAsset).approve(_lendingPool(), baseAssetAmountInVehicle);
            // Deposit
            ILendingPool(_lendingPool()).deposit(
                _baseAsset,
                baseAssetAmountInVehicle,
                address(this),
                referalCode
            );
        }
    }

    /** Interacting with underlying investment opportunities */
    function _pullFundsFromInvestment(uint256 _baseAmount) internal override{
        uint256 owned = invested();
        uint256 withdrawing = MathUpgradeable.min(owned, _baseAmount);

        ILendingPool(_lendingPool()).withdraw(
            baseAsset,
            _baseAmount,
            address(this)
        );
    }

    function _collectProfitAsBaseAsset() internal override returns (uint256) {

        address[] memory _assets = new address[](1);
        _assets[0] = aToken;

        uint256 rewardAmount = IAaveIncentivesController(
                _incentivesController()
            ).getRewardsBalance(_assets, address(this));

        //Claim and liquidate the reward to baseAsset
        if(rewardAmount > 0){
            IAaveIncentivesController(
            _incentivesController()
            ).claimRewards(
                _assets,
                rewardAmount,
                address(this)
            );
            address rewardToken = _rewardToken();

            address _swapCenter = swapCenter();

            IERC20Upgradeable(rewardToken).approve(_swapCenter, rewardAmount);

            ISwapCenter(_swapCenter).swapExactTokenIn(
                rewardToken, 
                baseAsset,
                IERC20Upgradeable(rewardToken).balanceOf(address(this)),            
                0
            );
        }

        return nativeProfitPending();
    }

    /** View functions */

    /// calculates the amount of base asset deposited into AAVE
    /// @return amount of base asset in AAVE
    function invested() public view override returns (uint256){
        return IaToken(aToken).balanceOf(address(this));
    }

    function profitsPending() public view override returns (uint256) {
        uint256 nativeProfit = nativeProfitPending();
        
        address[] memory _assets = new address[](1);
        _assets[0] = aToken;
        uint256 pendingReward = IAaveIncentivesController(
            _incentivesController()
        ).getRewardsBalance(_assets, address(this));

        address[] memory path = new address[](2);
        path[0] = _rewardToken();
        path[1] = baseAsset;

        // swapResult[0] is the input token amount
        // swapResult[1] is the output of the first swap along the path and so on.
        // swapResult.length-1 refers to the last token out => output token amount
        uint256[] memory swapResult = IUniswapV2Router02(
            _sushiRouter()).getAmountsOut(pendingReward, path);
        
        uint256 rewardProfit = swapResult[swapResult.length-1];
        return rewardProfit.add(nativeProfit); 

    }

    function nativeProfitPending() public view returns (uint256) {
        uint256 totalBalance = invested().add(IERC20Upgradeable(baseAsset).balanceOf(address(this)));
        return totalBalance.sub(totalDebt());
    }
}

