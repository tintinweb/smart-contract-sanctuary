/**
 * @title Interface for contracts conforming to ERC-20
 */
interface ERC20Interface {
    function transferFrom(
        address from,
        address to,
        uint256 tokens
    ) external returns (bool success);
}

/**
 * @title Interface for contracts conforming to ERC-721
 */
interface ERC721Interface {
    function ownerOf(uint256 _tokenId) external view returns (address _owner);

    function approve(address _to, uint256 _tokenId) external;

    function getApproved(uint256 _tokenId) external view returns (address);

    function isApprovedForAll(address _owner, address _operator) external view returns (bool);

    function safeTransferFrom(
        address _from,
        address _to,
        uint256 _tokenId
    ) external;

    function supportsInterface(bytes4) external view returns (bool);
}

interface ERC721Verifiable is ERC721Interface {
    function verifyFingerprint(uint256, bytes memory) external view returns (bool);
}

contract MarketplaceStorage {
    IRoyaltyRegistry public RoyaltyRegistry;
    IWeth public Weth;

    bytes4 internal constant _INTERFACE_ID_ERC2981 = 0x2a55205a;
    bytes4 internal constant _INTERFACE_ID_ROYALTYENGINEV1 = 0xcb23f816;

    struct Order {
        // Order ID
        bytes32 id;
        // Owner of the NFT
        address seller;
        // NFT registry address
        address nftAddress;
        // Id of the NFT token
        uint256 tokenId;
        // Price (in wei) for the published item
        uint256 price;
        // Time when this sale ends
        uint256 expiresAt;
        // Token accepted for payment
        address paymentToken;
    }

    // From ERC721 registry assetId to Order (to avoid asset collision)
    mapping(bytes32 => Order) public orderByOrderId;

    // Maps token address to bool for either true (token is accepted as payment)
    // or false (token is not accepted for payment)
    mapping(address => bool) public acceptedTokenMap;

    uint256 public OwnerCutPerMillion;
    uint256 public PublicationFeeInWei;
    address public FeeReceiver;

    bytes4 public constant ERC721_Interface = bytes4(0x80ac58cd);

    // EVENTS
    event OrderCreated(
        bytes32 id,
        uint256 indexed assetId,
        address indexed seller,
        address nftAddress,
        uint256 priceInWei,
        address paymentToken,
        uint256 expiresAt
    );
    event OrderFulfilled(
        bytes32 id,
        address indexed seller,
        uint256 totalPrice,
        address paymentToken,
        address indexed buyer
    );
    event OrderSuccessful(
        bytes32 id,
        uint256 indexed assetId,
        address indexed seller,
        address nftAddress,
        address indexed buyer
    );
    event OrderCancelled(bytes32 id, address indexed seller, address nftAddress);
    event ChangedPublicationFee(uint256 publicationFee);
    event ChangedOwnerCutPerMillion(uint256 ownerCutPerMillion);
    event ChangedRoyaltyRegistry(address royaltyRegistry);
    event ChangedAcceptedToken(address token, bool accepted);
    event ChangedFeeReceiver(address feeReceiver);
    event MarketFeePaid(uint256 amount);
}

