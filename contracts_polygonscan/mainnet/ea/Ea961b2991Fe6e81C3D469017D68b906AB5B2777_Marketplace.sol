contract Marketplace is IMarketplace, Ownable, Pausable, MarketplaceStorage {
    using Address for address;
    using SafeERC20 for IERC20;
    using ERC165Checker for address;

    constructor(address _royaltyRegistry, address _weth) {
        require(_msgSender() != address(0), "Marketplace: owner cannot be address zero");
        require(_weth != address(0), "Marketplace: Weth cannot be address zero");
        require(_royaltyRegistry != address(0), "Marketplace: royalty registry cannot be address zero");

        RoyaltyRegistry = IRoyaltyRegistry(_royaltyRegistry);
        Weth = IWeth(_weth);
    }

    receive() external payable {}

    /**
     * @dev Creates a new order
     * @param nftAddress - Non fungible registry address
     * @param tokenId - ID of the published NFT
     * @param priceInWei - Price in Wei for the supported coin
     * @param expiresAt - Duration of the order (in hours)
     */
    function createOrder(
        address nftAddress,
        uint256 tokenId,
        uint256 priceInWei,
        uint256 expiresAt,
        address paymentToken
    ) external payable whenNotPaused returns (bytes32) {
        require(acceptedTokenMap[paymentToken], "Marketplace: token not accepted for payments");

        if (PublicationFeeInWei > 0) {
            require(msg.value == PublicationFeeInWei, "Marketplace: funds must equal publication fee");
            Weth.deposit{ value: msg.value }();
        }
        return _createOrder(nftAddress, tokenId, priceInWei, expiresAt, paymentToken);
    }

    /**
     * @dev Cancel an already published order
     *  can only be canceled by seller or the contract owner
     * @param orderId - ID of the published order
     */
    function cancelOrder(bytes32 orderId) external whenNotPaused {
        _cancelOrder(orderId);
    }

    /**
     * @dev Executes the sale for a published NFT
     * @param orderId - ID of the published order
     */
    function executeOrder(bytes32 orderId) external payable whenNotPaused {
        if (orderByOrderId[orderId].paymentToken == address(Weth)) {
            require(msg.value == orderByOrderId[orderId].price, "Marketplace: input amount incorrect");
            Weth.deposit{ value: msg.value }();
        }
        _executeOrder(orderId);
    }

    /**
     * @dev Creates a new order
     * @param _nftAddress - Non fungible registry address
     * @param _tokenId   - ID of the published NFT
     * @param _priceInWei - Price in Wei for the supported coin
     * @param _expiresAt - Duration of the order (in hours)
     * @param _paymentToken - Token address for payment
     */
    function _createOrder(
        address _nftAddress,
        uint256 _tokenId,
        uint256 _priceInWei,
        uint256 _expiresAt,
        address _paymentToken
    ) internal returns (bytes32) {
        _requireERC721(_nftAddress);

        address sender = _msgSender();

        ERC721Interface nftRegistry = ERC721Interface(_nftAddress);

        address assetOwner = nftRegistry.ownerOf(_tokenId);
        require(sender == assetOwner, "Marketplace: Only the owner can create orders");
        require(
            nftRegistry.getApproved(_tokenId) == address(this) ||
                nftRegistry.isApprovedForAll(assetOwner, address(this)),
            "Marketplace: The contract is not authorized to manage the asset"
        );
        require(_priceInWei > 0, "Marketplace: Price should be bigger than 0");

        require(
            _expiresAt > block.timestamp + 1 minutes,
            "Marketplace: Publication should be more than 1 minute in the future"
        );

        bytes32 orderId = keccak256(
            abi.encodePacked(block.timestamp, assetOwner, _tokenId, _nftAddress, _priceInWei, _paymentToken)
        );

        orderByOrderId[orderId] = Order({
            id: orderId,
            seller: assetOwner,
            nftAddress: _nftAddress,
            tokenId: _tokenId,
            price: _priceInWei,
            expiresAt: _expiresAt,
            paymentToken: _paymentToken
        });

        emit OrderCreated(orderId, _tokenId, assetOwner, _nftAddress, _priceInWei, _paymentToken, _expiresAt);
        return orderId;
    }

    /**
     * @dev Cancel an already published order
     *  can only be canceled by seller or the contract owner
     * @param orderId - ID of published order
     */
    function _cancelOrder(bytes32 orderId) internal returns (Order memory) {
        address sender = _msgSender();
        Order memory order = orderByOrderId[orderId];

        require(order.id != 0, "Marketplace: Asset not published");
        require(order.seller == sender || sender == owner(), "Marketplace: Unauthorized user");

        address orderSeller = order.seller;
        address orderNftAddress = order.nftAddress;
        delete orderByOrderId[orderId];

        emit OrderCancelled(orderId, orderSeller, orderNftAddress);

        return order;
    }

    /**
     * @dev Executes the sale for a published NFT
     * @param _orderId - ID of the published order
     */
    function _executeOrder(bytes32 _orderId) internal returns (Order memory) {
        address sender = _msgSender();

        Order memory order = orderByOrderId[_orderId];
        _requireERC721(order.nftAddress);
        ERC721Verifiable nftRegistry = ERC721Verifiable(order.nftAddress);

        address seller = order.seller;

        require(seller != address(0), "Marketplace: Invalid address");
        require(seller != sender, "Marketplace: Unauthorized user");
        require(block.timestamp < order.expiresAt, "Marketplace: The order expired");
        require(seller == nftRegistry.ownerOf(order.tokenId), "Marketplace: The seller is no longer the owner");

        uint256 royaltiesTotal = 0;
        uint256 saleShareAmount = 0;

        address payable[] memory recipients;
        uint256[] memory amounts;

        bool success = false;

        address royaltyLookUp = RoyaltyRegistry.getRoyaltyLookupAddress(order.nftAddress);

        if (royaltyLookUp.supportsInterface(_INTERFACE_ID_ROYALTYENGINEV1) == true) {
            (recipients, amounts) = IRoyaltyEngineV1(royaltyLookUp).getRoyaltyView(
                order.nftAddress,
                order.tokenId,
                order.price
            );
            console.log("supports RoyaltyEngineV1");
            success = true;
        } else if (royaltyLookUp.supportsInterface(_INTERFACE_ID_ERC2981) == true) {
            (address receiver, uint256 amount) = IERC2981(royaltyLookUp).royaltyInfo(order.tokenId, order.price);

            recipients = new address payable[](1);
            recipients[0] = payable(receiver);

            amounts = new uint256[](1);
            amounts[0] = amount;
            console.log("supports IERC2981");

            success = true;
        }

        if (success && recipients.length > 0) {
            for (uint256 index = 0; index < recipients.length; index++) {
                if (order.paymentToken == address(Weth)) {
                    IERC20(order.paymentToken).safeTransfer(recipients[index], amounts[index]);
                } else {
                    IERC20(order.paymentToken).safeTransferFrom(sender, recipients[index], amounts[index]);
                }
                royaltiesTotal += amounts[index];
            }
        }

        {
            if (order.paymentToken == address(Weth)) {
                if (OwnerCutPerMillion > 0) {
                    // Calculate sale share
                    saleShareAmount = (order.price * OwnerCutPerMillion) / 1000000;
                    emit MarketFeePaid(saleShareAmount);
                    IERC20(order.paymentToken).transfer(FeeReceiver, saleShareAmount);
                }

                IERC20(order.paymentToken).safeTransfer(seller, order.price - royaltiesTotal - saleShareAmount);
            } else {
                if (OwnerCutPerMillion > 0) {
                    // Calculate sale share
                    saleShareAmount = (order.price * OwnerCutPerMillion) / 1000000;
                    emit MarketFeePaid(saleShareAmount);
                    IERC20(order.paymentToken).transferFrom(sender, FeeReceiver, saleShareAmount);
                }

                IERC20(order.paymentToken).safeTransferFrom(
                    sender,
                    seller,
                    order.price - royaltiesTotal - saleShareAmount
                );
            }
        }

        emit OrderFulfilled(order.id, seller, order.price, order.paymentToken, sender);

        nftRegistry.safeTransferFrom(seller, sender, order.tokenId);

        emit OrderSuccessful(order.id, order.tokenId, seller, order.nftAddress, sender);
        delete orderByOrderId[_orderId];

        return order;
    }

    /**
     * @dev Sets the publication fee that's charged to users to publish items
     * @param _publicationFee - Fee amount in wei this contract charges to publish an item
     */
    function setPublicationFee(uint256 _publicationFee) external onlyOwner {
        PublicationFeeInWei = _publicationFee;
        emit ChangedPublicationFee(PublicationFeeInWei);
    }

    /**
     * @dev Sets the royalty registry's address
     * @param royaltyRegistry - New address to be used for royalty registry
     */
    function setRoyaltyRegistry(address royaltyRegistry) external onlyOwner {
        RoyaltyRegistry = IRoyaltyRegistry(royaltyRegistry);
        emit ChangedRoyaltyRegistry(royaltyRegistry);
    }

    /**
     * @dev Sets the token to the specified 'accepted' state
     * @param token - Address of the token we want to edit
     * @param accepted - New accpeted state
     */
    function setAcceptedToken(address token, bool accepted) external onlyOwner {
        acceptedTokenMap[token] = accepted;
        emit ChangedAcceptedToken(token, accepted);
    }

    /**
     * @dev Sets the receipient address for fees
     * @param feeReceiver - Address of the address that receives fees
     */
    function setFeeReceiver(address feeReceiver) external onlyOwner {
        FeeReceiver = feeReceiver;
        emit ChangedFeeReceiver(FeeReceiver);
    }

    /**
     * @dev Sets the share cut for the owner of the contract that's
     *  charged to the seller on a successful sale
     * @param _ownerCutPerMillion - Share amount, from 0 to 999,999
     */
    function setOwnerCutPerMillion(uint256 _ownerCutPerMillion) public onlyOwner {
        require(_ownerCutPerMillion < 1000000, "The owner cut should be between 0 and 999,999");

        OwnerCutPerMillion = _ownerCutPerMillion;
        emit ChangedOwnerCutPerMillion(OwnerCutPerMillion);
    }

    function _requireERC721(address _nftAddress) internal view {
        require(_nftAddress.isContract(), "Marketplace: The NFT Address should be a contract");

        ERC721Interface nftRegistry = ERC721Interface(_nftAddress);
        require(
            nftRegistry.supportsInterface(ERC721_Interface),
            "Marketplace: The NFT contract has an invalid ERC721 implementation"
        );
    }

    /// @dev lets the owner pause this contract
    function pause() external onlyOwner {
        _pause();
    }

    /// @dev lets the owner unpause this contract
    function unpause() external onlyOwner {
        _unpause();
    }
}

