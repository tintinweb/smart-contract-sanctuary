pragma solidity ^0.8.7;
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

contract Int64ERC20 {
    using SafeERC20 for IERC20Metadata;

    function safeTransfer(
        IERC20Metadata token,
        address _address,
        int64 amount
    ) internal {
        token.safeTransfer(_address, scaleUpTokenAmount(token, amount));
    }

    function safeTransferFrom(
        IERC20Metadata token,
        address from,
        address to,
        int64 amount
    ) internal {
        token.safeTransferFrom(from, to, scaleUpTokenAmount(token, amount));
    }

    function scaleUpTokenAmount(IERC20Metadata token, int64 amount)
        internal
        view
        returns (uint256)
    {
        uint256 decimals = token.decimals();
        if (decimals > 6) {
            return uint256(uint64(amount)) * 10**(decimals - 6);
        } else if (decimals < 6) {
            return uint256(uint64(amount)) / 10**(6 - decimals);
        } else {
            return uint256(uint64(amount));
        }
    }
}

