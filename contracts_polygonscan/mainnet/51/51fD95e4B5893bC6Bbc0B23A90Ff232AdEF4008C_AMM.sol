pragma solidity ^0.8.7;

contract MathHelpers {
    function min(int64 a, int64 b) internal pure returns (int64) {
        return a < b ? a : b;
    }

    function max(int64 a, int64 b) internal pure returns (int64) {
        return a >= b ? a : b;
    }

    function calculateOutputAmount(
        int64 liquidityFee,
        int64 inputSupply,
        int64 outputSupply,
        int64 inputAmount
    ) internal pure returns (int64) {
        return
            outputSupply -
            (inputSupply * outputSupply) /
            (inputSupply + inputAmount - ((inputAmount * liquidityFee) / 1000));
    }

    function divideOrZero(int64 numerator, int64 denominator)
        internal
        pure
        returns (int64)
    {
        if (denominator == 0) {
            return 0;
        } else {
            return numerator / denominator;
        }
    }

    function proportionOf(
        int64 value,
        int64 x,
        int64 y
    ) internal pure returns (int64) {
        return ((value * x) / y);
    }
}

