pragma solidity ^0.8.7;

import "hardhat/console.sol";
import "./Issuable.sol";
import "./MathHelpers.sol";
import "./Int64ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract AMM is AccessControl, MathHelpers, Int64ERC20, Ownable {
    bytes32 public constant TRADER_ROLE = keccak256("TRADER_ROLE");
    int64 public constant YIELD_PRECISION = 1e6;
    Issuable public immutable rewardToken;
    int64 public immutable YIELD_START_TIME;
    int64 public immutable YIELD_END_TIME;
    struct Pool {
        int64 lastCheckpoint;
        IERC20Metadata token;
        int64 liquidityFee;
        int64 yieldPerSecond;
        int64 totalSupply;
        int64 totalSupplyOfToken;
        int64 totalSupplyOfBaseToken;
        int64 totalYieldPerToken;
    }
    mapping(uint256 => mapping(address => int64)) public balances;
    mapping(uint256 => mapping(address => int64)) public yieldDebt;
    using SafeERC20 for IERC20;

    IERC20 public baseToken;
    Pool[] public pools;
    event Buy(
        address indexed sender,
        uint256 outputPoolId,
        int64 inputAmount,
        int64 outputAmount
    );
    event Sell(
        address indexed sender,
        uint256 inputPoolId,
        int64 inputAmount,
        int64 outputAmount
    );
    event Convert(
        address indexed sender,
        uint256 inputPoolId,
        uint256 outputPoolId,
        int64 inputAmount,
        int64 outputAmount
    );
    event Harvest(address indexed sender, int64 pendingYield);
    event AddLiquidity(
        address indexed sender,
        uint256 poolId,
        int64 inputAmount
    );
    event RemoveLiquidity(address indexed sender, uint256 poolId, int64 amount);

    constructor(
        IERC20Metadata _baseToken,
        Issuable _rewardToken,
        int64 _YIELD_START_TIME,
        int64 _YIELD_END_TIME
    ) {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        baseToken = _baseToken;
        rewardToken = _rewardToken;
        YIELD_START_TIME = _YIELD_START_TIME;
        YIELD_END_TIME = _YIELD_END_TIME;
    }

    function currentBlockTimestamp() public view returns (uint256) {
        return block.timestamp;
    }

    function convert(
        uint256 inputPoolId,
        uint256 outputPoolId,
        int64 inputAmount
    ) public onlyRole(TRADER_ROLE) {
        require(inputAmount > 0, "inputAmount must be greater than 0");
        int64 baseTokenAmount = sell(inputPoolId, inputAmount);
        int64 outputAmount = buy(outputPoolId, baseTokenAmount);
        emit Convert(
            msg.sender,
            inputPoolId,
            outputPoolId,
            inputAmount,
            outputAmount
        );
    }

    function createPool(
        IERC20Metadata token,
        int64 liquidityFee,
        int64 yieldPerSecond,
        int64 initialBaseTokenAmount,
        int64 initialTokenAmount
    ) public onlyOwner {
        Pool memory pool =
            Pool(
                int64(int256(block.timestamp)),
                token,
                liquidityFee,
                yieldPerSecond,
                0,
                0,
                0,
                0
            );
        pools.push(pool);
        uint256 poolId = pools.length - 1;
        chargeBaseToken(poolId, initialBaseTokenAmount);
        chargeToken(poolId, initialTokenAmount);
        token.approve(owner(), type(uint256).max);
        mint(poolId, msg.sender, initialTokenAmount);
    }

    function addLiquidity(
        uint256 poolId,
        int64 amount,
        bool forwardToSafe
    ) public {
        require(amount > 0, "amount must be greater than 0");
        Pool storage pool = pools[poolId];
        mint(
            poolId,
            msg.sender,
            proportionOf(
                amount,
                int64(int256(pool.totalSupply)),
                pool.totalSupplyOfToken
            )
        );
        int64 baseTokenAmount =
            proportionOf(
                amount,
                pool.totalSupplyOfBaseToken,
                pool.totalSupplyOfToken
            );
        chargeBaseToken(poolId, baseTokenAmount);
        chargeToken(poolId, amount);

        if (forwardToSafe) {
            safeTransfer(pool.token, owner(), amount);
            baseToken.safeTransfer(owner(), uint256(uint64(baseTokenAmount)));
        }

        emit AddLiquidity(msg.sender, poolId, amount);
    }

    function removeLiquidity(uint256 poolId, int64 amount) public {
        require(amount > 0, "amount must be greater than 0");
        harvest(poolId);
        Pool storage pool = pools[poolId];
        int64 baseTokenToRemove =
            proportionOf(
                pool.totalSupplyOfBaseToken,
                amount,
                int64(int256(pool.totalSupply))
            );
        payBaseToken(poolId, baseTokenToRemove);
        int64 tokenToRemove =
            proportionOf(
                pool.totalSupplyOfToken,
                amount,
                int64(int256(pool.totalSupply))
            );
        payToken(poolId, tokenToRemove);
        burn(poolId, msg.sender, amount);
        emit RemoveLiquidity(msg.sender, poolId, amount);
    }

    function mint(
        uint256 poolId,
        address _address,
        int64 amount
    ) internal {
        updateCheckpoint(poolId);
        Pool storage pool = pools[poolId];
        pool.totalSupply += amount;
        balances[poolId][_address] += amount;
        pools[poolId] = pool;
        yieldDebt[poolId][_address] += getAccumulatedYield(poolId, amount);
    }

    function burn(
        uint256 poolId,
        address _address,
        int64 amount
    ) internal {
        updateCheckpoint(poolId);
        yieldDebt[poolId][_address] -= getAccumulatedYield(poolId, amount);
        Pool storage pool = pools[poolId];
        pool.totalSupply -= amount;
        balances[poolId][_address] -= amount;
        pools[poolId] = pool;
    }

    function buy(uint256 poolId, int64 inputAmount)
        public
        onlyRole(TRADER_ROLE)
        returns (int64 outputAmount)
    {
        require(inputAmount > 0, "inputAmount must be greater than 0");
        outputAmount = calculateBuyOutputAmount(poolId, inputAmount);
        payToken(poolId, outputAmount);
        chargeBaseToken(poolId, inputAmount);
        emit Buy(msg.sender, poolId, inputAmount, outputAmount);
    }

    function calculateBuyOutputAmount(uint256 poolId, int64 inputAmount)
        public
        view
        returns (int64)
    {
        Pool storage pool = pools[poolId];
        return
            calculateOutputAmount(
                pool.liquidityFee,
                pool.totalSupplyOfBaseToken,
                pool.totalSupplyOfToken,
                inputAmount
            );
    }

    function sell(uint256 poolId, int64 inputAmount)
        public
        onlyRole(TRADER_ROLE)
        returns (int64 outputAmount)
    {
        require(inputAmount > 0, "inputAmount must be greater than 0");
        outputAmount = calculateSellOutputAmount(poolId, inputAmount);
        payBaseToken(poolId, outputAmount);
        chargeToken(poolId, inputAmount);
        emit Sell(msg.sender, poolId, inputAmount, outputAmount);
    }

    function calculateSellOutputAmount(uint256 poolId, int64 inputAmount)
        public
        view
        returns (int64)
    {
        Pool storage pool = pools[poolId];
        return
            calculateOutputAmount(
                pool.liquidityFee,
                pool.totalSupplyOfToken,
                pool.totalSupplyOfBaseToken,
                inputAmount
            );
    }

    function payToken(uint256 poolId, int64 amount) internal {
        Pool storage pool = pools[poolId];
        safeTransfer(pool.token, msg.sender, amount);
        pool.totalSupplyOfToken -= amount;
        pools[poolId] = pool;
    }

    function chargeToken(uint256 poolId, int64 amount) public {
        Pool storage pool = pools[poolId];
        safeTransferFrom(pool.token, msg.sender, address(this), amount);

        pool.totalSupplyOfToken += amount;
        pools[poolId] = pool;
    }

    function transferBaseToken(
        uint256 inputPoolId,
        uint256 outputPoolId,
        int64 amount
    ) internal {
        Pool storage inputPool = pools[inputPoolId];
        Pool storage outputPool = pools[outputPoolId];
        inputPool.totalSupplyOfBaseToken -= amount;
        outputPool.totalSupplyOfBaseToken += amount;
        pools[inputPoolId] = inputPool;
        pools[outputPoolId] = outputPool;
    }

    function payBaseToken(uint256 poolId, int64 amount) internal {
        Pool storage pool = pools[poolId];
        pool.totalSupplyOfBaseToken -= amount;
        baseToken.safeTransfer(msg.sender, uint256(uint64(amount)));
        pools[poolId] = pool;
    }

    function chargeBaseToken(uint256 poolId, int64 amount) internal {
        Pool storage pool = pools[poolId];
        baseToken.safeTransferFrom(
            msg.sender,
            address(this),
            uint256(uint64(amount))
        );
        pool.totalSupplyOfBaseToken += amount;
        pools[poolId] = pool;
    }

    function updateCheckpoint(uint256 poolId) internal {
        Pool storage pool = pools[poolId];
        pools[poolId].totalYieldPerToken += divideOrZero(
            getTotalYieldSinceLastCheckpoint(pool) * YIELD_PRECISION,
            pool.totalSupply
        );
        pools[poolId].lastCheckpoint = int64(int256((block.timestamp)));
    }

    function harvest(uint256 poolId) public {
        updateCheckpoint(poolId);
        int64 accumulatedYield =
            getAccumulatedYield(poolId, balances[poolId][msg.sender]);
        int64 pendingYield = accumulatedYield - yieldDebt[poolId][msg.sender];
        rewardToken.mint(msg.sender, uint256(uint64(pendingYield)));
        yieldDebt[poolId][msg.sender] = accumulatedYield;
        emit Harvest(msg.sender, pendingYield);
    }

    function getPendingYield(uint256 poolId, address owner)
        public
        view
        returns (int64)
    {
        return
            getAccumulatedYield(poolId, balances[poolId][owner]) -
            yieldDebt[poolId][owner] +
            getYieldSinceLastCheckpoint(poolId, owner);
    }

    function getYieldSinceLastCheckpoint(uint256 poolId, address owner)
        public
        view
        returns (int64)
    {
        Pool storage pool = pools[poolId];
        if (pool.totalSupply == 0) {
            return 0;
        } else {
            return
                proportionOf(
                    getTotalYieldSinceLastCheckpoint(pool),
                    balances[poolId][owner],
                    pool.totalSupply
                );
        }
    }

    function getTotalYieldSinceLastCheckpoint(Pool memory pool)
        public
        view
        returns (int64)
    {
        return secondsSinceLastCheckpoint(pool) * pool.yieldPerSecond;
    }

    function getAccumulatedYield(uint256 poolId, int64 amount)
        public
        view
        returns (int64)
    {
        Pool storage pool = pools[poolId];
        return (amount * pool.totalYieldPerToken) / YIELD_PRECISION;
    }

    function secondsSinceLastCheckpoint(Pool memory pool)
        public
        view
        returns (int64)
    {
        int64 timestamp = int64(int256((block.timestamp)));
        if (
            timestamp <= YIELD_START_TIME ||
            pool.lastCheckpoint >= YIELD_END_TIME
        ) {
            return int64(0);
        } else {
            return
                MathHelpers.min(YIELD_END_TIME, timestamp) -
                MathHelpers.max(pool.lastCheckpoint, YIELD_START_TIME);
        }
    }

    function getPoolsLength() public view returns (uint256) {
        return pools.length;
    }

    function withdraw(IERC20 token, uint256 amount) public onlyOwner {
        token.transfer(msg.sender, amount);
    }

    function selfDestruct(address payable _address) public onlyOwner {
        for (uint256 i = 0; i < pools.length; i++) {
            IERC20 token = pools[i].token;
            token.transfer(_address, token.balanceOf(address(this)));
        }
        baseToken.transfer(_address, baseToken.balanceOf(address(this)));
        selfdestruct(_address);
    }
}

