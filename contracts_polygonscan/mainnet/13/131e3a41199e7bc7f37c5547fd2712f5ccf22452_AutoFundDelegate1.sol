interface IUniswapV2Router02 {

    function getAmountsOut(uint amountIn, address[] calldata path) external view returns (uint[] memory amounts);

    function swapExactTokensForTokens(
        uint amountIn,
        uint amountOutMin,
        address[] calldata path,
        address to,
        uint deadline
    ) external returns (uint[] memory amounts);
}

import "./libraries/UniversalERC20.sol";
// import "./CallStruct.sol";
struct CallStruct {
    address router;
    IERC20 token;
    address[] path;
    uint amount; // leave as 0 for swapping from multiple to one
}

interface IAutoFundController {
    function owner() external view returns (address);
    function isRouterWhitelisted( address ) external view returns (bool);
    function routersWhitelisted () external view returns (address[] memory);
    function isTokenWhitelisted( address ) external view returns (bool);
    function tokensWhitelisted () external view returns (address[] memory);
    function slippageFactor() external view returns (uint);
    function isFundPaused( uint ) external view returns (bool);
    function platformFeeFactor() external view returns (uint);
    function entranceFeeFactor() external view returns (uint);
}

interface IBatchSwap {
  function batchSwapsFromTokens(
      address user,
      IERC20 token,
      CallStruct[] calldata calls,
      address to,
      uint deadline
  ) external returns (uint outAmount);

 function batchSwapsToTokens(
      uint minOutAmount,
      IERC20 token,
      CallStruct[] calldata calls,
      address to,
      uint deadline
  ) external returns (uint outAmount);

}


contract AutoFundDelegate1 is ERC721Enumerable {
    using SafeERC20 for IERC20;
    using UniversalERC20 for IERC20;
    // using Address for address;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    
    address public owner;
    address public autoFundDelegate1;
    address public autoFundDelegate2;
    uint public fundId;
    address public manager;
    IERC20 public baseToken;
    IERC20[] public tokens;
    uint public tokensLength;
    uint public sharesTotal;
    string public fundTokenURI;

    uint public profitFeeFactor = 2000;
    uint public managementFeeFactor = 634195; // over 10000000 per second
    uint public lastManagementFeeTime = 0;

    bool public allowDeposits = true;
    mapping(uint => uint) public tokenShares;
    mapping(uint => uint) public tokenDepositAmount;

    event Deposit(
        address user, 
        uint itemId, 
        uint shares, 
        uint sharesTotal, 
        uint outAmount
    );

    event Withdraw(
        address user, 
        uint itemId, 
        uint shares, 
        uint sharesTotal, 
        uint outAmount
    );

    event ExecuteTrade(
        address _router, uint _amount, address[] _path
    );

    event SetAllowDeposits(
        bool setAllowDeposits
    );

    modifier onlyOwner() {
        console.log(owner, msg.sender);
        require(owner == msg.sender, "Ownable: caller is not the owner");
        _;
    }

    constructor() ERC721( "autofarm.network", "AutoFund") {
        owner = msg.sender;
    }

    // requires manager to have transfered tokens into fund beforehand
    // this initial tokens are not redeemable, to ensure fund is never empty with 0 sharesTotal
    
    function initialize(
        address _autoFundController,
        address _autoFundDelegate1,
        address _autoFundDelegate2,
        uint _fundId,
        string memory receiptTokenURI,
        address _manager,     
        IERC20 _baseToken,
        IERC20[] memory _tokens,
        uint initialShares
        // uint _profitFee
    ) public onlyOwner {

        autoFundDelegate1 = _autoFundDelegate1;
        autoFundDelegate2 = _autoFundDelegate2;
        fundId = _fundId;
        manager = _manager;
        baseToken = _baseToken;
        tokens = _tokens;
        tokensLength = _tokens.length;
        fundTokenURI = receiptTokenURI;

        // profitFee = _profitFee;

        require(sharesTotal == 0, "fund has already been initialized");
        sharesTotal = initialShares;
        lastManagementFeeTime = block.timestamp;

        owner = _autoFundController;
    }

    // tokens must have been transfered into contract by owner before calling
    function deposit(address user, uint shares, uint depositAmount) public onlyOwner {
        
        require(!IAutoFundController(owner).isFundPaused(fundId), "fundPaused");
        require(allowDeposits, "!allowDeposits");

        _handleEntranceFee(depositAmount);

        shares = shares * IAutoFundController(owner).entranceFeeFactor() / 10000;

        sharesTotal += shares;

        // mint NFT
        _tokenIds.increment();
        uint newItemId = _tokenIds.current();
        _mint(user, newItemId);

        // assoc data to NFT
        tokenShares[newItemId] = shares;
        tokenDepositAmount[newItemId] = depositAmount;

        emit Deposit(user, newItemId, shares, sharesTotal, depositAmount);

    }

    function withdraw(address user, uint itemId, uint minOutAmount, address batchSwapper, address[] calldata routers, address[][] calldata paths, uint deadline) public onlyOwner {

        require(ERC721(address(this)).ownerOf(itemId) == user , "user ! ownerOf itemId");

        handleManagementFee();

        uint shares = tokenShares[itemId];

        CallStruct[] memory calls = new CallStruct[](tokensLength);

        for (uint i = 0; i < tokensLength; i++) {
            calls[i] = CallStruct({
                token: tokens[i],
                router: routers[i],
                amount: tokens[i].balanceOf(address(this)) * shares / sharesTotal,
                path: paths[i]
            });
        }

        for (uint i = 0; i < calls.length; i++) {
            IERC20(calls[i].token).safeTransfer(batchSwapper, calls[i].amount);
        }

        uint outAmount = IBatchSwap(batchSwapper).batchSwapsToTokens(
            minOutAmount,
            baseToken,
            calls,
            address(this),
            deadline
        );

        _handleWithdraw(user, tokenDepositAmount[itemId], outAmount);      

        sharesTotal -= shares;

        // assoc data to NFT
        tokenShares[itemId] = 0;
        tokenDepositAmount[itemId] = 0;

        _burn(itemId);
           
        emit Withdraw(user, itemId, shares, sharesTotal, outAmount);

    }

    function handleManagementFee() public onlyOwner {

        uint platformFeeFactor = IAutoFundController(owner).platformFeeFactor();

        for (uint i = 0; i < tokensLength; i++) {
            uint tokenBal = tokens[i].balanceOf(address(this));

            uint fee = tokenBal * managementFeeFactor * (block.timestamp - lastManagementFeeTime) / 1000000000000;
            if ( fee > tokenBal / 10 ){ fee = tokenBal / 10; }  // to cater to idle funds, max management fee set to 10% of fund.
            uint feeToPlatform = fee * platformFeeFactor / 10000;
            uint feeToManager = fee - feeToPlatform;
     
            tokens[i].safeTransfer(manager, feeToManager);
            tokens[i].safeTransfer(IAutoFundController(owner).owner(), feeToPlatform);  
        }
            
        lastManagementFeeTime = block.timestamp;
    }

    function _handleEntranceFee( uint depositAmount) internal {
        uint fee = depositAmount * (10000 - IAutoFundController(owner).entranceFeeFactor()) / 10000;
        uint feeToPlatform = fee * IAutoFundController(owner).platformFeeFactor() / 10000;
        uint feeToManager = fee - feeToPlatform;
        IERC20(baseToken).safeTransfer(manager, feeToManager);
        IERC20(baseToken).safeTransfer(IAutoFundController(owner).owner(), feeToPlatform);
    }

    function _handleWithdraw( address user, uint initialAmount, uint outAmount) internal {

        uint profit = 0;
        if (outAmount > initialAmount){
            profit = outAmount - initialAmount;
        }

        uint fee = profit * profitFeeFactor / 10000;
        uint feeToPlatform = fee * IAutoFundController(owner).platformFeeFactor() / 10000;
        uint feeToManager = fee - feeToPlatform;

        IERC20(baseToken).safeTransfer(user, outAmount - feeToPlatform - feeToManager);
        IERC20(baseToken).safeTransfer(manager, feeToManager);
        IERC20(baseToken).safeTransfer(IAutoFundController(owner).owner(), feeToPlatform);

    }

    // function executeTrade(address _router, uint _amount, address[] memory _path) public {

    //     require(!IAutoFundController(owner).isFundPaused(fundId), "fundPaused");
    //     require(msg.sender == manager, "! manager");
    //     bool outtokenChecked = false;
    //     for (uint i = 0; i < tokens.length; i++) {
    //         if (_path[_path.length - 1] == address(tokens[i])){ outtokenChecked = true; break; }
    //     }

    //     require(outtokenChecked, "bad outtoken");
    //     require(IAutoFundController(owner).isRouterWhitelisted(_router), "bad router");
    //     for (uint i = 0; i < _path.length; i++) {
    //         require(IAutoFundController(owner).isTokenWhitelisted(_path[i]), "bad token");
    //     }

    //     IERC20(_path[0]).safeApprove(_router, 0);
    //     IERC20(_path[0]).safeIncreaseAllowance(_router, _amount);
    //     _safeSwap(
    //         _router,
    //         _amount,
    //         IAutoFundController(owner).slippageFactor(),
    //         _path,
    //         address(this),
    //         block.timestamp + 600
    //     );

    //     emit ExecuteTrade(_router, _amount, _path);
    
    // }

    // function _safeSwap(
    //     address _uniRouterAddress,
    //     uint _amountIn,
    //     uint _slippageFactor,
    //     address[] memory _path,
    //     address _to,
    //     uint _deadline
    // ) internal virtual {

    //     uint amountOut = 0;

    //     if (_slippageFactor > 0){
    //         uint[] memory amounts = IUniswapV2Router02(_uniRouterAddress).getAmountsOut(_amountIn, _path);
    //         amountOut = amounts[amounts.length - 1];
    //     }
        
    //     IUniswapV2Router02(_uniRouterAddress)
    //         .swapExactTokensForTokens(
    //         _amountIn,
    //         amountOut * _slippageFactor / 10000,
    //         _path,
    //         _to,
    //         _deadline
    //     );
    // }

    // function tokenURI(uint tokenId)
    //     public
    //     view
    //     override
    //     returns (string memory)
    // {
    //     return fundTokenURI;
    // }

    // function setAllowDeposits(bool _allowDeposits) public {
    //     require(msg.sender == manager, "! manager");
    //     allowDeposits = _allowDeposits;
    //     emit SetAllowDeposits(allowDeposits);
    // }
    


}

