// https://docs.aave.com/developers/guides/flash-loans

contract Protocol is IFlashLoanReceiver, OwnableUpgradeable {
	using SafeERC20 for IERC20;
	using SafeMath for uint256;

	enum Dex {
		QUICKSWAP,
		SUSHI,
		JETSWAP,
		POLYDEX,
		CURVE,
		MSTABLE
	}

	ILendingPoolAddressesProvider internal _addressesProvider;
	ILendingPool internal _lendingPool;
	address internal sushiSwapAddress;
	address internal quickSwapAddress;
	address internal jetSwapAddress;
	address internal polyDexAddress;
	address internal WMATIC;
	Dex[] internal dexes;
	address[] internal arbTokens;

	/**
    @notice initialize lending pool addresses and access control
   */
	function initialize(
		ILendingPoolAddressesProvider provider,
		address _sushiAddress,
		address _quickAddress,
		address _jetSwapAddress,
		address _polyDexAddress
	) external initializer {
		_addressesProvider = provider;
		_lendingPool = ILendingPool(
			ILendingPoolAddressesProvider(provider).getLendingPool()
		);
		sushiSwapAddress = _sushiAddress;
		quickSwapAddress = _quickAddress;
		jetSwapAddress = _jetSwapAddress;
		polyDexAddress = _polyDexAddress;
		WMATIC = address(0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270);
	}

	function toInt128(uint256 num) internal pure returns (int128) {
		return int128(int256(num));
	}

	function _swapUniswap(
		address from,
		address to,
		uint256 amt,
		address dexAddress
	) internal returns (uint256 received) {
		IUniswapV2Router _dexContract = IUniswapV2Router(dexAddress);

		//Path variable
		address[] memory _path = new address[](2);
		_path[0] = from;
		_path[1] = to;

		//Calculate _amountOutMin. It will be 95%
		uint256[] memory _amountOutToken = _dexContract.getAmountsOut(
			amt,
			_path
		);
		IERC20(from).safeApprove(dexAddress, amt);

		// Make Swap
		uint256[] memory _returnAmounts = _dexContract.swapExactTokensForTokens(
			amt,
			(_amountOutToken[1] * 95) / 100,
			_path,
			address(this),
			block.timestamp + 1 minutes
		);
		received = _returnAmounts[1];
	}

	function _swapPolyDex(
		address from,
		address to,
		uint256 amt
	) internal returns (uint256 received) {
		//Path variable
		address[] memory _path = new address[](2);
		_path[0] = from;
		_path[1] = to;
		console.log("antes");

		uint256[] memory _returnAmounts = IPolyDexRouterV2(polyDexAddress)
			.swapExactTokensForTokens(
				from,
				to,
				amt,
				1,
				_path,
				address(this),
				block.timestamp + 3 minutes
			);
		console.log("paso");
		received = _returnAmounts[1];
	}

	function _swapCurve(
		address from,
		address to,
		uint256 amt
	) internal returns (uint256 received) {
		uint256 i;
		uint256 j;

		// DAI / USDC / USDT swaps
		ICurvePool pool = ICurvePool(
			0x445FE580eF8d70FF569aB36e80c647af338db351
		);

		for (uint256 x = 1; x <= 3; x++) {
			if (pool.underlying_coins(x - 1) == from) i = x;
			if (pool.underlying_coins(x - 1) == to) j = x;
		}

		require(i != 0 && j != 0);

		IERC20(from).safeApprove(address(pool), amt);

		received = pool.exchange_underlying(
			toInt128(i - 1),
			toInt128(j - 1),
			amt,
			0
		);
	}

	function _swapMstable(
		address from,
		address to,
		uint256 amt
	) internal returns (uint256 received) {
		IMasset musd = IMasset(0xE840B73E5287865EEc17d250bFb1536704B43B21);

		// Approve mUSD
		IERC20(from).safeApprove(address(musd), amt);

		received = musd.swap(from, to, amt, 1, address(this));
	}

	function _swap(
		Dex dex,
		address from,
		address to,
		uint256 amt
	) internal returns (uint256 received) {
		if (dex == Dex.QUICKSWAP)
			received = _swapUniswap(from, to, amt, quickSwapAddress);
		if (dex == Dex.SUSHI)
			received = _swapUniswap(from, to, amt, sushiSwapAddress);
		if (dex == Dex.JETSWAP)
			received = _swapUniswap(from, to, amt, jetSwapAddress);
		if (dex == Dex.POLYDEX) received = _swapPolyDex(from, to, amt);
		if (dex == Dex.CURVE) received = _swapCurve(from, to, amt);
		if (dex == Dex.MSTABLE) received = _swapMstable(from, to, amt);
	}

	/**
    @notice called when funds are received
    @dev will pay back loan amount plus premium
   */
	function executeOperation(
		address[] calldata assets,
		uint256[] calldata amounts,
		uint256[] calldata premiums,
		address initiator,
		bytes calldata params
	) external override returns (bool) {
		// shhhhh
		initiator;
		params;
		//
		// This contract now has the funds requested.
		// Your logic goes here.
		//
		// At the end of your logic above, this contract owes
		// the flashloaned amounts + premiums.
		// Therefore ensure your contract has enough to repay
		// these amounts.
		// Arbitrage

		// Swap loan token for arb token
		uint256 received = _swap(
			dexes[0],
			arbTokens[0],
			arbTokens[1],
			amounts[0]
		);

		console.log("TOKEN", received);
		// Swap arb token for loan token
		uint256 received2 = _swap(
			dexes[1],
			arbTokens[1],
			arbTokens[0],
			received
		);
		console.log("WMATIC", received2);

		// Approve the LendingPool contract allowance to *pull* the owed amount
		for (uint256 i = 0; i < assets.length; i++) {
			uint256 amountOwing = amounts[i].add(premiums[i]);
			IERC20(assets[i]).approve(address(_lendingPool), amountOwing);
			console.log("Amount that should sent", amountOwing);
		}

		return true;
	}

	/**
	  @notice start flashloan
	  @param _amount flash loan amount
	  @param _loanToken flash loan token
	  @param _arbToken arbitrage token amount
	  @param _dexes array of dexes to use in arbitrage
	 */
	function initFlashloan(
		uint256 _amount,
		address _loanToken,
		address _arbToken,
		Dex[] memory _dexes
	) public {
		// Update state variables
		dexes = _dexes;
		arbTokens = [_loanToken, _arbToken];

		address receiverAddress = address(this);

		address[] memory assets = new address[](1);
		assets[0] = _loanToken;

		uint256[] memory amounts = new uint256[](1);
		amounts[0] = _amount;

		// 0 = no debt, 1 = stable, 2 = variable
		uint256[] memory modes = new uint256[](1);
		modes[0] = 0;

		address onBehalfOf = address(this);
		uint16 referralCode = 159;

		// Start flash loan
		_lendingPool.flashLoan(
			receiverAddress,
			assets,
			amounts,
			modes,
			onBehalfOf,
			"",
			referralCode
		);

		IERC20(_loanToken).transfer(
			owner(),
			IERC20(_loanToken).balanceOf(address(this))
		);
	}

	receive() external payable {}
}

