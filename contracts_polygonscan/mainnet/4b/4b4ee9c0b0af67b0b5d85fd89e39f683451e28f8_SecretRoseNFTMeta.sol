    function makeNFT(uint256 tokenId, uint256 hp) public pure returns (string memory) {

        string[41] memory canvas;

        string[3] memory borderChars = ["$", "#", "@"];
        string[8] memory FGColors = [
            "#FFFFFF", "#2568FB","#1A2238", "#394F8A", 
            "#490B3D", "#00458B", "#1D2228", "#051622"
            ];
        
        string[8] memory BGColors = [
            "#373A36","#FF6A3D","#5C6E58","#EB3A70",
             "#9E15BF","#FACD3D","#DEB992","#FA255E"
             ];

        canvas = _createCanvas(canvas);
        string memory borderChar = borderChars[_random(string(abi.encodePacked(tokenId, hp))) % 3];
        canvas = _createBox(canvas, borderChar, 0, 0, 47, 1);
        canvas = _createBox(canvas, borderChar, 0, 40, 47, 1);
        canvas = _createBox(canvas, borderChar, 0, 0, 1, 41);
        canvas = _createBox(canvas, borderChar, 47, 0, 1, 41);

        uint256 iter = hp % 120;

        if (iter <= 20) {
           canvas = _createInitialPattern(canvas, tokenId, hp);
        } else if (iter <= 90) {
            canvas = _createStage2Pattern(canvas, tokenId, hp);
        } else {
            canvas = _createStage3Pattern(canvas, tokenId, hp);
        }

        uint256 length = 2 + bytes(hp.toString()).length;
        canvas = _addText(canvas, string(abi.encodePacked("[", hp.toString(), "]")), length, 47 - length, 40); 

        string memory output;
        for (uint256 i = 0; i < 41; i++) {
            output = string(abi.encodePacked(
                output, '<text x="10" y="', (10 + i*10).toString(), '" class="base">', canvas[i], '</text>'
            ));
        }

        string[9] memory svgParts;
        svgParts[0] = '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin meet" viewBox="0 0 285 410"><style>.base { fill: ';
        svgParts[1] = FGColors[uint256(keccak256(abi.encodePacked("FGCOLOR", tokenId))) % 8];
        svgParts[2] = '; font-family: monospace; font-size: 10px; }</style><path d="M0 0h285v410H0z"><animate attributeName="fill" from="';
        svgParts[3] = BGColors[uint256(keccak256(abi.encodePacked("BGCOLOR", tokenId, "0"))) % 8];
        svgParts[4] = '" to="';
        svgParts[5] = BGColors[uint256(keccak256(abi.encodePacked("BGCOLOR", tokenId, "1"))) % 8];
        svgParts[6] = '" dur="3s" repeatDur="indefinite"/></path><rect width="100%" height="100%" fill="None" />';
        svgParts[7] = output;
        svgParts[8] = '</svg>';

        string memory jsonString = Base64.encode(abi.encodePacked(
            svgParts[0], svgParts[1], svgParts[2], 
            svgParts[3], svgParts[4], svgParts[5], 
            svgParts[6], svgParts[7], svgParts[8]));

        return jsonString;
    }

    function getTokenURI(uint256 tokenId, uint256 hp) override external pure returns (string memory) {
        
        string memory svg = makeNFT(tokenId, hp);

        string memory description = "A secret rose does not bloom hurriedly; for beauty, like any masterpiece, takes time to blossom. It slows down when you touch it and sometimes it must fade before it can bloom again.";
        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{"name": "#', tokenId.toHexString(), 
                        '", "description": "', description, 
                        '", "image": "data:image/svg+xml;base64,', svg, 
                        '"}'
                        )
                    )
                )
            );

        return string(abi.encodePacked("data:application/json;base64,", json));
    }
