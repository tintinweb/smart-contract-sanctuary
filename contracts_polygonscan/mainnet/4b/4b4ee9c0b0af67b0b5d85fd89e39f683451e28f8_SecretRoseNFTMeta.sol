        uint256 x, 
        uint256 y, 
        uint256 width, 
        uint256 height) 
        public pure returns (string[41] memory) {

        bytes1 byteChar = bytes(char)[0];
        for (uint256 row = 0; row < height; row++) {
            for (uint256 col = 0; col < width; col++) {
                bytes(canvas[row + y])[col + x] = byteChar;
            }
        }
        return canvas;
    }

    function _random(string memory seeds) private pure returns(uint256 result) {
        uint256 seed = uint256(keccak256(abi.encodePacked(seeds)));

        seed ^= seed << 13;
        seed ^= seed >> 17;
        seed ^= seed << 5;
        return seed;
    }

    function _fillLine(
        string[41] memory canvas, 
        uint256 line_no, 
        uint256 start, 
        uint256 end, 
        uint256 l_emphasis, 
        uint256 r_emphasis,
        uint256 tokenId,
        uint256 hp) 
        public pure returns (string[41] memory) { 
       
        string[9] memory emphasis_chars = ["O","X","M","Y","B","Q", "8", "D", "W"];
        string[17] memory rand_chars = ["p",")","]","}","i",",",":","o","c","!", "#", "*", "(", "a", "{", "^", "-"];

        for (uint256 i = start; i < end; i++) {
            if ((start + l_emphasis > 0) && (i <= start + l_emphasis - 1)) {
                bytes(canvas[line_no])[i] = bytes(emphasis_chars[_random(
                    string(abi.encodePacked("lemphasis", start, end, i, tokenId, hp))
                    ) % 9])[0];
            }
            else if ((end > r_emphasis + 1) && (i >= end - r_emphasis + 1)) {
                bytes(canvas[line_no])[i] = bytes(emphasis_chars[_random(
                    string(abi.encodePacked("remphasis", start, end, i, tokenId, hp))
                    ) % 9])[0];
            }
            else {
                bytes(canvas[line_no])[i] = bytes(rand_chars[_random(
                    string(abi.encodePacked("emphasis", start, end, i, tokenId, hp))
                    ) % 17])[0];
            }
        }      
                
        bytes(canvas[line_no])[start] = bytes(rand_chars[_random(
            string(abi.encodePacked("emphasis", start, end, line_no, tokenId, hp))
            ) % 17])[0];
        bytes(canvas[line_no])[end] = bytes(rand_chars[_random(
            string(abi.encodePacked("emphasis", start, end, line_no, tokenId, hp))
            )% 17])[0];

        return canvas;
    }

    function _addText(string[41] memory canvas, string memory message, uint256 messageLength, uint256 x, uint256 y) public pure returns (string[41] memory) {
        for (uint256 i = 0; i < messageLength; i++) {
            bytes(canvas[y])[x + i] = bytes(message)[i];
        }

        return canvas;
    }

