contract TestScorpioToken is ERC20("TESTSCORPIO Token", "TESTSCROP") {
    IUniswapV2Router02 public immutable uniswapV2Router;
    address public immutable uniswapV2Pair;
    address public immutable uniswapV2PairLeo;
    address constant leoTokenContract =
        0xbE8DAb8Ce8521ecFDe43a8Ff8d5C6644F4dCECb7;
    address constant burnAddress = 0x000000000000000000000000000000000000dEaD;

    IERC20 leoToken;

    address public leoTaxReceiveAddress;
    bool public leoSwapBurnActive;

    constructor(IERC20 _leoToken) public {
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff
        );
        leoToken = _leoToken;
        // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        // Create a uniswap pair for this new token and LEO
        uniswapV2PairLeo = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), leoTokenContract);

        // set the rest of the contract variables
        uniswapV2Router = _uniswapV2Router;

        leoTaxReceiveAddress = msg.sender;

        leoSwapBurnActive = false;

        bypassesMaxTxLimitAmount[owner()] = true;
        bypassesMaxTxLimitAmount[address(this)] = true;

        bypassesTransferTax[owner()] = true;
        bypassesTransferTax[address(this)] = true;
    }

    /// @notice Creates `_amount` token to `_to`. Must only be called by the owner (MasterChef).
    function mint(address _to, uint256 _amount) public onlyOwner {
        _mint(_to, _amount);
    }

    // Sets the new transaction limit. Limit starts at 0 to prevent bots from sniping on liquidity being added.
    function setMaxTxLimitBasisPoint(uint256 maxTxBp) external onlyOwner {
        _maxTxAmount = _totalSupply.mul(maxTxBp).div(10000);
    }

    // Allows an address to bypass the transaction limit. Add vault and masterchef contracts
    function addBypassMaxTxLimit(address account) external onlyAdmin {
        bypassesMaxTxLimitAmount[account] = true;
    }

    // Removes an address in the bypass the transaction limit list.
    function removeBypassMaxTxLimit(address account) external onlyAdmin {
        bypassesMaxTxLimitAmount[account] = false;
    }

    // Allows an address to bypass the transaction tax. Add vault and masterchef contracts
    function addBypassesTransferTax(address account) external onlyAdmin {
        bypassesTransferTax[account] = true;
    }

    // Removes an address in the bypass the transaction tax list.
    function removeBypassesTransferTax(address account) external onlyAdmin {
        bypassesTransferTax[account] = false;
    }

    function setLeoSwapBurn(bool _leoSwapBurnActive) external onlyAdmin {
        leoSwapBurnActive = _leoSwapBurnActive;
    }

    function setTaxRate(uint256 _trasferTaxRate) external onlyAdmin {
        require(_trasferTaxRate <= maxTrasferTaxRate, "Tax Rate too High");
        trasferTaxRate = _trasferTaxRate;
    }

    function setLeoTaxReceiveAddress(address _leoTaxReceiveAddress)
        external
        onlyAdmin
    {
        leoTaxReceiveAddress = _leoTaxReceiveAddress;
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override antiWhale(sender, recipient, amount) {
        if (bypassesTransferTax[sender] || bypassesTransferTax[recipient]) {
            super._transfer(sender, recipient, amount);
        } else {
            uint256 taxAmount;
            taxAmount = amount.mul(trasferTaxRate).div(10000);
            uint256 sendAmount;
            sendAmount = amount.sub(taxAmount);
            super._transfer(sender, recipient, sendAmount);
            super._transfer(sender, leoTaxReceiveAddress, taxAmount);
            // if (taxAmount > 0) {
            //     swapTokensForLeo(taxAmount);
            //     if (!leoSwapBurnActive) {
            //         withdrawLeoTokens();
            //     } else {
            //         burnLeoTokens();
            //     }
            // }
        }
    }

    function swapTokensForLeo(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = leoTokenContract;

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    // admin function to withdraw the tokens that have been deposited in the
    function withdrawLeoTokens() internal {
        uint256 leoBalance = leoToken.balanceOf(address(this));
        if (leoBalance > 0) {
            leoToken.transfer(leoTaxReceiveAddress, leoBalance);
        }
    }

    function burnLeoTokens() internal {
        uint256 leoBalance = leoToken.balanceOf(address(this));
        if (leoBalance > 0) {
            leoToken.transfer(burnAddress, leoBalance);
        }
    }
}