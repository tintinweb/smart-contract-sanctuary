pragma solidity ^0.8.11;







contract Mutanation is Ownable, ERC721 {
    using Strings for uint256;

    struct Signature {
        uint8 v;
        bytes32 r;
        bytes32 s;
    }

    uint256 public constant MAX_SUPPLY = 9999;
    uint256 public constant PRICE = 5 ether;
    uint256 public constant PRESALE_PRICE = 3 ether;
    uint256 public constant MINT_PER_ADDRESS = 5;

    uint256 public totalSupply = 1;
    string private _baseURI;
    address private _signerAddress = 0x0d89cf626F1EE1eB8372e7Acb939f17e8784ABE3;

    bool private _saleLive = true;
    bool private _presaleLive = true;

    constructor() ERC721("Mutanation Space Marauders", "MUTAN") {}

    function mint(
        bytes32 hash,
        Signature memory sig,
        uint256 tokenQuantity
    ) external payable {
        require(_saleLive, "LABORATORY_CLOSED");
        require(matchAddressSigner(hash, sig), "DIRECT_MINT_DISALLOWED");
        require(
            balanceOf[msg.sender] + tokenQuantity <= MINT_PER_ADDRESS,
            "EXCEED_MUTANT_ALLOC"
        );

        uint256 currentPrice = _presaleLive ? PRESALE_PRICE : PRICE;

        require(
            currentPrice * tokenQuantity == msg.value,
            "WRONG_PRICE_AMOUNT"
        );
        require(totalSupply + tokenQuantity <= MAX_SUPPLY, "EXCEED_MAX_SUPPLY");

        for (uint256 i = 0; i < tokenQuantity; i++) {
            _mint(msg.sender, totalSupply++);
        }
    }

    function gift(address addr, uint256 tokenQuantity) external {
        require(msg.sender == _owner, "LAB_OWNER_ONLY");
        require(tokenQuantity < 11, "MAX_10_PER_CALL");

        for (uint256 i = 0; i < tokenQuantity; i++) {
            _mint(addr, totalSupply++);
        }
    }

    function withdraw() external {
        require(msg.sender == _owner, "LAB_OWNER_ONLY");
        SafeTransferLib.safeTransferETH(msg.sender, address(this).balance);
    }

    function burn(uint256 id) external {
        require(msg.sender == ownerOf[id], "OWNER_ONLY");
        _burn(id);
    }

    function toggleSaleStatus() external {
        require(msg.sender == _owner, "LAB_OWNER_ONLY");
        _saleLive = !_saleLive;
    }

    function togglePreSaleStatus() external {
        require(msg.sender == _owner, "LAB_OWNER_ONLY");
        _presaleLive = !_presaleLive;
    }

    function setSignerAddress(address addr) external {
        require(msg.sender == _owner, "LAB_OWNER_ONLY");
        _signerAddress = addr;
    }

    function setBaseURI(string calldata URI) external {
        require(msg.sender == _owner, "LAB_OWNER_ONLY");
        _baseURI = URI;
    }

    function tokenURI(uint256 id) public view override returns (string memory) {
        require(ownerOf[id] != address(0), "TOKEN_URI_NOT_EXIST");

        return string(abi.encodePacked(_baseURI, id));
    }

    function matchAddressSigner(bytes32 hash, Signature memory sig)
        private
        view
        returns (bool)
    {
        bytes32 messageDigest = keccak256(
            abi.encodePacked("\x19Ethereum Signed Message:\n32", hash)
        );

        return _signerAddress == ecrecover(messageDigest, sig.v, sig.r, sig.s);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        pure
        override(Ownable, ERC721)
        returns (bool)
    {
        return
            interfaceId == 0x7f5828d0 || // ERC165 Interface ID for ERC173
            interfaceId == 0x80ac58cd || // ERC165 Interface ID for ERC721
            interfaceId == 0x5b5e139f || // ERC165 Interface ID for ERC165
            interfaceId == 0x01ffc9a7; // ERC165 Interface ID for ERC721Metadata
    }
}