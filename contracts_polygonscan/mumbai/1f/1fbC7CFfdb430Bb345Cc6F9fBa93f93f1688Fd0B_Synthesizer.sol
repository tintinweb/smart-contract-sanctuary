import "./interfaces/ISynthesizer.sol";
import "./interfaces/IMutations.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "hardhat/console.sol";

contract Synthesizer is ISynthesizer, VRFConsumerBase, Ownable {
    address public _buddyLogicAddr;
    IMutations public _mutations;

    uint32 public _ALGAE_COUNT = 400;
    uint32 public _PROTOZOA_COUNT = _ALGAE_COUNT + 325;
    uint32 public _PROTIST_COUNT = _PROTOZOA_COUNT + 325;
    uint32 public _ARCHAEA_COUNT = _PROTIST_COUNT + 205;

    uint32 public _AMOEBA_COUNT = _ARCHAEA_COUNT + 300;
    uint32 public _BACTERIA_COUNT = _AMOEBA_COUNT + 250;
    uint16 public _VIRUS_COUNT = uint16(_BACTERIA_COUNT) + 205;

    uint16 public _FUNGI_COUNT = _VIRUS_COUNT + 250;
    uint16 public _YEAST_COUNT = _FUNGI_COUNT + 200;

    uint16 public _WATERBEAR_COUNT = _YEAST_COUNT + 30;

    bytes32 internal keyHash;
    uint256 internal fee;

    uint256 private randomResult;
    uint256 private _nonce;

    /**
     * Constructor inherits VRFConsumerBase
     *
     * Network: Polygon Mumbai Testnet
     */
    constructor(address _mutationsAddr)
        VRFConsumerBase(
            0x8C7382F9D8f56b33781fE506E897a4F1e2d17255, // VRF Coordinator
            0x326C977E6efc84E512bB9C30f76E30c160eD06FB // LINK Token
        )
    {
        _mutations = IMutations(_mutationsAddr);
        keyHash = 0x6e75b569a01ef56d18cab6a8e71e6600d6ce853834d4a5748b720d06f878b3a4;
        fee = 0.0001 * 10**18;
    }

    function setBuddyLogicAddr(address buddyLogicAddr) public {
        require(_buddyLogicAddr == address(0), "Buddy addr already set");
        _buddyLogicAddr = buddyLogicAddr;
    }

    modifier onlyBuddyLogic() {
        require(msg.sender == _buddyLogicAddr, "Unauthorized");
        _;
    }

    function setMutationsContract(address _mutationsContractAddr)
        external
        override
        onlyBuddyLogic
    {
        _mutations = IMutations(_mutationsContractAddr);
    }

    function grabRandomBuddy() external onlyBuddyLogic returns (uint256) {
        require(randomResult != 0, "Randomness has not yet been generated.");
        uint256 dna = buildDNA(getSpecies());
        getRandomNumber();
        return dna;
    }

    function getSpecies() private returns (uint8 species) {
        require(_WATERBEAR_COUNT > 0, "All Gen 0s have been claimed!");
        uint256 r = rollNewGuess(_WATERBEAR_COUNT);

        if (r <= _ALGAE_COUNT) {
            _ALGAE_COUNT -= 1;
            _PROTOZOA_COUNT -= 1;
            _PROTIST_COUNT -= 1;
            _ARCHAEA_COUNT -= 1;
            _AMOEBA_COUNT -= 1;
            _BACTERIA_COUNT -= 1;
            _VIRUS_COUNT -= 1;
            _FUNGI_COUNT -= 1;
            _YEAST_COUNT -= 1;
            _WATERBEAR_COUNT -= 1;
            return 10;
        } else if (r <= _PROTOZOA_COUNT) {
            _PROTOZOA_COUNT -= 1;
            _PROTIST_COUNT -= 1;
            _ARCHAEA_COUNT -= 1;
            _AMOEBA_COUNT -= 1;
            _BACTERIA_COUNT -= 1;
            _VIRUS_COUNT -= 1;
            _FUNGI_COUNT -= 1;
            _YEAST_COUNT -= 1;
            _WATERBEAR_COUNT -= 1;
            return 9;
        } else if (r <= _PROTIST_COUNT) {
            _PROTIST_COUNT -= 1;
            _ARCHAEA_COUNT -= 1;
            _AMOEBA_COUNT -= 1;
            _BACTERIA_COUNT -= 1;
            _VIRUS_COUNT -= 1;
            _FUNGI_COUNT -= 1;
            _YEAST_COUNT -= 1;
            _WATERBEAR_COUNT -= 1;
            return 8;
        } else if (r <= _ARCHAEA_COUNT) {
            _ARCHAEA_COUNT -= 1;
            _AMOEBA_COUNT -= 1;
            _BACTERIA_COUNT -= 1;
            _VIRUS_COUNT -= 1;
            _FUNGI_COUNT -= 1;
            _YEAST_COUNT -= 1;
            _WATERBEAR_COUNT -= 1;
            return 7;
        } else if (r <= _AMOEBA_COUNT) {
            _AMOEBA_COUNT -= 1;
            _BACTERIA_COUNT -= 1;
            _VIRUS_COUNT -= 1;
            _FUNGI_COUNT -= 1;
            _YEAST_COUNT -= 1;
            _WATERBEAR_COUNT -= 1;
            return 6;
        } else if (r <= _BACTERIA_COUNT) {
            _BACTERIA_COUNT -= 1;
            _VIRUS_COUNT -= 1;
            _FUNGI_COUNT -= 1;
            _YEAST_COUNT -= 1;
            _WATERBEAR_COUNT -= 1;
            return 5;
        } else if (r <= _VIRUS_COUNT) {
            _VIRUS_COUNT -= 1;
            _FUNGI_COUNT -= 1;
            _YEAST_COUNT -= 1;
            _WATERBEAR_COUNT -= 1;
            return 4;
        } else if (r <= _FUNGI_COUNT) {
            _FUNGI_COUNT -= 1;
            _YEAST_COUNT -= 1;
            _WATERBEAR_COUNT -= 1;
            return 3;
        } else if (r <= _YEAST_COUNT) {
            _YEAST_COUNT -= 1;
            _WATERBEAR_COUNT -= 1;
            return 2;
        } else if (r <= _WATERBEAR_COUNT) {
            _WATERBEAR_COUNT -= 1;
            return 1;
        }
    }

    function buildDNA(uint8 species) private returns (uint256) {
        uint8[] memory traits = new uint8[](32);
        traits[0] = species;

        // Top (dominant, recessive, recessive, recessive, recessive, recessive)
        // We don't want 5 to be possible in rtop because that is the rare+
        bool common = false;
        uint256 rTop = rollNewGuess(5);
        for (uint256 i = 1; i < 6; i++) {
            uint256 rand = 0;

            if (i != rTop) {
                if (species > 3 && i > 1 && !common) {
                    rand = 255;
                    common = true;
                } else {
                    rand = raribleRandom(calcTraitType(0), i, species);
                }
            }

            traits[i] = uint8(rand);
            // console.log("Gen 0: TOP", traits[i], "/ 8");
        }
        // Mouth (dominant, recessive, recessive, recessive, recessive, recessive)
        common = false;
        for (uint256 i = 6; i < 11; i++) {
            uint256 rand = 0;

            if (species > 3 && i > 6 && !common) {
                rand = 255;
                common = true;
            } else {
                rand = raribleRandom(calcTraitType(1), i, species);
            }

            traits[i] = uint8(rand);
            // console.log("Gen 0: MOUTH", traits[i], "/ 24");
        }
        // Eyes (dominant, recessive, recessive, recessive, recessive, recessive)
        common = false;
        for (uint256 i = 11; i < 16; i++) {
            uint256 rand = 0;

            if (species > 3 && i > 11 && !common) {
                rand = 255;
                common = true;
            } else {
                rand = raribleRandom(calcTraitType(2), i, species);
            }

            traits[i] = uint8(rand);
            // console.log("Gen 0: EYES", traits[i], "/ 23");
        }
        // Body Patterns (dominant, recessive, recessive, recessive, recessive,
        // recessive)
        common = false;
        uint256 rBP = rollNewGuess(5);
        for (uint256 i = 16; i < 21; i++) {
            uint256 rand = 0;

            if (i - 15 != rBP) {
                if (species > 3 && i > 16 && !common) {
                    rand = 255;
                    common = true;
                } else {
                    rand = raribleRandom(calcTraitType(3), i, species);
                }
            }

            traits[i] = uint8(rand);
            // console.log("Gen 0: BODY PATTERNS", traits[i], "/ 13");
        }
        // Body Color (dominant, recessive, recessive, recessive, recessive, recessive)
        common = false;
        for (uint256 i = 21; i < 26; i++) {
            uint256 rand = 0;

            if (species > 3 && i > 21 && !common) {
                rand = 255;
                common = true;
            } else {
                rand = raribleRandom(calcTraitType(4), i, species);
            }

            traits[i] = uint8(rand);
            // console.log("Gen 0: BODY COLORS", traits[i], "/ 23");
        }
        // Bottoms (dominant, recessive, recessive, recessive, recessive, recessive)
        common = false;
        uint256 rB = rollNewGuess(5);
        for (uint256 i = 26; i < 31; i++) {
            uint256 rand = 0;

            if (i - 25 != rB) {
                if (species > 3 && i > 26 && !common) {
                    rand = 255;
                    common = true;
                } else {
                    rand = raribleRandom(calcTraitType(5), i, species);
                }
            }

            traits[i] = uint8(rand);
            // console.log("Gen 0: BOTTOMS", traits[i], "/ 7");
        }

        traits[31] = 0;
        //console.log("Traits gas", startGas - gasleft());
        return BuddyLib.encode(traits);
    }

    function calcTraitType(uint8 offset)
        private
        pure
        returns (uint8 traitType)
    {
        if (offset == 0) {
            traitType = 3;
        } else if (offset == 1) {
            traitType = 7;
        } else if (offset == 2) {
            traitType = 11;
        } else if (offset == 3) {
            traitType = 15;
        } else if (offset == 4) {
            traitType = 19;
        } else {
            traitType = 23;
        }
    }

    function raribleRandom(
        uint256 traitType,
        uint256 i,
        uint8 species
    ) private returns (uint256) {
        uint256 min = 1;
        uint256 max = 1;
        uint256 rarity;

        // Guarantees one trait is rare or better
        if (i % 5 == 0) {
            rarity = rollNewGuess(30) + 70;
        } else {
            rarity = rollNewGuess(100);
        }

        // Common
        if (rarity <= 70) {
            max = _mutations.getRarity(species, traitType - 3);
            // Rare
        } else if (rarity <= 95) {
            min = _mutations.getRarity(species, traitType - 3) + 1;
            max = _mutations.getRarity(species, traitType - 2);
            // Legendary
        } else if (rarity <= 98) {
            min = _mutations.getRarity(species, traitType - 2) + 1;
            max = _mutations.getRarity(species, traitType - 1);
            // Exalted
        } else {
            min = _mutations.getRarity(species, traitType - 1) + 1;
            max = _mutations.getRarity(species, traitType);
        }

        //console.log("RaribleRandom gas", startGas - gasleft());
        return traitRandom(min, max);
    }

    function traitRandom(uint256 min, uint256 max)
        private
        returns (uint256 trait)
    {
        uint256 n = max - (min - 1);
        uint256 total = ((n * 2 + 1) * n * (n + 1)) / 6;

        uint256 rand = rollNewGuess(total);

        for (uint256 i = min; i <= max; i++) {
            // Lower numbers = higher probability
            total -= BuddyLib.pow(max - (i - 1), 2);

            if (total <= rand) {
                return i;
            }
        }
    }

    function rollNewGuess(uint256 max) public returns (uint256 rand) {
        if (max == 0) {
            return 0;
        }

        _nonce++;
        return (uint256(keccak256(abi.encode(randomResult, _nonce))) % max) + 1;
    }

    function initRandom() external onlyOwner {
        getRandomNumber();
    }

    /**
     * Requests randomness
     */
    function getRandomNumber() private returns (bytes32 requestId) {
        require(
            LINK.balanceOf(address(this)) >= fee,
            "Not enough LINK to generate randomness"
        );
        return requestRandomness(keyHash, fee);
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness)
        internal
        override
    {
        randomResult = randomness;
        _nonce = 0;
    }
}

