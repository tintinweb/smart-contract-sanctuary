pragma solidity ^0.8.4;

import '@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC1155/IERC1155Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/metatx/ERC2771ContextUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol';
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

//security againts transactions for multiple requests
import 'hardhat/console.sol';

contract NFTBarter is ERC2771ContextUpgradeable, UUPSUpgradeable, OwnableUpgradeable {
    using AddressUpgradeable for address;

    /**
     * @dev Emitted when barter space is created
     */
    event BarterSpaceCreated(
        string indexed barterId,
        uint256 indexed tokenId,
        address indexed nftContract,
        address maker
    );

    /**
     * @dev Emitted when barter space is created
     */
    event BarterDone(
        string indexed barterId,
        uint256 indexed tokenIdA,
        uint256 indexed tokenIdB,
        address nftContractA,
        address nftContractB
    );

    /**
     * @dev Emitted when barter space is created
     */
    event BarterRequest(
        string indexed barterId,
        address indexed maker,
        uint256 indexed tokenId,
        uint256 itemId, 
        address nftContract
    );

    // Struct for barter
    struct Barters {
        address maker;
        bool isClosed;
        uint256 tokenId;
        uint256 itemId;
        address nftContract;
        mapping(address => Barter) _barterStruct;
    }

    struct Barter {
        address interested;
        uint256 marketTokenItem;
        uint256 tokenId;
    }

    mapping(string => Barters) private _barters;

    function initialize(address _trustedForwarder) initializer public {
        __ERC2771Context_init(_trustedForwarder);
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    function _msgSender() internal view override(ContextUpgradeable, ERC2771ContextUpgradeable)
      returns (address sender) {
      sender = ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData() internal view override(ContextUpgradeable, ERC2771ContextUpgradeable)
        returns (bytes memory) {
        return ERC2771ContextUpgradeable._msgData();
    }

    function _authorizeUpgrade(address) internal override onlyOwner {}

    function versionRecipient() external view virtual returns (string memory){
        return "2.2.0";
    }

    function setTrustedForwarder(address _trustedForwarder) public virtual onlyOwner{
        __ERC2771Context_init_unchained(_trustedForwarder);
    }

    modifier barterIsAlive(string memory barterId) {
        require(_barters[barterId].isClosed == false, "Barter session already closed");
        _;
    }

    function makeBarterSpace(string memory id, uint256 tokenId, address nftAddress, uint256 itemId, address marketPlaceAddress) external barterIsAlive(id) {
        
        NFTMarketplaceAbstact blmMarket = NFTMarketplaceAbstact(marketPlaceAddress);
        address _tempOwner = blmMarket.getMarketTokenItem(itemId).owner;
        uint256 _tempTokenId = blmMarket.getMarketTokenItem(itemId).tokenId;
        require(_tempOwner == _msgSender(), "Not The Owner of NFT");
        require(_tempTokenId == tokenId, "Not The Same NFT");

        Barters storage newBarter = _barters[id];
        if (newBarter.maker == address(0)) {
            newBarter.maker = _msgSender();
            newBarter.tokenId = tokenId;
            newBarter.nftContract = nftAddress;
        }
        emit BarterSpaceCreated(id, newBarter.tokenId, newBarter.nftContract, newBarter.maker);
    }

    function requestBarter(string memory barterId, uint256 tokenId, uint256 itemId, address nftContract, address marketPlaceAddress) external barterIsAlive(barterId) {        
        Barter memory newBarter;
        NFTMarketplaceAbstact blmMarket = NFTMarketplaceAbstact(marketPlaceAddress);
        address _tempOwner = blmMarket.getMarketTokenItem(itemId).owner;
        uint256 _tempTokenId = blmMarket.getMarketTokenItem(itemId).tokenId;
        require(_tempOwner == _msgSender(), "Not The Owner of NFT");
        require(_tempTokenId == tokenId, "Not The Same NFT");
        // Give approval to Barter Contract
        // if(!isListed) {
        //     IERC721Upgradeable(nftContract).approve(address(this), tokenId);
        // }
        newBarter = Barter(_msgSender(), itemId, tokenId);
        Barters storage newRequest = _barters[barterId];
        newRequest._barterStruct[_msgSender()] = newBarter;
        emit BarterRequest(barterId, _msgSender(), tokenId, itemId, nftContract);
    }

    // event DebugParameter(address itemMaker, string barterId, bool isListedA, bool isListedB, address participant, address nftContractA, address nftContractB, address marketPlaceAddress);
    // event DebugErcTrans1(address nftContractA, address caller, address participant, uint256 tokenId);
    // event DebugErcTrans2(address nftContractB, address participant, address caller, uint256 tokenId);

    function approveBarter(string memory barterId, bool isListedA, bool isListedB, address participant, address nftContractA, address nftContractB, address marketPlaceAddress, uint256 itemIdA, uint256 itemIdB) external barterIsAlive(barterId) {
        Barters storage item = _barters[barterId];
        Barter storage barterItem = item._barterStruct[participant];
        require(item.maker == _msgSender(), 'Caller have to be the owner of the asset');

        NFTMarketplaceAbstact blmMarket = NFTMarketplaceAbstact(marketPlaceAddress);

        if(isListedA) {
            address _tempOwner = blmMarket.getMarketTokenItem(itemIdA).owner;
            require(_tempOwner == _msgSender(), "Not The Owner of NFT");
            // blmMarket.sendBarterItem(participant, itemIdA, nftContractA);
            IERC721Upgradeable(nftContractA).safeTransferFrom(marketPlaceAddress, participant, item.tokenId);
        } else {
            IERC721Upgradeable(nftContractA).safeTransferFrom(_msgSender(), participant, item.tokenId);
        }

        if(isListedB) {
            address _tempOwner = blmMarket.getMarketTokenItem(itemIdB).owner;
            require(_tempOwner == participant, "Not The Owner of NFT");
            // blmMarket.sendBarterItem(_msgSender(), itemIdB, nftContractB);
            IERC721Upgradeable(nftContractB).safeTransferFrom(marketPlaceAddress, _msgSender(), barterItem.tokenId);
        } else {
            IERC721Upgradeable(nftContractB).safeTransferFrom(participant, _msgSender(), barterItem.tokenId);
        }

        item.isClosed = true;
        uint256 _temp = barterItem.tokenId;
        delete item._barterStruct[participant];

        // emit DebugParameter(item.maker, barterId, isListedA, isListedB, participant, nftContractA, nftContractB, marketPlaceAddress);
        // emit DebugErcTrans1(nftContractA, _msgSender(), participant, item.tokenId);
        // emit DebugErcTrans2(nftContractB, participant, _msgSender(), _temp);
        emit BarterDone(barterId, item.tokenId, _temp, nftContractA, nftContractB);
    }

    // function manualTransfer(address from, address to, uint256 tokenId, address nftAddress) internal {
    //     IERC721Upgradeable(nftAddress).safeTransferFrom(from, to, tokenId);
    // }

    // function getBartersTokenId(string memory id) external view returns (uint256) {
    //     return _barters[id].tokenId;
    // }

    // function getBarterRequest(string memory id, address participant) external view returns (Barter memory) {
    //     return _barters[id]._barterStruct[participant];
    // }

    // function getOwnerOf(uint256 tokenId, address nftAddress) external view returns(address) {
    //     return IERC721Upgradeable(nftAddress).ownerOf(tokenId);
    // }

    // function getMsgSender() external view returns (address) { 
    //     return _msgSender();
    // }
}

abstract contract NFTMarketplaceAbstact {
    struct MarketToken {
        uint itemId;
        address nftContract;
        uint256 tokenId;
        address payable seller;
        address payable owner;
        address payable creator;
        uint256 price;
        uint royaltiPercentage;
        bool isErc721;
        uint256 amount;
    }
    function getMarketTokenItem(uint itemId) public view virtual returns (MarketToken memory);
}

