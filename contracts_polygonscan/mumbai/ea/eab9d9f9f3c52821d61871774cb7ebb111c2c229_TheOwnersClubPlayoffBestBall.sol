    uint256 public fee = 0.0001 * 10**18; // 0.0001 LINK on Polygon
    uint256 unitPrice = 0.0055 * 10**18;

    // Move to decentralized instances when frozen
    string public baseImageURI;
    string public baseAnimationURI;

    string[] public playoffTeams = [

        // AFC
        "KC",
        "TEN",
        "CIN",
        "BUF",
        "NE",
        "IND",
        "LAC",

        // NFC
        "GB",
        "LAR",
        "TB",
        "DAL",
        "ARI",
        "SF",
        "PHI"
    ];

    string[] public positions = [
        "QB",
        "RB",
        "WR",
        "TE",
        "DST"
    ];

    struct PackSelector {
        uint index;
        uint[] teams;
     }

    struct BestBall {
        bool isOpened;
        bool isFrozen;
        uint256 seed;
        string[20] cards;
        string[12] lineup;
    }

    struct CardTuple {
        string team;
        string position;
    }

    address payable paymentAddress;

    mapping(bytes32 => address) requestToSender;
    mapping(bytes32 => uint256) requestToTokenId;
    mapping(string => string) teamToElement;
    mapping(uint256 => mapping (string => PackSelector)) packSelector;
    mapping(uint256 => mapping (string => bool)) tokenToCardLookup;

    BestBall[] public bestballs;

    event Created(uint256 indexed tokenId);
    event OpenedPack(uint256 indexed tokenId, string[20] indexed cards);
    event LineupSet(uint256 indexed tokenId, string[12] indexed lineup);

    string constant NAME = "The Owners Club: Playoff Best Ball 2021-2022";
    string constant SYMBOL = "TOCPBB";
    string constant DESCRIPTION = "A dynamic NFT created by token owners. The minted lineup gives you entry into the 2021-2022 playoff best ball contest hosted by The Owners Club with a guanranteed prize pool of $50,000.";

    constructor(address _VRFCoordinator, address _LinkToken, bytes32 _keyhash, address _token, address _openseaProxyAddress)
        VRFConsumerBase(_VRFCoordinator, _LinkToken)
        ERC721(NAME, SYMBOL)
    {
        VRFCoordinator = _VRFCoordinator;
        LinkToken = _LinkToken;
        keyHash = _keyhash;
        token = IERC20(_token);
        paymentAddress = payable(msg.sender);
        openseaProxyAddress = _openseaProxyAddress;

        _initializeEIP712(NAME);
    }

    function initRandomness(uint256 amount) internal {
        for(uint i = 0; i < amount; i++){
          bytes32 requestId = requestRandomness(keyHash, fee);
          requestToSender[requestId] = msg.sender;
        }
    }

    // Proxy owner mint
    function mintProxy(uint amount) public onlyOwner {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK - fill contract with faucet");
        initRandomness(amount);
    }

    // Public mint
    function mint(uint256 amount, uint256 tokenAmount) public {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK - fill contract with faucet");
        require(amount > 0, "Please submit an amount greater than 0");
        require(tokenAmount == amount * unitPrice, "Please submit the proper price in order to complete the purchase");
        require(token.transferFrom(msg.sender, paymentAddress, tokenAmount), "Unable to transfer funds");
        initRandomness(amount);
    }

    // Chainlink VRC response handler
    function fulfillRandomness(bytes32 requestId, uint256 randomNumber) internal override {
        uint256 tokenId = bestballs.length;

        bool isOpened = false;
        bool isFrozen = false;
        uint256 seed = randomNumber;
        string[20] memory cards;
        string[12] memory lineup;

        bestballs.push(
            BestBall(
                isOpened,
                isFrozen,
                seed,
                cards,
                lineup
            )
        );

        // Save lookup hash for validation later
        for(uint cardIndex = 0; cardIndex < cards.length; cardIndex++){
          tokenToCardLookup[tokenId][cards[cardIndex]] = true;
        }

        _safeMint(requestToSender[requestId], tokenId);

        emit Created(tokenId);

    }

    function setProxyAddress(address _openseaProxyAddress) public onlyOwner {
        openseaProxyAddress = _openseaProxyAddress;
    }

    function setPaymentAddress(address _paymentAddress) public onlyOwner {
        paymentAddress = payable(_paymentAddress);
    }

    function setBaseImageURI(string memory _baseImageURI) public onlyOwner {
        baseImageURI = _baseImageURI;
    }

    function setBaseAnimationURI(string memory _baseAnimationURI) public onlyOwner {
        baseAnimationURI = _baseAnimationURI;
    }

    function setUnitPrice(uint256 _unitPrice) public onlyOwner {
        unitPrice = _unitPrice;
    }

    function addTeamElement(string memory team, string memory element) public onlyOwner {
        teamToElement[team] = element;
    }

    function getTokenOverview(uint256 tokenId)
        public
        view
        returns (
            bool,
            bool,
            uint256,
            string[20] memory,
            string[12] memory
        ){
        require(_exists(tokenId), "Nonexistent token");
        return (
            bestballs[tokenId].isOpened,
            bestballs[tokenId].isFrozen,
            bestballs[tokenId].seed,
            bestballs[tokenId].cards,
            bestballs[tokenId].lineup
        );
    }

    function totalSupply() external view returns (uint256) {
        return bestballs.length;
    }

    // Fisher-Yates shuffle for randomizing an integer array that maps to playoff teams
    function shuffle(uint size, uint entropy) internal pure returns (uint[] memory) {
        uint[] memory result = new uint[](size);

        // Initialize array
        for (uint i = 0; i < size; i++) {
           result[i] = i + 1;
        }

        // Set the initial randomness based on the provided entropy
        bytes32 random = keccak256(abi.encodePacked(entropy));

        // Set the last item of the array which will be swapped
        uint last_item = size - 1;

        // We need to do `size - 1` iterations to completely shuffle the array
        for (uint i = 1; i < size - 1; i++) {
            // Select a number based on the randomness.
            uint selected_item = uint(random) % last_item;

            // Swap items `selected_item <> last_item`.
            uint aux = result[last_item];
            result[last_item] = result[selected_item];
            result[selected_item] = aux;

            // Decrease the size of the possible shuffle
            // to preserve the already shuffled items
            // The already shuffled items are at the end of the array
            last_item--;

            // Generate new randomness
            random = keccak256(abi.encodePacked(random));
        }

        return result;
    }

    // Generate 20 random cards w/ random seed as entropy
    // 2 QB,RB,WR,TE,DST and random unique team
    // 10 random position with random teams holding uniqueness
    function openPack(uint256 tokenId) public {
        require(_exists(tokenId), "Nonexistent token");
        require(ownerOf(tokenId) == msg.sender, "You must be the owner of the pack to open");
        require(!bestballs[tokenId].isOpened, "Best ball pack should not be opened!");

        string memory teamPart;
        string memory currentPosition;
        string memory currentPositionWithDelimeter;
        uint positionIndex = 0;
        uint positionLength = positions.length;

        for (; positionIndex < positions.length; positionIndex++) {
           packSelector[tokenId][positions[positionIndex]] = PackSelector({
             index: 0,
             teams: shuffle(playoffTeams.length, bestballs[tokenId].seed + positionIndex)
           });
        }

        for (positionIndex = 0; positionIndex < positions.length; positionIndex++) {

           currentPosition = positions[positionIndex];
           currentPositionWithDelimeter = string(abi.encodePacked("_", currentPosition));

           // Choose first random team
           teamPart = playoffTeams[packSelector[tokenId][currentPosition].teams[packSelector[tokenId][currentPosition].index] - 1];
           bestballs[tokenId].cards[positionIndex * 2] = string(abi.encodePacked(teamPart, currentPositionWithDelimeter));

           packSelector[tokenId][currentPosition].index += 1;

           // Choose second random team
           teamPart = playoffTeams[packSelector[tokenId][currentPosition].teams[packSelector[tokenId][currentPosition].index] - 1];
           bestballs[tokenId].cards[(positionIndex * 2) + 1] = string(abi.encodePacked(teamPart, currentPositionWithDelimeter));

           packSelector[tokenId][currentPosition].index += 1;
        }

        // 10 random positions and random team, no dupes
        for(uint randIndex = 0; randIndex < 10; randIndex++){

          // Randomly select a position between 0-<positionLength - 1>
          uint256 basis = ((positionLength * (randIndex + 1)) * 10);
          uint256 randomPositionIndex = (bestballs[tokenId].seed % basis / (basis / positionLength));
          currentPosition = positions[randomPositionIndex];
          currentPositionWithDelimeter = string(abi.encodePacked("_", currentPosition));

          // Based on position use array from before to avoid dupe teams
          teamPart = playoffTeams[packSelector[tokenId][currentPosition].teams[packSelector[tokenId][currentPosition].index] - 1];
          bestballs[tokenId].cards[randIndex + 10] = string(abi.encodePacked(teamPart, currentPositionWithDelimeter));

          // Update team index being used by 1 to get next if the position gets selected again
          packSelector[tokenId][currentPosition].index += 1;

        }

        // Set opened and cards (irreversible)
        bestballs[tokenId].isOpened = true;

        // Save lookup hash for validation later
        for(uint cardIndex = 0; cardIndex < bestballs[tokenId].cards.length; cardIndex++){
          tokenToCardLookup[tokenId][bestballs[tokenId].cards[cardIndex]] = true;
        }

        emit OpenedPack(tokenId, bestballs[tokenId].cards);

    }

    function compareStrings(string memory a, string memory b) private pure returns (bool) {
      return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
    }

    // Validate and set the finalized token lineup
    // Rule 1: Should only use cards associated to the NFT
    // Rule 2: Should have at least 1 of every position
    // Accepts arrays of tuples to avoid string manipulation in contract
    function setLineup(uint256 tokenId, CardTuple[12] memory lineup) public {
        require(_exists(tokenId), "Nonexistent token");
        require(ownerOf(tokenId) == msg.sender, "You must be the owner of the token to set lineup");
        require(bestballs[tokenId].isOpened, "Best ball pack should be opened first");
        require(!bestballs[tokenId].isFrozen, "Best ball lineup for the token has been set");

        bool hasValidReferences = true;
        bool hasQB = false;
        bool hasRB = false;
        bool hasWR = false;
        bool hasTE = false;
        bool hasDST = false;

        CardTuple memory tuple;
        string memory cardRef;
        string memory currentPositionWithDelimeter;

        // Validate card tuples
        for(uint lineupIndex = 0; lineupIndex < lineup.length; lineupIndex++){
          tuple = lineup[lineupIndex]; // pair of [<team>, <position>]
          currentPositionWithDelimeter = string(abi.encodePacked("_", tuple.position));

          // Merge back the tuple into a string to validate against card strings
          cardRef = string(abi.encodePacked(tuple.team, currentPositionWithDelimeter));

          // Verify lineup passed in contains the cards associated to the tokenId
          hasValidReferences = hasValidReferences && tokenToCardLookup[tokenId][cardRef] == true;

          // Verify at least one of each position exists by toggling off of position tuple
          hasQB = hasQB || compareStrings(tuple.position, "QB");
          hasRB = hasRB || compareStrings(tuple.position, "RB");
          hasWR = hasWR || compareStrings(tuple.position, "WR");
          hasTE = hasTE || compareStrings(tuple.position, "TE");
          hasDST = hasDST || compareStrings(tuple.position, "DST");
        }

        require(hasValidReferences, "Lineup has invalid city/position references not referred to by the token");
        require(hasQB && hasRB && hasWR && hasTE && hasDST, "Lineup must contain at least 1 of every position");

        // Set lineup as frozen (irreversible)
        bestballs[tokenId].isFrozen = true;

        // Set lineup in NFT
        for(uint lineupIndex = 0; lineupIndex < lineup.length; lineupIndex++){
          tuple = lineup[lineupIndex]; // pair of [<team>, <position>]
          currentPositionWithDelimeter = string(abi.encodePacked("_", tuple.position));

          // Merge back the tuple into a string to validate against card strings
          cardRef = string(abi.encodePacked(tuple.team, currentPositionWithDelimeter));

          // Set lineup reference
          bestballs[tokenId].lineup[lineupIndex] = cardRef;
        }

        emit LineupSet(tokenId, bestballs[tokenId].lineup);
    }

    function overviewToAttributes(BestBall memory instance) internal pure returns (string memory){

        string memory builder = "";

        if(instance.isFrozen){
            // Add frozen true
            builder = string(abi.encodePacked(
                builder,
                '{ "trait_type": "Frozen", "value": "true" }'
             ));
            for(uint i = 0; i < instance.lineup.length; i++){
                builder = string(abi.encodePacked(
                    builder,
                    ', {"value": "',
                    instance.lineup[i],
                    '"}'
                 ));
            }
        } else if(instance.isOpened){
            // Add frozen false
            builder = string(abi.encodePacked(
                builder,
                '{ "trait_type": "Frozen", "value": "false" }'
             ));
            for(uint i = 0; i < instance.cards.length; i++){
                builder = string(abi.encodePacked(
                    builder,
                    ', {"value": "',
                    instance.cards[i],
                    '"}'
                 ));
            }
        }

        return string(abi.encodePacked("[", builder, "]"));
    }

    function toJSONProperty(string memory key, string memory value) private pure returns (string memory) {
        return string(abi.encodePacked('"', key, ': "', value, '"'));
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory){
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        BestBall memory instance = bestballs[tokenId];

        string memory imageURI = string(abi.encodePacked(baseImageURI, "/", tokenId));
        string memory externalURI = string(abi.encodePacked(baseAnimationURI, "/", tokenId));

        // JSON Properties
        string memory name = toJSONProperty("name", NAME);
        string memory description = toJSONProperty("description", DESCRIPTION);
        string memory image = toJSONProperty("image", imageURI);
        string memory externalUrl = toJSONProperty("external_url", externalURI);
        string memory animationUrl = toJSONProperty("animation_url", externalURI);
        string memory attributes = string(abi.encodePacked('"attributes": ', overviewToAttributes(instance)));

        return string(
            abi.encodePacked(
                "data:application/json;base64,",
                Base64.encode(
                    bytes(
                        abi.encodePacked(
                            '{',
                                name,
                                ",",
                                description,
                                ",",
                                attributes,
                                ",",
                                image,
                                ",",
                                externalUrl,
                                ",",
                                animationUrl,
                            '"}'
                        )
                    )
                )
            )
        );
    }

    function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
      return this.onERC721Received.selector;
    }

    // OS integration for gas less transactions
    function _msgSender() internal override view returns (address sender){
      return ContextMixin.msgSender();
    }

    function isApprovedForAll(
            address _owner,
            address _operator
    ) public override view returns (bool isOperator) {

        // if OpenSea's ERC721 Proxy Address is detected, auto-return true
        if (_operator == openseaProxyAddress) {
            return true;
        }

        // otherwise, use the default ERC721.isApprovedForAll()
        return ERC721.isApprovedForAll(_owner, _operator);
    }
