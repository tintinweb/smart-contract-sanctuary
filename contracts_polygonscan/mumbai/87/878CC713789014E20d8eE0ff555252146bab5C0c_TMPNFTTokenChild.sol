pragma solidity 0.8.4;

import {ERC721, ERC721Enumerable} from "../ERC721Enumerable.sol";
import {Timers} from "@openzeppelin/contracts/utils/Timers.sol";
import {SafeCast} from "@openzeppelin/contracts/utils/math/SafeCast.sol";
import {AccessControlMixin} from "@the-meta-portal/helper-contracts/contracts/AccessControlMixin.sol";

// !!!!!
// This contract is for the polygon chain, so we can map our NFT token in Ethereum to polygon

contract TMPNFTTokenChild is ERC721Enumerable, AccessControlMixin {
  using SafeCast for uint256;
  using Timers for Timers.Timestamp;

  bytes32 public constant DEPOSITOR_ROLE = keccak256("DEPOSITOR_ROLE");

  // limit batching of tokens due to gas limit restrictions
  uint256 public constant BATCH_LIMIT = 5;

  struct DepositNFTTokenData {
    uint64 depositedOn;
  }

  bool public isMetadataFrozen = false;

  uint16 maxTokens;
  string private localBaseURI;
  address public childChainManager;

  mapping(uint256 => DepositNFTTokenData) public depositedTokens;
  mapping(uint256 => Timers.Timestamp) private tokenLockPeriodTracker;

  event WithdrawnBatch(address indexed user, uint256[] tokenIds);

  constructor(string memory _baseURI, uint16 _maxTokens, address _childChainManager) ERC721("The Meta Portal NFT", "TMPNFT") {
    maxTokens = _maxTokens;
    localBaseURI = _baseURI;
    _setupContractId("TMPNFTToken");
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _setupRole(DEPOSITOR_ROLE, _childChainManager);

    childChainManager = _childChainManager;
  }

  modifier isMetadataNotFrozen() {
    require(!isMetadataFrozen, "TMPNFTToken: Metadata is frozen!");

    _;
  }

  function freezeMetadata() public only(DEFAULT_ADMIN_ROLE) {
    isMetadataFrozen = true;
  }

  function deposit(address _user, bytes calldata _depositData)
  external
  only(DEPOSITOR_ROLE)
  {
    uint64 currentTimestamp = block.timestamp.toUint64();

    // deposit single
    if (_depositData.length == 32) {
      uint256 tokenId = abi.decode(_depositData, (uint256));

      depositedTokens[tokenId].depositedOn = currentTimestamp;
      _mint(_user, tokenId);

      // deposit batch
    } else {
      uint256[] memory tokenIds = abi.decode(_depositData, (uint256[]));
      uint256 length = tokenIds.length;

      for (uint256 i; i < length; i++) {
        depositedTokens[tokenIds[i]].depositedOn = currentTimestamp;
        _mint(_user, tokenIds[i]);
      }
    }
  }

  function askForWithdrawal(uint256 _tokenId) public {
    require(tokenLockPeriodTracker[_tokenId].isUnset(), "TMPNFTToken: A withdrawal has already been asked for token!");
    require(msg.sender == ownerOf(_tokenId), "TMPNFTToken: Only owner of token can ask for withdrawal!");

    // set unlock period for token available after 1 weeks
    tokenLockPeriodTracker[_tokenId].setDeadline((block.timestamp + 1 weeks).toUint64());
  }

  function askForWithdrawalBatch(uint256[] calldata _tokenIds) public {
    uint256 length = _tokenIds.length;

    for (uint256 i; i < length; i++) {
      askForWithdrawal(_tokenIds[i]);
    }
  }

  function withdraw(uint256 _tokenId) external {
    require(canWithdrawToken(_tokenId), "TMPNFTToken: Cannot withdraw token!");
    require(msg.sender == ownerOf(_tokenId), "TMPNFTToken: Only owner of token can withdraw token!");

    depositedTokens[_tokenId].depositedOn = 0;
    tokenLockPeriodTracker[_tokenId].reset();

    _burn(_tokenId);
  }

  function withdrawBatch(uint256[] calldata tokenIds) external {
    uint256 length = tokenIds.length;

    require(length <= BATCH_LIMIT, "TMPNFTTokenChild: Batch limit exceeded");

    // Iteratively burn ERC721 tokens, for performing
    // batch withdraw
    for (uint256 i; i < length; i++) {
      uint256 tokenId = tokenIds[i];

      require(canWithdrawToken(tokenId), string(abi.encodePacked("TMPNFTToken: Cannot withdraw token ", uint2str(tokenId))));
      require(msg.sender == ownerOf(tokenId), string(abi.encodePacked("TMPNFTToken: Only owner of token can withdraw token ", uint2str(tokenId))));

      depositedTokens[tokenId].depositedOn = 0;
      tokenLockPeriodTracker[tokenId].reset();

      _burn(tokenId);
    }

    // At last emit this event, which will be used
    // in MintableERC721 predicate contract on L1
    // while verifying burn proof
    emit WithdrawnBatch(_msgSender(), tokenIds);
  }

  function withdrawContractValue(address _withdrawAccount) public only(DEFAULT_ADMIN_ROLE) {
    require(_withdrawAccount != address(0), "TMPNFTToken: Withdraw account cannot be 0x0");

    (bool success,) = payable(_withdrawAccount).call{value : address(this).balance}("");
    require(success);
  }

  function setBaseURI(string memory _uri) public only(DEFAULT_ADMIN_ROLE) isMetadataNotFrozen {
    localBaseURI = _uri;
  }

  function setChildChainManager(address _childChainManager) public only(DEFAULT_ADMIN_ROLE) {
    require(_childChainManager != address(0), "TMPNFTToken: Child chain manager cannot be 0x0");

    // revoke role of old depositor
    _revokeRole(DEPOSITOR_ROLE, childChainManager);

    childChainManager = _childChainManager;
    _setupRole(DEPOSITOR_ROLE, _childChainManager);
  }

  function supportsInterface(bytes4 _interfaceId) public view virtual override(ERC721, AccessControlMixin) returns (bool) {
    return super.supportsInterface(_interfaceId);
  }

  function maxSupply() public view override returns (uint256) {
    return uint256(maxTokens);
  }

  function getDepositTime(uint256 _tokenId) public view returns (uint64) {
    return depositedTokens[_tokenId].depositedOn;
  }

  function canWithdrawToken(uint256 _tokenId) public view returns (bool) {
    Timers.Timestamp storage tokenLockPeriod = tokenLockPeriodTracker[_tokenId];

    if (tokenLockPeriod.isUnset()) {
      return false;
    }

    // we have a gratis period
    // where after the lock period is off users have 3 days to withdraw their token
    bool hasGratisPeriodExpired = tokenLockPeriod._deadline + 3 days <= block.timestamp.toUint64();

    return tokenLockPeriodTracker[_tokenId].isExpired() && !hasGratisPeriodExpired;
  }

  function _baseURI() internal view override returns (string memory) {
    return localBaseURI;
  }

  function uint2str(
    uint256 _i
  )
  internal
  pure
  returns (string memory str)
  {
    if (_i == 0)
    {
      return "0";
    }
    uint256 j = _i;
    uint256 length;
    while (j != 0)
    {
      length++;
      j /= 10;
    }
    bytes memory bstr = new bytes(length);
    uint256 k = length;
    j = _i;
    while (j != 0)
    {
      bstr[--k] = bytes1(uint8(48 + j % 10));
      j /= 10;
    }
    str = string(bstr);
  }

  // TODO: remove on production
  function destroy() public payable only(DEFAULT_ADMIN_ROLE) {
    // cast address to payable
    address payable addr = payable(address(msg.sender));
    selfdestruct(addr);
  }
}

