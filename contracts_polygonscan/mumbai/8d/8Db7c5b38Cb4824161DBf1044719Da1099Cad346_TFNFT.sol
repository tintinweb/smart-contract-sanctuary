pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "hardhat/console.sol";
import "./NFTSVG.sol";

import {Base64} from "./libraries/Base64.sol";

contract TFNFT is ERC721URIStorage {
  using Counters for Counters.Counter;
  Counters.Counter private _tokenIds;
  
  uint256 public constant COLLECTION_SIZE = 200;
  uint256 margin = 32;
  uint256 CIRCLE_RADIUS = 48;
  uint256 NB_TRAITS = 12;

  constructor() ERC721("TypeformNFT", "TYPEFORM") {
    console.log("Typeform NFT contract, yiha!");
  }

  function getTotalMinted() public view returns (uint256) {
    return _tokenIds.current();
  }

  function generateTraits(bytes memory hash)
    internal
    view
    returns (NFTSVG.Trait[] memory)
  {
    uint256 i = 0;
    NFTSVG.Trait[] memory traits = new NFTSVG.Trait[](NB_TRAITS); //12 cells
    for (i = 0; i < NB_TRAITS; i++) {
      uint256 colorIndex = toUint8(hash, i) % 8; // 6 colors
      uint256 shape = toUint8(hash, i + 1) % 2; // two type of shape, 0 = circle, 1 = arc
      uint256 rotation = shape == 1 ? (toUint8(hash, i + 2) % 4) : 0;
      traits[i] = NFTSVG.Trait(
        colorIndex,
        shape == 0 ? true : false,
        i,
        rotation
      );
    }
    return traits;
  }

  function mint(address recipient, string memory tfResponseId) public {
    uint256 newItemId = _tokenIds.current();
    require(
      newItemId <= COLLECTION_SIZE,
      "The units requested exceed the collection size"
    );

    bytes memory responseHash = abi.encodePacked(bytes(tfResponseId));

    NFTSVG.Trait[] memory traits = generateTraits(responseHash);

    NFTSVG.SVGParams memory svgParams;
    svgParams.tfResponseId = tfResponseId;
    svgParams.tokenId = newItemId;
    svgParams.hash = responseHash;
    svgParams.traits = traits;

    string memory image = generateSVGImage(svgParams);
    console.log(image);
    string memory attributes = generateAttributes(svgParams.traits);

    string memory json = Base64.encode(
      bytes(
        string(
          abi.encodePacked(
            '{"name": "TypeformNFT", "description": "A token of appreciation for taking part in this first experiment.", "image": "data:image/svg+xml;base64,',
            Base64.encode(bytes(image)),
            '","attributes": [',attributes,',{"display_type": "date", "trait_type": "minted","value":',
            Strings.toString(block.timestamp),
            "}]",
            "}"
          )
        )
      )
    );

    string memory finalTokenUri = string(
      abi.encodePacked("data:application/json;base64,", json)
    );

    console.log("\n--------------------");
    console.log(finalTokenUri);
    console.log("--------------------\n");

    _safeMint(recipient, newItemId);

    // Update your URI!!!
    _setTokenURI(newItemId, finalTokenUri);

    _tokenIds.increment();
    console.log("An NFT w/ ID %s has been minted to %s", newItemId, recipient);
    // emit NewEpicNFTMinted(recipient, newItemId);
  }

  function generateSVGImage(NFTSVG.SVGParams memory params)
    internal
    pure
    returns (string memory svg)
  {
    return NFTSVG.generateSVG(params);
  }

  function generateAttributes(NFTSVG.Trait[] memory traits)
    internal
    view
    returns (string memory attributes)
  {
    uint256 i = 0;
    string memory attrPartial = "";
    uint256 nbCircles = 0;
    uint256 nbArcs = 0;

    for (i = 0; i < NB_TRAITS; i++) {
      NFTSVG.Trait memory t = traits[i];
      string memory colorName = NFTSVG.getColorName(t.color);
      if(t.shape == true){
        nbCircles++;
      }else{
        nbArcs++;
      }
      attrPartial = 
      string(abi.encodePacked('{"trait_type": "Cell #',  Strings.toString(i+1), '", "value": "', t.shape == true ? 'Circle' : 'Arc', ' ', colorName,'"}'));
      attributes = string(abi.encodePacked(attributes,i>0 ? ',': '', attrPartial));
    }
    //nbCircles
    attrPartial = string(abi.encodePacked('{"display_type": "number", "trait_type": "Circles", "value":', Strings.toString(nbCircles),'}'));
    attributes = string(abi.encodePacked(attributes, ',', attrPartial));
    //nbArcs
    attrPartial = string(abi.encodePacked('{"display_type": "number", "trait_type": "Arcs", "value":', Strings.toString(nbArcs),'}'));
    attributes = string(abi.encodePacked(attributes,',', attrPartial));

    return attributes;
  }

  // helper function for generation
  // from: https://github.com/GNSPS/solidity-bytes-utils/blob/master/contracts/BytesLib.sol
  function toUint8(bytes memory _bytes, uint256 _start)
    internal
    pure
    returns (uint8)
  {
    require(_start + 1 >= _start, "toUint8_overflow");
    require(_bytes.length >= _start + 1, "toUint8_outOfBounds");
    uint8 tempUint;

    assembly {
      tempUint := mload(add(add(_bytes, 0x1), _start))
    }
    return tempUint;
  }
}

