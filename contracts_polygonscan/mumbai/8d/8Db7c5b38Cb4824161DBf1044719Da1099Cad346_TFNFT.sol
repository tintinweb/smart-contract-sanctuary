  function generateImage(Trait[] memory traits)
    internal
    pure
    returns (string memory svg)
  {
    uint256 k = 0;
    uint256 margin = 32;
    uint256 CIRCLE_RADIUS = 48;
    string memory svgPartial = "";

    for (k = 0; k < traits.length; k++) {
      Trait memory t = traits[k];
      uint256 shapeX = margin + (2 * (k % 3)) * CIRCLE_RADIUS;
      uint256 shapeY = 2 * margin + (2 * (k / 3)) * CIRCLE_RADIUS;
      if (t.shape == true) {
        svgPartial = string(
          abi.encodePacked(
            '<use x="',
            Strings.toString(shapeX),
            '" y="',
            Strings.toString(shapeY),
            '" href="#circle" fill="',
            getColor(t.color),
            '"></use>'
          )
        );
      } else {
        svgPartial = string(
          abi.encodePacked(
            '<use x="',
            Strings.toString(shapeX),
            '" y="',
            Strings.toString(shapeY),
            '" href="#quarterCircle" fill="',
            getColor(t.color),
            '" transform="rotate(',
            Strings.toString(90 * t.rotation),
            " ",
            Strings.toString(shapeX + CIRCLE_RADIUS),
            " ",
            Strings.toString(shapeY + CIRCLE_RADIUS),
            ')"></use>'
          )
        );
      }
      svg = string(abi.encodePacked(svg, svgPartial));
    }
    return svg;
  }

  // GENERIC helpers
  function getColor(uint256 _index)
    internal
    pure
    returns (string memory color)
  {
    if (_index == 0) {
      color = color1;
    } else if (_index == 1) {
      color = color2;
    } else if (_index == 2) {
      color = color3;
    } else if (_index == 3) {
      color = color4;
    } else if (_index == 4) {
      color = color5;
    } else if (_index == 5) {
      color = color6;
    } else {
      color = color7;
    }
  }

  function getColorName(uint256 _index)
    internal
    pure
    returns (string memory color)
  {
    if (_index == 0) {
      color = "Pistachio";
    } else if (_index == 1) {
      color = "Turquoise";
    } else if (_index == 2) {
      color = "Cerulan";
    } else if (_index == 3) {
      color = "Fuchsia";
    } else if (_index == 4) {
      color = "Tangerine";
    } else if (_index == 5) {
      color = "Seaweed";
    } else {
      color = "Saffron";
    }
  }
}

