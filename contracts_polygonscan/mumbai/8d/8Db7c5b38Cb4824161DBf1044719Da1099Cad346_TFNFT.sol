/// @title NFTSVG
/// @notice Provides a function for generating an SVG associated with a Typeform NFT
library NFTSVG {
  // string[] COLORS = ['#F8F6F1','#ADEBE4','#9BCEFD','#ECACCC','#FDD09B','#FDE69B'];
  string constant color1 = "#86A329"; //Pistachio
  string constant color2 = "#5CD6C8"; //Turquoise
  string constant color3 = "#379CFB"; //Cerulan
  string constant color4 = "#D65C99"; //Fuchsia
  string constant color5 = "#FA6B05"; //Tangerine
  string constant color6 = "#0487AF"; //Seaweed
  string constant color7 = "#FBA137"; //Saffron
  uint256 internal constant COLLECTION_SIZE = 200;

  struct SVGParams {
    string tfResponseId;
    uint256 tokenId;
    bytes hash;
    Trait[] traits;
  }

  struct Trait {
    uint256 color;
    bool shape;
    uint256 index;
    uint256 rotation;
  }

  function generateSVG(SVGParams memory params)
    internal
    pure
    returns (string memory svg)
  {
    return
      string(
        abi.encodePacked(
          generateSVGDefs(),
          generateImage(params.traits),
          generateHashText(params.hash),
          generateBubble(params.tokenId),
          generateTypeformLogo(),
          "</svg>"
        )
      );
  }

  function generateSVGDefs() private pure returns (string memory svg) {
    return
      string(
        abi.encodePacked(
          '<svg xmlns="http://www.w3.org/2000/svg" width="352" height="506" viewBox="0 0 352 506" fill="#F1ECE3" >',
          '<symbol id="quarterCircle" width="96" height="96" viewBox="0 0 96 96">',
          '<path d="M0,96 A 96 96 90 0 1 96,0 L96,96 Z" opacity="1"></path>',
          "</symbol>",
          '<symbol id="circle" width="96" height="96" viewBox="0 0 96 96">',
          '<circle cx="48" cy="48" r="48"/>',
          "</symbol>",
          '<rect width="352" height="506" rx="16" fill="#FCDA69"/>',
          '<path id="hashPath" d="M25,480 L25,0" />'
        )
      );
  }

  function generateHashText(bytes memory hash)
    internal
    pure
    returns (string memory svg)
  {
    return
      string(
        abi.encodePacked(
          '<text x="30" y="0" fill="#F8F6F1" font-size="10px" font-family="Courier, serif"><textPath href="#hashPath" transform="rotate(90)" >',
          hash,
          "</textPath></text>"
        )
      );
  }

  function generateBubble(uint256 tokenId)
    internal
    pure
    returns (string memory svg)
  {
    return
      string(
        abi.encodePacked(
          "<g>",
          '<rect x="240" y="456" width="80" height="28" rx="14" fill="#C4C4C4"/>',
          '<text x="280" y="470" font-family="Courier, serif" font-size="11" fill="#131313" dominant-baseline="middle" text-anchor="middle">',
          Strings.toString(tokenId),
          " of ",Strings.toString(COLLECTION_SIZE),"</text>",
          "</g>"
