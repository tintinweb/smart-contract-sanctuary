import './IContractRegistry.sol';
import './IBatchCollection.sol';
import './IRetirementBadgeCollection.sol';
import './CarbonProjects.sol';

contract ProjectERC20 is Context, ERC20, IERC721Receiver {
    // // A variable to cap the supply for a specific project-vintage-ERC20
    // uint256 public maxSupply;
    string public projectId;
    uint16 public vintage;
    // Project-vintage specific data
    bool public isCorsiaCompliant;
    bool public isCCPcompliant;
    string public coBenefits;
    string public correspAdjustment;

    address public contractRegistry;

    mapping(address => uint256) minterToId;
    mapping(address => uint256) retiredAmount;

    constructor(
        string memory name_,
        string memory symbol_,
        string memory _projectId,
        uint16 _vintage,
        address _contractRegistry
    ) ERC20(name_, symbol_) {
        projectId = _projectId;
        vintage = _vintage;
        contractRegistry = _contractRegistry;
    }

    // Function to get corresponding attributes from the CarbonProjects
    function getAttributes()
        public
        view
        returns (
            uint16,
            string memory,
            string memory,
            string memory
        )
    {
        address c = IContractRegistry(contractRegistry).carbonProjectsAddress();
        string memory standard;
        string memory methodology;
        string memory region;
        string memory storageMethod;
        (standard, methodology, region, storageMethod) = CarbonProjects(c)
            .getProjectDataByProjectId(projectId);
        return (vintage, standard, methodology, region);
    }

    /**
     * @dev function is called with `operator` as `_msgSender()` in a reference implementation by OZ
     * `from` is the previous owner, not necessarily the same as operator
     *  Function checks if NFT collection is whitelisted and next if attributes are matching this erc20 contract
     **/
    function onERC721Received(
        address, /* operator */
        address from,
        uint256 tokenId,
        bytes calldata /* data */
    ) external override returns (bytes4) {
        // msg.sender is the BatchCollection contract
        require(
            checkWhiteListed(msg.sender),
            'Error: Batch-NFT not from whitelisted contract'
        );

        (
            string memory _projectId,
            uint16 _vintage,
            ,
            uint256 quantity,
            bool confirmed
        ) = IBatchCollection(msg.sender).getBatchNFTData(tokenId);
        require(
            checkMatchingAttributes(_projectId, _vintage),
            'Error: non-matching NFT'
        );
        require(confirmed == true, 'BatchNFT not yet confirmed');

        minterToId[from] = tokenId;
        quantity = quantity * 10**decimals();

        _mint(from, quantity);
        return this.onERC721Received.selector;
    }

    // Check if BatchCollection is whitelisted (official)
    function checkWhiteListed(address collection) internal view returns (bool) {
        if (
            collection ==
            IContractRegistry(contractRegistry).batchCollectionAddress()
        ) {
            return true;
        } else {
            return false;
        }
    }

    /**
     * @dev Checks if attributes of sent NFT are matching the attributes of this ERC20
     **/
    function checkMatchingAttributes(string memory _projectId, uint16 _vintage)
        internal
        view
        returns (bool)
    {
        bytes32 pid721 = keccak256(abi.encodePacked(_projectId));
        bytes32 pid20 = keccak256(abi.encodePacked(projectId));

        if (pid20 == pid721 && vintage == _vintage) {
            return true;
        } else {
            return false;
        }
    }

    // Retirement of TCUs (the actual offsetting)
    function retire(uint256 amount) public {
        _retire(_msgSender(), amount);
    }

    /// @dev Allow for pools or third party contracts to retire for the user
    // Requires approve
    function retireFrom(address account, uint256 amount) public {
        uint256 currentAllowance = allowance(account, _msgSender());
        require(
            currentAllowance >= amount,
            'ERC20: retire amount exceeds allowance'
        );
        unchecked {
            _approve(account, _msgSender(), currentAllowance - amount);
        }
        _retire(account, amount);
    }

    // Alternative flow, where tokens are sent to a "retirement contract"
    function _retire(address account, uint256 amount) internal {
        _burn(account, amount);
        retiredAmount[account] += amount;
    }

    // Mint the Badge NFT showing how many tokens have been retired
    function mintBadgeNFT(address to, uint256 amount) public {
        address badgeAddr = IContractRegistry(contractRegistry)
            .retirementBadgeCollectionAddress();
        require(
            retiredAmount[msg.sender] >= amount,
            'Error: Cannot mint more than user has retired'
        );

        IRetirementBadgeCollection(badgeAddr).mintBadge(
            to,
            projectId,
            vintage,
            amount
        );
        retiredAmount[msg.sender] -= amount;
    }

    // -----------------------------
    //      Locked ERC20 safety
    // -----------------------------

    modifier validDestination(address to) {
        require(to != address(0x0));
        require(to != address(this));
        _;
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        validDestination(recipient)
        returns (bool)
    {
        super.transfer(recipient, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override validDestination(recipient) returns (bool) {
        super.transferFrom(sender, recipient, amount);
        return true;
    }
}

