// Project-Vintage ERC20 Factory contract for BatchNFT fractionalization
// Locks in received ERC721 BatchNFTs and can mint corresponding quantity of ERC20s
// Permissionless, anyone can deploy new ERC20s unless they do not yet exist and pid exists
contract ProjectERC20Factory is
    ProjectERC20FactoryStorage,
    OwnableUpgradeable,
    PausableUpgradeable,
    UUPSUpgradeable
{
    event TokenCreated(address tokenAddress);

    // ----------------------------------------
    //      Upgradable related functions
    // ----------------------------------------

    function initialize(address _contractRegistry) public initializer {
        __Context_init_unchained();
        __Ownable_init_unchained();
        __Pausable_init_unchained();
        contractRegistry = _contractRegistry;
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyOwner
    {}

    // ------------------------
    //      Admin functions
    // ------------------------

    /// @notice Emergency function to disable contract's core functionality
    /// @dev wraps _pause(), only Admin
    function emergencyStop() public virtual onlyOwner {
        _pause();
    }

    /// @dev unpause the system, wraps _unpause(), only Admin
    function unpause() public virtual onlyOwner {
        _unpause();
    }

    // ------------------------
    // Permissionless functions
    // ------------------------

    // Function to deploy new pERC20s
    // Note: Function could be internal, but that would disallow pre-deploying ERC20s without existing NFTs
    function deployNewToken(
        string memory pvId,
        string memory projectId,
        uint16 vintage,
        address _contractRegistry
    ) public virtual whenNotPaused {
        require(!checkExistence(pvId), 'pERC20 already exists');

        address c = IContractRegistry(contractRegistry).carbonProjectsAddress();
        require(
            CarbonProjects(c).projectIds(projectId) == true,
            'Project does not yet exist'
        );

        // Todo: Needs some consideration about automatic naming
        ProjectERC20 t = new ProjectERC20(
            pvId,
            pvId,
            projectId,
            vintage,
            _contractRegistry
        );

        // Register deployed ERC20 in ContractRegistry
        IContractRegistry(contractRegistry).addERC20(address(t));

        deployedContracts.push(address(t));
        pvIdtoERC20[pvId] = address(t);

        emit TokenCreated(address(t));
    }

    // Deploy providing a BatchNFT as template, currently would work only with one single collection
    function deployFromTemplate(uint256 tokenId) public virtual whenNotPaused {
        address collection = IContractRegistry(contractRegistry)
            .batchCollectionAddress();
        (string memory pid, uint16 vintage, , , ) = IBatchCollection(collection)
            .getBatchNFTData(tokenId);
        string memory pvId = projectVintageId(pid, Strings.toString(vintage));

        deployNewToken(pvId, pid, vintage, contractRegistry);
    }

    // Helper function to create unique project-vintage identifying string
    function projectVintageId(string memory pid, string memory vintage)
        internal
        pure
        virtual
        returns (string memory)
    {
        return string(abi.encodePacked(pid, vintage));
    }

    // Checks if same pToken has already been deployed
    function checkExistence(string memory _pvId)
        internal
        view
        virtual
        returns (bool)
    {
        if (pvIdtoERC20[_pvId] == address(0)) {
            return false;
        } else {
            return true;
        }
    }

    // Lists addresses of deployed contracts
    // Currently only used in test suite, might be deprecated soon
    function getContracts() public view virtual returns (address[] memory) {
        for (uint256 i = 0; i < deployedContracts.length; i++) {}
        return deployedContracts;
    }
}

