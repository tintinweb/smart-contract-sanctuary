import '@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol';
import '@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol';
import '@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol';

import './CarbonProjectsStorage.sol';
import './ICarbonProjects.sol';

// The CarbonProjects represents offsetting projects as ERC721 tokens
// This makes it possible to transfer ownership of projects via transfers
// The Projects added serve as a data source for the BatchNFTs and project-vintage ERC20s
contract CarbonProjects is
    ICarbonProjects,
    CarbonProjectsStorage,
    ERC721Upgradeable,
    OwnableUpgradeable,
    PausableUpgradeable,
    UUPSUpgradeable
{
    using CountersUpgradeable for CountersUpgradeable.Counter;

    event ProjectMinted(address receiver, uint256 tokenId);
    event ProjectUpdated(uint256 tokenId);
    event ProjectIdUpdated(uint256 tokenId);

    CountersUpgradeable.Counter private _tokenIds;

    // ----------------------------------------
    //      Upgradable related functions
    // ----------------------------------------

    function initialize() public initializer {
        __Context_init_unchained();
        __ERC721_init_unchained('CO2KEN Project Collection', 'CO2KEN-PNFT');
        __Ownable_init_unchained();
        __Pausable_init_unchained();
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyOwner
    {}

    // ------------------------
    //      Admin functions
    // ------------------------

    /// @notice Emergency function to disable contract's core functionality
    /// @dev wraps _pause(), only Admin
    function emergencyStop() public virtual onlyOwner {
        _pause();
    }

    /// @dev unpause the system, wraps _unpause(), only Admin
    function unpause() public virtual onlyOwner {
        _unpause();
    }

    // Updates the controller, the entity in charge of the ProjectData
    // Note: Questionable if needed if this stays ERC721, as this could be the NFT owner
    function updateController(uint256 tokenId, address _controller)
        external
        virtual
        whenNotPaused
    {
        require(
            msg.sender == ownerOf(tokenId),
            'Error: Caller is not the owner'
        );
        projects[tokenId].controller = _controller;
    }

    function mapStorageMethod(
        string[] memory methodology,
        string[] memory storageMethod
    ) external virtual onlyOwner whenNotPaused {
        uint256 methodologyLen = methodology.length;
        uint256 storageMethodLen = storageMethod.length;
        require(
            methodologyLen == storageMethodLen,
            'Error: Length of arrays do not match'
        );

        for (uint256 i = 0; i < methodologyLen; i++) {
            methodologyToStorageMethod[methodology[i]] = storageMethod[i];
        }
    }

    // Adding a new project is currently permissionless
    // updating will require permission
    function addNewProject(
        address to,
        string memory projectId,
        string memory standard,
        string memory methodology,
        string memory region,
        string memory metaDataHash
    ) external virtual override onlyOwner whenNotPaused returns (uint256) {
        require(!strcmp(projectId, ''), 'ProjectId cannot be empty');
        require(projectIds[projectId] == false, 'Project already exists');
        projectIds[projectId] = true;
        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();

        _mint(to, newItemId);

        projects[newItemId].projectId = projectId;
        projects[newItemId].standard = standard;
        projects[newItemId].methodology = methodology;
        projects[newItemId].region = region;

        // Make sure metadata does not exist twice
        require(
            metaDataHashes[metaDataHash] == false,
            'Error: metaDataHash already exists'
        );
        projects[newItemId].metaDataHash = metaDataHash;
        metaDataHashes[metaDataHash] = true;

        emit ProjectMinted(to, newItemId);
        pidToTokenId[projectId] = newItemId;
        return newItemId;
    }

    /// @dev owner can update project data except projectId in case of previous mistakes
    function updateProject(
        uint256 tokenId,
        string memory newStandard,
        string memory newMethodology,
        string memory newRegion,
        string memory newMetaDataHash
    ) external virtual onlyOwner whenNotPaused {
        require(_exists(tokenId), 'Project not yet minted');
        projects[tokenId].standard = newStandard;
        projects[tokenId].methodology = newMethodology;
        projects[tokenId].region = newRegion;

        require(
            metaDataHashes[newMetaDataHash] == false,
            'Error: metaDataHash already exists'
        );
        string memory oldHash = projects[tokenId].metaDataHash;

        projects[tokenId].metaDataHash = newMetaDataHash;
        metaDataHashes[newMetaDataHash] = true;
        metaDataHashes[oldHash] = false;

        emit ProjectUpdated(tokenId);
    }

    /// @dev Projects and their projectId's must be unique, changing them must be handled carefully
    function updateProjectId(uint256 tokenId, string memory newProjectId)
        external
        virtual
        onlyOwner
        whenNotPaused
    {
        require(_exists(tokenId), 'Project not yet minted');
        require(
            projectIds[newProjectId] == false,
            'Cant change current projectId to an existing one'
        );

        string memory oldProjectId = projects[tokenId].projectId;
        projectIds[oldProjectId] == false;

        projects[tokenId].projectId = newProjectId;
        projectIds[newProjectId] == true;

        emit ProjectIdUpdated(tokenId);
    }

    function removeProject(uint256 tokenId)
        external
        virtual
        onlyOwner
        whenNotPaused
    {
        delete projects[tokenId];
    }

    // Retrieve all data from ProjectNFT struct
    // Currently  not used
    function getProjectDataByTokenId(uint256 tokenId)
        external
        view
        virtual
        returns (
            string memory,
            string memory,
            string memory,
            string memory,
            string memory,
            string memory
        )
    {
        return (
            projects[tokenId].projectId,
            projects[tokenId].methodology,
            projects[tokenId].standard,
            projects[tokenId].region,
            projects[tokenId].metaDataHash,
            methodologyToStorageMethod[projects[tokenId].methodology]
        );
    }

    // Retrieve data from ProjectNFT relevant for pools
    function getProjectDataByProjectId(string memory projectId)
        external
        view
        virtual
        returns (
            string memory,
            string memory,
            string memory,
            string memory
        )
    {
        uint256 tokenId = pidToTokenId[projectId];
        return (
            projects[tokenId].standard,
            projects[tokenId].methodology,
            projects[tokenId].region,
            methodologyToStorageMethod[projects[tokenId].methodology]
        );
    }

    /**
     * @dev Returns the Uniform Resource Identifier (URI) for `tokenId` token.
     * based on the ERC721URIStorage implementation
     */
    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            'ERC721URIStorage: URI query for nonexistent token'
        );

        string memory metaDataHash = projects[tokenId].metaDataHash;
        string memory base = _baseURI();

        // If there is no base URI, return the token URI.
        if (bytes(base).length == 0) {
            return metaDataHash;
        }
        // If both are set, concatenate the baseURI and tokenURI (via abi.encodePacked).
        if (bytes(metaDataHash).length > 0) {
            return string(abi.encodePacked(base, metaDataHash));
        }

        return super.tokenURI(tokenId);
    }

    function memcmp(bytes memory a, bytes memory b)
        internal
        pure
        returns (bool)
    {
        return (a.length == b.length) && (keccak256(a) == keccak256(b));
    }

    function strcmp(string memory a, string memory b)
        internal
        pure
        returns (bool)
    {
        return memcmp(bytes(a), bytes(b));
    }
}

