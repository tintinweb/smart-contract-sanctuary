pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "./interface/IDMintV39.sol";

contract DRewardV11 is Ownable, VRFConsumerBase {
    using SafeMath for uint256; // Where is going to be used ?

    enum PrizeType {
        GrandPrize,
        RunnerUpPrize
    }

    struct PrizeWinner {
        uint256 level;
        address tokenHolder;
        uint256 timestamp; // block.timestamp
        PrizeType prizeType;
    }

    mapping(uint256 => PrizeWinner[]) public prizeWinners;

    struct RewardPrize {
        uint256 rewardMilestone; // 150 , 400 , ...
        uint256 grandPrizeValueAmount; // 1 ETH
        uint256 numberRunnerUpPrizes; // 2 ETH, 7 ETH, 19 ETH
        uint256 runnerUpPrizeValueAmount; // 0.5 ETH , 0.2 ETH, 0.08 ETH
    }

    uint256 internal constant _RNG_PRIME = 97;
    address public dMintAddress;
    address public charityAddress;

    uint256 public currentLevel;
    uint256[12] public randomResults; // Array of random numbers, 1 for each level

    RewardPrize[12] public rewardPrizes;
    // Generate Prizes once by level
    mapping(uint256 => bool) public generatedPrizeByLevel;

    bytes32 internal keyHash;
    uint256 internal fee;
    bytes32 public requestId;
    bytes32 public init_requestId;

    IERC20 public rewardToken;

    event GotRandom(bytes32 indexed requestId, uint256 indexed result);
    event GenerateGrandPrizeLog(uint256 indexed tokenId, uint256 indexed level);
    event Log(string message);
    event LogTotalSupply(uint256 indexed _totaSupply, uint256 indexed _level);

    // PROD ONES
    /*
        VRFConsumerBase(
            0x3d2341ADb2D31f1c5530cDC622016af293177AE0, // VRF Coordinator Polygon MainNet
            0xb0897686c545045aFc77CF20eC7A532E3120E0F1 // LINK Token Polygon MainNet
        );
        keyHash = 0xf86195cf7690c55907b2b611ebb7343a6f649bff128701cc542f0569e2c549da; // Polygon MainNet
        */

    // DEV ONES
    /*
        VRFConsumerBase(
            0x8C7382F9D8f56b33781fE506E897a4F1e2d17255, // VRF Coordinator Mumbai
            0x326C977E6efc84E512bB9C30f76E30c160eD06FB // LINK Token Mumbai
        );
        keyHash = 0x6e75b569a01ef56d18cab6a8e71e6600d6ce853834d4a5748b720d06f878b3a4; // Mumbai
        */

    constructor()
        VRFConsumerBase(
            0x8C7382F9D8f56b33781fE506E897a4F1e2d17255, // VRF Coordinator Mumbai
            0x326C977E6efc84E512bB9C30f76E30c160eD06FB // LINK Token Mumbai
        )
    {
        rewardPrizes[0] = RewardPrize(150, 1 ether, 2, 0.5 ether);
        rewardPrizes[1] = RewardPrize(400, 1 ether, 7, 0.5 ether);
        rewardPrizes[2] = RewardPrize(1000, 1 ether, 19, 0.3 ether);
        rewardPrizes[3] = RewardPrize(2000, 1 ether, 39, 0.2 ether);
        rewardPrizes[4] = RewardPrize(3000, 1 ether, 59, 0.15 ether);
        rewardPrizes[5] = RewardPrize(4000, 1 ether, 79, 0.125 ether);
        rewardPrizes[6] = RewardPrize(5000, 1 ether, 99, 0.1 ether);
        rewardPrizes[7] = RewardPrize(6000, 1 ether, 119, 0.08 ether);
        rewardPrizes[8] = RewardPrize(7000, 1 ether, 139, 0.07 ether);
        rewardPrizes[9] = RewardPrize(8000, 1 ether, 159, 0.07 ether);
        rewardPrizes[10] = RewardPrize(9000, 1 ether, 179, 0.06 ether);
        rewardPrizes[11] = RewardPrize(10000, 1 ether, 199, 0.06 ether);

        //keyHash = 0xf86195cf7690c55907b2b611ebb7343a6f649bff128701cc542f0569e2c549da; // Polygon MainNet
        keyHash = 0x6e75b569a01ef56d18cab6a8e71e6600d6ce853834d4a5748b720d06f878b3a4; // Mumbai

        // Address of DigiBudz smart contract ( V39 )
        dMintAddress = address(0x9a4915aabc0f69c5983A55b97BdF00fC9aC42641);

        // This is currently set to one of jonathans addresses
        // By default we should set it to our gnosis charity address
        // if we haven't figured one out. Gnosis address 0x68045D654cB56178f9ED48401dEECA6286dAA301
        charityAddress = address(0xc8Dd0a3A62f5604C96aDE697A32293B9Fa1244B2);

        //fee = 100000000000000; // 0.0001 LINK
        fee = 0.0001 * 10**18; // 0.0001 LINK

        // WETH 0x7ceB23fD6bC0adD59E62ac25578270cFf1b9f619
        address _rewardToken = address(
            0xaf3dd790404f0ae9AF0142838031FF21cDd20846
        );
        rewardToken = IERC20(_rewardToken);
    }

    // https://docs.chain.link/docs/chainlink-vrf-best-practices/
    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 _requestId, uint256 _randomness)
        internal
        override
    {
        requestId = _requestId;

        randomResults[currentLevel + 1] = _randomness;

        _generateGrandPrizeWinner(
            rewardPrizes[currentLevel + 1].rewardMilestone
        );

        emit GotRandom(requestId, _randomness);
    }

    function setCharityAddress(address _charityAddress) external onlyOwner {
        require(_charityAddress != address(0), "Not Valid Address");
        charityAddress = _charityAddress;
    }

    /**
     * Internal Requests randomness
     */
    function _getRandomNumber() internal returns (bytes32 _requestId) {
        require(LINK.balanceOf(address(this)) >= fee, "Fund with LINK");
        init_requestId = requestRandomness(keyHash, fee);
        return init_requestId;
    }

    /**
     * Requests randomness
     */
    function getRandomNumber(uint256 _rewardLevel)
        external
        onlyOwner
        returns (bytes32 _requestId)
    {
        require(currentLevel + 1 == _rewardLevel, "Wrong level");

        require(randomResults[_rewardLevel] == 0, "Random already generated");

        uint256 totalSupply = IDMintV39(dMintAddress).totalSupply();

        require(
            totalSupply >= rewardPrizes[_rewardLevel].rewardMilestone,
            "Cannot generate level"
        );

        // Don't update here use in _generateGrandPrizeWinner() on callback instead
        // currentLevel = _rewardLevel;
        emit LogTotalSupply(totalSupply, _rewardLevel);
        return _getRandomNumber();
    }

    function _getRandomTokenID(uint256 seed, uint256 upper_limit)
        internal
        pure
        returns (uint256)
    {
        return (seed % upper_limit) + 1;
    }

    function _generateGrandPrizeWinner(uint256 upper_limit) internal {
        uint256 tokenId = _getRandomTokenID(
            randomResults[currentLevel + 1],
            upper_limit
        );

        prizeWinners[tokenId].push(
            PrizeWinner(
                currentLevel + 1,
                address(0),
                block.timestamp,
                PrizeType.GrandPrize
            )
        );

        currentLevel += 1;
        emit GenerateGrandPrizeLog(tokenId, currentLevel);
    }

    function generateRunnerUpPrizes(uint256 _level) external onlyOwner {
        require(
            randomResults[_level] != 0,
            "Random number not generated for this level."
        );
        require(!generatedPrizeByLevel[_level], "Re-Generate not allowed");

        // Mark as generated RunnerUpPrizes for this level
        generatedPrizeByLevel[_level] = true;

        for (
            uint256 prize_count = 0;
            prize_count < rewardPrizes[_level].numberRunnerUpPrizes;
            prize_count++
        ) {
            uint256 tokenId = _getRandomTokenID(
                (_RNG_PRIME * prize_count + 1) + randomResults[_level],
                rewardPrizes[_level].rewardMilestone
            );

            prizeWinners[tokenId].push(
                PrizeWinner(
                    _level,
                    address(0),
                    block.timestamp,
                    PrizeType.RunnerUpPrize
                )
            );
        }
    }

    function getHolderAddress(uint256 _tokenId)
        internal
        view
        returns (address)
    {
        address holderAddress = IDMintV39(dMintAddress).ownerOf(_tokenId);
        return holderAddress;
    }

    function _transferPrizes(
        uint256 donationPercent,
        uint256 rewardAmount,
        address _tokenIdHolderAddress
    ) internal returns (uint256) {
        uint256 donationAmount;

        if (donationPercent > 0) {
            donationAmount = (rewardAmount * donationPercent) / 100;

            //DigiBudz will match up to 25%
            if (donationPercent > 25) {
                uint256 matchAmount = (rewardAmount * 25) / 100;
                rewardToken.transfer(charityAddress, matchAmount);
            } else {
                rewardToken.transfer(charityAddress, donationAmount);
            }

            rewardToken.transfer(charityAddress, donationAmount);
        }
        rewardAmount -= donationAmount;
        rewardToken.transfer(_tokenIdHolderAddress, rewardAmount);

        return rewardAmount;
    }

    function claimRewardAll(uint256 _tokenId, uint256 donationPercent)
        external
        returns (uint256)
    {
        // We check that _tokenId is not greater that currentlevel rewardMilestone value
        require(
            _tokenId > 0 &&
                _tokenId <= rewardPrizes[currentLevel].rewardMilestone,
            "tokenID beyond rewardMilestone"
        );

        address tokenIdHolderAddress = getHolderAddress(_tokenId);
        require(
            msg.sender == tokenIdHolderAddress,
            "You don't own this tokenID"
        );

        uint256 rewardAmount;

        for (
            uint256 index = 0;
            index < prizeWinners[_tokenId].length;
            index++
        ) {
            if (prizeWinners[_tokenId][index].tokenHolder != address(0)) {
                continue;
            } else if (
                prizeWinners[_tokenId][index].prizeType ==
                PrizeType.RunnerUpPrize
            ) {
                // RunnerUpPrize
                rewardAmount += rewardPrizes[
                    prizeWinners[_tokenId][index].level
                ].runnerUpPrizeValueAmount;

                prizeWinners[_tokenId][index]
                    .tokenHolder = tokenIdHolderAddress; // Update claim
            } else {
                // GrandPrize
                rewardAmount += rewardPrizes[
                    prizeWinners[_tokenId][index].level
                ].grandPrizeValueAmount;

                prizeWinners[_tokenId][index]
                    .tokenHolder = tokenIdHolderAddress; // Update claim
            }
        }

        if (rewardAmount > 0) {
            rewardAmount = _transferPrizes(
                donationPercent,
                rewardAmount,
                tokenIdHolderAddress
            );
        } else {
            revert("Nothing to claim");
        }

        return rewardAmount;
    }

    function claimRewardByLevel(
        uint256 _tokenId,
        uint256 _level,
        uint256 donationPercent
    ) external returns (uint256) {
        require(_level <= currentLevel, "Not valid level");

        // We check that _tokenId is not greater that currentlevel rewardMilestone value
        require(
            _tokenId > 0 &&
                _tokenId <= rewardPrizes[currentLevel].rewardMilestone,
            "tokenID beyond rewardMilestone"
        );

        address tokenIdHolderAddress = getHolderAddress(_tokenId);
        require(
            msg.sender == tokenIdHolderAddress,
            "You don't own this tokenID"
        );

        uint256 rewardAmount;

        for (
            uint256 index = 0;
            index < prizeWinners[_tokenId].length;
            index++
        ) {
            if (prizeWinners[_tokenId][index].tokenHolder != address(0)) {
                continue;
            } else if (prizeWinners[_tokenId][index].level == _level) {
                if (
                    prizeWinners[_tokenId][index].prizeType ==
                    PrizeType.RunnerUpPrize
                ) {
                    // RunnerUpPrize
                    rewardAmount += rewardPrizes[
                        prizeWinners[_tokenId][index].level
                    ].runnerUpPrizeValueAmount;

                    prizeWinners[_tokenId][index]
                        .tokenHolder = tokenIdHolderAddress; // Update claim
                } else {
                    // GrandPrize
                    rewardAmount += rewardPrizes[
                        prizeWinners[_tokenId][index].level
                    ].grandPrizeValueAmount;

                    prizeWinners[_tokenId][index]
                        .tokenHolder = tokenIdHolderAddress; // Update claim
                }
            }
        }

        if (rewardAmount > 0) {
            rewardAmount = _transferPrizes(
                donationPercent,
                rewardAmount,
                tokenIdHolderAddress
            );
        } else {
            revert("Nothing to claim");
        }

        return rewardAmount;
    }
}

