pragma solidity ^0.8.0;
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./NFTTransfer.sol";

contract PuzzleBattle is
    Ownable,
    ERC721Holder,
    ERC1155Holder,
    ReentrancyGuard,
    NFTTransfer
{
    using ECDSA for bytes32;
    using ERC165Checker for address;
    using SafeERC20 for IERC20;

    enum GameStatus {
        CREATED,
        CANCELED,
        STARTED,
        COMPLETE,
        COMPLETE_NO_WINNER
    }

    struct GameConfiguration {
        uint256 entryPrice;
        uint16 numberOfPieces; // total number of pieces
        uint256 drawPrice;
        uint16 minPlayersCount;
        uint16 maxPlayersCount;
        uint256 startTime;
        uint256 maxDuration;
        uint256 minDuration;
        uint256 drawThreshold;
        uint256 bidPrice;
        address creator;
        Reward reward; // e.g. NFT being played
        uint256 gameServerId;
    }

    struct Reward {
        address contractAddress;
        uint256 tokenId;
        uint256 chainId;
        bool bridge; // is reward controlled by this contract or custodian through the bridge
    }

    struct CreateGameRequest {
        uint256 entryPrice;
        uint16 numberOfPieces;
        uint256 drawPrice;
        uint16 minPlayersCount;
        uint16 maxPlayersCount;
        uint256 startTime;
        uint256 maxDuration;
        uint256 minDuration;
        uint256 drawThreshold;
    }

    struct PlayerInfo {
        bool initialized;
        bool[] pieces;
        uint16 drawCount;
        uint16 uniquePieces;
        bool fundsClaimed;
    }

    struct GameState {
        mapping(address => PlayerInfo) playerInfos; // player address => Player info
        mapping(uint16 => uint16) drawnPieces; // piece_id => drawn piece count
        uint16 playersCount;
        uint16 drawCount;
        bool complete;
        address winner;
        bool nftTransferedOut;
    }

    struct Lot {
        uint256 gameId;
        uint16 pieceId;
        address seller;
        uint256 highestBid;
        address highestBidder;
        uint256 highestBidWinTime;
    }

    event GameCreated(uint256 indexed gameId, address indexed creator);
    event Drawn(
        uint256 indexed gameId,
        address indexed player,
        uint16 indexed piece
    );
    event GameWon(uint256 indexed gameId, address indexed winner);
    event Cashback(uint256 indexed gameId, address indexed player); // cachback to player
    event Rollback(uint256 indexed gameId, address indexed player); // rollback to player
    event RollbackNFT(uint256 indexed gameId); // NFT has been returned to the Provider ( internal Custody flow )
    event JoinedGame(uint256 indexed gameId, address indexed player); // player Joined the game
    event NewLot(uint256 indexed lotId);
    event Bid(
        uint256 indexed lotId,
        address indexed player,
        uint256 indexed amount
    );
    event Claim(uint256 indexed lotId); // piece is transferred to the winner

    address public PlatformToken;

    uint256 public nextGameId;

    uint16 public minPlayersCount = 10;
    uint16 public minNumberOfPieces = 4;
    uint256 public minEntryPrice = 1e18;
    uint256 public minBidPrice = 1e18;
    uint256 public minDrawPrice = 1e18;
    uint256 public minDuration = 10 minutes;
    uint256 public maxDuration = 1 weeks;
    uint256 public maxDrawThreshold = 12 hours;
    uint256 public minDrawThreshold = 1 minutes;
    uint256 public minStartInterval = 10 minutes;
    uint256 public lotWindelay = 5 minutes;
    address public Signer;

    uint256 private _nftProviderRewardPercent = 85;
    address private _system;
    uint256 private _auctionId;

    mapping(uint256 => Lot) public auction;
    mapping(uint256 => GameConfiguration) public games;
    mapping(uint256 => GameState) private gameStates;

    // contract address -> token -> seq
    mapping(address => mapping(uint256 => uint256)) public bridgeSeq;
    mapping(uint256 => mapping(address => uint256)) public drawSeq;

    constructor(address token, address system, address signer) {
        PlatformToken = token;
        _system = system;
        Signer = signer;
    }

    function createFromBridge(
        CreateGameRequest memory createGame,
        address rewardAddress,
        uint256 rewardTokenId,
        uint256 chainId,
        uint256 seq,
        uint256 signatureExpirationTime,
        uint256 gameId,
        bytes memory signature
    ) external {
        require(
            bridgeSeq[rewardAddress][rewardTokenId] == seq,
            "createBridge: wrong seq"
        );
        require(
            signatureExpirationTime > block.timestamp,
            "createFromBridge: signature expired"
        );
        bytes32 hash = keccak256(
            abi.encodePacked(
                rewardAddress,
                rewardTokenId,
                chainId,
                seq,
                msg.sender,
                signatureExpirationTime,
                address(this),
                gameId
            )
        );
        bytes32 hashEth = hash.toEthSignedMessageHash();
        require(
            hashEth.recover(signature) == Signer,
            "createBridge: wrong signature"
        );

        bridgeSeq[rewardAddress][rewardTokenId]++;

        _create(createGame, rewardAddress, rewardTokenId, chainId, true, gameId);
    }

    // create game with internally controlled Reward (no custodian and no bridge)
    function create(
        CreateGameRequest memory createGame,
        address rewardAddress,
        uint256 rewardTokenId
    ) external nonReentrant {
        _transfer(rewardAddress, rewardTokenId, msg.sender, address(this), 1);
        _create(createGame, rewardAddress, rewardTokenId, block.chainid, false, 0);
    }

    function join(uint256 gameId) external {
        GameState storage gameState = gameStates[gameId];
        GameConfiguration memory game = games[gameId];
        GameStatus status = gameStatus(gameId);

        require(
            gameState.playersCount < game.maxPlayersCount,
            "join: too many players"
        );
        require(
            status == GameStatus.CREATED,
            "join: game has already been started"
        );
        require(
            !gameState.playerInfos[msg.sender].initialized,
            "join: already joined"
        );

        IERC20(PlatformToken).safeTransferFrom(
            msg.sender,
            address(this),
            game.entryPrice
        );

        gameState.playersCount++;
        gameState.playerInfos[msg.sender].initialized = true;

        gameState.playerInfos[msg.sender].pieces = new bool[](
            game.numberOfPieces
        );
        emit JoinedGame(gameId, msg.sender);
    }

    function gameStatus(uint256 gameId) public view returns (GameStatus) {
        GameConfiguration memory game = games[gameId];

        if (game.startTime > block.timestamp) return GameStatus.CREATED;

        GameState storage gameState = gameStates[gameId];

        if (gameState.complete) return GameStatus.COMPLETE;

        if (game.startTime + game.maxDuration < block.timestamp)
            return GameStatus.COMPLETE_NO_WINNER;

        if (
            game.startTime < block.timestamp &&
            gameState.playersCount < game.minPlayersCount
        ) return GameStatus.CANCELED;

        if (
            game.startTime < block.timestamp &&
            gameState.playersCount >= game.minPlayersCount
        ) return GameStatus.STARTED;

        revert("gameStatus: unknow state");
    }
    
    function draw(
        uint256 gameId,
        uint256 seed,
        uint8 multipleCount,
        uint256 seq,
        bytes memory signature
    ) external nonReentrant {
        GameState storage gameState = gameStates[gameId];
        PlayerInfo storage player = gameState.playerInfos[msg.sender];
        GameConfiguration memory game = games[gameId];

        GameStatus status = gameStatus(gameId);
        require(status == GameStatus.STARTED, "draw: wrong game status");
        require(
            game.startTime +
                (player.drawCount + multipleCount - 1) *
                game.drawThreshold <
                block.timestamp,
            "draw: too often"
        );
        require(
            gameState.drawCount <
                (game.numberOfPieces *
                    gameState.playersCount +
                    multipleCount -
                    1),
            "draw: no items in pool"
        );

        require(drawSeq[gameId][msg.sender] == seq, "draw: wrong seq");
        bytes32 hash = keccak256(
            abi.encodePacked(gameId, seed, multipleCount, seq, msg.sender)
        );
        bytes32 hashEth = hash.toEthSignedMessageHash();
        require(hashEth.recover(signature) == Signer, "draw: wrong signature");
        uint8 i = 0;
        for (
            ;
            i < multipleCount && player.uniquePieces < game.numberOfPieces;
            i++
        ) {
            uint16 piecePoolId = uint16(
                (uint256(hash) %
                    (game.numberOfPieces *
                        gameState.playersCount -
                        gameState.drawCount))
            );
            uint8 piece = 0;
            for (; piece < game.numberOfPieces; piece++) {
                if (
                    gameState.playersCount - gameState.drawnPieces[piece] >
                    piecePoolId
                ) break;

                piecePoolId -=
                    gameState.playersCount -
                    gameState.drawnPieces[piece];
            }
            gameState.drawnPieces[piece]++;
            gameState.drawCount++;
            if (!player.pieces[piece]) {
                player.pieces[piece] = true;
                player.uniquePieces++;
            } else {
                _sell(gameId, piece, game.drawPrice, msg.sender);
            }
            player.drawCount++;
            drawSeq[gameId][msg.sender]++;
            emit Drawn(gameId, msg.sender, piece);
        }
        IERC20(PlatformToken).safeTransferFrom(
            msg.sender,
            address(this),
            game.drawPrice * i
        );
        _checkOrCompleteGame(gameId, msg.sender);
    }
    
    function sellAll(uint256 gameId) external {
        GameState storage gameState = gameStates[gameId];
        PlayerInfo storage player = gameState.playerInfos[msg.sender];
        GameConfiguration memory game = games[gameId];
        for (uint16 piece; piece < game.numberOfPieces; piece++) {
            if(player.pieces[piece]){
                _sell(gameId, piece, game.drawPrice, msg.sender);
            }
        }
    }

    function sell(uint256 gameId, uint16 pieceId) external {
        GameStatus status = gameStatus(gameId);
        require(status == GameStatus.STARTED, "sell: wrong game status");

        GameState storage gameState = gameStates[gameId];
        GameConfiguration memory game = games[gameId];
        PlayerInfo storage player = gameState.playerInfos[msg.sender];
        require(player.pieces[pieceId], "sell: no pieces");
        player.pieces[pieceId] = false;
        if (!player.pieces[pieceId]) player.uniquePieces--;

        _sell(gameId, pieceId, game.drawPrice, msg.sender);
    }

    function _sell(
        uint256 gameId,
        uint16 pieceId,
        uint256 drawPrice,
        address seller
    ) private {
        uint256 id = _auctionId++;
        GameConfiguration memory game = games[gameId];

        uint256 lastGlobalBidTime = game.startTime +
            game.maxDuration -
            lotWindelay *
            2;

        auction[id] = Lot(
            gameId,
            pieceId,
            seller,
            drawPrice * 3, // TODO: make configurable
            address(0x0),
            lastGlobalBidTime
        );

        emit NewLot(id);
    }

    function bid(uint256 lotId, uint256 amount) external {
        Lot storage lot = auction[lotId];
        GameStatus status = gameStatus(lot.gameId);
        require(status == GameStatus.STARTED, "bid: wrong game status");

        GameConfiguration memory game = games[lot.gameId];
        if (lot.highestBidder != address(0x0))
            require(
                lot.highestBidWinTime > block.timestamp,
                "bid: auction already finished"
            );
        GameState storage gameState = gameStates[lot.gameId];
        require(
            !gameState.playerInfos[msg.sender].pieces[lot.pieceId],
            "bid: can't bid on duplicate piece"
        );
        require(amount > lot.highestBid, "bid: amount too small");
        require(
            amount - lot.highestBid >= game.bidPrice,
            "bid: bid increase step too small"
        );

        IERC20(PlatformToken).safeTransferFrom(
            msg.sender,
            address(this),
            amount
        );

        if (lot.highestBidder != address(0x0))
            IERC20(PlatformToken).safeTransfer(
                lot.highestBidder,
                lot.highestBid
            );
        lot.highestBidder = msg.sender;
        lot.highestBid = amount;
        uint256 delay = lotWindelay;
        if (game.drawThreshold > delay) delay = game.drawThreshold;
        uint256 lastGlobalBidTime = game.startTime +
            game.maxDuration -
            delay *
            2;

        uint256 lotWinTime = block.timestamp + delay;
        if (lotWinTime > lastGlobalBidTime) {
            lotWinTime = lastGlobalBidTime;
        }
        lot.highestBidWinTime = lotWinTime;

        emit Bid(lotId, msg.sender, amount);
    }

    function claim(uint256 lotId) external {
        Lot storage lot = auction[lotId];
        require(lot.highestBidder != address(0x0), "claim: incorrect lot");

        GameConfiguration memory game = games[lot.gameId];
        uint256 delay = lotWindelay;
        if (game.drawThreshold > delay) delay = game.drawThreshold;

        require(
            lot.highestBidWinTime < block.timestamp,
            "claim: auction not complete for the lot"
        );

        uint256 sellerReward = (lot.highestBid * 90) / 100; // TODO: make configurable
        IERC20(PlatformToken).safeTransfer(lot.seller, sellerReward);
        IERC20(PlatformToken).safeTransfer(
            _system,
            lot.highestBid - sellerReward
        );

        GameState storage gameState = gameStates[lot.gameId];
        PlayerInfo storage player = gameState.playerInfos[lot.highestBidder];
        if (!player.pieces[lot.pieceId]) {
            player.uniquePieces++;
            player.pieces[lot.pieceId] = true;
        } else {
            _sell(lot.gameId, lot.pieceId, game.drawPrice, lot.seller);
        }

        _checkOrCompleteGame(lot.gameId, lot.highestBidder);
        emit Claim(lotId);
    }

    function rollbackNft(uint256 gameId) external {
        GameStatus status = gameStatus(gameId);
        require(
            status == GameStatus.COMPLETE_NO_WINNER,
            "rollback: wrong game status"
        );
        GameState storage gameState = gameStates[gameId];
        require(!gameState.nftTransferedOut, "rollback: nft transfered");
        GameConfiguration memory game = games[gameId];
        if (!game.reward.bridge) {
            _transfer(
                game.reward.contractAddress,
                game.reward.tokenId,
                address(this),
                game.creator,
                1
            );
            gameState.nftTransferedOut = true;
            emit RollbackNFT(gameId);
        }
    }

    function rollback(uint256 gameId) external {
        GameStatus status = gameStatus(gameId);
        require(
            status == GameStatus.COMPLETE_NO_WINNER,
            "rollback: wrong game status"
        );

        GameState storage gameState = gameStates[gameId];
        PlayerInfo storage player = gameState.playerInfos[msg.sender];
        GameConfiguration memory game = games[gameId];
        require(!player.fundsClaimed, "rollback: already claimed");

        player.fundsClaimed = true;
        uint256 amount = uint256(player.drawCount * game.drawPrice) +
            game.entryPrice;
        IERC20(PlatformToken).safeTransfer(msg.sender, amount);
        emit Rollback(gameId, msg.sender);
    }

    function cashback(uint256 gameId) external {
        GameStatus status = gameStatus(gameId);
        require(status == GameStatus.COMPLETE, "cashback: wrong game status");

        GameState storage gameState = gameStates[gameId];
        PlayerInfo storage player = gameState.playerInfos[msg.sender];
        GameConfiguration memory game = games[gameId];
        require(
            msg.sender != gameState.winner,
            "cashback: winner is not eligible for cashback"
        );
        require(!player.fundsClaimed, "cashBack: already claimed");

        player.fundsClaimed = true;
        uint256 amount = uint256(player.drawCount * game.drawPrice) / 2;
        IERC20(PlatformToken).safeTransfer(msg.sender, amount);
        emit Cashback(gameId, msg.sender);
    }

    function _checkOrCompleteGame(uint256 gameId, address winner)
        private
        returns (bool result)
    {
        GameState storage gameState = gameStates[gameId];
        PlayerInfo storage player = gameState.playerInfos[winner];
        GameConfiguration memory game = games[gameId];
        result = player.uniquePieces >= game.numberOfPieces;
        if (result) {
            gameState.complete = true;
            gameState.winner = winner;
            emit GameWon(gameId, winner);

            uint256 winnerSpendForDraw = player.drawCount * game.drawPrice;
            uint256 foundsCollected = gameState.playersCount *
                game.entryPrice +
                winnerSpendForDraw +
                (gameState.drawCount * game.drawPrice - winnerSpendForDraw) /
                2;
            uint256 nftProviderReward = (foundsCollected *
                _nftProviderRewardPercent) / 100;
            uint256 systemReward = foundsCollected - nftProviderReward;
            IERC20(PlatformToken).safeTransfer(game.creator, nftProviderReward);
            IERC20(PlatformToken).safeTransfer(_system, systemReward);

            if (!game.reward.bridge) {
                _transfer(
                    game.reward.contractAddress,
                    game.reward.tokenId,
                    address(this),
                    winner,
                    1
                );
                gameState.nftTransferedOut = true;
            }
        }
    }

    function _create(
        CreateGameRequest memory createGame,
        address rewardAddress,
        uint256 rewardTokenId,
        uint256 chainId,
        bool bridge,
        uint256 gameServerId
    ) private {
        if (createGame.entryPrice < minEntryPrice) {
            createGame.entryPrice = minEntryPrice;
        }
        if (createGame.drawPrice < minDrawPrice) {
            createGame.drawPrice = minDrawPrice;
        }
        if (createGame.minPlayersCount < minPlayersCount) {
            createGame.minPlayersCount = minPlayersCount;
        }
        if (createGame.maxPlayersCount < createGame.minPlayersCount) {
            createGame.maxPlayersCount = createGame.minPlayersCount;
        }
        if (createGame.minDuration < minDuration) {
            createGame.minDuration = minDuration;
        }
        if (createGame.maxDuration > maxDuration) {
            createGame.maxDuration = maxDuration;
        }
        if (createGame.maxDuration < createGame.minDuration) {
            createGame.maxDuration = createGame.minDuration;
        }
        if (createGame.drawThreshold > maxDrawThreshold) {
            createGame.drawThreshold = maxDrawThreshold;
        }
        if (createGame.drawThreshold < minDrawThreshold) {
            createGame.drawThreshold = minDrawThreshold;
        }
        if (createGame.numberOfPieces < minNumberOfPieces) {
            createGame.numberOfPieces = minNumberOfPieces;
        }
        if (createGame.startTime < block.timestamp + minStartInterval) {
            createGame.startTime = block.timestamp + minStartInterval;
        }
        
        uint256 bidPrice = createGame.drawPrice;
        if(bidPrice<minBidPrice)
            bidPrice = minBidPrice;
        GameConfiguration memory game = GameConfiguration(
            createGame.entryPrice,
            createGame.numberOfPieces * createGame.numberOfPieces,
            createGame.drawPrice,
            createGame.minPlayersCount,
            createGame.maxPlayersCount,
            createGame.startTime,
            createGame.maxDuration,
            createGame.minDuration,
            createGame.drawThreshold,
            bidPrice,
            msg.sender,
            Reward(rewardAddress, rewardTokenId, chainId, bridge),
            gameServerId
        );
        uint256 gameId = nextGameId++;
        games[gameId] = game;
        emit GameCreated(gameId, msg.sender);
    }

    function setMinPlayersCount(uint16 _minPlayersCount) external onlyOwner {
        minPlayersCount = _minPlayersCount;
    }

    function setMinBidPrice(uint16 _minBidPrice) external onlyOwner {
        minBidPrice = _minBidPrice;
    }

    function setMinNumberOfPieces(uint16 _minNumberOfPieces)
        external
        onlyOwner
    {
        minNumberOfPieces = _minNumberOfPieces;
    }
    
    function setMinEntryPrice(uint256 _minEntryPrice)
        external
        onlyOwner
    {
        minEntryPrice = _minEntryPrice;
    }

    function setMinDrawPrice(uint256 _minDrawPrice)
        external
        onlyOwner
    {
        minDrawPrice = _minDrawPrice;
    }

    function setMinDuration(uint256 _minDuration)
        external
        onlyOwner
    {
        minDuration = _minDuration;
    }

    function setMaxDuration(uint256 _maxDuration)
        external
        onlyOwner
    {
        maxDuration = _maxDuration;
    }

    function setMaxDrawThreshold(uint256 _maxDrawThreshold)
        external
        onlyOwner
    {
        maxDrawThreshold = _maxDrawThreshold;
    }

    function setMinDrawThreshold(uint256 _minDrawThreshold)
        external
        onlyOwner
    {
        minDrawThreshold = _minDrawThreshold;
    }
    
    function setMinStartInterval(uint256 _minStartInterval)
        external
        onlyOwner
    {
        minStartInterval = _minStartInterval;
    }
    
    function setLotWindelay(uint256 _lotWindelay)
        external
        onlyOwner
    {
        lotWindelay = _lotWindelay;
    }
    
    function setNftProviderRewardPercent(uint256 nftProviderRewardPercent)
        external
        onlyOwner
    {
        _nftProviderRewardPercent = nftProviderRewardPercent;
    }

    function setSigner(address signer)
        external
        onlyOwner
    {
        Signer = signer;
    }

    function setSystem(address system)
        external
        onlyOwner
    {
        _system = system;
    }
}

