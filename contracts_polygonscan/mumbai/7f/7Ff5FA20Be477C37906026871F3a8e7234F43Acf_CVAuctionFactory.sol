pragma solidity ^0.8.10;
////import "@openzeppelin/contracts/access/Ownable.sol";
////import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
////import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
////import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
////import "@openzeppelin/contracts/security/Pausable.sol";
////import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
////import "@openzeppelin/contracts/utils/Address.sol";

contract StandardAuctionV2 is Ownable, Pausable, ReentrancyGuard {
    event Start();
    event Bid(address indexed sender, uint256 amount);
    event Withdraw(address indexed bidder, uint256 amount);
    event End(address winner, uint256 amount);
    event PaymentReleased(address to, uint256 amount);
    event PayeeAdded(address account, uint256 shares);

    IERC1155 public nft1155;
    IERC721 public nft721; 

    address public nft;
    uint256 public nftId;
    uint256 public nftAmount;

    address payable public seller;
    uint256 public endAt;
    uint256 public startAt;
    bool public started;
    bool public ended;

    address public highestBidder;
    uint256 public highestBid;
    uint256 public startingBid;
    mapping(address => uint256) public bids;

    address[] fPayees;
    uint256[] fShares;
    address[] private payees;
    mapping(address => uint256) private shares_;
    mapping(address => uint256) private _released;
    uint256 private _totalShares;
    uint256 private _totalReleased;

    constructor(
        address _nft,
        uint256 _nftId,
        uint256 _nftAmount,
        uint256 _startingBid, 
        uint256 _endAt, 
        uint256 _startAt,
        address[] memory _payees, 
        uint256[] memory _shares
        
    ) { 
        if (ERC165Checker.supportsInterface(_nft, 0x80ac58cd)) {
            nft721 = IERC721(_nft);
        } else if(ERC165Checker.supportsInterface(_nft, 0xd9b67a26)) {
            nft1155 = IERC1155(_nft);
        } else {
            revert("NFT not supported");
        }
        nft = _nft;
        nftId = _nftId;
        nftAmount = _nftAmount;
        endAt = _endAt;
        startAt = _startAt;

        seller = payable(msg.sender);
        startingBid = _startingBid;
        highestBid = 0;

        require(_payees.length == _shares.length, "PaymentSplitter: payees and shares length mismatch");
        require(_payees.length > 0, "PaymentSplitter: no payees");
        for (uint256 i = 0; i < _payees.length; i++) {
            _addPayee(_payees[i], _shares[i]);
        }
        fPayees = _payees;
        fShares = _shares;
    }


    function start() external onlyOwner whenNotPaused {
        require(endAt > startAt, "Auction must end after it starts");
        require(!started, "the auction already started by the seller");
        require(msg.sender == seller, "sorry you are not the seller");
        if (ERC165Checker.supportsInterface(nft, 0x80ac58cd)) {
            require(nft721.balanceOf(msg.sender) >= 1, "auction can't be started as you don't have the nft");
        } else {
            require(nft1155.balanceOf(msg.sender, nftId) >= 1, "auction can't be started as you don't have the nft");
        }

        started = true; 

        emit Start();
    }

    function bid() external payable whenNotPaused nonReentrant {
        require(msg.sender.balance >= startingBid + tx.gasprice, "Insufficient balance to pay for both NFT and gas"); 
        require(started, "owner haven't start the auction yet");
        require(block.timestamp < endAt, "the auction has ended");
        require(block.timestamp > startAt, "the auction hasn't started yet");
        require(msg.value >= startingBid, "you must bid at least the starting bid");
        require(msg.value > highestBid, "value < highest");

        // map value to bids
        if (highestBidder != address(0)) {
            bids[highestBidder] += highestBid;
        }
        
        // refund money back to last bidder execpt the first bidder
        if (highestBid > 0) {
            (bool sent, ) = highestBidder.call{value: highestBid}("");
            require(sent, "Failed to return money to last bidder");
        }

        // Set new highest bidder
        highestBidder = msg.sender;
        highestBid = msg.value;

        // map value to bids
        if (highestBidder != address(0)) {
            bids[highestBidder] += highestBid;
        }

        emit Bid(msg.sender, msg.value);
    }

    function withdraw(address bidderAddress) external whenPaused nonReentrant onlyOwner{
        uint256 bal = bids[bidderAddress];
        bids[bidderAddress] = 0;
        (bool sent, ) = bidderAddress.call{value: bal}("");
        require(sent, "Failed to withdraw to bidder");

        emit Withdraw(bidderAddress, bal);
    }

    function forceAcceptEnd() external whenNotPaused onlyOwner nonReentrant {
        require(started, "the auction had not started by the seller yet");
        require(block.timestamp > startAt, "auction hasn't started yet");
        require(!ended, "the auction had already ended by someone");

        ended = true;
        if (highestBidder != address(0)) {
            if (ERC165Checker.supportsInterface(nft, 0x80ac58cd)) {
                nft721.safeTransferFrom(msg.sender, highestBidder, nftId);
            } else {
                nft1155.safeTransferFrom(msg.sender, highestBidder, nftId, nftAmount, "");
            }
            for (uint256 i = 0; i < fPayees.length; i++) {
                release(payable(fPayees[i]));
            }
        } else {
            if (ERC165Checker.supportsInterface(nft, 0x80ac58cd)) {
                nft721.safeTransferFrom(msg.sender, seller, nftId);
            } else {
                nft1155.safeTransferFrom(msg.sender, seller, nftId, nftAmount, "");
            }
        }

        emit End(highestBidder, highestBid);
    }

    function end() external whenNotPaused nonReentrant {
        require(started, "the auction had not started by the seller yet");
        require(block.timestamp >= endAt, "the auction had not ended");
        require(!ended, "the auction had already ended by someone");

        ended = true;
        if (highestBidder != address(0)) {
            if (ERC165Checker.supportsInterface(nft, 0x80ac58cd)) {
                nft721.safeTransferFrom(msg.sender, highestBidder, nftId);
            } else {
                nft1155.safeTransferFrom(msg.sender, highestBidder, nftId, nftAmount, "");
            }
            for (uint256 i = 0; i < fPayees.length; i++) {
                release(payable(fPayees[i]));
            }  
        } else {
            if (ERC165Checker.supportsInterface(nft, 0x80ac58cd)) {
                nft721.safeTransferFrom(msg.sender, seller, nftId);
            } else {
                nft1155.safeTransferFrom(msg.sender, seller, nftId, nftAmount, "");
            }
        }

        emit End(highestBidder, highestBid);
    }

    // Can be removed
    function getBalance() external view returns (uint256) {
        return address(this).balance;
    }

    // Can be removed
    function getIsApproved() external view returns (bool) {
        if (ERC165Checker.supportsInterface(nft, 0x80ac58cd)) {
                return nft721.isApprovedForAll(seller, address(this));
            } else {
                return nft1155.isApprovedForAll(seller, address(this));
            }
    }

    function pause() external {
        _pause();
    }

    function unpause() external {
        _unpause();
    }

    function totalReleased() public view returns (uint256) {
        return _totalReleased;
    }

    function released(address account) public view returns (uint256) {
        return _released[account];
    }

    function _pendingPayment(
        address account,
        uint256 totalReceived,
        uint256 alreadyReleased
    ) private view returns (uint256) {
        return (totalReceived * shares_[account]) / _totalShares - alreadyReleased;
    }

    function release(address payable account) private {
        require(shares_[account] > 0, "PaymentSplitter: account has no shares");

        uint256 totalReceived = address(this).balance + totalReleased();
        uint256 payment = _pendingPayment(account, totalReceived, released(account));

        require(payment != 0, "PaymentSplitter: account is not due payment");

        _released[account] += payment;
        _totalReleased += payment;

        Address.sendValue(account, payment);
        emit PaymentReleased(account, payment);
    }

    function _addPayee(address account, uint256 _shares) private {
        require(account != address(0), "PaymentSplitter: account is the zero address");
        require(_shares > 0, "PaymentSplitter: shares are 0");
        require(shares_[account] == 0, "PaymentSplitter: account already has shares");

        payees.push(account);
        shares_[account] = _shares;
        _totalShares = _totalShares + _shares;
        emit PayeeAdded(account, _shares);
    }

}



