pragma solidity ^0.8.10;

////import "./auction-v2.sol";

contract CVAuctionFactory {
    StandardAuctionV2[] public auctions;

    function create(address _nft,
        uint256 _nftId,
        uint256 _nftAmount,
        uint256 _startingBid, 
        uint256 _endAt, 
        uint256 _startAt,
        address[] memory _payees, 
        uint256[] memory _shares) public {
        StandardAuctionV2 auction = new StandardAuctionV2(_nft, _nftId, _nftAmount, _startingBid, _endAt, _startAt, _payees, _shares);
        auctions.push(auction);
    }

    function getAuction(uint _index)
        public
        view
        returns (
            uint256 nftId, 
            uint256 nftAmount,
            address seller, 
            uint256 endAt, 
            uint256 startAt, 
            bool started, 
            bool ended, 
            address highestBidder,
            uint256 highestBid,
            uint256 startingBid, 
            uint256 balance
        )
    {
        StandardAuctionV2 auction = auctions[_index];

        return (auction.nftId(), auction.nftAmount(), auction.seller(), auction.endAt(), auction.startAt(), auction.started(), auction.ended(), auction.highestBidder(), auction.highestBid(), auction.startingBid(), address(auction).balance);
    }
}