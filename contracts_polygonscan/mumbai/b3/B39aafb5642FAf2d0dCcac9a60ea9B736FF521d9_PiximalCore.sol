// PiximalCore.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
//import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./PixiCharacters.sol";
import "./GameItems.sol";

contract PiximalCore is Initializable, AccessControlUpgradeable {
    using SafeMath for uint256;

    bytes32 public constant GAME_ADMIN_CORE = keccak256("GAME_ADMIN_CORE");

    address public creatorAddress;
    uint256 public feeMintPixiChar;
    uint256 public feeMintPixiWearable;
    uint256 public adoptStartBlock;
    uint256 public maxNumberOfPixis;

    PixiCharacters public pixiCharacters;
    GameItems public gameItems;

    function initialize(PixiCharacters _pixiCharacters, GameItems _gameItems) public initializer {
        // Support multiple inheritance upgradable
        // __Context_init_unchained();
        // __ERC165_init_unchained();
        // __AccessControl_init_unchained();

        // Setup Role to deployer
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(GAME_ADMIN_CORE, msg.sender);

        // Initial
        creatorAddress = msg.sender;
        feeMintPixiChar = 1000000000000000000;
        feeMintPixiWearable = 1000000000000000000;
        adoptStartBlock = 0xffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff;
        maxNumberOfPixis = 10000;

        pixiCharacters = _pixiCharacters;
        gameItems = _gameItems;
    }

    modifier onlyGameAdmin() {
        require(hasRole(GAME_ADMIN_CORE, msg.sender), "PiximalCore: Only Game Admin");
        _;
    }

    // ███    ███ ██ ███    ██ ████████ 
    // ████  ████ ██ ████   ██    ██    
    // ██ ████ ██ ██ ██ ██  ██    ██    
    // ██  ██  ██ ██ ██  ██ ██    ██    
    // ██      ██ ██ ██   ████    ██    

    function adoptPiximal(uint256 _numPixi) public payable {
        require(adoptStarted(), "You act before your time");
        require(pixiCharacters.totalSupply() < maxNumberOfPixis, "All wizards have been summoned");
        // msg.value / address(this).balance
        require(msg.value >= feeMintPixiChar.mul(_numPixi), "PiximalCore: Insufficient balance.");
        require(
            pixiCharacters.totalSupply().add(_numPixi) <= maxNumberOfPixis,
            "Failed to adopt too many Pixis"
        );
        require(
            _numPixi > 0 && _numPixi <= 5,
            "You can Adopt no fewer than 1, and no more than 5"
        );
        
        pixiCharacters.mintBatch(msg.sender, _numPixi);
        //gameItems.mintBatchSS1WhenAdopt(msg.sender, _numPixi);
    }

    function buyWearable() public payable {
        require(msg.value > feeMintPixiWearable, "PiximalCore: Insufficient balance.");
    }

    // ██ ███    ██ ███████  ██████  
    // ██ ████   ██ ██      ██    ██ 
    // ██ ██ ██  ██ █████   ██    ██ 
    // ██ ██  ██ ██ ██      ██    ██ 
    // ██ ██   ████ ██       ██████  

    function adoptStarted() public view returns (bool) {
        return block.number >= adoptStartBlock;
    }

    //  ██████   █████  ███    ███ ███████      █████  ██████  ███    ███ ██ ███    ██ 
    // ██       ██   ██ ████  ████ ██          ██   ██ ██   ██ ████  ████ ██ ████   ██ 
    // ██   ███ ███████ ██ ████ ██ █████       ███████ ██   ██ ██ ████ ██ ██ ██ ██  ██ 
    // ██    ██ ██   ██ ██  ██  ██ ██          ██   ██ ██   ██ ██  ██  ██ ██ ██  ██ ██ 
    //  ██████  ██   ██ ██      ██ ███████     ██   ██ ██████  ██      ██ ██ ██   ████ 

    function setAdoptStartBlock(uint256 _newAdoptStartBlock) public onlyGameAdmin {
        adoptStartBlock = _newAdoptStartBlock;
    }

    function setPixiPrice(uint256 _newPrice) public onlyGameAdmin { 
        require(!adoptStarted(),"Price cannot be changed once The Adopting has begun");
        feeMintPixiChar = _newPrice;
    }

    function setWearablePrice(uint256 _newPrice) public onlyGameAdmin {
        require(!adoptStarted(),"Price cannot be changed once The Adopting has begun");
        feeMintPixiWearable = _newPrice;
    }

    function setVault(address _creatorAddress) public onlyGameAdmin {
        creatorAddress = _creatorAddress;
    }

    function withdraw(uint256 _amount) public onlyGameAdmin {
        require(address(creatorAddress) != address(0), "no creatorAddress");
        require(payable(creatorAddress).send(_amount), "Someting Wrong");
    }

    function withdrawAll() public payable onlyGameAdmin {
        require(address(creatorAddress) != address(0), "no creatorAddress");
        require(payable(creatorAddress).send(address(this).balance), "Someting Wrong");
    }
}

