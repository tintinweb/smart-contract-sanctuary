pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/token/ERC1155/extensions/ERC1155SupplyUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";

import {ItemInfo, LibItem} from "./libraries/LibItem.sol";

contract GameItems is ERC1155SupplyUpgradeable, AccessControlUpgradeable {
    using SafeMathUpgradeable for uint256;

    bytes32 public constant GAME_ADMIN_CORE = keccak256("GAME_ADMIN_CORE");

    string private baseURI;

    mapping(uint256 => ItemInfo) private itemInfos; // tokenId -> ItemInfo
    uint256[] private wearableItems;

    //mapping(uint256 => ItemInfo) private wearableItems; // tokenId -> ItemInfo(wearableItems)

    event ItemAdded(uint256 indexed tokenId);
    event ItemMinted(address indexed owner, uint256 indexed tokenId, uint256 amount);
    
    
    function initialize() public initializer {
        // __Context_init_unchained();
        // __ERC165_init_unchained();
        // __ERC1155Supply_init_unchained();
        // __AccessControl_init_unchained();

        baseURI = "ipfs://QmUNLLsPACCz1vLxQVkXqqLX5R1X345qqfHbsf67hvA3Nn/";
        __ERC1155_init(baseURI);

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

    }

    modifier onlyGameCore() {
        require(hasRole(GAME_ADMIN_CORE, msg.sender), "GameItems: Only Game Core");
        _;
    }

    modifier onlyGameAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "GameItems: Only Game Admin");
        _;
    }

    // ███    ███ ██ ███    ██ ████████ 
    // ████  ████ ██ ████   ██    ██    
    // ██ ████ ██ ██ ██ ██  ██    ██    
    // ██  ██  ██ ██ ██  ██ ██    ██    
    // ██      ██ ██ ██   ████    ██    


    function addItem(ItemInfo memory _itemInfo) external onlyGameAdmin {
        uint256 tokenId = _itemInfo.tokenId;
        require(itemInfos[tokenId].tokenId == 0, "Item tokenId is exist!!!");
        _setItemInfo(_itemInfo, tokenId);
        emit ItemAdded(tokenId);
    }

    function addItems(ItemInfo[] memory _itemInfos) external onlyGameAdmin {
        for (uint256 i; i < _itemInfos.length; i++) {
            uint256 tokenId = _itemInfos[i].tokenId;
            require(itemInfos[tokenId].tokenId == 0, "Item tokenId is exist!!!");
            _setItemInfo(_itemInfos[i], tokenId);
            emit ItemAdded(tokenId);
        }
    }

    function mintItem(address _owner, uint256 _tokenId, uint256 _amount) public onlyGameCore {
        require(wearableItems.length > 0, "There is no Wearable Items");
        require(itemInfos[_tokenId].tokenId != 0, "Item tokenId does not exist!!!");
        _mint(_owner, _tokenId, _amount, "");
        emit ItemMinted(_owner, _tokenId, _amount);
    }

    function mintBatchSS1WhenAdopt(address _owner, uint256 _numPixi) public onlyGameCore {
        require(wearableItems.length > 0, "There is no Wearable Items");
        for (uint256 i = 0; i < _numPixi; i++) {
            uint256 tokenId = wearableItems[_randomItem(i, wearableItems.length)];
            require(itemInfos[tokenId].tokenId != 0, "Item tokenId does not exist!!!");
            _mint(_owner, tokenId, 1, "");
            emit ItemMinted(_owner, tokenId, 1);
        }
        // Free 5 Pixil per Pixi
        _mint(_owner, 1, _numPixi.mul(5), "");
        emit ItemMinted(_owner, 1, _numPixi.mul(5));
    }

    function _randomItem(uint256 _itemIndex,uint256 _maxItems) private view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(
            _itemIndex, 
            block.timestamp, 
            block.difficulty, 
            msg.sender))) % _maxItems;
    }

    // initial piximal value when Item minted (first time only)
    function _setItemInfo(ItemInfo memory _itemInfo, uint256 _tokenId) private onlyGameAdmin{
        itemInfos[_tokenId] = _itemInfo;
    }
    
    function setUri(string memory _uri) public{
        _setURI(_uri);
    }

    // ██ ███    ██ ███████  ██████  
    // ██ ████   ██ ██      ██    ██ 
    // ██ ██ ██  ██ █████   ██    ██ 
    // ██ ██  ██ ██ ██      ██    ██ 
    // ██ ██   ████ ██       ██████  

    function getInfo(uint256 _tokenId) public view returns (ItemInfo memory) {
        require(itemInfos[_tokenId].tokenId != 0, "Item tokenId does not exist!!!");
        return (itemInfos[_tokenId]);
    }

    
    function supportsInterface(bytes4 interfaceId) 
    public 
    view 
    override(ERC1155Upgradeable, AccessControlUpgradeable) returns (bool) {
        return
            interfaceId == type(IAccessControlUpgradeable).interfaceId ||
            interfaceId == type(IERC1155Upgradeable).interfaceId ||
            interfaceId == type(IERC1155MetadataURIUpgradeable).interfaceId ||
            super.supportsInterface(interfaceId);
    }
}

