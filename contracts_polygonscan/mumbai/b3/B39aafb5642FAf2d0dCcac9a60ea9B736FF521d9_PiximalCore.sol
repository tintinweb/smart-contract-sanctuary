// PixiCharacter.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import {PixiInfo, LibPixiCharacter} from "./libraries/LibPixiCharacter.sol";

contract PixiCharacters is ERC721EnumerableUpgradeable, AccessControlUpgradeable {
    using CountersUpgradeable for CountersUpgradeable.Counter;

    bytes32 public constant GAME_ADMIN_CORE = keccak256("GAME_ADMIN_CORE");

    CountersUpgradeable.Counter private _tokenIds;
    string private baseURI;

    mapping(uint256 => PixiInfo) private pixiInfos; // tokenId -> PixiInfo

    event MintedPixi(address indexed owner, uint256 indexed tokenId);
    event ChangeName(uint256 indexed tokenId);
    event LevelUp(address indexed owner, uint256 indexed tokenId, uint16 level);
    event Burned(uint256 indexed tokenId);

    function initialize() public initializer {
        // __Context_init_unchained();
        // __ERC165_init_unchained();
        // __ERC721Enumerable_init_unchained();
        // __AccessControl_init_unchained();

        __ERC721_init("Piximal Characters", "PXMC");
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

        baseURI = "https://gateway.pinata.cloud/ipfs/";
    }

    // ███    ███ ██ ███    ██ ████████ 
    // ████  ████ ██ ████   ██    ██    
    // ██ ████ ██ ██ ██ ██  ██    ██    
    // ██  ██  ██ ██ ██  ██ ██    ██    
    // ██      ██ ██ ██   ████    ██    

    function mint(address _playerAddr) public onlyGameCore{
        //_tokenIds + 1 before mint
        _tokenIds.increment();

        uint256 tokenId = _tokenIds.current();
        _safeMint(_playerAddr, tokenId);
        _setLittlePixiInfo(tokenId);
        
        emit MintedPixi(_playerAddr, tokenId);
    }

    function mintBatch(address _playerAddr, uint256 _numPixi) public onlyGameCore{
        for (uint256 i = 0; i < _numPixi; i++) {
            //_tokenIds + 1 before mint
            _tokenIds.increment();

            uint256 tokenId = _tokenIds.current();
            _safeMint(_playerAddr, tokenId);
            _setLittlePixiInfo(tokenId);
            
            emit MintedPixi(_playerAddr, tokenId);
        }
    }

    // initial piximal value when pixi minted
    function _setLittlePixiInfo(uint256 _tokenId) private {
        require(pixiInfos[_tokenId].tokenId == 0, "Pixi tokenId is exist!!!");
        pixiInfos[_tokenId].tokenId = _tokenId;
        pixiInfos[_tokenId].name = "Wild Pixi";
        // Need to write some random function here
        pixiInfos[_tokenId].relationship = 100;
        pixiInfos[_tokenId].lastInteractedTimeStamp = block.timestamp;
        pixiInfos[_tokenId].numericTraits[0] = 0;
        pixiInfos[_tokenId].numericTraits[1] = 0;
        pixiInfos[_tokenId].numericTraits[2] = 0;
        pixiInfos[_tokenId].numericTraits[3] = 0;
    }

    // ██ ███    ██ ███████  ██████  
    // ██ ████   ██ ██      ██    ██ 
    // ██ ██ ██  ██ █████   ██    ██ 
    // ██ ██  ██ ██ ██      ██    ██ 
    // ██ ██   ████ ██       ██████  

    modifier onlyGameCore() {
        require(hasRole(GAME_ADMIN_CORE, msg.sender), "PixiCharacters: Only Game Core");
        _;
    }

    // Modifier to check that the orgId is the Exist of the contract.
    modifier mustExist(uint256 _tokenId) {
        require(pixiInfos[_tokenId].tokenId != 0, "PixiCharacters tokenId does not exist!!!");
        // Underscore is a special character only used inside
        // a function modifier and it tells Solidity to
        // execute the rest of the code.
        _;
    }

    function getInfoById(uint256 _tokenId) public view mustExist(_tokenId) returns (PixiInfo memory) {
        require(pixiInfos[_tokenId].tokenId != 0, "Pixi tokenId does not exist!!!");
        return (pixiInfos[_tokenId]);
    }

    // Pagination of owner tokens
    function getChactersPaging(address _owner, uint8 _page, uint8 _num) public view returns(uint256[] memory) {
        require(_page > 0, "_page should be greater than 0");
        require(_num > 0, "_num should be greater than 0");

        uint256 _tokenCount = balanceOf(_owner);
        uint256 _offset = (_page - 1) * _num;
        uint256 _range = _offset > _tokenCount ? 0 : min(_tokenCount - _offset, _num);

        uint256[] memory _tokens = new uint256[](_range);
        for (uint256 i = 0; i < _range; i++) {
            _tokens[i] = tokenOfOwnerByIndex(_owner, _offset + i);
        }
        return _tokens;
    }

    function min(uint256 a, uint256 b) private pure returns (uint256) {
        return a >= b ? b : a;
    }


