contract PowerFanMarketplace is ReentrancyGuard, Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Address for address;
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.Bytes32Set;

    enum Side {
        SELL,
        BUY
    }

    // order
    struct Order {
        uint256 id;
        Side side;
        address maker;
        uint256 amount;
        address pairA;
        address pairB;
        uint256 priceA;
        uint256 priceB;
    }

    struct Pair {
        string name;
        address token0;
        address token1;
    }

    EnumerableSet.Bytes32Set private pairNames;
    Counters.Counter public currentOrderId;

    // order id -> order
    mapping(uint256 => Order) public orders;
    mapping(uint256 => bool) public isActive;
    mapping(address => bool) public tokens;
    mapping(string => address[2]) public nameToPair;
    mapping(address => mapping(address => string)) public pairs;

    // weiSend of current tx
    uint256 private weiSend = 0;

    // makes sure weiSend of current tx is reset
    modifier weiSendGuard() {
        weiSend = msg.value;
        _;
        weiSend = 0;
    }

    modifier isActiveTokens(address token0, address token1) {
        require(tokens[token0]);
        require(tokens[token1]);
        _;
    }

    modifier isValidPair(address token0, address token1) {
        require(bytes(pairs[token0][token1]).length > 0);
        _;
    }

    // logs
    event LogMakeOrder(
        uint256 id,
        Side side,
        address indexed maker,
        uint256 amount,
        address indexed pairA,
        address indexed pairB,
        uint256 priceA,
        uint256 priceB
    );
    event LogTakeOrder(
        uint256 indexed id,
        address indexed taker,
        uint256 amount
    );
    event LogCancelOrder(uint256 indexed id);

    event ADDPAIR(address indexed token0, address indexed token1, string name);

    function getOrders(uint256 start, uint256 end)
        external
        view
        returns (Order[] memory)
    {
        Order[] memory _orders = new Order[](end);
        for (uint256 i = start; i <= end; i++) {
            Order storage tOrder = orders[i];
            _orders[i - 1] = tOrder;
        }
        return _orders;
    }

    // internal
    function _makeOrder(
        uint256 id,
        Side side,
        uint256 amount,
        address pairA,
        address pairB,
        uint256 priceA,
        uint256 priceB,
        address maker
    ) internal returns (bool) {
        // validate input
        require(pairA != pairB);
        require(orders[id].id != id);
        require(id > 0 && amount > 0);
        require(priceA > 0 && priceB > 0);

        bool pairIsEther;
        IERC20 token;

        if (side == Side.SELL) {
            pairIsEther = Utils.isEther(pairA);
            token = IERC20(pairA);
        }
        if (side == Side.BUY) {
            pairIsEther = Utils.isEther(pairB);
            token = IERC20(pairB);
        }
        // validate maker's deposit
        require(
            !pairIsEther &&
                (token.allowance(maker, address(this)) > amount ||
                    token.balanceOf(maker) > amount)
        );
        // retrieve makers amount
        if (pairIsEther) {
            // eth already received, subtract used wei
            require(pairIsEther && (weiSend > 0 || weiSend > amount));
            weiSend = weiSend.sub(amount);
        } else {
            // pull tokens
            token.safeTransferFrom(maker, address(this), amount);
        }

        // update state
        orders[id] = Order(
            id,
            side,
            maker,
            amount,
            pairA,
            pairB,
            priceA,
            priceB
        );
        isActive[id] = true;
        LogMakeOrder(id, side, maker, amount, pairA, pairB, priceA, priceB);
        return true;
    }

    function _takeOrder(
        uint256 id,
        uint256 amount,
        address taker
    ) internal returns (bool) {
        // validate inputs
        require(id > 0 && amount >= 0);
        require(isActive[id]);
        // get order
        Order storage order = orders[id];
        // validate order
        require(order.id == id);
        // amount of pairA usable
        uint256 usableAmount = amount > order.amount ? order.amount : amount;
        order.amount = order.amount.sub(usableAmount);
        // token interfaces
        IERC20 sellToken = order.side == Side.SELL
            ? IERC20(order.pairB)
            : IERC20(order.pairA);
        IERC20 buyToken = order.side == Side.SELL
            ? IERC20(order.pairA)
            : IERC20(order.pairB);

        bool sellTokenIsEther = order.side == Side.SELL
            ? Utils.isEther(order.pairB)
            : Utils.isEther(order.pairA);
        bool buyTokenIsEther = order.side == Side.SELL
            ? Utils.isEther(order.pairA)
            : Utils.isEther(order.pairB);

        uint256 total;
        total = order.side == Side.SELL
            ? usableAmount.mul(order.priceB).div(order.priceA)
            : usableAmount.mul(order.priceA).div(order.priceB);

        require(
            (!sellTokenIsEther &&
                (sellToken.allowance(taker, address(this)) >= total ||
                    sellToken.balanceOf(taker) >= amount))
        );
        // pay maker
        if (sellTokenIsEther) {
            require(sellTokenIsEther && (weiSend >= 0 || weiSend > total));
            weiSend = weiSend.sub(total);
            payable(order.maker).transfer(total);
        } else {
            sellToken.safeTransferFrom(taker, order.maker, total);
        }

        // pay taker
        if (buyTokenIsEther) {
            payable(taker).transfer(usableAmount);
        } else {
            buyToken.safeTransfer(taker, usableAmount);
        }

        LogTakeOrder(id, taker, usableAmount);
        return true;
    }

    function _cancelOrder(uint256 id, address maker) internal returns (bool) {
        // validate inputs
        if (id <= 0) return false;
        if (!isActive[id]) return false;

        // get order
        Order storage order = orders[id];
        if (order.id != id || order.maker != maker) return false;

        uint256 amount = order.amount;
        bool pairAisEther = Utils.isEther(order.pairA);

        // actions
        if (pairAisEther) {
            payable(order.maker).transfer(amount);
        } else {
            IERC20(order.pairA).safeTransfer(order.maker, amount);
        }

        LogCancelOrder(id);

        return true;
    }

    function _updateOrder(uint256 id, Order memory _order)
        internal
        returns (bool)
    {
        if (id <= 0) return false;
        Order storage order = orders[id];
        if (order.id != id) return false;
        order.priceA = _order.priceA;
        order.priceB = _order.priceB;
        return true;
    }

    function _unCancelOrder(uint256 id) internal returns (bool) {
        if (id <= 0) return false;
        Order storage order = orders[id];
        if (order.id != id) return false;
        if (!isActive[id]) return false;
        isActive[id] = true;
        return true;
    }

    function _addTokens(address[] memory _tokens) internal {
        require(_tokens.length > 0);
        for (uint8 i = 0; i < _tokens.length; i++) {
            require(_tokens[i] != address(0));
            require(_tokens[i].isContract());
            require(tokens[_tokens[i]] == false);
            tokens[_tokens[i]] = true;
        }
    }

    function _removeTokens(address[] memory _tokens) internal {
        require(_tokens.length > 0);
        for (uint8 i = 0; i < _tokens.length; i++) {
            delete tokens[_tokens[i]];
        }
    }

    // single
    function makeOrder(
        Side side,
        uint256 amount,
        address pairA,
        address pairB,
        uint256 priceA,
        uint256 priceB
    )
        external
        payable
        weiSendGuard
        isActiveTokens(pairA, pairB)
        isValidPair(pairA, pairB)
        nonReentrant
        returns (uint256)
    {
        currentOrderId.increment();
        uint256 id = currentOrderId.current();
        bool success = _makeOrder(
            id,
            side,
            amount,
            pairA,
            pairB,
            priceA,
            priceB,
            msg.sender
        );

        if (weiSend > 0) msg.sender.transfer(weiSend);
        return id;
    }

    function takeOrder(uint256 id, uint256 amount)
        external
        payable
        weiSendGuard
        nonReentrant
        returns (bool)
    {
        bool success = _takeOrder(id, amount, msg.sender);
        if (weiSend > 0) msg.sender.transfer(weiSend);
        return success;
    }

    function cancelOrder(uint256 id) external nonReentrant returns (bool) {
        return _cancelOrder(id, msg.sender);
    }

    // Admin Functions //

    function addTokens(address[] memory _tokens) external onlyOwner {
        _addTokens(_tokens);
    }

    function addPairs(address[2] memory _tokens, string memory name)
        external
        onlyOwner
    {
        require(tokens[_tokens[0]]);
        require(tokens[_tokens[1]]);
        pairs[_tokens[0]][_tokens[1]] = name;
        nameToPair[name] = _tokens;
        bytes32 stringToBytes = Utils.toBytes32(name);
        pairNames.add(stringToBytes);
        emit ADDPAIR(_tokens[0], _tokens[1], name);
    }

    function getPairAddresses(string memory name)
        public
        view
        returns (address[2] memory)
    {
        return nameToPair[name];
    }

    function getAllPairs() public view returns (Pair[] memory) {
        uint256 length = pairNames.length();
        Pair[] memory all = new Pair[](length);
        string[] memory allPairs = new string[](length);
        for (uint256 i = 0; i < length; i++) {
            allPairs[i] = Utils.toString(pairNames.at(i));
            address[2] memory addresses = nameToPair[allPairs[i]];
            all[i] = Pair({
                name: allPairs[i],
                token0: addresses[0],
                token1: addresses[1]
            });
        }
        return all;
    }

    function removeTokens(address[] memory _tokens) external onlyOwner {
        _removeTokens(_tokens);
    }

    function updateOrder(uint256 id, Order memory order)
        external
        onlyOwner
        nonReentrant
        returns (bool)
    {
        return _updateOrder(id, order);
    }

    function cancelUserOrder(uint256 id)
        external
        onlyOwner
        nonReentrant
        returns (bool)
    {
        Order storage order = orders[id];
        return _cancelOrder(id, order.maker);
    }

    function unCancelOrder(uint256 id) external onlyOwner returns (bool) {
        return _unCancelOrder(id);
    }

    // multi
    function makeOrders(
        uint256[] memory ids,
        uint256[] memory amounts,
        address[] memory pairAs,
        address[] memory pairBs,
        uint256[] memory priceAs,
        uint256[] memory priceBs
    ) external payable weiSendGuard nonReentrant returns (bool) {
        require(
            amounts.length == ids.length &&
                pairAs.length == ids.length &&
                pairBs.length == ids.length &&
                priceAs.length == ids.length &&
                priceBs.length == ids.length
        );

        bool allSuccess = true;

        for (uint256 i = 0; i < ids.length; i++) {
            // update if any of the orders failed
            // the function is like this because "stack too deep" error
            if (
                allSuccess &&
                !_makeOrder(
                    ids[i],
                    Side.SELL,
                    amounts[i],
                    pairAs[i],
                    pairBs[i],
                    priceAs[i],
                    priceBs[i],
                    msg.sender
                )
            ) allSuccess = false;
        }

        if (weiSend > 0) msg.sender.transfer(weiSend);

        return allSuccess;
    }

    function takeOrders(uint256[] memory ids, uint256[] memory amounts)
        external
        payable
        weiSendGuard
        nonReentrant
        returns (bool)
    {
        require(ids.length == amounts.length);

        bool allSuccess = true;

        for (uint256 i = 0; i < ids.length; i++) {
            bool success = _takeOrder(ids[i], amounts[i], msg.sender);

            // update if any of the orders failed
            if (allSuccess && !success) allSuccess = success;
        }

        if (weiSend > 0) msg.sender.transfer(weiSend);

        return allSuccess;
    }

    function cancelOrders(uint256[] memory ids)
        external
        nonReentrant
        returns (bool)
    {
        bool allSuccess = true;

        for (uint256 i = 0; i < ids.length; i++) {
            bool success = _cancelOrder(ids[i], msg.sender);

            // update if any of the orders failed
            if (allSuccess && !success) allSuccess = success;
        }

        return allSuccess;
    }
}

