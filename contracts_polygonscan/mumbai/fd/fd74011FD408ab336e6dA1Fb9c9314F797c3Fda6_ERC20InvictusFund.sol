        //test if contract address exists
        ERC20Upgradeable erc20 = ERC20Upgradeable(currencies[ticker].contractAddress);
        
        erc20.transferFrom(msg.sender, address(this), amount);
        emit AddLiquidity(msg.sender, currencies[ticker].contractAddress, amount);
    }

    /// Removes liquidity, allowing owner to transfer stable coin to the fund wallet.
    function removeLiquidity(uint256 amount, string memory ticker) external onlyOwner {
        require(address(currencies[ticker].contractAddress) != address(0), "ticker not supported unsupported");
        ERC20Upgradeable(currencies[ticker].contractAddress).safeTransfer(msg.sender, amount);
        emit RemoveLiquidity(msg.sender, amount);
    }

    function setMaxWithdrawalsPerTx(uint256 newRedemptionsPerTx) external onlyOwner {
        require(newRedemptionsPerTx > 0, "Must be greater than 0");
        maxRedemptionsPerTx = newRedemptionsPerTx;
    }

    function setMaxInvestmentsPerTx(uint256 newMaxInvestmentsPerTx) external onlyOwner {
        require(newMaxInvestmentsPerTx > 0, "Must be greater than 0");
        maxInvestmentsPerTx = newMaxInvestmentsPerTx;
    }

    function addCurrency(address stableContractAddress, uint numerator) external onlyPricingOracle {
        require(numerator > 0, "numerator to be greater than 0");
        require(address(stableContractAddress) != address(0), "Invalid address");
        currencies[ERC20Upgradeable(stableContractAddress).symbol()] = Currency(Price(numerator, 1 ether), stableContractAddress);
        emit AddCurrency(ERC20Upgradeable(stableContractAddress).symbol());
    }

    function addCurrency(string memory ticker, uint numerator) external onlyPricingOracle {
        require(numerator > 0, "numerator to be greater than 0");
        currencies[ticker] = Currency(Price(numerator, 1 ether), address(0));
        emit AddCurrency(ticker);
    }

    function increaseTokenAmendment(address participant, uint256 amount, string memory reason) external onlyAdjustable {
        require(amount > 0, "cannot mint 0 tokens");
        mint(participant, amount);
        emit IncreaseTokensAmendment(participant, amount, reason);
    }

    function decreaseTokensAmendment(address participant, uint256 amount, string memory reason) external onlyAdjustable {
        require(amount > 0, "cannot burnFrom 0 tokens");
        burnFrom(participant, amount);
        emit DecreaseTokensAmendment(participant, amount, reason);
    }


    ////////////////////////////////////////////////////////////////////// Public functions ////////////////////////////////////////////////////////////////////////////////////////////

    function burnFrom(address account, uint256 amount) public virtual override onlyBurner{
        super.burnFrom(account, amount);
    }

    //TODO: add role modifier
    //Should Public
    function priceUpdate(string[] memory _currencies, uint256[] memory rates, uint256 _fundNav) public onlyPricingOracle{
        updateCurrenciesPrices(_currencies, rates);
        updateNavPerToken(_fundNav);
    }


    function isBurner(address participant) public view returns (bool){
        return hasRole(BURNER_ROLE, participant);
    }

    //Should this be public
    function updateNavPerToken(uint256 _fundNav) public onlyPricingOracle{
        require(_fundNav > 0);
        currencies[symbol()].price.numerator = _fundNav;
        currencies[symbol()].price.denominator = totalSupply();
        emit UpdateNavPerToken(_fundNav, totalSupply());
    }

    //Should this be public
    function updateCurrenciesPrices(string[] memory _currencies, uint256[] memory rates) public onlyPricingOracle{
        //require, can't set currency if it doesn't exist.
        for(uint256 i = 0 ; i < _currencies.length; i++){
            require(address(currencies[_currencies[i]].contractAddress) != address(0), "currency not supported");
            require(rates[i] > 0, "price cannot be zero");
            currencies[_currencies[i]].price = Price(rates[i], 1 ether);
            emit UpdateCurrencyPrice(rates[i], _currencies[i]);
        }
    }

    //should this be public
    function updateCutOfftime(uint80 time) public onlyPricingOracle{
        cutOffTime = time;
    }

    //should this be public
    function updateDealingNonce(uint80 _dealingNonce) public onlyPricingOracle{
        dealingNonce = _dealingNonce;
    }

    //Handle Dealing
    //should this be public
    function dealRedemption(uint256 eventId, address participant,uint256 counterCurrencyAmount, uint256 tokenAmount, string memory ticker) public onlyPricingOracle {
        address stableContract = currencies[ticker].contractAddress;
        _burnForParticipant(participant, tokenAmount);
        ERC20Upgradeable(stableContract).safeTransfer(participant, counterCurrencyAmount);
        emit RedemptionEvent(eventId, tokenAmount, counterCurrencyAmount, ticker, participant, dealingNonce);
    }
    
    //should this be public
    function dealInvestment(
        uint256 eventId,
        address participant, 
        uint256 counterCurrencyAmount,
        uint256 fundTokenAmount,
        string memory ticker,
        string memory transactionHash
        ) public onlyPricingOracle{
        _mint(participant, fundTokenAmount);
        emit InvestmentEvent(eventId, counterCurrencyAmount, fundTokenAmount, ticker, transactionHash, participant, dealingNonce);
    }

    /**
     * @dev Function to mint tokens when not paused.
     * @param to The address that will receive the minted tokens.
     * @param value The amount of tokens to mint.
     */
    function mint(address to, uint256 value) public override onlyMinter whenNotPaused {
        _mint(to, value);
    }

    ////////////////////////////////////////////////////////////////////// Internal functions ////////////////////////////////////////////////////////////////////////////////////////////

    function calulateTokenAmount(string memory ticker, uint256 counterCurrencyAmount) internal view returns(uint256) {
        Currency memory currency = currencies[ticker];
        Currency memory navPerToken = currencies[symbol()];
        uint256 counterCurrencyDollar = counterCurrencyAmount.mul(currency.price.numerator).div(currency.price.denominator);
        uint256 tokenPrice =  navPerToken.price.numerator.mul(currency.price.denominator).div(navPerToken.price.denominator);
        return (counterCurrencyDollar).div(tokenPrice);
    }

    function calculateCounterCurrencyAmount(string memory ticker, uint256 tokenAmount) internal view returns(uint256) {
        Currency memory currency = currencies[ticker];
        Currency memory navPerToken = currencies[symbol()];
        uint256 tokenDollar = (tokenAmount.mul(navPerToken.price.numerator).div(navPerToken.price.denominator));
        uint256 priceDollar = currency.price.numerator;
        return (tokenDollar.mul(currency.price.denominator)).div(priceDollar);
    }

    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _amount
    ) internal virtual override {
        require(
            InvictusWhitelist(whitelistContract).isWhitelisted(_to),
            'Must be whitelisted'
        );
        super._beforeTokenTransfer(_from, _to, _amount);
    }

    function _burnForParticipant(address account, uint256 value) internal {
        _burn(account, value);
    }
    ////////////////////////////////////////////////////////////////////// Private functions ////////////////////////////////////////////////////////////////////////////////////////////

    //Handle with single array
    function _handleRedemptionsDealing() private {
        uint256 numberOfRedemptions = MathUpgradeable.min(redemptions.length, maxRedemptionsPerTx);
        uint256 startingIndex = redemptions.length;
        uint256 endingIndex = redemptions.length.sub(numberOfRedemptions);
        uint256 tokens; 
        address participant;
        string memory ticker;
        uint256 counterCurrencyAmount;
        Redemption memory redemption;
        address stableContract;

        for(uint i = startingIndex; i > endingIndex; i--){
            redemption = redemptions[i-1];
            if(redemption.timestamp < cutOffTime){
                participant = redemption.participant;
                tokens = redemption.amount;
                ticker = redemption.ticker;
                redemptions.pop();
                counterCurrencyAmount = calculateCounterCurrencyAmount(ticker, tokens);
                stableContract = currencies[ticker].contractAddress;
                //dealRedmption
                dealRedemption(i-1,participant, counterCurrencyAmount, tokens, ticker);
            }
        }

        if(redemptions.length != 0 && redemptions[redemptions.length - 1].timestamp < cutOffTime){
            emit RedemptionsLeftInQueue(dealingNonce);
        }
    }

    //Handle with single array
    function _handleInvestmentsDealing() private {
        uint256 numberOfInvestments = MathUpgradeable.min(investments.length, maxInvestmentsPerTx);
        uint256 startingIndex = investments.length;
        uint256 endingIndex = investments.length.sub(numberOfInvestments);
        uint256 counterCurrencyAmount;
        address participant;
        string memory ticker;
        string memory transactionHash;
        uint256 fundTokenAmount;
        Investment memory investment;

        for(uint i = startingIndex; i > endingIndex; i --){
            investment = investments[i-1];
            if(investment.timestamp < cutOffTime){
                participant = investment.participant;
                counterCurrencyAmount = investment.amount;
                ticker = investment.ticker;
                transactionHash = investment.transactionHash;
                fundTokenAmount = calulateTokenAmount(ticker, counterCurrencyAmount);
                investments.pop();
                dealInvestment(i-1,participant, counterCurrencyAmount, fundTokenAmount, ticker, transactionHash);     
            }
        }
 
        if(investments.length != 0 && investments[investments.length - 1].timestamp < cutOffTime){
            emit InvestmentsLeftInQueue(dealingNonce);
        }
    }

    ////////////////////////////////////////////////////////////////////// Modifiers functions ////////////////////////////////////////////////////////////////////////////////////////////
    
    modifier onlyWhitelisted() {
        require(
            InvictusWhitelist(whitelistContract).isWhitelisted(_msgSender()),
            'Must be whitelisted'
        );
        _;
    }

    modifier onlyPauser() {
        require(hasRole(MINTER_ROLE, msg.sender), 'ERC20InvictusFund: must have pauser role to pause');
        _;
    }

    modifier onlyMinter() {
        require(hasRole(MINTER_ROLE, msg.sender), 'ERC20InvictusFund: must have minter role to mint');
        _;
    }

    modifier onlyBurner(){
        require(hasRole(BURNER_ROLE, msg.sender), 'ERC20InvictusFund: must have burner role to burn');
        _;
    }

    modifier onlyPricingOracle() {
        require(hasRole(PRICING_ORACLE_ROLE, msg.sender), 'ERC20InvictusFund: must have pricing oracle role');
        _;
    }

    modifier onlyAdjustable() {
        require(hasRole(ADJUSTABLE_ROLE, msg.sender), 'ERC20InvictusFund: must have adjustable role');
        _;
    }

    modifier onlyInvestmentOracle() {
        require(hasRole(INVESTMENT_ORACLE_ROLE, msg.sender), 'ERC20InvictusFund: must have Investment oracle role');
        _;
    }
