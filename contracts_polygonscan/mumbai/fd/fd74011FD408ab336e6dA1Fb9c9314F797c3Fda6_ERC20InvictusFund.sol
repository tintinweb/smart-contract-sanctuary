        return hasRole(WHITELIST_USER_ROLE, account);
    }
    
    modifier onlyWhitelistAdmin() {
        require(hasRole(WHITELIST_ADMIN_ROLE, msg.sender), 'InvictusWhitelist: must have whitelist admin role');
        _;
    }

}


