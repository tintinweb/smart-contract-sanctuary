pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@chainlink/contracts/src/v0.8/ChainlinkClient.sol";


contract RandomPrizeNFT is ERC721, Ownable, ChainlinkClient {
// contract RandomPrizeNFT is ERC721, Ownable {
    using Chainlink for Chainlink.Request;

    using Counters for Counters.Counter;
    Counters.Counter private totalMinted;


    uint256 public volume;
    
    address private oracle;
    bytes32 private jobId;
    uint256 private fee;

    constructor() ERC721("TEST", "TEST") {
        setChainlinkToken(0x70d1F773A9f81C852087B77F6Ae6d3032B02D2AB);
        oracle = 0x0bDDCD124709aCBf9BB3F824EbC61C87019888bb;
        jobId = "c6a006e4f4844754a6524445acde84a0";
        fee = 0.1 * 10 ** 18;
    }

    function _baseURI() internal pure override returns (string memory) {
        return "ipfs://hash/";
    }

    function mint(address player)
        public
        returns (uint256) {
        require(totalMinted.current() < totalSupply(), "All available NFTs were minted");
        uint256 newItemId = random();
        _mint(player, newItemId);
        totalMinted.increment();
        return newItemId;
    }

    function random() private view returns (uint8) {
        return uint8(uint256(keccak256(abi.encode(block.timestamp)))%1001);
    }

    function requestRandom() public returns (bytes32 requestId) {
        Chainlink.Request memory request = buildChainlinkRequest(jobId, address(this), this.fulfill.selector);
        request.add("get", "https://min-api.cryptocompare.com/data/pricemultifull?fsyms=ETH&tsyms=USD");
        request.add("path", "RAW.ETH.USD.VOLUME24HOUR");
        int timesAmount = 10**18;
        request.addInt("times", timesAmount);
        return sendChainlinkRequestTo(oracle, request, fee);
    }

    function fulfill(bytes32 _requestId, uint256 _volume) public recordChainlinkFulfillment(_requestId) {
        volume = _volume;
    }

    function getVolume() public view returns (uint256) {
        return volume;
    }

    function totalSupply() public pure returns (uint256) {
        return 1000;
    }
}

