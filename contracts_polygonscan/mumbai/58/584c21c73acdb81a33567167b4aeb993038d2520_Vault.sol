// File: iglabs/Vault.sol

// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.6.2;





contract Vault is OwnableBaseRelayRecipient {
    using SafeMath for uint256;
    using Address for address;

    address caller;
    uint256 public createdVaults;
    uint256 public activeVaults;
    uint256 public interestRate = 1000400;
    uint256 public interestInterval = 60; // 86400 - 1 day // 60 - 1 min
    uint256 minimumDuration = 3;
    uint256 maximumDuration = 30;
    uint256 orbPrice = 4;

    /**
     * struct representing vault info
     * @param owner address of user who created the vault
     * @param deposit amount of tokens deposited in the vault
     * @param depositTime block timestamp of deposit
     * @param duration length of time the vault
     * @param id uint representing index in  mapping  of the vault
     * @param rate uint representing the interest rate at the time of creation
     * @param burnAmount uint representing the amount of tokens burned upon creation
     * @param reward uint representing the amount of orbs the user will receive
     * @param orbPrice uint representing the IGG:ORB price at time of capsule creation
     */

    struct vault {
        address owner;
        uint256 deposit;
        uint256 depositTime;
        uint256 duration;
        uint256 id;
        uint256 rate;
        uint256 burnAmount;
        uint256 reward;
        uint256 orbPrice;
    }

    // mapping from b
    mapping(uint256 => bool) public isActive;

    mapping(uint256 => vault) vaults;

    mapping(address => uint256[]) public ownedVaults;

    modifier isCaller() {
        require(_msgSender() == caller);
        _;
    }

    event VaultCreated(
        address owner,
        uint256 timestamp,
        uint256 deposit,
        uint256 duration,
        uint256 id,
        uint256 interestRate,
        uint256 burnAmount,
        uint256 orbs,
        uint256 orbPrice
    );
    event VaultOpened(uint256 id, uint256 timestamp);

    event InterestIntervalChanged(uint256 interval);
    event InterestRateChanged(uint256 rate);
    event MaxDurationChanged(uint256 duration);
    event MinDurationChanged(uint256 duration);
    event OrbPriceChanged(uint256 price);

    constructor(address _caller) public {
        caller = _caller;
    }

    /**
     * Creates a new Vault struct and assign it to the vault
     * mapping. Uses the current block as a timestamp
     *
     * @param a address of vault owner
     * @param deposit amount of tokens deposited into contract
     * @param duration  maximum length of vault lock
     * @param burnAmount amount of tokens burned
     */
    function CreateVault(
        address a,
        uint256 deposit,
        uint256 duration,
        uint256 burnAmount
    ) public isCaller() {
        require(
            (duration >= minimumDuration) && (duration <= maximumDuration),
            "invalid duration"
        );
        createdVaults += 1;
        uint256 reward = CalculateInterest(
            deposit - burnAmount,
            duration,
            interestRate
        );
        vault memory v = vault(
            a,
            deposit,
            now,
            duration,
            createdVaults,
            interestRate,
            burnAmount,
            reward,
            orbPrice
        );
        vaults[createdVaults] = v;
        activeVaults += 1;
        ownedVaults[a].push(createdVaults);
        isActive[createdVaults] = true;

        emit VaultCreated(
            a,
            now,
            deposit,
            duration,
            createdVaults,
            interestRate,
            burnAmount,
            reward,
            orbPrice
        );
    }

    /**
     * Return the accumulated compound interest on a vault deposit.
     * Based on the number of interest interval periods that have
     * passed since the vault creation
     *
     * @param depositMinusBurn amount of tokens deposited by user, minus the amount burnt
     * @param duration in days
     * @param rate ratio of interest being paid
     */
    function CalculateInterest(
        uint256 depositMinusBurn,
        uint256 duration,
        uint256 rate
    ) public view returns (uint256) {
        uint256 amount = depositMinusBurn;
        for (uint256 i = 0; i < duration; i++) {
            amount = (amount * rate) / 1000000;
        }
        return ((amount - depositMinusBurn) / orbPrice) / 10000;
    }

    /**
     * Changes the active status of a given vault so it cannot be called again.
     * Decreases the count of active vaults and emits a VaultOpened event
     *
     * @param v capsule ID
     */
    function deActivate(uint256 v) public isCaller() {
        isActive[v] = false;
        activeVaults -= 1;

        emit VaultOpened(v, now);
    }

    function setInterestInterval(uint256 interval) public onlyOwner() {
        interestInterval = interval;
        emit InterestIntervalChanged(interval);
    }

    function setInterestRate(uint256 rate) public onlyOwner() {
        interestRate = rate;
        emit InterestRateChanged(rate);
    }

    function returnOwnedVaults(address a)
        public
        view
        returns (uint256[] memory)
    {
        return ownedVaults[a];
    }

    function setMaxDuration(uint256 duration) public onlyOwner() {
        maximumDuration = duration;
        emit MaxDurationChanged(duration);
    }

    function setMinDuration(uint256 duration) public onlyOwner() {
        minimumDuration = duration;
        emit MinDurationChanged(duration);
    }

    function setOrbPrice(uint256 price) public onlyOwner() {
        orbPrice = price;
        emit OrbPriceChanged(price);
    }

    function returnVaultInfo(uint256 v)
        public
        view
        returns (
            address,
            uint256,
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        return (
            vaults[v].owner,
            vaults[v].deposit,
            vaults[v].depositTime,
            vaults[v].duration,
            vaults[v].burnAmount,
            vaults[v].reward
        );
    }
}