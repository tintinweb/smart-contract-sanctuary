interface IProphetItem {
    function burn(uint256) external;

    function ownerOf(uint256) external view returns (address);
}

interface IProphet is IProphetItem {
    function mint(
        address,
        uint16,
        uint16,
        uint16,
        uint16
    ) external returns (uint256);

    function prophets(uint256)
        external
        view
        returns (
            uint16,
            uint16,
            uint16,
            uint16,
            uint256,
            uint256
        );
}

interface IItem is IProphetItem {
    function mint(
        address,
        uint16,
        uint16,
        uint16,
        uint16,
        uint16
    ) external returns (uint256);

    function items(uint256)
        external
        view
        returns (
            uint16,
            uint16,
            uint16,
            uint16,
            uint16,
            uint256,
            uint256
        );
}

/**
 * @title Crypto prophecies summoning contract
 * @notice Summoning contract to mint prophet/item/pet from orb, upgrade prophet/items
 */
contract Summoning is Ownable, VRFConsumerBase {
    using SafeERC20 for ERC20Burnable;

    event SummoningRequested(
        address indexed requester,
        uint256 orbID,
        bytes32 requestID
    );

    struct SummoningRequest {
        address requester;
        uint256 orbID;
    }

    IOrb public orb;

    IProphet public prophet;
    IItem public item;

    // Magic token
    ERC20Burnable public magic;

    // Magic token amount required to summon an orb
    uint256 public summoningAmount = 10 * 1e18;

    uint256[4] public upgradeAmounts = [
        5 * 1e18,
        25 * 1e18,
        125 * 1e18,
        625 * 1e18
    ];

    // Keep track of summoning requests
    mapping(bytes32 => SummoningRequest) public summoningRequests; // maps requestID to summoning requests

    // VRT
    bytes32 internal vrfKeyHash;
    uint256 internal vrfFee;

    constructor(
        address _orb,
        address _prophet,
        address _item,
        address _magic,
        address _vrfCoordinator,
        address _link,
        bytes32 _vrfKeyHash,
        uint256 _vrfFee
    )
        VRFConsumerBase(
            _vrfCoordinator, // VRF Coordinator
            _link // LINK Token
        )
    {
        orb = IOrb(_orb);
        prophet = IProphet(_prophet);
        item = IItem(_item);
        magic = ERC20Burnable(_magic);

        vrfKeyHash = _vrfKeyHash;
        vrfFee = _vrfFee;
    }

    function summon(uint256 orbID) external {
        require(orb.balanceOf(msg.sender, orbID) > 0, "not owning the orb");

        magic.burnFrom(msg.sender, summoningAmount);

        bytes32 requestID = getRandomNumber();
        SummoningRequest storage request = summoningRequests[requestID];
        request.requester = msg.sender;
        request.orbID = orbID;

        emit SummoningRequested(msg.sender, orbID, requestID);
    }

    function _summon(bytes32 requestID, uint256 randomness) internal {
        SummoningRequest memory request = summoningRequests[requestID];

        require(request.requester != address(0), "no request");
        require(
            orb.balanceOf(request.requester, request.orbID) > 0,
            "not owning the orb"
        );

        (uint256 orbType, uint256 orbRarity, uint16 generation) = orb.orbs(
            request.orbID
        );

        uint16 rarity = _getRarity(orbRarity, randomness);

        // Burn orb item
        orb.burn(request.requester, request.orbID, 1);

        // Generate Prophet/Item NFT
        if (orbType == 0) {
            prophet.mint(
                request.requester,
                generation,
                rarity,
                uint16(randomness % 4),
                uint16(randomness % 10)
            );
        } else {
            item.mint(
                request.requester,
                generation,
                rarity,
                uint16(orbType - 1),
                uint16(randomness % 4),
                uint16(randomness % 10)
            );
        }
    }

    function upgradeProphet(uint256[] memory ids) external {
        require(ids.length == 5, "require 5 prophets");
        require(
            prophet.ownerOf(ids[0]) == msg.sender &&
                prophet.ownerOf(ids[1]) == msg.sender &&
                prophet.ownerOf(ids[2]) == msg.sender &&
                prophet.ownerOf(ids[3]) == msg.sender &&
                prophet.ownerOf(ids[4]) == msg.sender,
            "not owning the prophets"
        );
        require(
            _hasSameGenerationRarityRace(ids[0], ids[1]) &&
                _hasSameGenerationRarityRace(ids[1], ids[2]) &&
                _hasSameGenerationRarityRace(ids[2], ids[3]) &&
                _hasSameGenerationRarityRace(ids[3], ids[4]),
            "prophets should have same generation, rarity and race"
        );

        (uint16 generation, uint16 rarity, uint16 race, , , ) = prophet
            .prophets(ids[0]);

        require(rarity < 4, "can't upgrade legendary prophets");

        // transfer magic token required to upgrade
        magic.burnFrom(msg.sender, upgradeAmounts[rarity]);

        prophet.burn(ids[0]);
        prophet.burn(ids[1]);
        prophet.burn(ids[2]);
        prophet.burn(ids[3]);
        prophet.burn(ids[4]);

        prophet.mint(
            msg.sender,
            generation,
            rarity + 1,
            race,
            uint16(_simpleRandomNumber() % 10)
        );
    }

    function upgradeItem(uint256[] memory ids) external {
        require(ids.length == 5, "require 5 items");
        require(
            item.ownerOf(ids[0]) == msg.sender &&
                item.ownerOf(ids[1]) == msg.sender &&
                item.ownerOf(ids[2]) == msg.sender &&
                item.ownerOf(ids[3]) == msg.sender &&
                item.ownerOf(ids[4]) == msg.sender,
            "not owning the items"
        );
        require(
            _hasSameGenerationRarityClassMagicSource(ids[0], ids[1]) &&
                _hasSameGenerationRarityClassMagicSource(ids[1], ids[2]) &&
                _hasSameGenerationRarityClassMagicSource(ids[2], ids[3]) &&
                _hasSameGenerationRarityClassMagicSource(ids[3], ids[4]),
            "items should have same generation, rarity and magic source"
        );

        (
            uint16 generation,
            uint16 rarity,
            uint16 class,
            uint16 magicSource,
            ,
            ,

        ) = item.items(ids[0]);

        require(rarity < 4, "can't upgrade legendary items");

        // transfer magic token required to upgrade
        magic.burnFrom(msg.sender, upgradeAmounts[rarity]);

        item.burn(ids[0]);
        item.burn(ids[1]);
        item.burn(ids[2]);
        item.burn(ids[3]);
        item.burn(ids[4]);

        item.mint(
            msg.sender,
            generation,
            rarity + 1,
            class,
            magicSource,
            uint16(_simpleRandomNumber() % 10)
        );
    }

    function _hasSameGenerationRarityRace(uint256 id1, uint256 id2)
        internal
        view
        returns (bool)
    {
        (uint16 generation1, uint16 rarity1, uint16 race1, , , ) = prophet
            .prophets(id1);
        (uint16 generation2, uint16 rarity2, uint16 race2, , , ) = prophet
            .prophets(id2);

        return
            generation1 == generation2 && rarity1 == rarity2 && race1 == race2;
    }

    function _hasSameGenerationRarityClassMagicSource(uint256 id1, uint256 id2)
        internal
        view
        returns (bool)
    {
        (
            uint16 generation1,
            uint16 rarity1,
            uint16 class1,
            uint16 magicSource1,
            ,
            ,

        ) = item.items(id1);
        (
            uint16 generation2,
            uint16 rarity2,
            uint16 class2,
            uint16 magicSource2,
            ,
            ,

        ) = item.items(id2);

        return
            generation1 == generation2 &&
            rarity1 == rarity2 &&
            class1 == class2 &&
            magicSource1 == magicSource2;
    }

    function _simpleRandomNumber() internal view returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        blockhash(block.number - 1),
                        block.timestamp
                    )
                )
            );
    }

    function _getRarity(uint256 orbRarity, uint256 randomness)
        internal
        view
        returns (uint16)
    {
        (
            uint16 common,
            uint16 uncommon,
            uint16 rare,
            uint16 epic,
            uint16 legendary
        ) = orb.rarities(orbRarity);
        uint16 total = common + uncommon + rare + epic + legendary;
        uint16 mod = uint16(randomness % uint256(total)) + 1;

        if (common > 0) {
            if (mod <= common) return 0;
            mod = mod - common;
        }
        if (uncommon > 0) {
            if (mod <= uncommon) return 1;
            mod = mod - uncommon;
        }
        if (rare > 0) {
            if (mod <= rare) return 2;
            mod = mod - rare;
        }
        if (epic > 0 && mod <= epic) return 3;
        return 4;
    }

    /**
     * Requests randomness from a user-provided seed
     */
    function getRandomNumber() internal returns (bytes32 requestID) {
        require(LINK.balanceOf(address(this)) >= vrfFee, "Not enough LINK");
        return requestRandomness(vrfKeyHash, vrfFee);
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 requestID, uint256 randomness)
        internal
        override
    {
        _summon(requestID, randomness);
    }
}

