pragma solidity ^0.8.0;

import "@maticnetwork/fx-portal/contracts/tunnel/FxBaseRootTunnel.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/metatx/ERC2771ContextUpgradeable.sol";

import "../ASSPLayer/ASSPLayer.sol";

contract PlaNFT {
    uint256 public constant GROWTH_CYCLE = 7 days;
    uint256 public constant WATERING_WINDOW = 3 hours;

    struct Plant {
        uint16 waterLevel;
        uint256 plantedAt;
        address owner;
        string tokenUri;
        bool alive;
    }

    struct ContentData {
        string mime;
        string content;
        string uri;
        bytes32 hash;
    }

    struct ProjectConfig {
        address ownerAddress;
        mapping(bool => ContentData) content;
    }

    mapping(address => mapping(uint256 => Plant)) public plants;
    mapping(address => ProjectConfig) public projects;
}

contract PlaNFTChild is ASSPLayerChild, PlaNFT {
    event StateUpdated(address contractAddress, uint256 tokenId, address owner, uint256 plantedAt, uint16 waterLevel);

    function _onRegistration(
        address mainnetContract,
        uint256 tokenId,
        address ownerAddress
    ) internal override {
        require(plants[mainnetContract][tokenId].owner == address(0), "401");

        plants[mainnetContract][tokenId] = Plant({
            waterLevel: 0,
            plantedAt: block.timestamp,
            owner: ownerAddress,
            tokenUri: "",
            alive: true
        });

        StateUpdated(mainnetContract, tokenId, ownerAddress, block.timestamp, 0);
    }

    function _onProjectRegistration(address mainnetContractAddress, address contractOwnerAddress) internal override {
        projects[mainnetContractAddress].ownerAddress = contractOwnerAddress;
    }

    function alive(address contractAddress, uint256 tokenId) public view returns (bool) {
        Plant storage plant = plants[contractAddress][tokenId];
        if (plant.plantedAt == 0) return false;
        if (plant.alive == false) return false;

        uint256 elapsed = block.timestamp - plant.plantedAt;
        uint256 fullCycles = elapsed / GROWTH_CYCLE;

        if (plant.waterLevel == fullCycles) {
            return true;
        }

        if (plant.waterLevel + 1 == fullCycles && elapsed % GROWTH_CYCLE < WATERING_WINDOW) {
            return true;
        }

        return false;
    }

    function water(address contractAddress, uint256 tokenId) public onlyApproved(_msgSender()) {
        Plant storage plant = plants[contractAddress][tokenId];

        if (!alive(contractAddress, tokenId)) {
            plant.alive = false;
            StateUpdated(contractAddress, tokenId, plant.owner, plant.plantedAt, plant.waterLevel);
            return;
        }

        // overwatered - storing alive = false in struct prevents overwatered plants
        // from becoming alive again after some time
        if (plant.waterLevel > (block.timestamp - plant.plantedAt) / GROWTH_CYCLE) {
            plant.alive = false;
        }

        plant.waterLevel += 1;

        StateUpdated(contractAddress, tokenId, plant.owner, plant.plantedAt, plant.waterLevel);
    }

    function content(
        address contractAddress,
        uint256 tokenId,
        string calldata mimeType
    ) public returns (ContentData memory) {
        Plant memory plant = plants[contractAddress][tokenId];
        ProjectConfig storage project = projects[contractAddress];

        if (!alive(contractAddress, tokenId)) {
            return project.content[false];
        }
    }
}

contract PlaNFTParent is ASSPLayerParent {
    constructor(address _checkpointManager, address _fxRoot) ASSPLayerParent(_checkpointManager, _fxRoot) {}

    function _processMessageFromChild(bytes memory data) internal override {}
}

