import {FxBaseChildTunnelUpgradeable} from "../Libraries/tunnel/FxBaseChildTunnelUpgradeable.sol";
import {WrasslersGasStation} from "../WrasslersGasStation/WrasslersGasStation.sol";

abstract contract ASSPLayer {
    mapping(address => bool) public registeredContract;

    function registerToken(address mainnetContract, uint256 tokenId) external virtual {}

    function registerProject(address contractAddress, bytes calldata data) public payable virtual {}

    function _onProjectRegistration(address contractAddress, address contractOwnerAddress) internal virtual {}

    function _beforeProjectRegistration(address contractAddress, address contractOwnerAddress) internal virtual {}

    function jsonDataForToken(address mainnetContract, uint256 tokenId) public virtual returns (string memory) {}
}

contract ASSPLayerChild is
    ASSPLayer,
    ERC2771ContextUpgradeable,
    FxBaseChildTunnelUpgradeable,
    AccessControlUpgradeable
{
    struct OwnedToken {
        address contractAddress;
        uint256 tokenId;
    }

    /**
     * @dev Emitted when `owner` enables or disables (`approved`) `operator` to manage all of its assets.
     */
    event ApprovalForAll(address indexed owner, address indexed operator, bool approved);

    // Mapping from owner to operator approvals
    mapping(address => mapping(address => bool)) private _operatorApprovals;

    // Mapping from contract to token ID to owner address
    mapping(address => mapping(uint256 => address)) private _owners;

    // Mapping owner address to token count
    mapping(address => uint256) private _balances;

    // Mapping from owner to list of OwnedToken structs
    mapping(address => mapping(uint256 => OwnedToken)) private _ownedTokens;

    // Mapping from contract to token ID to index of the owner tokens list
    mapping(address => mapping(uint256 => uint256)) private _ownedTokensIndex;

    modifier onlyForwarder() {
        require(isTrustedForwarder(msg.sender), "ASSPLayer:429");
        _;
    }

    modifier onlyApproved(address owner) {
        require(isApprovedForAll(owner, msg.sender), "ASSPLayer:429");
        _;
    }

    function initialize(WrasslersGasStation trustedForwarder, address _fxChild) public initializer {
        _setupRole(DEFAULT_ADMIN_ROLE, address(msg.sender));
        ERC2771ContextUpgradeable.__ERC2771Context_init_unchained(address(trustedForwarder));
        FxBaseChildTunnelUpgradeable.initialize(_fxChild);
    }

    function registerProject(address contractAddress, bytes calldata data) public payable override onlyForwarder {
        _beforeProjectRegistration(contractAddress, _msgSender());
        registeredContract[contractAddress] = true;
        _onProjectRegistration(contractAddress, _msgSender());
    }

    function registerToken(address mainnetContract, uint256 tokenId) external override onlyForwarder {
        address existingOwner = _owners[mainnetContract][tokenId];

        if (existingOwner != address(0)) {
            _removeTokenFromOwnerEnumeration(existingOwner, mainnetContract, tokenId);
        }

        _addTokenToOwnerEnumeration(_msgSender(), mainnetContract, tokenId);
        _onRegistration(mainnetContract, tokenId, _msgSender());
    }

    function registerTokenDev(address mainnetContract, uint256 tokenId) public {
        address existingOwner = _owners[mainnetContract][tokenId];

        if (existingOwner != address(0)) {
            _removeTokenFromOwnerEnumeration(existingOwner, mainnetContract, tokenId);
        }

        _addTokenToOwnerEnumeration(_msgSender(), mainnetContract, tokenId);
        _onRegistration(mainnetContract, tokenId, _msgSender());
    }

    function transferOwnership(address mainnetContract, uint256 tokenId) internal {}

    function _processMessageFromRoot(
        uint256,
        address sender,
        bytes memory data
    ) internal view override validateSender(sender) {
        // solhint-disable-next-line no-unused-vars
        (bool _success, bytes memory _v) = address(this).staticcall(data);
    }

    /**
     * @dev See {IERC721-balanceOf}.
     */
    function balanceOf(address owner) public view virtual returns (uint256) {
        require(owner != address(0), "ERC721: balance query for the zero address");
        return _balances[owner];
    }

    /**
     * @dev See {IERC721-ownerOf}.
     */
    function ownerOf(address contractAddress, uint256 tokenId) public view virtual returns (address) {
        address owner = _owners[contractAddress][tokenId];
        require(owner != address(0), "ERC721: owner query for nonexistent token");
        return owner;
    }

    /**
     * @dev See {IERC721Enumerable-tokenOfOwnerByIndex}.
     */
    function tokenOfOwnerByIndex(address owner, uint256 index) public view virtual returns (OwnedToken memory) {
        require(index < balanceOf(owner), "ERC721Enumerable: owner index out of bounds");
        return _ownedTokens[owner][index];
    }

    /**
     * @dev Private function to add a token to this extension's ownership-tracking data structures.
     * @param to address representing the new owner of the given token ID
     * @param tokenId uint256 ID of the token to be added to the tokens list of the given address
     */
    function _addTokenToOwnerEnumeration(
        address to,
        address contractAddress,
        uint256 tokenId
    ) private {
        uint256 length = _balances[_msgSender()];
        _ownedTokens[to][length] = OwnedToken({contractAddress: contractAddress, tokenId: tokenId});
        _ownedTokensIndex[contractAddress][tokenId] = length;
        _owners[contractAddress][tokenId] = to;
        _balances[_msgSender()] += 1;
    }

    /**
     * @dev Private function to remove a token from this extension's ownership-tracking data structures. Note that
     * while the token is not assigned a new owner, the `_ownedTokensIndex` mapping is _not_ updated: this allows for
     * gas optimizations e.g. when performing a transfer operation (avoiding double writes).
     * This has O(1) time complexity, but alters the order of the _ownedTokens array.
     * @param from address representing the previous owner of the given token ID
     * @param tokenId uint256 ID of the token to be removed from the tokens list of the given address
     */
    function _removeTokenFromOwnerEnumeration(
        address from,
        address contractAddress,
        uint256 tokenId
    ) private {
        // To prevent a gap in from's tokens array, we store the last token in the index of the token to delete, and
        // then delete the last slot (swap and pop).

        uint256 lastTokenIndex = balanceOf(from) - 1;
        uint256 tokenIndex = _ownedTokensIndex[contractAddress][tokenId];

        // When the token to delete is the last token, the swap operation is unnecessary
        if (tokenIndex != lastTokenIndex) {
            OwnedToken memory lastToken = _ownedTokens[from][lastTokenIndex];

            _ownedTokens[from][tokenIndex] = lastToken; // Move the last token to the slot of the to-delete token
            _ownedTokensIndex[contractAddress][tokenId] = tokenIndex; // Update the moved token's index
        }

        // This also deletes the contents at the last position of the array
        delete _ownedTokensIndex[contractAddress][tokenId];
        delete _ownedTokens[from][lastTokenIndex];
        _balances[from] -= 1;
    }

    /**
     * @dev See {IERC721-setApprovalForAll}.
     */
    function setApprovalForAll(address operator, bool approved) public {
        require(operator != _msgSender(), "ERC721: approve to caller");

        _operatorApprovals[_msgSender()][operator] = approved;
        emit ApprovalForAll(_msgSender(), operator, approved);
    }

    /**
     * @dev See {IERC721-isApprovedForAll}.
     */
    function isApprovedForAll(address owner, address operator) public view returns (bool) {
        return _operatorApprovals[owner][operator];
    }

    function _msgSender()
        internal
        view
        virtual
        override(ERC2771ContextUpgradeable, ContextUpgradeable)
        returns (address sender)
    {
        return ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData()
        internal
        view
        virtual
        override(ContextUpgradeable, ERC2771ContextUpgradeable)
        returns (bytes calldata)
    {
        if (isTrustedForwarder(msg.sender)) {
            return msg.data[:msg.data.length - 20];
        } else {
            return ERC2771ContextUpgradeable._msgData();
        }
    }

    function _onRegistration(
        address mainnetContract,
        uint256 tokenId,
        address ownerAddress
    ) internal virtual {}

    function _onProjectRegistration(address contractAddress, address contractOwnerAddress) internal virtual override {}

    function _beforeProjectRegistration(address contractAddress, address contractOwnerAddress)
        internal
        virtual
        override
    {}
}

contract ASSPLayerParent is ASSPLayer, FxBaseRootTunnel {
    constructor(address _checkpointManager, address _fxRoot) FxBaseRootTunnel(_checkpointManager, _fxRoot) {}

    function registerProject(address contractAddress, bytes calldata) public payable override {
        _beforeProjectRegistration(contractAddress, msg.sender);
        try Ownable(contractAddress).owner() returns (address owner) {
            require(owner == msg.sender, "APP:409");
            bytes4 selector = bytes4(keccak256("_onProjectRegistration(address,address)"));
            bytes memory message = abi.encodeWithSelector(selector, contractAddress, owner);
            _sendMessageToChild(message);
        } catch {}
    }

    function registerToken(address contractAddress, uint256 tokenId) public override {
        address owner = ERC721(contractAddress).ownerOf(tokenId);
        bytes4 selector = bytes4(keccak256("_onRegistration(address,uint256,address)"));

        bytes memory message = abi.encodeWithSelector(selector, contractAddress, tokenId, owner);

        _sendMessageToChild(message);
    }

    function _processMessageFromChild(bytes memory data) internal virtual override {}
}

