pragma solidity >=0.7.0 <0.9.0;
////import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
/** 
 * @title Ballot
 * @dev Implements voting process along with vote delegation
 */
contract CheckERC {
   constructor(){}
   function checkERC721(address addr) public view {
       bool result =  ERC165Checker.supportsInterface(addr, 0x5b5e139f);
       require(result==true,"Not ERC721");
   }
   
   function checkAnyERC(address addr, bytes4 interfaceId) public view {
       bool result =  ERC165Checker.supportsInterface(addr, interfaceId);
       require(result==true,"Not supporting");
   }
}