    function mint(address _toAddress, uint256 _id, uint256 _amount, bytes memory _data) public multiSigReq
            {
                LibDiamond.enforceIsContractOwner();
                if (_id == 1) {
                        require(totalSupply(_id) < 1000000000, "Reached Maximum ATK Distribution.");
                    }

                _mint(_toAddress, _id, _amount, _data);
            }

    function run_proxy_conditions(ProxyFunctionCaller storage _function, string[] memory _values) private multiSigReq returns (bool)
        {
            
            //Decouple Lengthy functionality from mint functions -

            LibDiamond.enforceIsContractOwner();
            LibDiamond.DiamondStorage storage ds = LibDiamond.diamondStorage();
            bytes32 position = LibDiamond.DIAMOND_STORAGE_POSITION;
            // get diamond storage
            assembly {
                ds.slot := position
            }
    
            //string memory functionName = _function._call;
            bytes4 functionSelector = bytes4(keccak256(abi.encodePacked(_function._call)));

            // Get facet address of function
            address facet = ds.facetAddressAndSelectorPosition[functionSelector].facetAddress;
            bytes memory myFunctionCall = abi.encodeWithSelector(functionSelector, _values);
            (bool success, ) = address(facet).delegatecall(myFunctionCall);
            string memory _err_message = string(abi.encodePacked(_function._name,"- error 'function call was unsuccessful"));
            require(success, _err_message);
            return true;

        }

    modifier multiSigReq()
        {
             require(msg.sender == multisign_acc, "Unauthorized Request");
             _;
        }


    //Minting interface for ATK, ARC, SG_ITEMS Function with Proxy(public visible)
    function _mintRequest(address _toAddress, uint256 _id, uint256 _value, bytes memory _data, uint256 prxy_id, string[] memory _values) public multiSigReq

        {
            
            require(eval_prxy_call(prxy_id, _values), 'Mint request was unsuccessful!');
            
            mint(_toAddress, _id, _value, _data);

        }
   


