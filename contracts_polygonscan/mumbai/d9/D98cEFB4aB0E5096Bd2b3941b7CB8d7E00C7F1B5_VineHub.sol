pragma solidity ^0.6.12;

contract Ownable{

    address payable internal _owner;

    modifier onlyOwner(){
        require(msg.sender == _owner,
            "You need to be owner of the contract in order to access this functionality!");
        _;
    }

    constructor() public{
        _owner = msg.sender;
    }
}

