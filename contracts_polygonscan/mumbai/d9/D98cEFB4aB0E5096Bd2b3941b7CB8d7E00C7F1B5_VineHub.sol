// note:
// token = 1 $
// ticket = 0.1 $


contract VineHub is Ownable {
    using SafeMath for uint256;
    using SafeMath for uint8;
    using SafeERC20 for IERC20;
    address public token = address(0xC6EEA9a70c3169ae33E336c0457EaEBe93204eB8); // mdo polygon

    mapping(address => bool) public admin;

    uint priceTicket = 0.1 ether;

    struct Player{
        uint id;
        address account;
        uint balance;
        uint ticket;
    }

    struct Game {
        uint id;
        address ownerGame;
        uint typeGame;
        string name;
        string description;
        string avata;
        bool isFree;
        uint ticket;
        bool isActive;
        uint deposit;
        string urlOfGame;
    }

    struct RegisReward{
        uint idGame;
        address[] accounts;
        uint deposit; // require of game
        uint streasure;
        uint timeStart;
        uint timeEnd;
    }


    Player[] public Players;
    Game[] public Games;
    mapping(address => uint[]) public GamesOf;
    mapping(address => Player ) public PlayerOf;


    modifier onlyAdmin(){
        require(admin[msg.sender], "contract is not allowed");
        _;
    }

    constructor() public {
        admin[msg.sender] = true;
        _owner = msg.sender;

    }

    function regisStoreGame(address _ownerGame, uint _typeGame ,string memory _name, string memory _description,
        string memory _avata, uint _ticket , bool _isFree, uint _deposit, string memory _urlOfGame ) public onlyAdmin {

        require( bytes(_name).length > 0, "Name game is not null");
        require( bytes(_description).length > 0, "Description game is not null");
        require( bytes(_avata).length > 0, "Avata game is not null");
        require( _ticket >= 0, "Ticket game invalid");
        require( _deposit >= 0, "Deposit game invalid");

        uint _idx = Games.length;
        Games.push(Game({
        id: _idx,
        ownerGame: _ownerGame,
        typeGame: _typeGame,
        name: _name,
        description: _description,
        avata: _avata,
        isFree: _isFree,
        ticket: _ticket,
        isActive: true,
        deposit: _deposit,
        urlOfGame: _urlOfGame
        }));
        GamesOf[_ownerGame].push(_idx);

    }

    function totalGame() public view returns(uint) {
        return Games.length;
    }

    function getMyGame(address _ownerGame) public view returns(uint[] memory){
        return GamesOf[_ownerGame];
    }

    function getGame(uint idx) public view returns (uint _idx , address _ownerGame, uint _typeGame, string memory _name,
        string memory _description, string memory _avata, bool _isFree, uint _ticket, bool _isActive, uint _deposit, string memory _urlOfGame){

        Game memory game = Games[idx];

        _idx = game.id;
        _ownerGame = game.ownerGame;
        _typeGame = game.typeGame;
        _name = game.name;
        _description = game.description;
        _avata = game.avata;
        _isFree = game.isFree;
        _ticket = game.ticket;
        _isActive = game.isActive;
        _deposit = game.deposit;
        _urlOfGame = game.urlOfGame;

    }

    function buyTicket(uint _mount) public  {
        require(_mount > 0, "Amount invalid");
        require(msg.sender != _owner, "Account is not allow");

        IERC20(token).safeTransferFrom(msg.sender, _owner, _mount );

        uint _ticket = _mount * priceTicket;

        uint _idx = Players.length;
        Players.push(Player({
        id: _idx,
        account: msg.sender,
        balance: 1 ether, // fix balance of player
        ticket : _ticket
        }));

        PlayerOf[msg.sender]=(Player({
        id: _idx,
        account: msg.sender,
        balance: 1 ether,
        ticket : _ticket
        }));

    }

    function getPlayer() public view returns(uint _idx, address _account, uint _balance, uint _ticket) {

        _idx = PlayerOf[msg.sender].id;
        _account = PlayerOf[msg.sender].account;
        _balance = PlayerOf[msg.sender].balance;
        _ticket = PlayerOf[msg.sender].ticket;

    }


}

