}







library UserLibrary {
    struct User {
        uint64 registrationTime;
        uint64 badgesCount;
        uint64 unclamReward;
        uint64 clamedReward;
        address parent;
        bool isKYCed;

        mapping(uint => bool) badges;
    }

    /**
    * @dev this function is used to get information about user existance
    * send true if yes else false
     */
    function _exists(User storage self) internal view returns (bool) {
        return self.registrationTime != uint(0);
    }

    /**
    * @dev this function is used to add used
    * parent is a referrer
     */

    function _addUser(User storage self, address _parent) internal returns (bool) {
        self.parent = _parent;
        self.registrationTime = uint64(block.timestamp);
        return true;
    }

    /**
    * @dev this function is used to set KYC
     */

    function _approveKYC(User storage self) internal {
        self.isKYCed = true;
    }

    /**
    * @dev this function is used to revoke the kyc
     */

    function _revokeKYC(User storage self) internal {
        if (self.isKYCed == true) {
            self.isKYCed = false;
        }
    }

    /**
    * @dev this function is used give badges to user
    * @param _badgesId is the badge id to set true
     */
    function _setBadges(User storage self, uint _badgesId) internal {
        self.badges[_badgesId] = true;
        self.badgesCount++;
    }

    /**
    * @dev this function is used to revoke the badges
    * @param _badgesId is the badge id to set false
     */

    function _revertBadges(User storage self, uint _badgesId) internal {
        if(self.badges[_badgesId] = true){
            self.badges[_badgesId] = false;
            self.badgesCount--;
        }
    }
}








library SurveyLibrary {
    using UserSurveyLibrary for UserSurveyLibrary.UserSurvey;

    struct Survey {
        uint32 startTime;
        uint32 thresholdParticipants;
        uint32 endTime;
        uint32 noOfQuestions;
        uint32 responseCount;
        uint64 poolAmount;
        uint16 Criteria;
        uint16 status; // 1 for active; 2 for complete; 3 for revoke
        string discription;
        mapping(uint32 => uint32) questions;
        mapping(uint32 => address) responsers;
    }

    /**
    * @dev this function is used to set all information about survey
    *   @param _noOfQuestions for total number of questions in survey
    *   @param maxParticipants for thresholdParticipants
    *   @param _questions array for all the questions ID
    *   @param _endTime for survey life time
    *   @param _poolAmount is total amount for reward
    *   @param _discription is small string about survey
    *   @param _criteria is lower limit to answer for having successful response
     */
    function _createSurvey(Survey storage self, uint32 _noOfQuestions, uint32 maxParticipants, 
                            uint32[] calldata _questions, uint32 _endTime, uint32 _poolAmount, 
                            string memory _discription, uint16 _criteria) internal returns (bool) {
        self.startTime = uint32(block.timestamp);
        self.thresholdParticipants = maxParticipants;
        self.endTime = _endTime;
        self.poolAmount = _poolAmount;
        self.status = 1;
        self.noOfQuestions = _noOfQuestions;
        self.discription = _discription;
        self.Criteria = _criteria;
        for(uint32 i = 1; i <= _noOfQuestions; i++) {
            self.questions[i] = _questions[i-1];
        }
        return true;
    }

    /**
    * @dev this function is used to set all information about survey
    *   @param _noOfQuestions for total number of questions in survey
    *   @param _questions array for all the questions ID
    *   @param _discription is small string about survey
    *   @param _criteria is lower limit to answer for having successful response
     */
    function _createSurveyForBadges(Survey storage self, uint32 _noOfQuestions, uint32[] calldata _questions, 
                                    string memory _discription, uint16 _criteria) internal returns (bool) {
        self.startTime = uint32(block.timestamp);
        self.status = 1;
        self.noOfQuestions = _noOfQuestions;
        self.discription = _discription;
        self.Criteria = _criteria;
        for(uint32 i = 1; i <= _noOfQuestions; i++) {
            self.questions[i] = _questions[i-1];
        }
        return true;
    }

    /**
    * @dev this function is used to add responses 
    * @param id is the response token ID
     */

    function _takeSurvey(Survey storage self, address id) internal returns (bool) {
        self.responseCount++;
        self.responsers[self.responseCount] = id;
        return true;
    }

    function testMap(Survey storage self, uint32 id) internal view returns(uint32){
        return(self.questions[id]);
    }
}





