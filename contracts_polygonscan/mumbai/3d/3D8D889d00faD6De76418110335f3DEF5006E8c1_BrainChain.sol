contract BrainChain is ReentrancyGuard, CustomOwnable , brainchainEvents{
    using UserLibrary for UserLibrary.User;
    using SurveyLibrary for SurveyLibrary.Survey;
    using AnswerLibrary for AnswerLibrary.Answer;
    using UserSurveyLibrary for UserSurveyLibrary.UserSurvey;   
    using SafeMath for uint64;
    
    EQ8token equity;
    NFT public surveyNFT;
    NFT public questionNFT;
    NFT public answerNFT;

    uint32 answerId;
    uint32 kycReward;
    uint32 newUserReward;
    uint32 totalBadges;
    uint64 questionId;
    uint64 surveyId;
    uint private constant DECIMAL_FACTOR = 10 ** 18;
    bool internal isInitialized;
    address public root;

    mapping(address => UserLibrary.User) public users;
    mapping(address => UserSurveyLibrary.UserSurvey) public userSurveys;
    mapping(uint => SurveyLibrary.Survey) public surveys;  //SurveyId => Survey Detail Object
    mapping(uint => AnswerLibrary.Answer) public answers;  //AnswerId => Answer Detail Object
    mapping(string => bool) public questionURI; //QuestionHash => is already Exist
    mapping(uint => uint64) public rewardPercentages; // uint => percentage for kyc, badge, rarity
    mapping(uint => uint) public badgesSurvey; //surveyId => badgeId;


    /**
    * @dev this modifier is used to confirm that 
    *       @param user should be registered
     */
    modifier isExist(address user) {
        require(users[user]._exists(), "Brainchain: User Not Exist");
        _;
    }

    /**
    * @dev this modifier is used to confirm that 
    *       @param user should not be registered
     */
    modifier shouldNotExist(address user) {
        require(!users[user]._exists(), "Brainchain: User Already Exist");
        _;
    }

    /**
    * @dev this modifier is used to confirm that
    *   @param user is @param _surveyId 's creater
     */

    modifier onlySurveyCreater(uint _surveyId, address user) {
        require(surveyNFT.ownerOf(_surveyId) == user, "BrainChain: User is not survey's creater");
        _;
    }

    /**
    * @dev this modifier is used to check
    *   is @param _surveyId 's survey active or not
     */

    modifier isSurveyActive(uint _surveyId) {
        if(uint32(block.timestamp) > surveys[_surveyId].endTime || 
            surveys[_surveyId].responseCount == surveys[_surveyId].thresholdParticipants){
                surveys[_surveyId].status = 2;
        }
        require(surveys[_surveyId].status == 1, "Survey is not active");
        _;
    }

    /**
    * @dev this modifier is used to check
    *   is @param _surveyId 's survey completed or not
     */

     modifier isSurveyComplete(uint _surveyId) {
         if(uint32(block.timestamp) > surveys[_surveyId].endTime || 
            surveys[_surveyId].responseCount == surveys[_surveyId].thresholdParticipants){
                surveys[_surveyId].status = 2;
        }
        require(surveys[_surveyId].status == 2, "Survey is not complete");
        _;
    }

    /**
    * @dev this modifier is used to check
    *   is @param _surveyId is belongs to badge's survey
     */

     modifier isBadgeSurvey(uint _surveyId) {
        require(badgesSurvey[_surveyId] != 0, "Survey is not belongs to badge's survey");
        _;
    }

    /**
    * @dev this fucntion is initalize the contract
    *
    * @param _root is the owner address
    * @param _equity is the ECR20 token address
    * @param _nft is survey NFT address
    * @param question_nft is question's NFT address
    * @param answer_nft is answer's NFT address
    * 
    * Note
    * it sets all the counter with 1
    * and register the owner
     */

    function initialize(address _root, address _equity, address _nft, address question_nft, 
                        address answer_nft) public {
        require(!isInitialized,"BrainChain: Already initialized");
        isInitialized = true;
        _setOwner(_root);
        root = _root;
        equity = EQ8token(_equity);
        surveyNFT = NFT(_nft);
        questionNFT = NFT(question_nft);
        answerNFT = NFT(answer_nft);

        surveyId = 1;
        questionId = 1;
        answerId = 1;
        users[root].registrationTime = uint64(block.timestamp);
        users[root].isKYCed = true;
    }

    /**
    * @dev this function is used to set reward amount for getting KYC
     */

    function setKYCReward(uint val) external onlyOwner{
        kycReward = uint32(val);
        emit SetKYCReward(val, block.timestamp);
    }

    /**
    * @dev this function is used to set reward amount for new user
     */

    function setNewUserReward(uint val) external onlyOwner{
        newUserReward = uint32(val);
        emit setNewUserRewardAmount(val, block.timestamp);
    }

    /**
    * @dev this function is used to register the user
    * anyone can call it
    *
    * Requirement: 
    *   @param referrer should be already registerd
    *
    * Note user will get some reward
     */

    function registerUser(address referrer) external isExist(referrer) shouldNotExist(_msgSender()) returns (bool) {
        users[_msgSender()]._addUser(referrer);
        equity._mint(_msgSender(), newUserReward);
        users[_msgSender()].clamedReward += newUserReward;
        emit registerNewUser(_msgSender(),referrer, block.timestamp);
        emit EQ8MintTransfer(_msgSender(),newUserReward, block.timestamp);
        return true;
    }

    function isUserExist(address user) external view returns(bool){
        return(users[user]._exists());
    }

    /**
    * @dev this function is used to set KYC 
    * only called by owner
    *
    * Requirement:
    *   @param user address to identify the user
    * Note
    *    get some Reward and have more reward perenctage
     */

    function setKYC(address user) external onlyOwner {
        users[user]._approveKYC();
        equity._mint(user, kycReward);
        users[user].clamedReward += kycReward;
        emit setKYCForUser(user, block.timestamp);
        emit EQ8MintTransfer(user,kycReward, block.timestamp);
    }

    /**
    * @dev this function is used to revert the revert the kyc
    * only called by the owner
    *
    * Requirement:
    *   @param user address to identify the user
     */
    function revertKYC(address user) external onlyOwner {
        users[user]._revokeKYC();
        emit revertKYCOfUser(user, block.timestamp);
    }

    /**
    * @dev this function is used to give badges to the users
    * only called by the owner
    *
    * Requirement:
    *   @param _badge to be given
    *   @param _user address to identify the user 
     */
    function setBadges(uint _badge, address _user) external onlyOwner{
        users[_user]._setBadges(_badge);
        emit RewardBadge(_badge, _user, block.timestamp);
    }

    /**
    * @dev the function is used to create questions 
    * one question has one ID
    * user is not able to pass same question again
    * 
    * Requirement:
    *   @param _URI is the question
    *   @param _hash of the question is used to identify existance
     */

    function createQuestion(string memory _URI,string memory _hash) external isExist(_msgSender()){
        require(!questionURI[_hash],"This Question is already Exist");
        questionNFT.mint(_msgSender(), questionId, _URI);
        // questions[questionId]._createQuestion(_msgSender(),questionId,_URI);
        questionURI[_hash] = true;
        questionId++;
        emit QuestionCreation(_msgSender(), questionId, _URI, _hash, block.timestamp);
    }

    /**
    * @dev the function is used to create questions in batch
    * one question has one ID
    * user is not able to pass same question again
    * 
    * Requirement:
    *   @param _URI is the questions
    *   @param _hash of the questions is used to identify existance
     */

    function createQuestionInBatch(string[] calldata _URI, string[] calldata _hash) 
                                    external isExist(_msgSender()){
        require(_URI.length == _hash.length, "Brainchain: URI and hashes length won't match");
        for(uint i = 0; i < _URI.length; i++){
            require(!questionURI[_hash[i]],"This Question is already Exist");
            questionNFT.mint(_msgSender(), questionId, _URI[i]);
            questionURI[_hash[i]] = true;
            questionId++;
            emit QuestionCreation(_msgSender(), questionId, _URI[i], _hash[i], block.timestamp);
        }
    }

    /**
    * @dev this function used to create survey
    * Registed user can call this function
    * 
    * Requirements:
    *   before creating survey make sure your all questions have it's ID by CreateQuestion function
    *   @param noOfQuestions must be equal to array pass for @param questionIds
    *   @param maxParticipants is for thresholdParticipants
    *   @param _endTime should be greater than now and it should be in UNIX format
    *   @param _poolAmount is total amount for reward
    *   @param _discription is small string about survey
    *   @param _criteria is lower limit to answer for having successful response
    *
    * Note survey create will get NFT token
     */

    function createSurvey(uint32 noOfQuestions, uint32[] calldata questionIds, 
                        uint32 maxParticipants, uint32 _endTime, uint32 _poolAmount,
                        string memory _discription, uint16 _criteria) external isExist(_msgSender()) {
        require(_endTime > block.timestamp, "BRAINCHAIN: Invalid end time");
        require( _poolAmount > 0, 'Amount must be greater than zero');
        require(noOfQuestions > 0,"Number of questions must be greater than zero");
        require(questionIds.length == noOfQuestions,"Array length must be equal to number of questions");
        require(maxParticipants > 0,"Participants must be greater than zero");
        for(uint i = 0; i < noOfQuestions;i++){
            require(questionNFT.ownerOf(questionIds[i]) != address(0),"ERC721: owner query for nonexistent token");
        }
        surveyNFT.mint(_msgSender(), surveyId, _discription);
        surveys[surveyId]._createSurvey(noOfQuestions, maxParticipants, questionIds,
                                         _endTime, _poolAmount, _discription, _criteria);
        emit SurveyCreation(_msgSender(), surveyId, noOfQuestions, questionIds, maxParticipants, _endTime,
                         _poolAmount, _discription, _criteria, block.timestamp);
        surveyId++;
    }

    /**
    * @dev this function used to create survey for badges
    * Only owner can call this function
    * 
    * Requirements:
    *   before creating survey make sure your all questions have it's ID by CreateQuestion function
    *   @param noOfQuestions must be equal to array pass for @param questionIds
    *   @param _discription is small string about survey
    *   @param _criteria is lower limit to answer for having successful response
    *
    * Note survey create will get NFT token
     */

    function createSurveyForBadges(uint32 noOfQuestions, uint32[] calldata questionIds, 
                                    string memory _discription, uint16 _criteria, uint _badgeId) 
                                    external onlyOwner{
        require(noOfQuestions > 0,"Number of questions must be greater than zero");
        require(questionIds.length == noOfQuestions,"Array length must be equal to number of questions");
        for(uint i = 0; i < noOfQuestions;i++){
            require(questionNFT.ownerOf(questionIds[i]) != address(0),"ERC721: owner query for nonexistent token");
        }
        surveyNFT.mint(_msgSender(), surveyId, _discription);
        surveys[surveyId]._createSurveyForBadges(noOfQuestions, questionIds, _discription, _criteria);
        badgesSurvey[surveyId] = _badgeId;
        emit SurveyCreationForBadges(surveyId, noOfQuestions, questionIds, _discription, _criteria, 
                                 _badgeId,  block.timestamp);
        surveyId++;
    }

    /**
    * @dev this function used to response the survey 
    * only reqistered user can call
    * 
    * Requirement:
    *   @param _surveyId should be in active state
    *   @param _questionId should exist in this survey
    *   @param _answerURI is answer of that question you passed
    *
    * Note
    *   After successful response responser will get NFT token
     */

    function responseForBadges(uint64 _surveyId, uint _questionId, string memory _answerURI) 
                    external isExist(_msgSender()) isBadgeSurvey(_surveyId){
        require(_surveyId != 0, 'BRAINCHAIN: Survey Not Exists');
        require(_surveyId < surveyId, 'BRAINCHAIN: Survey Not Exists');
        if(userSurveys[_msgSender()].surveyQuestions[_surveyId][_questionId] == 0){
            answerNFT.mint(_msgSender(), answerId, _answerURI);
            answers[answerId]._response(uint(_surveyId));
            if(userSurveys[_msgSender()].questionCount[_surveyId] + 1 == surveys[_surveyId].Criteria){
                surveys[_surveyId]._takeSurvey(_msgSender());
                users[_msgSender()]._setBadges(badgesSurvey[_surveyId]);
                emit RewardBadge(badgesSurvey[_surveyId], _msgSender(), block.timestamp);
            }
            userSurveys[_msgSender()].updateResponseList(_surveyId, _questionId, answerId);
            answerId++;
            emit newResponseForBadges(_msgSender(), _surveyId, _questionId, _answerURI, 
                                    answerId-1, block.timestamp);
        } else{
            answerNFT.setURI(answerId, _answerURI);
            emit ChangeResponseForBadges(_msgSender(), _surveyId, _questionId, _answerURI, 
                                        answerId, block.timestamp);
        }
    }

    /**
    * @dev this function used to response the survey 
    * only reqistered user can call
    * 
    * Requirement:
    *   @param _surveyId should be in active state
    *   @param _questionId should exist in this survey
    *   @param _answerURI is answer of that question you passed
    *
    * Note
    *   After successful response responser will get NFT token
     */

    function responseForBadgesInBatch(uint64 _surveyId, uint[] calldata _questionId, string[] calldata _answerURI) 
                    external isExist(_msgSender()) isBadgeSurvey(_surveyId){
        require(_surveyId != 0, 'BRAINCHAIN: Survey Not Exists');
        require(_surveyId < surveyId, 'BRAINCHAIN: Survey Not Exists');
        require(_questionId.length == _answerURI.length, 'No. of Questions should be equal to No. of Answers');
        for(uint i = 0; i < _questionId.length; i++){
            if(userSurveys[_msgSender()].surveyQuestions[_surveyId][_questionId[i]] == 0){
            answerNFT.mint(_msgSender(), answerId, _answerURI[i]);
            answers[answerId]._response(uint(_surveyId));
            if(userSurveys[_msgSender()].questionCount[_surveyId] + 1 == surveys[_surveyId].Criteria){
                surveys[_surveyId]._takeSurvey(_msgSender());
                users[_msgSender()]._setBadges(badgesSurvey[_surveyId]);
                emit RewardBadge(badgesSurvey[_surveyId], _msgSender(), block.timestamp);
            }
            userSurveys[_msgSender()].updateResponseList(_surveyId, _questionId[i], answerId);
            answerId++;
            emit newResponseForBadges(_msgSender(), _surveyId, _questionId[i], _answerURI[i], 
                                    answerId-1, block.timestamp);
        } else{
            answerNFT.setURI(userSurveys[_msgSender()].surveyQuestions[_surveyId][_questionId[i]], _answerURI[i]);
            emit ChangeResponseForBadges(_msgSender(), _surveyId, _questionId[i], _answerURI[i], 
                                        answerId, block.timestamp);
        }
        }
    }

    function revokeSurvey(uint64 _surveyId) external onlySurveyCreater(_surveyId, _msgSender()){
            surveys[_surveyId].status = 3;
            emit surveyRevoke(_surveyId, block.timestamp);
    } 

    /**
    * @dev this function used to response the survey 
    * only reqistered user can call
    * 
    * Requirement:
    *   @param _surveyId should be in active state
    *   @param _questionId should exist in this survey
    *   @param _answerURI is answer of that question you passed
    *
    * Note
    *   After successful response responser will get NFT token
     */

    function response(uint64 _surveyId, uint _questionId, string memory _answerURI) 
                    external isExist(_msgSender()) isSurveyActive(_surveyId){
        require(_surveyId != 0, 'BRAINCHAIN: Survey Not Exists');
        require(_surveyId < surveyId, 'BRAINCHAIN: Survey Not Exists');
        if(userSurveys[_msgSender()].surveyQuestions[_surveyId][_questionId] == 0){
            answerNFT.mint(_msgSender(), answerId, _answerURI);
            answers[answerId]._response(uint(_surveyId));
            if(userSurveys[_msgSender()].questionCount[_surveyId] + 1 == surveys[_surveyId].Criteria){
                surveys[_surveyId]._takeSurvey(_msgSender());
            }
            userSurveys[_msgSender()].updateResponseList(_surveyId, _questionId, answerId);
            emit newResponse(_msgSender(), _surveyId, _questionId, _answerURI, answerId-1, block.timestamp);
            answerId++;
        } else{
            answerNFT.setURI(answerId, _answerURI);
            emit ChangeResponse(_msgSender(), _surveyId, _questionId, _answerURI, answerId, block.timestamp);

        }
    }

    /**
    * @dev this function used to response the survey in batch
    * only reqistered user can call
    * 
    * Requirement:
    *   @param _surveyId should be in active state
    *   @param _questionId should exist in this survey
    *   @param _answerURI is answer of that question you passed
    *
    * Note
    *   After successful response responser will get NFT token
     */

    function responseInBatch(uint64 _surveyId, uint[] calldata _questionId, string[] calldata _answerURI) 
                    external isExist(_msgSender()) isSurveyActive(_surveyId){
        require(_surveyId != 0, 'BRAINCHAIN: Survey Not Exists');
        require(_surveyId < surveyId, 'BRAINCHAIN: Survey Not Exists');
        require(_questionId.length == _answerURI.length, 'No. of Questions should be equal to No. of Answers');
        for(uint i = 0 ; i < _questionId.length; i++){
            if(userSurveys[_msgSender()].surveyQuestions[_surveyId][_questionId[i]] == 0){
                answerNFT.mint(_msgSender(), answerId, _answerURI[i]);
                answers[answerId]._response(uint(_surveyId));
                if(userSurveys[_msgSender()].questionCount[_surveyId] + 1 == surveys[_surveyId].Criteria){
                    surveys[_surveyId]._takeSurvey(_msgSender());
                }
                userSurveys[_msgSender()].updateResponseList(_surveyId, _questionId[i], answerId);
                emit newResponse(_msgSender(), _surveyId, _questionId[i], _answerURI[i], answerId-1, block.timestamp);
                answerId++;
            } else{
                answerNFT.setURI(answerId, _answerURI[i]);
                emit ChangeResponse(_msgSender(), _surveyId, _questionId[i], _answerURI[i], answerId, block.timestamp);

        }
        }
    }

    /**
    * @dev this function is used to set percentage 
    *       for kyc, badges and rarity to use in reward calculation.
    *   @param _kyc is kyc percentage
    *   @param _badges is badges percentage
        @param _rarity is rarity percentage
    *
    * Note
    * only called by owner
    * Sum of all three parameter should be 100
     */

    function setRewardPercentages(uint64 _kyc, uint64 _badges, uint64 _rarity) external onlyOwner{
        require(_kyc.add(_badges.add(_rarity)) == 100, "Sum of all three parameter should be 100");
        rewardPercentages[1] = _kyc;
        rewardPercentages[2] = _badges;
        rewardPercentages[3] = _rarity;
        emit setPercentagesForRewards(_kyc, _badges, _rarity, block.timestamp);
    }

    /**
    * @dev this function used to set totalBadges
    * and only called by owner
     */

    function setBadgesAmount(uint val) external onlyOwner{
        totalBadges = uint32(val);
        emit setNoOfBadges(val, block.timestamp);
    }

    /**
    * @dev This function used to get reward amount a responser deserve
    * perPerson = poolAmount/ thresholdParticipant
    * reward% he/she get = kyc% + badge% + rarity%
    *
    * Requirement:
    *      set kyc, badge, rarity percentage for rewards
    *         by calling setRewardPercentages function
    *      set total Badges too 
    *         by calling setBadgesAmount
    * @param userWallet address to identify the responser
    * @param _surveyId to get information about survey
     */

    function rewardCalculation(address userWallet, uint _surveyId) public view 
                        isExist(_msgSender()) returns(uint){
        require(totalBadges != 0, "Set total Badges first");
        require(rewardPercentages[1].add(rewardPercentages[2].add(rewardPercentages[3])) != 0, 
                        "Set all three percentage dependences first");
        uint perPerson = (uint64(surveys[_surveyId].poolAmount)).div(surveys[_surveyId].thresholdParticipants);
        uint64 kycPercentage = rewardPercentages[1];
        if(!users[userWallet].isKYCed){
            kycPercentage = 0;
        }
        uint badgesPercentage = (uint64(((users[userWallet].badgesCount)
                                .mul(rewardPercentages[2]))).div(uint64(totalBadges)));
        uint RewardPercentage = kycPercentage.add(uint64(badgesPercentage).add(rewardPercentages[3]));
        uint reward = uint64((uint64(perPerson).mul(uint64(RewardPercentage)))).div(uint64(100));
        return(reward);
    }

    /**
    * @dev This function Distribute rewards to the responsers
    * Remaining Equity send to the Survey Owner address
    * Only called by owner
    *
    *Requirements: 
    *   @param _surveyId which rewards going to be distributed
    *   survey should be End before distribution 
    *
    * NOTE it calls rewardCalculation function
    *         to get reward to be sent to individual
     */

    function rewardDistribution(uint _surveyId) external onlyOwner isSurveyComplete(_surveyId){
        require(_surveyId != 0, 'BRAINCHAIN: Survey Not Exists');
        require(_surveyId < surveyId, 'BRAINCHAIN: Survey Not Exists');
        // require(!endSurvey(uint64(_surveyId)), 'BRAINCHAIN: Survey is NOT END');
        uint remainPoolAmount = surveys[_surveyId].poolAmount;
        for(uint i = 1; i <= surveys[_surveyId].responseCount; i++){
            uint reward = rewardCalculation(surveys[_surveyId].responsers[uint32(i)], _surveyId);
            equity._mint(surveys[_surveyId].responsers[uint32(i)], reward);
            users[surveys[_surveyId].responsers[uint32(i)]].clamedReward +=  uint64(reward);
            remainPoolAmount -= reward;
            emit EQ8MintTransfer(surveys[_surveyId].responsers[uint32(i)], reward, block.timestamp);
        }
        equity._mint(surveyNFT.ownerOf(_surveyId), remainPoolAmount);
        users[surveyNFT.ownerOf(_surveyId)].unclamReward += uint64(remainPoolAmount);
        emit EQ8MintTransfer(surveyNFT.ownerOf(_surveyId), remainPoolAmount, block.timestamp);
    }

