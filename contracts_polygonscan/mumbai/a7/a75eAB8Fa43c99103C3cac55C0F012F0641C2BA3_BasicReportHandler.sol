/*contract ReportProgressHandler {
}*/

contract BasicReportHandler is IReportHandler, IStorage {
    function reportProgress(
        IBillingUnits.Message[] memory _messages /*address _bconContract*/
    ) external override {
        Metadata storage md = metadata();
        address _bconContract = md.bconContract;

        BillingData storage bd = billingData();

        // ReportConfig laden
        IConfigurationHandler.ReportConfig memory config = IBconContract(_bconContract)
            .getReportConfig();
        require(config.bconContract == _bconContract);

        require(_messages.length > 0, 'empty messagelist');

        for (uint256 i = 0; i < _messages.length; i++) {
            IBillingUnits.Message memory message = _messages[i];

            // minimal check
            require(message.msgType == SharedStates.MessageType.ReportDone, 'invalid report type');
            //msg.sender
            require(msg.sender == _bconContract, 'sender must be bconContract');

            string memory _itemId = message.billingUnitItemId;
            IBillingUnits.BillingUnitItem storage item = bd.billingUnitItems[_itemId];

            // MsgType = ReportDone + BillingUnit-Origin = Issue => Fertigstellungsmeldung f체r M채ngel
            if (bd.billingUnits[item.billingUnitId].origin == SharedStates.Origin.Issue) {
                console.log('[ReportHandler] throw message into bconContract');
                IBconContract(_bconContract).throwRectificationWorkReported(
                    IBconContract.BillingUnitItemEventPayload({
                        billingUnitItem: item,
                        timestamp: block.timestamp,
                        reportId: message.id,
                        sender: md.contractor,
                        reportType: SharedStates.MessageType.ReportDone,
                        fileIds: message.fileIds
                    })
                );
            }

            // Daten abgleichen: Meldung + Soll
            require(
                (item.completionRateTotal + item.completionRateClaimed + message.completionRate) <=
                    1000000000,
                'invalid completion rate; must be lower than 100%'
            );

            // Update des Datenbestands ermitteln
            item.state = SharedStates.BillingUnitItemState.CompletionStarted;

            // throw event in BconContract
            console.log('[ReportHandler] throw message into bconContract');
            IBconContract(_bconContract).throwBillingUnitItemReported(
                IBconContract.BillingUnitItemEventPayload({
                    billingUnitItem: item,
                    timestamp: block.timestamp,
                    reportId: message.id,
                    sender: md.contractor,
                    reportType: SharedStates.MessageType.ReportDone,
                    fileIds: message.fileIds
                }),
                message.completionQuantity,
                message.completionPrice,
                message.completionRate
            );

            if (
                (item.completionRateTotal + item.completionRateClaimed + message.completionRate) ==
                1000000000
            ) {
                item.state = SharedStates.BillingUnitItemState.CompletionClaimed;
                console.log('[ReportHandler] completion ready!');
                // throw event in BconContract
                IBconContract(_bconContract).throwBillingUnitItemCompleted(
                    IBconContract.BillingUnitItemEventPayload({
                        billingUnitItem: item,
                        timestamp: block.timestamp,
                        reportId: message.id,
                        sender: md.contractor,
                        reportType: SharedStates.MessageType.ReportDone,
                        fileIds: message.fileIds
                    }),
                    message.completionQuantity,
                    message.completionPrice,
                    message.completionRate
                );
            }

            item.outstandingReports += 1;
            item.completionRateClaimed += message.completionRate;

            // Message sichern
            bd.itemMessages[message.billingUnitItemId].push(message);
            IBconContract(_bconContract).throwNewMessage(message);
            console.log('[BillingUnits] added new item', message.billingUnitItemId, message.id);
        }
    }
}

