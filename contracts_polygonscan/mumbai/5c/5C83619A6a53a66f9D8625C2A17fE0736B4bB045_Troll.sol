pragma solidity ^0.8.7;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";

import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";


import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "./ERC721EnumerableUpgradeable.sol";
import "./VRFConsumerBaseUpgradeable.sol";

interface IInvite {
    function balanceOf(address owner) external returns (uint256);
}

interface IBeryl {
    function burn(address from, uint256 amount) external;
    function balanceOf(address owner) external returns (uint256);
}

interface IMine {
    function randomTrollOwner(uint256 seed) external view returns (address);
}

interface IRenderer {
    function tokenMetadata(uint256 tokenId, bool isDwarf, uint8[15] calldata traitarray) external view returns (string memory);
}

contract Troll is ReentrancyGuardUpgradeable, ERC721EnumerableUpgradeable, OwnableUpgradeable, PausableUpgradeable, VRFConsumerBaseUpgradeable {

    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using AddressUpgradeable for address;
    using SafeERC20Upgradeable for IERC20Upgradeable;

    // mint variables
    uint256 public constant MAX_TOKENS = 25000;     // max number of tokens that can be minted
    uint256 public PAID_TOKENS;     // max number of tokens that can be minted for WETH

    uint16 public minted;                           // number of tokens have been minted so far
    string public baseURI;

    uint256 public constant MINT_PRICE = .03 ether;                         // mint price
    uint256 public constant MINT_PRICE_INV = .01 ether;                     // mint price for invited

    uint256 public constant MINT_ADDR_LIMIT = 50;                     // mint limit

    // mappings
    mapping(uint256 => DwarfTroll) private tokenTraits;                     // mapping from tokenId to a struct containing the token's traits
    mapping(uint256 => uint256) private existingCombinations;              // mapping from hashed(tokenTrait) to the tokenId it's associated with

    struct DwarfTroll {bool isDwarf; uint8[15] traitarray; uint8 alphaIndex;}

    // 0 - 8 are associated with dwarf, 9 - 13 are associated with troll
    uint8[][18] public rarities;
    uint8[][18] public aliases;

    IMine public mine;      // STAKING
    IBeryl public beryl;    // ERC20 Token
    IInvite public invite;  // Invites
    IRenderer public renderer;  // Renderer and Metadata

    IERC20Upgradeable public weth;

    address private project_wallet;

    bool isPublicSale;

    mapping(address => uint256) private mintersList;

    //Chainlink Setup:
    bytes32 internal keyHash;
    uint256 public fee;
    uint256 internal randomResult;
    uint256 internal randomNumber;
    address public linkToken;
    uint256 public vrfcooldown;

    struct Minting {
        address minter;
        uint256 tokenId;
        bool fulfilled;
    }

    mapping(bytes32=>Minting) mintings;

    function initialize(address _beryl, address _invite, address _renderer, address _vrfCoordinator, address _link, address _weth) initializer public {

        __ERC721_init("Troll Game", "TROLLGAME");
        __ERC721Enumerable_init();
        __Ownable_init();
        __ReentrancyGuard_init();
        __Pausable_init();
        __VRFConsumerBase_init(_vrfCoordinator,_link);


        // Chainklink
        //keyHash = 0xf86195cf7690c55907b2b611ebb7343a6f649bff128701cc542f0569e2c549da;
        keyHash = 0x6e75b569a01ef56d18cab6a8e71e6600d6ce853834d4a5748b720d06f878b3a4;
        fee = 0.0001 * 10 ** 18; // 0.0001 LINK (Varies by network)
        linkToken = _link;
        vrfcooldown = 300;

        // Team wallet
        project_wallet = 0x15064dE4a9EB4d5498796ab870B52a0af84a9548;

        isPublicSale = false;

        PAID_TOKENS = 5000;

        _pause();

        // Initate Interfaces
        beryl = IBeryl(_beryl);
        invite = IInvite(_invite);
        renderer = IRenderer(_renderer);

        weth = IERC20Upgradeable(_weth);

        rarities[0] = [255,255];
        rarities[1] = [39,51,59,67,125,131,189,197,204,217];
        rarities[2] = [39,51,59,67,125,131,189,197,204,217];
        rarities[3] = [59,111,145,197];
        rarities[4] = [34,59,118,164,197,222];
        rarities[5] = [39,51,59,67,125,131,189,197,204,217];
        rarities[6] = [59,111,145,197];

        aliases[0] = [1,0]; // skin
        aliases[1] = [9,8,7,6,5,4,3,2,1,0];  //Back
        aliases[2] = [9,8,7,6,5,4,3,2,1,0]; //Wear
        aliases[3] = [3,2,1,0]; //Beard
        aliases[4] = [5,4,3,2,1,0]; //Foot
        aliases[5] = [9,8,7,6,5,4,3,2,1,0]; // Head
        aliases[6] = [3,2,1,0]; //Tool

        rarities[7] = [32,64,96,128,160,192,244,255];
        rarities[8] = [39,51,59,67,125,131,189,197,204,217];
        rarities[9] = [39,51,59,67,125,131,189,197,204,217];
        rarities[10] = [39,131,212];
        rarities[11] = [39,51,59,67,125,131,189,197,204,217];
        rarities[12] = [39,51,59,67,125,131,189,197,204,217];
        rarities[13] = [51,102,153,204];
        rarities[14] = [255];

        aliases[7] = [7,6,5,4,3,2,1,0]; // skin
        aliases[8] = [9,8,7,6,5,4,3,2,1,0]; //Head
        aliases[9] = [9,8,7,6,5,4,3,2,1,0]; //Accessories
        aliases[10] = [2,1,0]; //Armor
        aliases[11] = [9,8,7,6,5,4,3,2,1,0]; //Foot
        aliases[12] = [9,8,7,6,5,4,3,2,1,0]; //Clothing
        aliases[13] = [3,2,1,0]; //Weapon
        aliases[14] = [0]; //bg

    }


    function mintCostForBeryl(uint256 tokenId) public view returns (uint256) {
        if (tokenId <= PAID_TOKENS) return 0;
        if (tokenId <= 10000) return 200 ether;
        if (tokenId <= 15000) return 400 ether;
        if (tokenId <= 20000) return 600 ether;
        return 800 ether;
    }

    function mint(uint256 amount) external payable whenNotPaused nonReentrant() {

        address msgSender = _msgSender();

        require(!_msgSender().isContract(), "Contracts are not allowed");
        require(tx.origin == msgSender, "Only EOA");
        require(minted + amount <= MAX_TOKENS, "All tokens minted");
        require(amount > 0 && amount <= 10, "Invalid mint amount");
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK - fill contract with faucet");

        if (minted < PAID_TOKENS) {

            if (!isPublicSale){
                require(invite.balanceOf(msgSender) > 0, "Sales are only for those who have invitations, try in an hour");
            }

            uint256 ethPrice;

            if (invite.balanceOf(msgSender) > 0)    //Have invitation
                ethPrice = MINT_PRICE_INV;          //0.1
            else
                ethPrice = MINT_PRICE;              //0.3

            uint256 mintCostEther = ethPrice * amount;

            uint256 balance = weth.balanceOf(msgSender);

            require(minted + amount <= PAID_TOKENS, "All gen0 tokens on-sale already sold");
            require(mintersList[msgSender] + amount <= MINT_ADDR_LIMIT, 'Exceed limit per wallet');

            require(balance >= mintCostEther, "Not enough wETH");
            weth.safeTransferFrom(msgSender, address(this), mintCostEther);

        }
        else {


            uint256 amountPrice = 0;

            for (uint i = 0; i < amount; i++) {
                amountPrice += mintCostForBeryl(minted + 1 + i);
            }

            require(amountPrice <= beryl.balanceOf(msgSender), "You need to send enough beryl");
            beryl.burn(msgSender, amountPrice);

        }


        for (uint i = 0; i < amount; i++) {

            minted++;

            bytes32 requestId = requestRandomness(keyHash, fee);
            mintings[requestId] = Minting(msgSender, minted, false);

            if (minted - 1 < PAID_TOKENS) {
                mintersList[msgSender] += 1;
            }

        }

    }


    //generator

    function generate(uint256 tokenId, uint256 seed) internal returns (DwarfTroll memory t) {

        t = selectTraits(seed);
        if (existingCombinations[structToHash(t.isDwarf, t.traitarray, t.alphaIndex)] == 0) {
            tokenTraits[tokenId] = t;
            existingCombinations[structToHash(t.isDwarf, t.traitarray, t.alphaIndex)] = tokenId;
            return t;
        }
        return generate(tokenId, random(seed));
    }




    function selectTrait(uint16 seed, uint8 traitType) private view returns (uint8) {

        uint8 trait = uint8(seed) % uint8(rarities[traitType].length);
        if (seed >> 8 < rarities[traitType][trait]) return trait;
        return aliases[traitType][trait];

    }

    // selects the species and all of its traits based on the seed value
    function selectTraits(uint256 seed) internal view returns (DwarfTroll memory t) {
        t.isDwarf = (seed & 0xFFFF) % 10 != 0;
        uint8 shift = t.isDwarf ? 0 : 7;                                          // 0 if its a Dwarf, 7 if its Troll

        seed >>= 16;
        if (t.isDwarf) {

            // / 0 - 6 are associated with dwarf,

            t.traitarray[0] = selectTrait(uint16(seed & 0xFFFF), 0 + shift);
            seed >>= 16;
            t.traitarray[1] = selectTrait(uint16(seed & 0xFFFF), 1 + shift);
            seed >>= 16;
            t.traitarray[2] = selectTrait(uint16(seed & 0xFFFF), 2 + shift);
            seed >>= 16;
            t.traitarray[3] = selectTrait(uint16(seed & 0xFFFF), 3 + shift);
            seed >>= 16;
            t.traitarray[4] = selectTrait(uint16(seed & 0xFFFF), 4 + shift);
            seed >>= 16;
            t.traitarray[5] = selectTrait(uint16(seed & 0xFFFF), 5 + shift);
            seed >>= 16;
            t.traitarray[6] = selectTrait(uint16(seed & 0xFFFF), 6 + shift);
            seed >>= 16;

            t.alphaIndex = 0;

        } else {
            // 7 - 13 are associated with Trolls

            t.traitarray[7] = selectTrait(uint16(seed & 0xFFFF), 0 + shift);
            seed >>= 16;
            t.traitarray[8] = selectTrait(uint16(seed & 0xFFFF), 1 + shift);
            seed >>= 16;
            t.traitarray[9] = selectTrait(uint16(seed & 0xFFFF), 2 + shift);
            seed >>= 16;
            t.traitarray[10] = selectTrait(uint16(seed & 0xFFFF), 3 + shift);
            seed >>= 16;
            t.traitarray[11] = selectTrait(uint16(seed & 0xFFFF), 4 + shift);
            seed >>= 16;
            t.traitarray[12] = selectTrait(uint16(seed & 0xFFFF), 5 + shift);
            seed >>= 16;
            t.traitarray[13] = selectTrait(uint16(seed & 0xFFFF), 6 + shift);

            t.alphaIndex = t.traitarray[13];

        }
    }

    // converts a struct to a 256 bit hash to check for uniqueness
    function structToHash(bool isDwarf, uint8[15] memory traitarray, uint8 alphaIndex) internal pure returns (uint256) {
        if(isDwarf){
            return uint256(bytes32(abi.encodePacked(true,
                traitarray[0],
                traitarray[1],
                traitarray[2],
                traitarray[3],
                traitarray[4],
                traitarray[5],
                traitarray[6],
                "0",
                "0",
                "0",
                "0",
                "0",
                "0",
                "0",
                alphaIndex)));
        }
        else{
            return uint256(bytes32(abi.encodePacked(false,
                "0",
                "0",
                "0",
                "0",
                "0",
                "0",
                "0",
                traitarray[7],
                traitarray[8],
                traitarray[9],
                traitarray[10],
                traitarray[11],
                traitarray[12],
                traitarray[13],
                alphaIndex)));
        }

    }


    // Select who the NFT goes to --- The first 20% (ETH purchases) go to the minter & the remaining 80% have a 10% chance to be given to a random staked Troll
    function selectRecipient(uint256 seed) private view returns (address) {

        if (minted <= PAID_TOKENS || ((seed >> 245) % 10) != 0) return _msgSender();

        address thief = mine.randomTrollOwner(seed >> 144);

        if (thief == address(0x0)) return _msgSender();
        return thief;
    }


    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override {
        if (_msgSender() != address(mine))
            require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");
        _transfer(from, to, tokenId);
    }

    function getTokenTraits(uint256 tokenId) public view returns (DwarfTroll memory) {
        return tokenTraits[tokenId];
    }

    // called after deployment, set Mine contract
    function setMine(address _mine) external onlyOwner {
        mine = IMine(_mine);
        getRandomChainlink();
    }

    function setBeryl(address erc20Address) public onlyOwner {
        beryl = IBeryl(erc20Address);
    }

    function setRenderer(address _renderer) public onlyOwner {
        renderer = IRenderer(_renderer);
    }

    function setPaused(bool _paused) external onlyOwner {
        if (_paused) _pause();
        else _unpause();
    }

    function flipPublicSale() external onlyOwner{
        isPublicSale = !isPublicSale;
    }


    function withdraw() public payable onlyOwner {
        payable(project_wallet).transfer(address(this).balance);
    }

    function withdrawWeth() public onlyOwner {
        uint256 balance = weth.balanceOf(address(this));
        weth.transfer(project_wallet, balance);
    }


    function airDrop(address[] calldata addressArrays) external onlyOwner  {

        uint256 addrLength = addressArrays.length;

        require(addrLength + minted <= 1000, 'There are no freemint left');

        getRandomChainlink();

        for (uint256 i; i < addrLength; i++ ){

            minted++;

            uint256 seed = random(minted);
            generate(minted, seed);
            _safeMint(addressArrays[i], minted);

            mintersList[addressArrays[i]] += 1;

        }

    }

    function setPaidTokens(uint256 num) external onlyOwner {
        PAID_TOKENS = num;
    }

    function setBaseURI(string memory newUri) public onlyOwner {
        baseURI = newUri;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function getTokenIds(address _owner) public view returns (uint256[] memory _tokensOfOwner) {
        _tokensOfOwner = new uint256[](balanceOf(_owner));
        for (uint256 i;i<balanceOf(_owner);i++){
            _tokensOfOwner[i] = tokenOfOwnerByIndex(_owner, i);
        }
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {

        DwarfTroll memory traits = tokenTraits[tokenId];

        return renderer.tokenMetadata(tokenId, traits.isDwarf, traits.traitarray);

    }

    //Randomness

    function random(uint256 seed) private view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(
                tx.origin,
                blockhash(block.number - 1),
                block.timestamp,
                seed,
                randomNumber
            )));
    }

    function changeLinkFee(uint256 _fee) external onlyOwner {
        fee = _fee;
    }

    function getRandomChainlink() internal returns (bytes32 requestId) {

        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK - fill contract with faucet");
        return requestRandomness(keyHash, fee);

    }

    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {

        randomNumber = randomness;

        Minting storage minting = mintings[requestId];

        if (minting.minter != address(0)){

            uint256 seed;

            seed = random(minting.tokenId);
            generate(minting.tokenId, seed);                        // Generates Token Traits and adds it to the array
            address recipient = selectRecipient(seed);     // Selects who the NFT is going to.

            _mint(recipient, minting.tokenId);

        }

    }

    function withdrawLINK() external onlyOwner {
        uint256 tokenSupply = IERC20Upgradeable(linkToken).balanceOf(address(this));
        IERC20Upgradeable(linkToken).transfer(msg.sender, tokenSupply);
    }

    function changeLinkAddress(address _newaddress) external onlyOwner{
        linkToken = _newaddress;
    }

    function changeLinkHash(bytes32 _newhash) external onlyOwner{
        keyHash = _newhash;
    }

}

