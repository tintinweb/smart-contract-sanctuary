        view
        returns(uint256)
    {
        uint256 newRewardPerToken = pool.rewardsPerTokenPoints.sub(pool.stakers[_user].lastRewardPoints);
        uint256 rewards = pool.stakers[_user].balance.mul(newRewardPerToken)
                                                .div(1e18)
                                                .div(pointMultiplier);


        return rewards;
    }

