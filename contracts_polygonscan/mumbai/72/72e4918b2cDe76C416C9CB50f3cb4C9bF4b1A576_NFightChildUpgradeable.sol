pragma solidity >=0.8.0 <0.9.0;

import "@openzeppelin/contracts-upgradeable/metatx/ERC2771ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PullPaymentUpgradeable.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

import {INFight} from "../Interfaces/NFight/INFight.sol";
import {VRFConsumerBase} from "../Libraries/UpgradeableVRFConsumerBase/UpgradeableVRFConsumerBase.sol";
import {Base64} from "../Libraries/Base64.sol";

import {ASSPLayerChild} from "../ASSPLayer/ASSPLayerChild.sol";

contract NFightChildUpgradeable is ASSPLayerChild, VRFConsumerBase, PullPaymentUpgradeable, ReentrancyGuardUpgradeable {
    using Strings for uint256;

    ERC20 public NFTXP;

    bytes32 public constant REFEREE_ROLE = keccak256("REFEREE_ROLE");
    uint256 public constant attributeBaseCost = 1e16; // 0.01 MATIC

    bytes32 public keyHash;
    uint256 public fee;

    struct MainnetNFT {
        address contractAddress;
        uint256 tokenId;
    }

    struct Player {
        uint8 maxDefeated;
        uint256 wins;
        uint256 losses;
        uint256 spend;
    }

    event FighterUpdated(
        address contractAddress,
        uint256 tokenId,
        address ownerAddress,
        uint256 aggression,
        uint256 awareness,
        uint256 determination,
        uint256 power,
        uint256 resilience,
        uint256 speed
    );

    mapping(address => mapping(uint256 => INFight.Fighter)) public nFighters;
    mapping(address => address) internal contractOwners;
    mapping(address => Player) public players;

    mapping(uint256 => uint256) internal randomnessForToken;
    mapping(bytes32 => MainnetNFT) internal tokenForRequest;

    function initialize(
        address trustedForwarder,
        address wrassToken,
        address vRFCoordinator,
        address linkToken,
        bytes32 _keyhash,
        address _fxChild
    ) public initializer {
        NFTXP = ERC20(wrassToken);
        __PullPayment_init_unchained();
        VRFConsumerBase.unchainedInitialize(vRFCoordinator, linkToken);
        ASSPLayerChild.initialize(trustedForwarder, _fxChild);
        keyHash = _keyhash;
        fee = 0.0001 * 10**18;
    }

    function _msgData() internal view override(ERC2771ContextUpgradeable, ASSPLayerChild) returns (bytes calldata) {
        return super._msgData();
    }

    function _msgSender() internal view override(ERC2771ContextUpgradeable, ASSPLayerChild) returns (address sender) {
        return super._msgSender();
    }

    /* solhint-disable quotes */

    function jsonDataForToken(address mainnetContract, uint256 tokenId) public view override returns (string memory) {
        INFight.Fighter memory fighter = nFighters[mainnetContract][tokenId];

        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(bytes(abi.encodePacked('{"attributes":', encodedAttributes(fighter), '"}}')))
                )
            );
    }

    function encodedAttributes(INFight.Fighter memory fighter) internal pure returns (bytes memory) {
        return
            abi.encodePacked(
                '{"aggression":"',
                fighter.aggression.toString(),
                '", "awareness":"',
                fighter.awareness.toString(),
                '", "determination":"',
                fighter.determination.toString(),
                '", "power":"',
                fighter.power.toString(),
                '", "resilience":"',
                fighter.resilience.toString(),
                '", "speed":"',
                fighter.speed.toString()
            );
    }

    /* solhint-enable quotes */

    function _beforeProjectRegistration(address, address) internal override {
        // need to swap MATIC for LINK
        // https://pegswap.chain.link
        // require(msg.value == 1 ether, "no payment");
    }

    function _onProjectRegistration(address wrassleableContract, address contractOwner) internal override {}

    function setMatchAddress(address matchAddress) public onlyRole(DEFAULT_ADMIN_ROLE) {
        grantRole(REFEREE_ROLE, matchAddress);
    }

    function reportWin(address player) public onlyRole(REFEREE_ROLE) {
        players[player].wins += 1;
    }

    function reportLoss(address player) public onlyRole(REFEREE_ROLE) {
        players[player].losses += 1;
    }

    function _onRegistration(
        address mainnetContract,
        uint256 tokenId,
        address ownerAddress
    ) internal override {
        require(nFighters[mainnetContract][tokenId].power == 0, "WA:rW:409");

        nFighters[mainnetContract][tokenId] = INFight.Fighter({
            power: 0,
            speed: 0,
            aggression: 0,
            determination: 0,
            resilience: 0,
            awareness: 0
        });

        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK");
        bytes32 requestId = requestRandomness(keyHash, fee);
        tokenForRequest[requestId] = MainnetNFT({contractAddress: mainnetContract, tokenId: tokenId});
    }

    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        MainnetNFT storage token = tokenForRequest[requestId];
        INFight.Fighter storage fighter = nFighters[token.contractAddress][token.tokenId];
        uint256[] memory attributes = calculateAttributes(randomness, 6);

        fighter.aggression = attributes[0];
        fighter.awareness = attributes[1];
        fighter.determination = attributes[2];
        fighter.power = attributes[3];
        fighter.resilience = attributes[4];
        fighter.speed = attributes[5];

        emit FighterUpdated(
            token.contractAddress,
            token.tokenId,
            ownerOf(token.contractAddress, token.tokenId),
            fighter.aggression,
            fighter.awareness,
            fighter.determination,
            fighter.power,
            fighter.resilience,
            fighter.speed
        );
    }

    function calculateAttributes(uint256 randomValue, uint256 n)
        internal
        view
        returns (uint256[] memory expandedValues)
    {
        expandedValues = new uint256[](n);
        for (uint256 i = 0; i < n; i++) {
            uint256 raw = uint256(keccak256(abi.encode(randomValue, i, block.timestamp)));
            expandedValues[i] = (raw % 400) + 350;
        }
        return expandedValues;
    }

    receive() external payable {}

    function upgradeWrassler(
        address mainnetContract,
        uint256 tokenId,
        INFight.Fighter memory upgradeParams
    ) public nonReentrant {
        INFight.Fighter storage fighter = nFighters[mainnetContract][tokenId];

        uint256 total = calculateUpgrades(fighter, upgradeParams);
        bool success = NFTXP.transferFrom(_msgSender(), address(this), total);

        require(success, "buy failed");

        fighter.power += upgradeParams.power;
        fighter.speed += upgradeParams.speed;
        fighter.aggression += upgradeParams.aggression;
        fighter.determination += upgradeParams.determination;
        fighter.resilience += upgradeParams.resilience;
        fighter.awareness += upgradeParams.awareness;

        // _asyncTransfer(wrassleableContractOwners[mainnetContract], total / 2);

        emit FighterUpdated(
            mainnetContract,
            tokenId,
            ownerOf(mainnetContract, tokenId),
            fighter.aggression,
            fighter.awareness,
            fighter.determination,
            fighter.power,
            fighter.resilience,
            fighter.speed
        );
    }

    function calculateUpgrades(INFight.Fighter storage fighter, INFight.Fighter memory upgradeParams)
        internal
        view
        returns (uint256)
    {
        return
            calculateUpgradeCost(fighter.power, upgradeParams.power) +
            calculateUpgradeCost(fighter.speed, upgradeParams.speed) +
            calculateUpgradeCost(fighter.aggression, upgradeParams.aggression) +
            calculateUpgradeCost(fighter.determination, upgradeParams.determination) +
            calculateUpgradeCost(fighter.resilience, upgradeParams.resilience) +
            calculateUpgradeCost(fighter.awareness, upgradeParams.awareness);
    }

    function calculateUpgradeCost(uint256 startPoints, uint256 toAdd) public pure returns (uint256) {
        return attributeBaseCost * ((startPoints + toAdd)**3 - startPoints**3);
    }

    function withdraw() public onlyRole(DEFAULT_ADMIN_ROLE) {
        payable(msg.sender).transfer(address(this).balance);
    }

    function getFighter(address mainnetContract, uint256 _tokenId)
        public
        view
        returns (
            uint256 power,
            uint256 speed,
            uint256 aggression,
            uint256 determination,
            uint256 resilience,
            uint256 awareness
        )
    {
        INFight.Fighter memory fighter = nFighters[mainnetContract][_tokenId];

        return (
            fighter.power,
            fighter.speed,
            fighter.aggression,
            fighter.determination,
            fighter.resilience,
            fighter.awareness
        );
    }
}

