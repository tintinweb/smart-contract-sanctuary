import "./ASSPLayer.sol";
import "./ERC721ContractEnumerable.sol";

contract ASSPLayerChild is
    ASSPLayer,
    ERC2771ContextUpgradeable,
    FxBaseChildTunnelUpgradeable,
    AccessControlUpgradeable,
    ERC721ContractEnumerable
{
    modifier onlyForwarder() {
        require(isTrustedForwarder(msg.sender), "ASSPLayer:429");
        _;
    }

    modifier onlyApproved(address contractAddress, uint256 tokenId) {
        address owner = ownerOf(contractAddress, tokenId);
        require(isApprovedForAll(owner, _msgSender()), "ASSPLayer:429");
        _;
    }

    modifier onlySelf() {
        if (msg.sender != address(this)) revert("500");
        _;
    }

    function initialize(address trustedForwarder, address _fxChild) internal {
        _setupRole(DEFAULT_ADMIN_ROLE, address(msg.sender));
        ERC2771ContextUpgradeable.__ERC2771Context_init_unchained(address(trustedForwarder));
        FxBaseChildTunnelUpgradeable.initialize(_fxChild);
    }

    function registerProject(address contractAddress) external payable override onlyForwarder {
        _registerProject(contractAddress, _msgSender());
    }

    function _registerProject(address contractAddress, address owner) public onlySelf {
        _beforeProjectRegistration(contractAddress, owner);
        registeredContract[contractAddress] = true;
        _onProjectRegistration(contractAddress, owner);
    }

    function registerToken(address mainnetContract, uint256 tokenId) external override onlyForwarder {
        _registerToken(mainnetContract, tokenId, _msgSender());
    }

    function _registerToken(
        address mainnetContract,
        uint256 tokenId,
        address owner
    ) internal {
        address existingOwner = _owners[mainnetContract][tokenId];

        if (existingOwner != address(0)) {
            _removeTokenFromOwnerEnumeration(existingOwner, mainnetContract, tokenId);
        }

        _addTokenToOwnerEnumeration(_msgSender(), mainnetContract, tokenId);
        _onRegistration(mainnetContract, tokenId, owner);
    }

    // TODO: Get test suite to support meta transactions - https://github.com/ProjectOpenSea/meta-transactions/blob/main/test/erc721-test.js
    function registerTokenDev(
        address mainnetContract,
        uint256 tokenId,
        address owner
    ) external {
        _registerToken(mainnetContract, tokenId, owner);
    }

    function setApprovalForAll(address operator, bool approved) external {
        _setApprovalForAll(_msgSender(), operator, approved);
    }

    // quick little storage to help us debug
    bytes public lastMessage;

    function _processMessageFromRoot(
        uint256,
        address sender,
        bytes memory data
    ) internal override validateSender(sender) {
        // solhint-disable-next-line no-unused-vars
        lastMessage = data;
        (address contractAddress, address contractOwner, uint256 tokenId, address tokenOwner) = abi.decode(
            data,
            (address, address, uint256, address)
        );

        if (contractOwner != address(0)) {
            _registerProject(contractAddress, contractOwner);
        }

        if (tokenId != 0) {
            _registerToken(contractAddress, tokenId, tokenOwner);
        }
    }

    function _msgSender()
        internal
        view
        virtual
        override(ERC2771ContextUpgradeable, ContextUpgradeable)
        returns (address sender)
    {
        return ERC2771ContextUpgradeable._msgSender();
    }

    function _msgData()
        internal
        view
        virtual
        override(ContextUpgradeable, ERC2771ContextUpgradeable)
        returns (bytes calldata)
    {
        if (isTrustedForwarder(msg.sender)) {
            return msg.data[:msg.data.length - 20];
        } else {
            return ERC2771ContextUpgradeable._msgData();
        }
    }

    function _onRegistration(
        address mainnetContract,
        uint256 tokenId,
        address ownerAddress
    ) internal virtual {}

    function _onProjectRegistration(address contractAddress, address contractOwnerAddress) internal virtual override {}

    function _beforeProjectRegistration(address contractAddress, address contractOwnerAddress)
        internal
        virtual
        override
    {}
}

