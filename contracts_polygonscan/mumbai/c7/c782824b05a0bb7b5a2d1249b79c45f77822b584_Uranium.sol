contract Uranium is BEP20 {

  address private factory;
  address public pair;

  uint public halfLife;

  uint private holdMaxRatio = 10000000000;
  uint private sellPerDayMaxRatio = 100000000000;

  uint private decayLatest;

  mapping (address => bool) public whitelist;
  mapping (address => uint) private lock;

  mapping (address => mapping (uint => uint)) private sell;

  constructor(
    address _factory,
    string memory _name,
    string memory _symbol,
    uint totalSupply_,
    uint liquidity,
    uint _halfLife
  ) public BEP20(_name, _symbol) {
    factory = _factory;

    halfLife = _halfLife;
    decayLatest = block.timestamp;

    whitelist[_msgSender()] = true;
    whitelist[_factory] = true;

    _mint(_msgSender(), totalSupply_ - liquidity);
    _mint(factory, liquidity);
  }

  function holdMax() public view returns (uint) {
    return totalSupply() / holdMaxRatio;
  }

  function sellPerDayMax() public view returns (uint) {
    return totalSupply() / sellPerDayMaxRatio;
  }

  function decay() external {
    uint remaining = HalfLife.remaining(
      halfLife,
      block.timestamp - decayLatest,
      balanceOf(pair)
    );
    _burn(pair, balanceOf(pair) - remaining);
    IPancakePair(pair).sync();
    decayLatest = block.timestamp;
    _lock();
  }

  function burn(uint amount) external {
    _burn(_msgSender(), amount);
  }

  function devalue(address holder) public {
    uint _holdMax = totalSupply() / holdMaxRatio;
    if(!whitelist[holder] && balanceOf(holder) > _holdMax) _burn(holder, balanceOf(holder) - _holdMax);
  }

  function devalueBatch(address[] calldata holders) external {
    for(uint i = 0; i < holders.length; i++) devalue(holders[i]);
  }

  function setPair(address _pair) external {
    require(_msgSender() == owner() || _msgSender() == factory, "pair");
    pair = _pair;
    whitelist[_pair] = true;
  }

  function addToWhitelist(address holder, bool value) external {
    require(_msgSender() == owner(), "whitelist");
    whitelist[holder] = value;
  }

  function setMaxRatios(uint _holdMaxRatio, uint _sellPerDayMaxRatio) external {
    require(
      _msgSender() == owner() &&
      _holdMaxRatio <= holdMaxRatio &&
      _sellPerDayMaxRatio <= sellPerDayMaxRatio &&
      _holdMaxRatio < _sellPerDayMaxRatio &&
      _holdMaxRatio > 0, "maxRatios"
    );
    holdMaxRatio = _holdMaxRatio;
    sellPerDayMaxRatio = _sellPerDayMaxRatio;
  }

  function _afterTokenTransfer(address sender, address recipient, uint256 amount) internal virtual override {
    uint _holdMax = totalSupply() / holdMaxRatio;
    if(pair == sender && !whitelist[recipient]) {
      if(balanceOf(recipient) > _holdMax) _burn(recipient, balanceOf(recipient) - _holdMax);
      _lock();
    } else if(pair == recipient && !whitelist[sender]) {
      uint day = block.timestamp / 86400;
      sell[sender][day] += amount;
      require(sell[sender][day] <= totalSupply() / sellPerDayMaxRatio, 'sellPerDayMax');
      _lock();
    } else if(balanceOf(recipient) > _holdMax && !whitelist[recipient]) {
      _burn(recipient, balanceOf(recipient) - _holdMax);
    }
  }

  function _lock() internal {
    require(lock[tx.origin] < block.timestamp, "lock");
    lock[tx.origin] = block.timestamp;
  }

}