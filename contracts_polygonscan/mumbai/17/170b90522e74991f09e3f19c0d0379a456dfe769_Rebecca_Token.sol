pragma solidity ^0.8.0;
// import "./Order_Detail.sol";





library SN_State_OBJ {
  struct cfg {   
     uint256 ActiveTime;
     uint256 ExpTime;
     bool is_actived;
   }
}



 contract Project  is ERC1155 {
    event Sold(
       address buyer_addr,
       uint item_id,
       uint qty
    );
        string Project_Name ;
        address Project_Owner;
        uint public is_actived;
        uint[] Sale_List;
 
        mapping(address => uint[]) Buyer_Details;    
        mapping(bytes32=>bool)  SN_Used_List;//確認商品序號是不是用過了
        mapping(bytes32=>address)  SN_Order_Index;//提供索引找到主商品
        // mapping(address=>Order_Detail)  Order_Detail_Obj_List;
        mapping(uint=>bytes ) Project_Product_id_name;//產品id名對照表
        mapping(uint=>uint) Project_Exp_Time_id;//產品id名對照表
        mapping(address=>SN_State_OBJ.cfg) User_SN_State;
        
    constructor(string memory _Project_Name,string memory meta_url)  ERC1155(""){
        Project_Name = _Project_Name;
        Project_Owner = msg.sender;
        is_actived = 1;
        _setURI(meta_url);

        Project_Product_id_name[0] = "1 month use";
        Project_Exp_Time_id[0] = 30 days;
        
        Project_Product_id_name[1] = "6 month use";
        Project_Exp_Time_id[1] = 30*6 days;
        Project_Product_id_name[2] = "12 month use";
        Project_Exp_Time_id[2] = 30*12 days;
    }



    function Check_Self_Active_State() public returns(SN_State_OBJ.cfg memory){
        return User_SN_State[msg.sender];
    }
    
    function Sale_Item(address buyer_addr,uint id,uint qty) public {
        bytes  memory title = Project_Product_id_name[id];
        _mint(buyer_addr,id,qty,title);
        emit Sold(buyer_addr,id,qty);
    }

    function Use_Item(address owner_addr,uint id,uint qty)public {

        SN_State_OBJ.cfg memory Config = User_SN_State[owner_addr];
        if(Config.ActiveTime == 0){
            //首次激活
            
            Config.ActiveTime = block.timestamp;
            Config.ExpTime = Config.ActiveTime;
        }

        uint this_nft_life_time =  Project_Exp_Time_id[id]; 
        
        Config.ExpTime = Config.ExpTime + this_nft_life_time;
        Config.is_actived = true;
        User_SN_State[owner_addr] = Config;
        
        _burn(owner_addr,id,qty);
    }

 }
