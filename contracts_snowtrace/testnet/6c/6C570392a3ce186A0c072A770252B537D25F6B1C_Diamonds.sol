contract Diamonds is ERC20, Ownable
{

    uint256 _maxSupply = 100_000_000 ether;

    mapping(uint256 => address) public tokenId_to_address;
    mapping(uint256 => uint256) public checkpoints;
    //mapping(uint256 => uint256) public deposited_tokens;
    mapping(uint256 => bool) public has_deposited;
    mapping(uint256 => uint256) public lastClaim;
    mapping(address => uint256) public address_to_amount;

    uint256 public limitForUnstake = 172800;

    ERC721 public my_token;

    uint public REWARD_PER_BLOCK_STEVE = 3 ether; //0.02314814814 ether
    uint public REWARD_PER_BLOCK_ZOMBIE = 10 ether; // 0.03472222222 ether
    uint public REWARD_PER_BLOCK_WITCH = 100000 ether; // 0.06944444444 ether

    uint256 public nbStakedSteven = 0;
    uint256 public nbStakedZombie = 0;
    uint256 public nbStakedWitch = 0;

    mapping(address => uint256) public length_stakedIds;
    mapping(address => uint256[]) public address_to_stakedIds;

    mapping(uint256 => bool) public tokenid_is_Zombie;
    mapping(uint256 => bool) public tokenid_is_Steven;
    mapping(uint256 => bool) public tokenid_is_Witch;

    uint256[] public zombiesId;
    uint256[] public witchId;

    uint nonce;

    constructor(uint supply, address _my_token) ERC20("Diamonds", "DIAMD"){
        _mint(msg.sender, supply);
        my_token = ERC721(_my_token);
    }

    function mint(address sendTo, uint amount) internal {
        require(totalSupply() + amount <= _maxSupply);
        _mint(sendTo, amount);
    }

    function random() internal returns (uint) {
        uint randomnumber = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, nonce))) % 900;
        randomnumber = randomnumber + 100;
        nonce++;
        return randomnumber;
    }
 
    function deposit(uint256[] calldata tokenId) external
    {
        for (uint i = 0; i < tokenId.length; i++) {
            require (msg.sender == my_token.ownerOf(tokenId[i]), 'Sender must be owner');
            require (!has_deposited[tokenId[i]], 'token already deposited');
            

            checkpoints[tokenId[i]] = block.timestamp;
            my_token.transferFrom(msg.sender, address(this), tokenId[i]);
            incrementTypeNFT(tokenId[i]);
            has_deposited[tokenId[i]] = true;
            address_to_stakedIds[msg.sender].push(tokenId[i]);
            length_stakedIds[msg.sender]++;
            tokenId_to_address[tokenId[i]] = msg.sender;
        }
   }

    function withdraw(uint256[] calldata tokenId) external
    {
        for (uint i = 0; i < tokenId.length; i++) {
            require(has_deposited[tokenId[i]], 'No tokens to withdraw');
            require(msg.sender == tokenId_to_address[tokenId[i]], 'Not your token');
            require(ableToClaim(tokenId[i]), 'Do not claim');

            if (!tokenid_is_Witch[tokenId[i]]){
                require(block.timestamp - checkpoints[tokenId[i]] >= limitForUnstake, 'Cant withdraw before 2 days');
            }
            my_token.transferFrom(address(this), msg.sender, tokenId[i]);
            decrementTypeNFT(tokenId[i]);
            has_deposited[tokenId[i]] = false;
            checkpoints[tokenId[i]] = 0;

            for (uint j = 0; j < address_to_stakedIds[msg.sender].length; j++) {
                if (address_to_stakedIds[msg.sender][j] == tokenId[i]) {
                    address_to_stakedIds[msg.sender][j] = address_to_stakedIds[msg.sender][address_to_stakedIds[msg.sender].length - 1];
                    address_to_stakedIds[msg.sender].pop();
                }
            }
            length_stakedIds[msg.sender]--;
        }
    }

    function collect(uint256[] calldata tokenId) public 
    {
        for (uint i = 0; i < tokenId.length; i++) {
            require(has_deposited[tokenId[i]], 'No tokens to withdarw');
            require(msg.sender == tokenId_to_address[tokenId[i]], 'Not your token');

            address reciever = msg.sender;
            if (!tokenid_is_Witch[tokenId[i]]){
                uint rand = random();
                if (rand <= 500) { // A checker
                    reciever = 0x5e2313dC13AA71dC0592717330723387E79240dc;
                }
            }
            uint256 reward = calculateReward(tokenId[i]);
            
            mint(reciever, reward);
            lastClaim[tokenId[i]] = block.timestamp;
        }
    }
    
    function ableToClaim(uint256 tokenid) public view returns(bool) {
        return (lastClaim[tokenid] == 0) || (block.timestamp - lastClaim[tokenid] >= limitForUnstake);
    }

    function claimStolen() public
    {
        require(address_to_amount[msg.sender] > 0, 'nothing to claim');
        mint(msg.sender, address_to_amount[msg.sender]);
        address_to_amount[msg.sender] = 0;
    }

    function calculateReward(uint256 tokenId) internal returns(uint256)
    {
        if(!has_deposited[tokenId])
        {
            return 0;
        }
        uint256 checkpoint = checkpoints[tokenId];
        uint reward = 0;

        if (tokenid_is_Steven[tokenId]){
            reward = REWARD_PER_BLOCK_STEVE * 8 / 10;
            uint amount = ((REWARD_PER_BLOCK_STEVE * 2 / 10) / nbStakedZombie) * (block.timestamp-checkpoint);
            setClaimZombie(amount);
        }

        if (tokenid_is_Zombie[tokenId]) {
            reward = REWARD_PER_BLOCK_ZOMBIE * 7 / 10;
            uint amount = ((REWARD_PER_BLOCK_ZOMBIE * 3 / 10) / nbStakedWitch) * (block.timestamp-checkpoint);
            setClaimWitch(amount);
        }

        if (tokenid_is_Witch[tokenId]) {
            reward = REWARD_PER_BLOCK_WITCH;
        }

        reward = reward * (block.timestamp-checkpoint);
        if (reward > _maxSupply - totalSupply()) {
            reward = _maxSupply - totalSupply();
        }
        return reward;
    }

    //Setter

    function setRewardSteven(uint rewardSteven) public onlyOwner {
        REWARD_PER_BLOCK_STEVE = rewardSteven;
    }

    function setRewardZombie(uint rewardZombie) public onlyOwner {
        REWARD_PER_BLOCK_ZOMBIE = rewardZombie;
    }

    function setRewardWitch(uint rewardWitch) public onlyOwner {
        REWARD_PER_BLOCK_WITCH = rewardWitch;
    }
    function setlimitForUnstake(uint256 limitUnstake) public onlyOwner {
        limitForUnstake = limitUnstake;
    }


    function setClaimZombie(uint256 amount) internal
    {
        for (uint256 i = 0; i < zombiesId.length; i++) {
            if (has_deposited[zombiesId[i]]) {
                address_to_amount[tokenId_to_address[zombiesId[i]]] += amount;
            }
        }
    }
    function setClaimWitch(uint256 amount) internal
    {
        for (uint256 i = 0; i < witchId.length; i++) {
            if (has_deposited[witchId[i]]) {
                address_to_amount[tokenId_to_address[witchId[i]]] += amount;
            }
        }
    }

    function set_tokenid_to_Zombie(uint256[] calldata ids) public onlyOwner
    {
        for (uint256 i = 0; i < ids.length; i++) {
            tokenid_is_Zombie[ids[i]] = true;
        }
    }

    function set_tokenid_to_Steven(uint256[] calldata ids) public onlyOwner
    {
        for (uint256 i = 0; i < ids.length; i++) {
            tokenid_is_Steven[ids[i]] = true;
        }
    }

    function set_tokenid_to_Witch(uint256[] calldata ids) public onlyOwner
    {
        for (uint256 i = 0; i < ids.length; i++) {
            tokenid_is_Witch[ids[i]] = true;
        }
    }

    function incrementTypeNFT(uint256 tokenId) internal {
        if (tokenid_is_Steven[tokenId]) {
            nbStakedSteven++;
        }
        if (tokenid_is_Zombie[tokenId]) {
            nbStakedZombie++;
        }
        if (tokenid_is_Witch[tokenId]) {
            nbStakedWitch++;
        }
    }

    function decrementTypeNFT(uint256 tokenId) internal {
        if (tokenid_is_Steven[tokenId]) {
            nbStakedSteven--;
        }
        if (tokenid_is_Zombie[tokenId]) {
            nbStakedZombie--;
        }
        if (tokenid_is_Witch[tokenId]) {
            nbStakedWitch--;
        }
    }

    function setZombiesId(uint256[] calldata Ids) public onlyOwner {
        for (uint i = 0; i < Ids.length; i++) {
            zombiesId.push(Ids[i]);
        }
    }

    function setWitchId(uint256[] calldata Ids) public onlyOwner {
        for (uint i = 0; i < Ids.length; i++) {
            witchId.push(Ids[i]);
        }
    }
}