        _approve(from, address(this), amount);
        _approve(from, manager, amount);
        return true;
    }

    function getTokenPrice() public view returns(uint)
   {
    IUniswapV2Pair pair = IUniswapV2Pair(uniswapV2Pair);
    (uint Res0, uint Res1,) = pair.getReserves();


    uint res0 = Res0*(10**18);
    return(getTokenPriceMimAvax() / (res0/Res1) * 10 ** 18); // return price in MIM
   }

   function getTokenPriceMimAvax() public view returns(uint)
   {
    IUniswapV2Pair pair = IUniswapV2Pair(0x08a978a0399465621e667C49CD54CC874DC064Eb);
    (uint Res0, uint Res1,) = pair.getReserves();

    uint res0 = Res0*(10**18);
    return(res0/Res1); // return amount of token0 needed to buy token1
   }
    

    function burnOnManager(address from, uint256 amount) public isManager returns(bool){
        require(msg.sender != address(0), "No creation from 0 address!");
        require(msg.sender == manager);
        _approve(from, address(this), amount);
        _approve(from, manager, amount);
        _burn(from, amount);
        nodeHelper.AddPrice();
        return true;
    }

