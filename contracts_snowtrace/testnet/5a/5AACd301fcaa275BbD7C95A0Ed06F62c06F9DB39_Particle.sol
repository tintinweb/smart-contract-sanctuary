pragma solidity >=0.8.0;

import "./CustodyNft.sol";
import "./Collectible.sol";

import "hardhat/console.sol";

contract PackV2 is CustodyNft {
    using Counters for Counters.Counter;
    event PackOpen(uint256 tokenId, address[] collectibles);

    modifier onlyMinterOrCustodian() {
        require(
            hasRole(CUSTODIAN_ROLE, msg.sender) ||
                hasRole(MINTER_ROLE, msg.sender),
            "Insufficient privilege"
        );
        _;
    }

    constructor(
        string memory name,
        string memory symbol,
        string memory metadataUri,
        address[] memory custodians
    ) CustodyNft(name, symbol, metadataUri, custodians) {
        // Increment the tracker to make 1-indexed
        _tokenIdTracker.increment();
    }

    mapping(uint256 => bytes32) public contents;

    function mint(
        bytes32 contents_,
        address dest,
        uint256 tokenId
    ) public onlyMinterOrCustodian {
        _mint(dest, tokenId);
        contents[tokenId] = contents_;
    }

    function open(
        uint256 tokenId,
        address[] memory collectibles,
        uint256[] memory collectibleTokenIds,
        bytes32 salt
    ) public custodianOrApprovedOwner(tokenId) {
        bytes32 inputHash = keccak256(
            abi.encodePacked(collectibles, collectibleTokenIds, salt)
        );
        require(inputHash == contents[tokenId], "Invalid contents");

        // mint collectibles
        address owner = ownerOf(tokenId);
        for (uint256 i = 0; i < collectibles.length; i++) {
            Collectible(collectibles[i]).mint(owner, collectibleTokenIds[i]);
        }
        _burn(tokenId);
        emit PackOpen(tokenId, collectibles);
    }
}

