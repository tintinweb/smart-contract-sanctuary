import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

abstract contract CustodyNft is AccessControlEnumerable, ERC721Enumerable {
    using Counters for Counters.Counter;

    enum Currency {AVAX, USD}

    struct TransferHistory {
        address previousOwner;
        Currency denomination;
        uint256 amount;
    }

    Counters.Counter internal _tokenIdTracker;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    bytes32 public constant CUSTODIAN_ROLE = keccak256("CUSTODIAN_ROLE");

    string private baseURI;

    mapping(uint256 => TransferHistory[]) public previousOwners;

    mapping(uint256 => bool) public outOfCustody;

    mapping(uint256 => bool) public acceptCustody;

    modifier custodianOrApprovedOwner(uint256 tokenId) {
        if (!outOfCustody[tokenId]) {
            require(
                hasRole(CUSTODIAN_ROLE, msg.sender),
                "Insufficient privilege"
            );
        } else {
            require(
                _isApprovedOrOwner(_msgSender(), tokenId),
                "ERC721: transfer caller is not owner nor approved"
            );
        }
        _;
    }
    modifier onlyCustodian() {
        require(hasRole(CUSTODIAN_ROLE, msg.sender), "Insufficient privilege");
        _;
    }

    modifier onlyApprovedOrOwner(uint256 tokenId) {
        require(
            _isApprovedOrOwner(_msgSender(), tokenId),
            "ERC721: transfer caller is not owner nor approved"
        );
        _;
    }

    modifier onlyMinter() {
        require(hasRole(MINTER_ROLE, msg.sender), "Insufficient privilege");
        _;
    }

    event CustodialTransfer(
        address from,
        address to,
        uint256 tokenId,
        bool isAvax,
        uint256 value
    );

    constructor(
        string memory name,
        string memory symbol,
        string memory metadataUri,
        address[] memory custodians
    ) ERC721(name, symbol) {
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setRoleAdmin(DEFAULT_ADMIN_ROLE, DEFAULT_ADMIN_ROLE);
        _setupRole(CUSTODIAN_ROLE, _msgSender());
        _setRoleAdmin(CUSTODIAN_ROLE, CUSTODIAN_ROLE);
        _setupRole(MINTER_ROLE, _msgSender());
        _setRoleAdmin(MINTER_ROLE, MINTER_ROLE);

        for (uint256 i = 0; i < custodians.length; i++) {
            grantRole(CUSTODIAN_ROLE, custodians[i]);
        }

        baseURI = metadataUri;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        return baseURI;
    }

    // WARNING
    // DO NOT USE BOTH _mint and minTo, it will break the counter and fail
    // due to tokenId collisions
    function mintTo(address dest) internal onlyMinter returns (uint256) {
        uint256 tokenId = _tokenIdTracker.current();
        _mint(dest, tokenId);
        _tokenIdTracker.increment();
        return tokenId;
    }

    function leaveCustody(uint256 tokenId) external onlyCustodian {
        require(!outOfCustody[tokenId], "Token not in custody");
        outOfCustody[tokenId] = true;
    }

    function approveCustody(uint256 tokenId)
        external
        onlyApprovedOrOwner(tokenId)
    {
        require(outOfCustody[tokenId], "Token in custody");
        acceptCustody[tokenId] = true;
    }

    function unapproveCustody(uint256 tokenId)
        external
        onlyApprovedOrOwner(tokenId)
    {
        require(outOfCustody[tokenId], "Token in custody");
        acceptCustody[tokenId] = false;
    }

    function reclaimCustody(uint256 tokenId) external onlyCustodian {
        require(outOfCustody[tokenId], "Token in custody");
        require(acceptCustody[tokenId], "Custody must be accepted");
        acceptCustody[tokenId] = false;
        outOfCustody[tokenId] = false;
    }

    function custodialTransfer(
        address from,
        address to,
        uint256 tokenId,
        bool isAvax,
        uint256 amount
    ) public onlyCustodian {
        Currency curr;
        if (isAvax) {
            curr = Currency.AVAX;
        } else {
            curr = Currency.USD;
        }

        TransferHistory memory hist =
            TransferHistory({
                previousOwner: from,
                denomination: curr,
                amount: amount
            });

        emit CustodialTransfer(from, to, tokenId, isAvax, amount);

        previousOwners[tokenId].push(hist);
        _transfer(from, to, tokenId);
    }

    /**
     * @dev See {IERC721-transferFrom}.
     */
    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override custodianOrApprovedOwner(tokenId) {
        TransferHistory memory hist =
            TransferHistory({
                previousOwner: from,
                denomination: Currency.USD,
                amount: 0
            });
        previousOwners[tokenId].push(hist);
        _transfer(from, to, tokenId);
    }

    /**
     * @dev See {IERC721-safeTransferFrom}.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override custodianOrApprovedOwner(tokenId) {
        TransferHistory memory hist =
            TransferHistory({
                previousOwner: from,
                denomination: Currency.USD,
                amount: 0
            });
        previousOwners[tokenId].push(hist);
        safeTransferFrom(from, to, tokenId, "");
    }

    /**
     * @dev See {IERC721-safeTransferFrom}.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) public virtual override custodianOrApprovedOwner(tokenId) {
        require(
            _isApprovedOrOwner(_msgSender(), tokenId),
            "ERC721: transfer caller is not owner nor approved"
        );
        TransferHistory memory hist =
            TransferHistory({
                previousOwner: from,
                denomination: Currency.USD,
                amount: 0
            });
        previousOwners[tokenId].push(hist);
        _safeTransfer(from, to, tokenId, _data);
    }

    /**
     * @dev See {IERC165-supportsInterface}.
     */
    function supportsInterface(bytes4 interfaceId)
        public
        view
        virtual
        override(AccessControlEnumerable, ERC721Enumerable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

