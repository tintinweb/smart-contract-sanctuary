import "./CustodyNft.sol";
import "./Collectible.sol";

import "hardhat/console.sol";

contract Pack is CustodyNft {
    using Counters for Counters.Counter;
    event PackOpen(uint tokenId, address[] collectibles);

    constructor(string memory name,
                string memory symbol,
                string memory metadataUri,
                address[] memory custodians)
        CustodyNft(name, symbol, metadataUri, custodians) {
            // Increment the tracker to make 1-indexed
            _tokenIdTracker.increment();
        }

    mapping (uint => bytes32) public contents;

    function mint(bytes32 contents_, address dest) public onlyMinter {
        uint tokenId = mintTo(dest);
        contents[tokenId] = contents_;
    }

    function mintCollection(bytes32[] memory contentHashes, address dest) external onlyMinter {
        for (uint i = 0; i < contentHashes.length; i++) {
            mint(contentHashes[i], dest);
        }
    }

    function open(uint tokenId, address[] memory collectibles, uint[] memory collectibleTokenIds, bytes32 salt) public custodianOrApprovedOwner(tokenId) {
        bytes32 inputHash = keccak256(abi.encodePacked(collectibles, collectibleTokenIds, salt));
        require(inputHash == contents[tokenId], "Invalid contents");

        // mint collectibles
        address owner = ownerOf(tokenId);
        for (uint i = 0; i < collectibles.length; i++) {
            Collectible(collectibles[i]).mint(owner, collectibleTokenIds[i]);
        }
        _burn(tokenId);
        emit PackOpen(tokenId, collectibles);
    }

}

