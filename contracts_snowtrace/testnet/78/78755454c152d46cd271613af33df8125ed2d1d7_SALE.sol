contract SALE is Context, Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    mapping(uint => address) public earlyholderArray;
    mapping(address => uint256) public earlyholder;
    mapping(address => bool) public isearlyholder;

    uint private earlyholderCount = 0;

    uint256 private withdrawTimestamp = 0;

    // Sale
    bool private isEnd = true;
    bool public isStart = false;
    uint256 public DepositTimeout = 0;

    address private constant MIM = 0xB1281404C191efC9Fd370884226831b467fa9de5;
    address private constant TESTTST = 0xdddE54CDF6A58b3Cc5d451a1E9C7a10870644547;
    address private constant burnAddress = 0x000000000000000000000000000000000000dEaD;

    // trader joe
    address private constant JoeRouter = 0x5db0735cf88F85E78ed742215090c465979B5006;
    address public JoeLpToken = address(0);

    // TODO - set this!
    address private constant TREASURY_ADDRESS = 0x41b6b7fC4a5B26f16C742312D0378F1F9Be19d74;

    uint256 private constant MIMdepositAmount = 30000 * 10**18;

    uint256 private constant MIM_to_Treasury = 60000 * 10**18;

    uint256 private constant LP_MIM = 240000 * 10**18;
    uint256 private constant LP_TESTTST = 12000 * 10**9;

    uint256 private constant withdrawMaxAmount = 9000 * 10**18;
    uint256 private constant singleWithdrawTime = 7 days;
    uint256 private constant singleWithdraw = 5000 * 10**18;


    event Depositby( address indexed debtor, uint amount, address indexed token);
    event Transfer(address indexed from, address indexed to, uint256 value, address indexed token);

    constructor() public {
        address JoeFactory = IJoeRouter01(JoeRouter).factory();
        JoeLpToken = IJoeFactory( JoeFactory ).createPair(MIM, TESTTST);

        earlyholderArray[0] = address(0xDF0862D97441accB31758f6DcEA4e26037D98C44); // account 1
        earlyholderArray[1] = address(0x7D35506A55DAD4f7eEb16BeBD22C7C85C94450aB); // account 2
        earlyholderArray[2] = address(0x23A77806A802DAa2c8922C4c55ec70eBF49CD5b0); // account 3
        earlyholderCount = 3;

        _addearlyholder(0);
    }

    function _addearlyholder(uint i) internal {
        if(i < earlyholderCount){
            earlyholder[earlyholderArray[i]] = 0;
            isearlyholder[earlyholderArray[i]] = true;
            _addearlyholder(i + 1);
        }
    }

    function Withdraw() external {
        require(block.timestamp > withdrawTimestamp.add(singleWithdrawTime), "It is too early to call this function, at least 7 days must pass since the last use");

        __withdraw();
    }

    function __withdraw() internal {
        uint256 balance = IERC20(TESTTST).balanceOf(address(this));

        if(balance > 0){
            if(balance > singleWithdraw){
                // _withdraw(0, singleWithdraw);
            }else{
                // _withdraw(0, balance);
            }
        }

        withdrawTimestamp = block.timestamp;
    }

    function _withdraw(uint i, uint256 amount) internal {
        if(i < earlyholderCount){
            if(earlyholder[earlyholderArray[i]] > 0){
                _lowlevel_transfer(TESTTST, address(this), address(earlyholderArray[i]), amount);
            }
            _withdraw(i + 1, amount);
        }
    }

    function Deposit() public {
        require(isEnd, "Sale is over");
        require(isStart, "Sale is not yet started");
        require(isearlyholder[msg.sender], "Only addresses from the whitelist may participate");
        require(earlyholder[msg.sender].add(MIMdepositAmount) <= MIMdepositAmount, "You have reached the maximum amount for the address");

        emit Depositby( msg.sender, MIMdepositAmount, MIM);

        earlyholder[msg.sender] = earlyholder[msg.sender].add(MIMdepositAmount);

        _lowlevel_transfer(MIM, msg.sender, address(this), MIMdepositAmount);
    }

    function _lowlevel_transfer(address token, address sender, address recipient, uint256 amount) internal {
        require(sender != address(0), "BEP20: transfer from the zero address");
        require(recipient != address(0), "BEP20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        IERC20(token).safeTransferFrom(sender, recipient, amount);

        emit Transfer(sender, recipient, amount, token);
    }


    function SaleEnable() public onlyOwner {
        isStart = true;
        DepositTimeout = block.timestamp.add(7 days);
    }

    function SaleRefund() public payable {
        require(isEnd, "Sale is over");
        require(block.timestamp > DepositTimeout, "Sale is still ongoing");
        require(earlyholder[msg.sender] > 0, "No refund is available for this address");
        _lowlevel_transfer(MIM, address(this), msg.sender, MIMdepositAmount);
        earlyholder[msg.sender] = 0;
    }

    function AddLiquidity() public onlyOwner {

        IERC20(MIM).approve(address(JoeRouter), LP_MIM);
        IERC20(TESTTST).approve(address(JoeRouter), LP_TESTTST);

        IJoeRouter01(JoeRouter).addLiquidity(
            MIM,
            TESTTST,
            LP_MIM,
            LP_TESTTST,
            LP_MIM,
            LP_TESTTST,
            address(this),
            block.timestamp.add(10 minutes)
        );

        // transfer LP to TREASURY
        uint256 lpbalance = IERC20(JoeLpToken).balanceOf(address(this));

        IERC20(JoeLpToken).approve(address(TREASURY_ADDRESS), lpbalance);
        TimeTreasury(TREASURY_ADDRESS).deposit(lpbalance, JoeLpToken, 0);


        // transfer MIM to TREASURY
        TimeTreasury(TREASURY_ADDRESS).deposit(MIM_to_Treasury, MIM, 0);


        // burn TESTTST above limit
        uint256 balance = IERC20(TESTTST).balanceOf(address(this));
        _lowlevel_transfer(TESTTST, address(this), msg.sender, balance.sub(withdrawMaxAmount));


        __withdraw();

        //
        isEnd = false;

    }

}