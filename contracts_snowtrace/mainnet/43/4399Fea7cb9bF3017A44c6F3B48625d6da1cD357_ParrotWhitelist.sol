        return _owner;
    }

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(owner() == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

    /**
     * @dev Leaves the contract without owner. It will not be possible to call
     * `onlyOwner` functions anymore. Can only be called by the current owner.
     *
     * NOTE: Renouncing ownership will leave the contract without an owner,
     * thereby removing any functionality that is only available to the owner.
     */
    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }
}

pragma solidity ^0.8.0;


contract ParrotWhitelist is Ownable {
    using SafeMath for uint256;
    using Address for address;
    using Strings for uint256;

    mapping(address => bool) whiteList;
    mapping(address => uint256) public contributions;
    
    function addToWhiteList(address user) public onlyOwner() {
        whiteList[user] = true;
    }

    
    uint256 public maxContractLimit = 500000000000000000000; //500AVAX
    //uint256 public maxContractLimit = 5000000000000000000; //5 AVAX
    uint256 public ratePerAvax = 13000000000;
    address public liquidityAddress = 0x12AdC78ad71D808bA151e2033b83B059BBbea3c6;
    uint256 contractBalance = address(this).balance;
    address public parrToken = 0xf10b5a72f0abeB27A5DdBa950082e4eDFF4c825d;
    bool public whitelistActive = true;
    bool public saleActive = false;

    function withdrawAll() public payable onlyOwner {
        uint256 balances = address(this).balance;

        require(balances > 0);
        _withdraw(liquidityAddress, balances);

    }

    function toggleSale(bool value) public onlyOwner() {
        saleActive = value;
    }

    function toggleWhitelist(bool value) public onlyOwner() {
        whitelistActive = value;
    }

    function changeLiquidityProvider(address _liquidityProvider) public onlyOwner {
        liquidityAddress = _liquidityProvider;
    }

    function changeContractLimit(uint256 _maxContractLimit) public onlyOwner {
        maxContractLimit = _maxContractLimit;
    }

    uint256 public percentFilled = ((contractBalance.div(maxContractLimit)).mul(100));

    function _withdraw(address _address, uint256 _amount) private {
        (bool success, ) = _address.call{value: _amount}("");
        require(success, "Transfer failed.");
    }
    
    function stakeForWhitelist(uint256 deposit, address userAddress) public payable {  //deposit AVAX for Whitelist
        if(saleActive) {
            if(whitelistActive) {
                if(whiteList[userAddress] == true) {
                    require(msg.value == deposit);
                    uint256 balances = address(this).balance;
                    require(balances < maxContractLimit, "Whitelist Filled");
                    require(balances.add(deposit) < maxContractLimit, "Deposit Over Whitelist Max - Check Contract Balance if its below limit");
                    deposit = msg.value;
                    percentFilled = ((balances.div(maxContractLimit)).mul(100));
                    uint256 _amount = deposit.mul(ratePerAvax);
                    uint256 transfertoken = _amount.div(1e18);
                    uint256 _existingContribution = contributions[userAddress];
                    uint256 _newContribution = _existingContribution.add(deposit);
                    require(_newContribution <= 1000000000000000000);
                    contributions[userAddress] = _newContribution;
                    IERC20( parrToken ).transfer( userAddress, transfertoken );
                }
                else {
                    revert("Not In Whitelist");
                }
            }
            else {
                require(msg.value == deposit);
                uint256 balances = address(this).balance;
                require(balances < maxContractLimit, "Whitelist Filled");
                require(balances.add(deposit) < maxContractLimit, "Deposit Over Whitelist Max - Check Contract Balance if its below limit");
                deposit = msg.value;
                percentFilled = ((balances.div(maxContractLimit)).mul(100));
                uint256 _amount = deposit.mul(ratePerAvax);
                uint256 transfertoken = _amount.div(1e18);
                IERC20( parrToken ).transfer( userAddress, transfertoken );
                
            }
        }
        else {
            revert("Sale not active");
        }
    }
    
}