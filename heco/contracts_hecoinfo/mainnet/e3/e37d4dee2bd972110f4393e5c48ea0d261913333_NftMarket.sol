        require(newOwner != address(0), "Ownable: new owner is the zero address");
        _transferOwnership(newOwner);
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Internal function without access restriction.
     */
    function _transferOwnership(address newOwner) internal virtual {
        address oldOwner = _owner;
        _owner = newOwner;
        emit OwnershipTransferred(oldOwner, newOwner);
    }
}


abstract contract ReentrancyGuard {
    // Booleans are more expensive than uint256 or any type that takes up a full
    // word because each write operation emits an extra SLOAD to first read the
    // slot's contents, replace the bits taken up by the boolean, and then write
    // back. This is the compiler's defense against contract upgrades and
    // pointer aliasing, and it cannot be disabled.

    // The values being non-zero value makes deployment a bit more expensive,
    // but in exchange the refund on every call to nonReentrant will be lower in
    // amount. Since refunds are capped to a percentage of the total
    // transaction's gas, it is best to keep them low in cases like this one, to
    // increase the likelihood of the full refund coming into effect.
    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;

    uint256 private _status;

    constructor() {
        _status = _NOT_ENTERED;
    }

    /**
     * @dev Prevents a contract from calling itself, directly or indirectly.
     * Calling a `nonReentrant` function from another `nonReentrant`
     * function is not supported. It is possible to prevent this from happening
     * by making the `nonReentrant` function external, and making it call a
     * `private` function that does the actual work.
     */
    modifier nonReentrant() {
        // On the first call to nonReentrant, _notEntered will be true
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");

        // Any calls to nonReentrant after this point will fail
        _status = _ENTERED;

        _;

        // By storing the original value once again, a refund is triggered (see
        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }
}


interface IERC721 {
    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) external;
}

contract NftMarket is Ownable, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeBEP20 for IERC20;
    uint256 public orderNum = 0;
    uint256 public tradeFee = 10;
    address[] public feeAddressList;
    uint256[] public tradeFeeList;

    struct orderItem {
        uint256 orderId;
        IERC721 erc721Token;
        address erc20Token;
        uint256[] tokenIdList;
        uint256 price;
        uint256 shelfTime;
        uint256 withdrawTime;
        uint256 tradeTime;
        address seller;
        address buyer;
    }
    
    struct orderItem2 {
        uint256 orderId;
        IERC721 erc721Token;
        address erc20Token;
        uint256 tokenId;
        uint256 price;
        uint256 shelfTime;
        uint256 withdrawTime;
        uint256 tradeTime;
        address seller;
        address buyer;
    }

    mapping(IERC20 => bool) public erc20TokenWhiteList;
    mapping(IERC721 => bool) public erc721TokenWhiteList;
    mapping(uint256 => orderItem) public orderList;
    mapping(uint256=>uint256[]) public orderList2;

    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;

    EnumerableSet.UintSet private validOrderList;
    EnumerableSet.UintSet private completedOrderList;
    EnumerableSet.AddressSet private erc20TokenList;
    EnumerableSet.AddressSet private erc721TokenList;
    mapping(address => EnumerableSet.UintSet) private validOrderListForErc20Token;
    mapping(IERC721 => EnumerableSet.UintSet) private validOrderListForNft;
    mapping(address => EnumerableSet.UintSet) private validOrderListForUser;
    mapping(address => EnumerableSet.UintSet) private buyOrderListForUser;


    function setTradeFee(uint256[] memory _tradeFeeList, address[] memory _feeTo,uint256 _tradeFee) external onlyOwner {
        require(_tradeFeeList.length == _feeTo.length);
        tradeFeeList = _tradeFeeList;
        feeAddressList = _feeTo;
        tradeFee = _tradeFee;
    }

    function setTokenWhiteList(IERC20[] memory _erc20TokenList, IERC721[] memory _erc721TokenList, bool _status) external onlyOwner {
        for (uint256 i = 0; i < _erc20TokenList.length; i++) {
            erc20TokenWhiteList[_erc20TokenList[i]] = _status;
            if (_status) {
                erc20TokenList.add(address(_erc20TokenList[i]));
            } else {
                erc20TokenList.remove(address(_erc20TokenList[i]));
            }
        }
        for (uint256 j = 0; j < _erc721TokenList.length; j++) {
            erc721TokenWhiteList[_erc721TokenList[j]] = _status;
            if (_status) {
                erc721TokenList.add(address(_erc721TokenList[j]));
            } else {
                erc721TokenList.remove(address(_erc721TokenList[j]));
            }
        }
    }

    function addOrder(IERC721 _erc721Token, address _erc20Token, uint256[] memory _tokenIdList, uint256 _price) external nonReentrant {
        if (_erc20Token != address(0)) {
            require(erc20TokenWhiteList[IERC20(_erc20Token)], "e001");
        }
        require(erc721TokenWhiteList[_erc721Token], "e002");
        require(feeAddressList.length>0, "e003");
        for (uint256 i = 0; i < _tokenIdList.length; i++) {
            _erc721Token.transferFrom(msg.sender, address(this), _tokenIdList[i]);
        }
        orderList[orderNum] = (new orderItem[](1))[0];
        orderList[orderNum].orderId = orderNum;
        orderList[orderNum].erc721Token = _erc721Token;
        orderList[orderNum].erc20Token = _erc20Token;
        orderList[orderNum].tokenIdList = _tokenIdList;
        orderList[orderNum].price = _price;
        orderList[orderNum].shelfTime = block.timestamp;
        orderList[orderNum].seller = msg.sender;
        validOrderList.add(orderNum);
        validOrderListForErc20Token[_erc20Token].add(orderNum);
        validOrderListForNft[_erc721Token].add(orderNum);
        validOrderListForUser[msg.sender].add(orderNum);
        orderList2[orderNum] = _tokenIdList;
        orderNum = orderNum.add(1);
    }
    
    function doOrder(uint256 _orderId) external payable nonReentrant {
        require(validOrderList.contains(_orderId), "e001");
        orderItem memory orderInfo = orderList[_orderId];
        uint256 fee = (orderInfo.price).mul(tradeFee).div(100);
        uint256 tradeAmount = (orderInfo.price).sub(fee);
        if (orderInfo.erc20Token == address(0)) {
            require(msg.value == orderInfo.price, "e002");
            for (uint256 m=0;m<feeAddressList.length;m++) {
                payable(feeAddressList[m]).transfer((orderInfo.price).mul(tradeFeeList[m]).div(100));
            }
            payable(orderInfo.seller).transfer(tradeAmount);
        } else {
            require(msg.value == 0, "e003");
            for (uint256 m=0;m<feeAddressList.length;m++) {
                IERC20(orderInfo.erc20Token).transferFrom(msg.sender,feeAddressList[m],(orderInfo.price).mul(tradeFeeList[m]).div(100));
            }
            IERC20(orderInfo.erc20Token).transferFrom(msg.sender, orderInfo.seller, tradeAmount);
        }
        for (uint256 i=0;i<orderList2[_orderId].length;i++) {
        orderInfo.erc721Token.transferFrom(address(this), msg.sender, orderList2[_orderId][i]);
        }
        orderList[_orderId].buyer = msg.sender;
        orderList[_orderId].tradeTime = block.timestamp;
        validOrderList.remove(_orderId);
        validOrderListForErc20Token[orderInfo.erc20Token].remove(_orderId);
        validOrderListForNft[orderInfo.erc721Token].remove(_orderId);
        validOrderListForUser[orderInfo.seller].remove(_orderId);
        buyOrderListForUser[msg.sender].add(_orderId);
        completedOrderList.add(_orderId);
    }

    function removeOrder(uint256 _orderId) external nonReentrant {
        require(validOrderList.contains(_orderId), "e001");
        orderItem memory orderInfo = orderList[_orderId];
        require(orderInfo.seller == msg.sender, "e002");
        for (uint256 i=0;i<orderList2[_orderId].length;i++) {
        orderInfo.erc721Token.transferFrom(address(this), msg.sender, orderList2[_orderId][i]);
        }
        orderList[_orderId].withdrawTime = block.timestamp;
        validOrderList.remove(_orderId);
        validOrderListForErc20Token[orderInfo.erc20Token].remove(_orderId);
        validOrderListForNft[orderInfo.erc721Token].remove(_orderId);
        validOrderListForUser[orderInfo.seller].remove(_orderId);
    }
    
    function getOrderInfo(uint256 _orderId)  external view returns (orderItem memory info1) {
        info1 = orderList[_orderId];
        // info2 = orderList2[_orderId];
    }

    function getValidOrderList() external view returns (uint256[] memory) {
        return validOrderList.values();
    }

    function getCompletedOrderList() external view returns (uint256[] memory) {
        return completedOrderList.values();
    }

    function getErc20TokenList() external view returns (address[] memory) {
        return erc20TokenList.values();
    }

    function getErc721TokenList() external view returns (address[] memory) {
        return erc721TokenList.values();
    }

    function getUserBuyOrderList(address _account) external view returns (uint256[] memory) {
        return buyOrderListForUser[_account].values();
    }
    
    function getValidOrderListForErc20Token(address _erc20Token) external view returns (uint256[] memory) {
        return validOrderListForErc20Token[_erc20Token].values();
    }

    function getValidOrderListForNft(IERC721 _erc721Token) external view returns (uint256[] memory) {
        return validOrderListForNft[_erc721Token].values();
    }

    function getValidOrderListForUser(address _account) external view returns (uint256[] memory) {
        return validOrderListForUser[_account].values();
    }
}