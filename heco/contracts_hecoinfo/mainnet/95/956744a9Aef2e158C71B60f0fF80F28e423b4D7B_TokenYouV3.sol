    function initialize(address owner) external {
        //require(!bInited,"Initialized");
        _owner = owner;
        _totalSupply = 2*10**6*10**uint256(_decimals);
        _balanceOf[_owner] = _totalSupply;
        bInited = true;
        emit Transfer(address(0),_owner,_totalSupply);

