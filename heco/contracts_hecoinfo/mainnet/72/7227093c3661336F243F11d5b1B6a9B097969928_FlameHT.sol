    mapping (address => bool) private _isExcludedFromFee;
    mapping (address => bool) private _isExcluded;
    uint256 public startTime;
    uint256 private constant MAX = ~uint256(0);
    uint256 private _tTotal;
    uint256 private _rTotal;
    uint256 private _tFeeTotal;
    uint256 public _maxTxAmount;
    bool public swapAction = true;
    string private _name = "FlameHT";
    string private _symbol = "FlameHT";
    uint8 private _decimals;
    address private _destroyAddress = address(0x000000000000000000000000000000000000dEaD);
    address payable _receiveBuy = payable(address(0x955630Ce062153E976dDD535f98b046c00750348));
	address payable _receiveSell = payable(address(0x955630Ce062153E976dDD535f98b046c00750348));
    address _tokenOwner;
    address[] buyUser;
    mapping(address => bool) public havePush;
    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair;
    bool public swapping;
    IERC20 public pair;
    bool public swapAndLiquifyEnabled = true;
