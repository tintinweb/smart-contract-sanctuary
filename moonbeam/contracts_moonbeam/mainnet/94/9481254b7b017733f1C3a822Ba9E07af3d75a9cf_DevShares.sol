interface MINTER {
    function mint() external returns (uint256);
}

contract DevShares is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    modifier splitTokens {
        _;
        uint256 rewards = pad.balanceOf(address(this));
        profitPerShare = SafeMath.add(profitPerShare, (rewards * magnitude) / sharesSupply);
    }

    event onSharesTransfer(
        address indexed from,
        address indexed to,
        uint256 amount
    );

    event onDonation(
        address indexed walletAddress,
        uint256 amount
    );
    
    event onWithdraw(
        address indexed walletAddress,
        uint256 amount
    );
    
    event onTokenMint(
        uint256 mintedPad,
        uint256 timestamp
    );
    
    uint128 constant private magnitude = 2 ** 64;
    uint16 constant private sharesSupply = 10000;

    mapping(address => uint256) private sharesBalanceLedger;
    mapping(address => int256) private payoutsTo;

    uint256 public padReceived;


    uint256 private profitPerShare;

    MINTER minterContract;
    IERC20 pad;

    constructor(address minter_, address pad_) {
        minterContract = MINTER(minter_);
        pad = IERC20(pad_);

        addShares(0xB255338C46C25ACA1D9e0cBD02377Dd089856dF1, 3334); //multi sig wallet
        addShares(0xa354bC96fE245a16DC74868766caF08B445dF195, 2666); //toadGuy 26.6% of 15% (4%)
        addShares(0xC62F6e0cb86c7BB987eE69aE400A82eCe702e34a, 2666); //snake 26.6% of 15% (4%)
        addShares(0xF566B5879be6cC78809FC6CAfC05b2291c4bA90c, 667); //stripey 6.68% of 15% (1%)
        addShares(0x4c6dC058fDFBa23Bc7240088950F48CDadF0Dd25, 667); //slumdog 6.68% of 15% (1%)
    }


    fallback() external payable {
        revert();
    }

    receive() external payable {
        revert();
    }

    function donateToPool(uint256 _amount) public splitTokens {
        pad.safeTransferFrom(msg.sender, address(this), _amount);
        padReceived += _amount;
        emit onDonation(msg.sender, _amount);
    }
    
    function mintPad() public splitTokens {
        uint256 _mintedAmount = minterContract.mint();
        padReceived += _mintedAmount;
        emit onTokenMint(_mintedAmount, block.timestamp);
    }

    function withdraw(address walletAddress_) public {
        uint256 _rewards = rewardsOf(walletAddress_);
        require(_rewards > 0, "ERROR: No balance to withdraw");
        payoutsTo[walletAddress_] += (int256) (_rewards.mul(magnitude));
        pad.transfer(walletAddress_, _rewards);
        emit onWithdraw(walletAddress_, _rewards);
    }

    function transferShares(address from_, address to_, uint256 amountOfShares_) external onlyOwner {
        removeShares(from_, amountOfShares_);
        addShares(to_, amountOfShares_);
        emit onSharesTransfer(from_, to_, amountOfShares_);
    }

    function addShares(address walletAddress_, uint256 amountOfShares_) private {
        require(amountOfShares_ > 0);

        sharesBalanceLedger[walletAddress_] = sharesBalanceLedger[walletAddress_].add(amountOfShares_);

        int256 _updatedPayouts = (int256) (profitPerShare.mul(amountOfShares_));
        payoutsTo[walletAddress_] += _updatedPayouts;

    }
        
    function removeShares(address walletAddress_, uint256 amountOfShares_) private {
        require(amountOfShares_ > 0 && amountOfShares_ <= sharesBalanceLedger[walletAddress_]);

        sharesBalanceLedger[walletAddress_] = sharesBalanceLedger[walletAddress_].sub(amountOfShares_);

        int256 _updatedPayouts = (int256) (profitPerShare.mul(amountOfShares_));
        payoutsTo[walletAddress_] -= _updatedPayouts;

    }

    function getSharesSupply() public pure returns(uint128) {
        return sharesSupply;
    }

    function rewardsOf(address _walletAddress) public view returns (uint256) {
        return (uint256) ((int256) (profitPerShare * sharesBalanceLedger[_walletAddress]) - payoutsTo[_walletAddress]) / magnitude;
    }

    function sharesOf(address _walletAddress) public view returns (uint256) {
        return sharesBalanceLedger[_walletAddress];
    }
    
}