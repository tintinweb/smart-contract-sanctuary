contract titanuniverse is ERC721, ReentrancyGuard,Ownable {
    uint public next_hero;
    
    string constant public name = "Titan Heroes";
    string constant public symbol = "TITAN";
    
	uint constant public max_genius = 10;
	uint constant public max_normal = 200;
	
    mapping(uint => uint) public class;
	
	struct HeroCount {
        uint genius;
		uint normal;
    }
	
	//gene: 0 is genius,1 is normal
	mapping(uint => uint) public gene;
	mapping(uint => uint) public level;
    
	mapping(uint => HeroCount) public herolimit;
	
	using EnumerableSet for EnumerableSet.AddressSet;
    EnumerableSet.AddressSet private _operators;
	
    string public tokenURIPrefix = "https://titan.fun/hero/info/";
    string public tokenURISuffix = ".json";
    
    event created(address indexed owner, uint class, uint gene, uint hero);
	event leveled(address indexed owner, uint level, uint summoner);
    
    function hero(uint _hero) external view returns ( uint _class,  uint _gene) {
        _class = class[_hero];
        _gene = gene[_hero];
        
    }
    
	function level_up(uint _hero) external onlyOperator{
        uint _level = level[_hero];
        level[_hero] = _level+1;
        emit leveled(msg.sender, _level, _hero);
    }
	
    
    function setTokenURIAffixes( string calldata _prefix, string calldata _suffix) external onlyOwner {
        tokenURIPrefix = _prefix;
        tokenURISuffix = _suffix;
    }
  
    function create(uint _class,uint _gene ) external nonReentrant onlyOwner  {
        require(1 <= _class && _class <= 30);
		require(0 == _gene || _gene == 1);
		
		HeroCount storage limit = herolimit[_class];
		if( _gene == 0 ){
			require(limit.genius + 1 <= max_genius,"reached the maxium genius");
			limit.genius ++;
		}
		else if( _gene == 1 ){
			require(limit.normal + 1 <= max_normal,"reached the maxium normal hero");
			limit.normal ++;
		}
		
        uint _next_hero = next_hero;
        class[_next_hero] = _class;
        gene[_next_hero] = _gene;
		level[_next_hero] = 1;
        _safeMint(msg.sender, _next_hero);  
        emit created(msg.sender, _class, _gene, _next_hero);
        next_hero++;
    }
    
    function tokenURI(uint256 _hero) public view returns (string memory) {
      
        string memory output = string(abi.encodePacked(tokenURIPrefix, toString(_hero), tokenURISuffix));
        
        return output;
    }
    
    function classes(uint id) public pure returns (string memory description) {
        if (id == 1) {
            return "Zeus";
        } else if (id == 2) {
            return "Hera";
        } else if (id == 3) {
            return "Poseidon";
        }  else if (id == 4) {
            return "Athena";
        }  else if (id == 5) {
            return "Hades";
        }  
    }
	
	
    
    function toString(uint256 value) internal pure returns (string memory) {
    // Inspired by OraclizeAPI's implementation - MIT license
    // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
	
	
	function addOperator(address _addOperator ) public onlyOwner returns (bool) {
        require(_addOperator != address(0), "_addOperator is the zero address");
        
        return EnumerableSet.add(_operators, _addOperator);
    }


    function delOperator(address _delOperator) public onlyOwner returns (bool) {
        require(_delOperator != address(0), "_delOperator is the zero address");
        return EnumerableSet.remove(_operators, _delOperator);
    }

    function getOperatorLength() public view returns (uint256) {
        return EnumerableSet.length(_operators);
    }

    function isOperator(address account) public view returns (bool) {
        return EnumerableSet.contains(_operators, account);
    }

    function getOperator(uint256 _index) public view onlyOwner returns (address){
        require(_index <= getOperatorLength() - 1, "index out of bounds");
        return EnumerableSet.at(_operators, _index);
    }

    // modifier for operator function
    modifier onlyOperator() {
        require(isOperator(msg.sender), "caller is not the Operator");
        _;
    }
}

