    uint256 internal constant fee = 0.2 * 10 ** 18;

    // the  minimum factor by which contract balance must be greater than
    // the wager, 1000 = 1x
    uint256 public wagerReserveMultiple;
    uint256 public pendingPrecalcPayouts;
    uint256 public pendingCalculatedPayouts;
    uint256 public maxPayoutPercent;

    uint256 public lifetimePayouts;
    mapping(address => uint256) public userLifetimePayouts;

    address public immutable wrappedLinkToken;

    uint256 public minimumWager = 400 * 1e18;

    uint256 public gamesInProgress = 0;

    constructor(
        address _currency,
        uint256[] memory _wheel1,
        uint256[] memory _wheel2,
        uint256[] memory _wheel3,
        uint256[][] memory payoutLabels,
        uint256[] memory payoutAmounts,
        uint256 _wagerReserveMultiple,
        bytes32 _keyHash,
        address vrfCoord,
        address linkToken
    )
        public
        Ownable()
        VRFConsumerBase(
            vrfCoord,
            linkToken
        )
        ReentrancyGuard()
    {
        wrappedLinkToken = linkToken;

        wheel1 = _wheel1;
        wheel2 = _wheel2;
        wheel3 = _wheel3;

        require(wheel1.length == wheel2.length && wheel2.length == wheel3.length, "wheels cant be different lengths");
        require(payoutLabels.length > 0, "not enough labels");

        currency = IERC20(_currency);

        // set the payouts denominated in percent
        uint256 maxPayout = 0;
        for (uint256 i; i < payoutLabels.length; i++) {
            uint256[] memory pp = payoutLabels[i];

            require(pp.length == 3, "wrong number of wheel lanes");

            payouts[pp[0]][pp[1]][pp[2]] = payoutAmounts[i];
            if (payoutAmounts[i] > maxPayout) {
                maxPayout = payoutAmounts[i];
            }
        }
        maxPayoutPercent = maxPayout;

        wagerReserveMultiple = _wagerReserveMultiple;
        keyHash = _keyHash;
    }

    /// Central function to kick off a game or roll over from previous game
    function playGame(Games game, uint256 wager) external whenNotPaused nonReentrant {
        require(game != Games.FINISHED, "!finished");

        currency.safeTransferFrom(msg.sender, address(this), wager);

        // first roll over from any pre-existing game
        bytes32 gameId = mostRecentGame[msg.sender];
        PlayerGame storage pg = playerGames[gameId];

        require(pg.wager == 0 || pg.game == Games.FINISHED, "Previous game isn't finished yet");

        if (pg.payout > 0) {
            // roll over earnings
            wager += pg.payout;

            // handle math defensively for availability
            if (pendingCalculatedPayouts >= pg.payout) {
                pendingCalculatedPayouts -= pg.payout;
            } else {
                pendingCalculatedPayouts = 0;
            }

            pg.payout = 0;
        }

        require(wager >= minimumWager, "Wager too small");

        // commence new game
        uint256 maxPayout = maxPayoutPerGameWager(wager);
        require(checkWagerRatio(maxPayout), "Wager exceeds current max");

        // request randomness and set up pending game
        bytes32 requestId = getRandomNumber();
        playerGames[requestId] = PlayerGame({ game: game, payout: 0, maxPayout: maxPayout, wager: wager, randn: 0 });
        mostRecentGame[msg.sender] = requestId;
        pendingPrecalcPayouts += maxPayout;

        gamesInProgress++;

        emit GameStarted(requestId, msg.sender);
    }

    /// Check whether submitted wager is too much for contract balance or not
    function checkWagerRatio(uint256 maxPayout) public view returns (bool) {
        uint256 availableBalance = currency.balanceOf(address(this));

        require(availableBalance > pendingPrecalcPayouts + pendingCalculatedPayouts, "Contract balance is too low");
        availableBalance -= pendingPrecalcPayouts + pendingCalculatedPayouts;
        return availableBalance > maxPayout * wagerReserveMultiple / 1000;
    }

    /// Internal callback to retrieve a random number from chainlink
    function getRandomNumber() internal returns (bytes32 requestId) {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK - fill contract with wrapped LINK");

        requestId = requestRandomness(keyHash, fee);
    }

    /// Internal function called from chainlink to finalize the spin
    function fulfillRandomness(bytes32 requestId, uint256 randomness) internal override {
        PlayerGame storage pg = playerGames[requestId];

        (uint256 pos1, uint256 pos2, uint256 pos3) = rand2pos(randomness);
        uint256 wager = pg.wager;
        uint256 payout = game2payout(pos1, pos2, pos3, pg.game, wager);

        pg.payout += payout;
        pg.game = Games.FINISHED;
        pg.randn = randomness;

        // we can undo our pessimistic estimation of pending payout
        if (pendingPrecalcPayouts > pg.maxPayout) {
            pendingPrecalcPayouts -= pg.maxPayout;
        } else {
            pendingPrecalcPayouts = 0;
        }

        pendingCalculatedPayouts += payout;

        gamesInProgress--;

        emit GameFullfilment(requestId, payout);
    }

    //  withdraw the payout for a specific game id
    function withdrawGamePayout() external nonReentrant {
        bytes32 gameId = mostRecentGame[msg.sender];
        PlayerGame storage pg = playerGames[gameId];
        require(pg.game == Games.FINISHED, "Game not finished yet");

        if (pg.payout > 0) {
            if (pendingCalculatedPayouts >= pg.payout) {
                pendingCalculatedPayouts -= pg.payout;
            } else {
                pendingCalculatedPayouts = 0;
            }

            currency.safeTransfer(msg.sender, pg.payout);

            emit GameWithdrawal(msg.sender, pg.payout);

            lifetimePayouts+=pg.payout;
            userLifetimePayouts[msg.sender]+=pg.payout;

            pg.payout = 0;
        }
    }

    /// calculate max potential payout per wager per game
    function maxPayoutPerGameWager(uint256 wager) public view returns (uint256) {
        return wager * maxPayoutPercent / 100;
    }

    /// check if player's most recent game is finished
    function viewGameFinished(address player) external view returns (bool) {
        bytes32 gameId = mostRecentGame[player];
        PlayerGame storage pg = playerGames[gameId];
        return pg.game == Games.FINISHED;
    }

    /// check the amount won in most recent game
    function viewPlayerPayout(address player) external view returns (uint256) {
        bytes32 gameId = mostRecentGame[player];
        PlayerGame storage pg = playerGames[gameId];
        // depending on what behavior you want remove this check
        //require(pg.game == Games.FINISHED, "Game not finished yet");
        return pg.payout;
    }

    /// Get player's position on wheels
    function viewPlayerPosition(address player) external view returns (uint256, uint256, uint256) {
        bytes32 gameId = mostRecentGame[player];
        PlayerGame storage pg = playerGames[gameId];
        // depending on what behavior you want remove this check
        //require(pg.game == Games.FINISHED, "Game not finished yet");
        return rand2pos(pg.randn);
    }

    /// convert random number to wheel position
    function rand2pos(uint256 randomness) internal view returns (uint256, uint256, uint256) {
        return (randomness % wheel1.length, expand(randomness) % wheel2.length, expand(expand(randomness)) % wheel3.length);
    }

    /// expand random number
    function expand(uint256 randomValue) public pure returns (uint256 expandedValues) {
        return uint256(keccak256(abi.encode(randomValue)));
    }

    /// Calculate payout for a game
    function game2payout(uint256 pos1, uint256 pos2, uint256 pos3, Games game, uint256 wager) internal view returns (uint256) {
        // no overflows because max payouts are limited by contract balance
        // theoretically requires wheel configurations that don't double payout rows
        // or wagerReserveMultiple larger than 3000 basis points
        if (game == Games.ONE_A) {
            return (wager * pos2payout(pos1, pos2, pos3)) / 100;
        } else if (game == Games.ONE_B) {
            return (wager * pos2payout(pos1 + 1, pos2 + 1, pos3 + 1)) / 100;
        } else if (game == Games.ONE_C) {
            return(wager * pos2payout(pos1 + 2, pos2 + 2, pos3 + 2)) / 100;
        } else if (game == Games.TWO_A) {
            return (wager * (pos2payout(pos1, pos2, pos3) + pos2payout(pos1 + 1, pos2 + 1, pos3 + 1))) / 200;
        } else if (game == Games.TWO_B) {
            return (wager * (pos2payout(pos1 + 1, pos2 + 1, pos3 + 1) + pos2payout(pos1 + 2, pos2 + 2, pos3 + 2))) / 200;
        } else if (game == Games.TWO_C) {
            return (wager * (pos2payout(pos1, pos2, pos3) + pos2payout(pos1 + 2, pos2 + 2, pos3 + 2))) / 200;
        } else if (game == Games.THREE) {
            return (wager * (pos2payout(pos1, pos2, pos3)
                + pos2payout(pos1 + 1, pos2 + 1, pos3 + 1)
                + pos2payout(pos1 + 2, pos2 + 2, pos3 + 2)))
                / 300;
        }
    }

    /// Convert wheel position to raw payout multiplier
    function pos2payout(uint256 pos1, uint256 pos2, uint256 pos3) internal view returns (uint256) {
        uint256 l1 = wheel1[pos1 % wheel1.length];
        uint256 l2 = wheel2[pos2 % wheel2.length];
        uint256 l3 = wheel3[pos3 % wheel3.length];

        return payouts[l1][l2][l3];
    }

    /// Set payout for label combination
    function setPayout(uint256 label1, uint256 label2, uint256 label3, uint256 payout) external onlyOwner {
         require(gamesInProgress == 0, "can only call with no games in progress!");

        // note: if this overwrites the previous max payout, lowering it, we don't catch that
        // see function below
        payouts[label1][label2][label3] = payout;
        if (payout > maxPayoutPercent) {
            // scale pendingPayouts
            pendingPrecalcPayouts = pendingPrecalcPayouts * payout / maxPayoutPercent;
            maxPayoutPercent = payout;
        }

        emit SetPayout(label1, label2, label3, payout);
    }

    /// set max payout as fallback
    function setMaxPayoutPercent(uint256 payout) external onlyOperator {
        require(gamesInProgress == 0, "can only call with no games in progress!");

        pendingPrecalcPayouts = pendingPrecalcPayouts * payout / maxPayoutPercent;
        maxPayoutPercent = payout;

        emit SetMaxPayoutPercent(payout);
    }

    /// Set max wager ratio
    function setWagerReserveMultiple(uint256 ratio) external onlyOperator {
        wagerReserveMultiple = ratio;

        emit SetWagerReserveMultiple(ratio);
    }

    /// Set min wager
    function setMinWager(uint256 _minWager) external onlyOwner {
        require(_minWager > 0, "!0");
        minimumWager = _minWager;

        emit SetMinWager(minimumWager);
    }


    function drainChainlink(address recipient, uint256 amount) external onlyOwner {
        require(IERC20(wrappedLinkToken).balanceOf(address(this)) >= amount);
        IERC20(wrappedLinkToken).safeTransfer(recipient, amount);

        emit LinkDrained(recipient, amount);
    }

    function drainFunds(address recipient, uint256 amount) external onlyOwner {
        require(currency.balanceOf(address(this)) >= amount + pendingPrecalcPayouts + pendingCalculatedPayouts);
        currency.safeTransfer(recipient, amount);

        emit PlayerFundsDrained(recipient, amount);
    }

    // Update the status of the operator
    function updateOperator(address _operator) external onlyOwner {
        require(_operator != address(0), "operator cannot be the 0 address");
        require(operator == address(0), "operator is already set!");

        operator = _operator;

        emit OperatorUpdated(_operator);
    }
}


