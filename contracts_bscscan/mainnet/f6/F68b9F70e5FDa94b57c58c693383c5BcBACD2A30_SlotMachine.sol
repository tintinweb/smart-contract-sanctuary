contract SlotMachine is VRFConsumerBase, Ownable, Pausable, ReentrancyGuard {
    using SafeERC20 for IERC20;

    event GameStarted(bytes32 indexed gameId, address indexed player);
    event PlayerFundsDrained(address recipient, uint256 amount);
    event LinkDrained(address recipient, uint256 amount);
    event SetWagerReserveMultiple(uint256 ratio);
    event SetMinWager(uint256 minWager);
    event SetMaxPayoutPercent(uint256 payout);
    event SetPayout(uint256 label1, uint256 label2, uint256 label3, uint256 payout);
    event GameFullfilment(bytes32 indexed requestId, uint256 payout);
    event GameWithdrawal(address player, uint256 payout);
    event OperatorUpdated(address indexed operator);

    address public operator;

    // operator is time lock
    modifier onlyOperator {
        require(operator == msg.sender, "Operator: caller is not the operator");
        _;
    }

    // used to wager and pay out
    IERC20 public immutable currency;

    // lists of jumbled up integers representing labels on the wheel
    uint256[] public wheel1;
    uint256[] public wheel2;
    uint256[] public wheel3;

    // map representing labels on wheels mapped to their payout in percent of players' wager
    mapping(uint256 => mapping(uint256 => mapping(uint256 => uint256))) public payouts;

    // games get used to calculate payouts, so finished games get no payouts
    enum Games {
        FINISHED,
        ONE_A,
        ONE_B,
        ONE_C,
        TWO_A,
        TWO_B,
        TWO_C,
        THREE
    }

    // represents the basic information of an ongoing or past game
    struct PlayerGame {
        uint256 wager;
        uint256 payout;
        uint256 maxPayout;

        uint256 randn;

        Games game;
    }

