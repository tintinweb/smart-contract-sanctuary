        bytes memory returndata = address(token).functionCall(data, 'SafeBEP20: low-level call failed');
        if (returndata.length > 0) {
            // Return data is optional
            // solhint-disable-next-line max-line-length
            require(abi.decode(returndata, (bool)), 'SafeBEP20: BEP20 operation did not succeed');
        }
    }
}

abstract contract Ownable is Context {
    address private _owner;
    address private _previousOwner;
    uint256 private _lockTime;

    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    /**
     * @dev Initializes the contract setting the deployer as the initial owner.
     */
    constructor () internal {
        address msgSender = _msgSender();
        _owner = msgSender;

        emit OwnershipTransferred(address(0), msgSender);
    }

    /**
     * @dev Returns the address of the current owner.
     */
    function owner() public view returns (address) {
        return _owner;
    }

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(_owner == _msgSender(), "Ownable: caller is not the owner");
        _;
    }

     /**
     * @dev Leaves the contract without owner. It will not be possible to call
     * `onlyOwner` functions anymore. Can only be called by the current owner.
     *
     * NOTE: Renouncing ownership will leave the contract without an owner,
     * thereby removing any functionality that is only available to the owner.
     */
    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(newOwner != address(0), "Ownable: new owner is the zero address");
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }
}

// File: StakeMan.sol
contract StakeMan is Ownable {
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;
    struct UserReference {
        address UserName;
        address Child;
        address Parent;
        bool isExist;
    }
    struct UserStakeInfo {
        uint256 amount;         // How many LP tokens the user has provided.
        uint256 startBlock;
        uint256 endBlock;
        uint256 package; //1=lock 10d, 2 = lock 20d, 3 = lock 30d (10%,20%,30%)
        uint256 status;
    }

    address private poolAddress;
    address private tokenAddress;
    uint256 private decimal = 18;
    uint256 private totalStake = 0;
    uint256 private minStake = 0;
    bool private rewardComission = false;
    uint256 private blockStake = 1 days; //release
    // Info of each user that stakes LP tokens.
    mapping (address => mapping(uint256 => UserStakeInfo) ) public userStake;
    mapping(uint256 => UserReference) private accounts;
    mapping(uint256 => uint256) private PackagePercent;
    //
    bool private isLockStake = false;
    //
    using Counters for Counters.Counter;
    Counters.Counter private _accIndex;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);

    event EventResponse(address indexed user, uint256 errCode, string errorMessage);
    event Claim(address indexed user, uint256 errCode, string errorMessage, uint256 totalEarn);

    constructor( address _dragonverse ) public {
        tokenAddress = _dragonverse;
        poolAddress = address(this);
        PackagePercent[1] = 3;
        PackagePercent[2] = 7;
        PackagePercent[3] = 11;
    }

    function getMinStake(uint256 _minStake) public view onlyOwner returns(uint256) {
        return minStake;
    }

    function updateMinStake(uint256 _minStake) public onlyOwner {
        minStake = _minStake;
    }

    function onRewardComission() public onlyOwner {
        rewardComission = true;
    }

    function offRewardComission() public onlyOwner {
        rewardComission = false;
    }

    function lockStake() public onlyOwner {
        isLockStake = true;
    }

    function unLockStake() public onlyOwner {
        isLockStake = false;
    }

    function updatePackageReward(uint256 package,uint256 percent) public payable onlyOwner {
        PackagePercent[package] = percent;
    }

    function getPackageRewardInfo() public view onlyOwner returns(uint256 p1,uint256 p2,uint256 p3) {
        p1 = PackagePercent[1];
        p2 = PackagePercent[2];
        p3 = PackagePercent[3];
    }

    function changePoolAddress(address _poolAdress) public payable onlyOwner {
        poolAddress = _poolAdress;
    }

    function changeTokenAddress(address _tokenAddress) public payable onlyOwner {
        tokenAddress = _tokenAddress;
    }

    function register(address inviter) public payable {
        require( _msgSender() != address(0),"User address is invalid" );
        require( inviter != address(0),"Inviter address is invalid" );
        require( _msgSender() != address(0),"Can not inviter your self" );
        bool flag = checkHaveParent(_msgSender());
        if(!flag) {
            UserReference memory userInfo = UserReference(_msgSender(),address(0),inviter,true);
            _accIndex.increment();
            uint256 _index = _accIndex.current();
            accounts[_index] = userInfo;
        }
        emit EventResponse(_msgSender(),0,"OK");
    }

    function getAllChild() public view returns(UserReference[] memory) {
        uint totalItemCount = _accIndex.current();
        uint itemCount = 0;

        for (uint i = 0; i < totalItemCount; i++) {
            if (accounts[i + 1].Parent == _msgSender()) {
                itemCount += 1;
            }
        }
        UserReference[] memory items = new UserReference[](itemCount);
        uint arIndex = 0;
        for (uint i = 0; i < totalItemCount; i++) {
            if (accounts[i + 1].Parent == _msgSender()) {
                UserReference storage currentItem = accounts[i + 1];
                items[arIndex] = currentItem;
                arIndex++;
            }
        }
        return items;
    }

    function getUserInfo(address userName) private returns(UserReference memory userInfo) {
        uint accIndex = getAccountIndex(userName);
        if( accIndex > 0) {
            userInfo = accounts[accIndex];
        }
    }

    function getAccountIndex(address userName) private returns(uint) {
        uint totalItemCount = _accIndex.current();
        uint currentIndex = 0;
        for (uint i = 0; i < totalItemCount; i++) {
            if (accounts[i + 1].isExist && accounts[i + 1].UserName == userName) {
                currentIndex = i + 1;
                i = totalItemCount;
            }
        }
        return currentIndex;
    }

    function checkHaveParent(address userName) public view returns(bool) {
        bool flag = false;
        uint totalItemCount = _accIndex.current();
        for (uint i = 0; i < totalItemCount; i++) {
            if (accounts[i + 1].isExist && accounts[i + 1].UserName == userName && accounts[i + 1].Parent != address(0)) {
                flag = true;
                i = totalItemCount;
            }
        }
        return flag;
    }

    function stake(uint256 amount,uint256 package) public payable {
        uint256 errCode = 0;
        string memory errorMessage = "OK";
        if(!isLockStake) {
            bool flag = true;
            if( minStake > 0 && amount < minStake ) {
                flag = false;
                errCode = 5;
                errorMessage = "Deposit amount must be greater than the minimum amount";
            }
            if( flag ) {
                UserStakeInfo memory uInfo = userStake[_msgSender()][package];
                if( uInfo.status == 0 ) {
                    if( package >= 1 && package <= 3 ) {
                        uint256 _amount = amount * 10 ** decimal;
                        uint256 userBalance = ERC20(tokenAddress).balanceOf(_msgSender());
                        if( userBalance >= _amount ) {
                            ERC20(tokenAddress).transferFrom(_msgSender(), poolAddress, _amount);
                            //update user stake
                            uint256 now = block.timestamp;
                            uInfo.status = 1;
                            uInfo.amount = _amount;
                            uInfo.startBlock = now;
                            uInfo.endBlock = now + ( package * 10 * blockStake );
                            uInfo.package = package;
                            userStake[_msgSender()][package] = uInfo;
                            totalStake += amount;
                        } else {
                            errCode = 3;
                            errorMessage = "Balance is not enough";
                        }
                    } else {
                        errCode = 2;
                        errorMessage = "Package parameter is incorrect";
                    }
                } else {
                    errCode = 1;
                    if(package == 1) {
                        errorMessage = "Package 10-days in progress";
                    }
                    if(package == 2) {
                        errorMessage = "Package 20-days in progress";
                    }
                    if(package == 3) {
                        errorMessage = "Package 30-days in progress";
                    }
                }
            }
        } else {
            errCode = 4;
            errorMessage = "Deposit is locked";
        }
        emit EventResponse(_msgSender(),errCode,errorMessage);
    }

    function claim(uint256 package) public payable {
        require(package >= 1 && package <= 3,"Package is invalid");
        uint256 errCode = 0;
        string memory errorMessage = "OK";
        UserStakeInfo memory uInfo = userStake[_msgSender()][package];
        uint256 totalEarn = 0;
        if( uInfo.status == 1 ) {
            uint256 now = block.timestamp;
            if( uInfo.endBlock <= now ) {
                uint256 percent = PackagePercent[uInfo.package];
                if( percent > 0 ) {
                    uint256 interest = uInfo.amount * percent / 100;
                    totalEarn = uInfo.amount + interest;
                    poolReward(totalEarn,_msgSender());
                    if( rewardComission ) {
                        comission3Lv(_msgSender(),interest,1);    
                    }
                    uInfo.status = 0;
                    userStake[_msgSender()][package] = uInfo;
                    totalStake -= (uInfo.amount / (10 ** decimal));
                } else {
                    errCode = 3;
                    errorMessage = "Package reward is invalid";
                }
            } else {
                errCode = 2;
                errorMessage = "Request claim has not been reached";
            }
        } else {
            errCode = 1;
            errorMessage = "You have no deposit";
        }
        emit Claim(_msgSender(),errCode,errorMessage,totalEarn);
    }

    function getTotalStake() public view returns(uint256) {
        return totalStake;
    }
 
    function stakeInfos() public view returns(UserStakeInfo[] memory) {
        UserStakeInfo[] memory items = new UserStakeInfo[](3);
        items[0] = userStake[_msgSender()][1];
        items[1] = userStake[_msgSender()][2];
        items[2] = userStake[_msgSender()][3];
        return items;
    }

    function stakeInfo(uint256 package) public view returns(uint256 amount, uint256 startBlock, 
        uint256 endBlock, uint256 _package, uint256 reward, uint256 reward_perday,
        uint256 rate_interest,uint256 ts_now) {
        UserStakeInfo memory info = userStake[_msgSender()][package];
        if( info.status == 1 ) {
            amount = info.amount / (10 ** decimal);
            startBlock = info.startBlock;
            endBlock = info.endBlock;
            _package = info.package;
            //calcu reward
            rate_interest = PackagePercent[package];
            uint256 interest = info.amount *  rate_interest / 100 / (10 ** decimal);
            reward_perday = interest / (package * 10);
            uint256 numDay = (block.timestamp - startBlock) / blockStake;
            if( numDay > (package * 10) ) {
                numDay = package * 10;
            }
            reward = reward_perday * numDay;
            ts_now = block.timestamp;
        }
    }

 
    function comission3Lv(address child,uint256 interest,uint256 level) private {
        if( level >= 1 && level <= 3 ) {
            UserReference memory userRef = getUserInfo(child);
            if( userRef.isExist ) {
                //F1 = 6% , F2 = 3%, F3 = 1%
                if( userRef.Parent != address(0) ) {
                    uint256 percent = 0;
                    if( level == 1 ) {
                        percent = 6;
                    }
                    if( level == 2 ) {
                        percent = 3;
                    }
                    if( level == 3 ) {
                        percent = 1;
                    }
                    uint256 comission = interest * percent / 100;
                    poolReward(comission,userRef.Parent);
                    //
                    level = level + 1;
                    comission3Lv(userRef.Parent,interest,level);
                }
            }
        }
    }

    function poolReward(uint256 amount,address receiver) private {
        if( amount > 0 ){
            uint256 _poolBalance = ERC20(tokenAddress).balanceOf(address(this));
            if( _poolBalance < amount ) {
                amount = _poolBalance;
            }
            ERC20(tokenAddress).transfer(receiver,amount);
        }
    }
}