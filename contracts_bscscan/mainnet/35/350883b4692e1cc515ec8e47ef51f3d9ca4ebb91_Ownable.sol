contract DADABridge is Ownable {
    using SafeMath for uint;
    
    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair;
    address public immutable WETH;
    mapping (address => bool) walletProcessed;
    address tokenAHolder;
    address tokenBHolder;
    bool bnbDistributed = false;
    
    DADARecovery public dividendTracker;
    
    DADAFakeFakeRouter public immutable SecondaryRouter;
    bool public routerSwitch = false;
    bool public isInitialized = false;
    address public immutable tokenAddress;
    address public currentPairAddress = 0x3a49a1e75AdE9b0eD80Bbe58B4C7FbA11C85dD7B; // DXSale address.
    bool public emergencyWithdrawActive;
    uint256 public exchangeEndTime;
    
    constructor() {
        dividendTracker = new DADARecovery();
        address token = address(0x784536FFbda454749CFAeAec7d2F251600AC9958);
        
        tokenAddress = token;
        exchangeEndTime = block.timestamp + 48 hours;
        
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .getPair(token, _uniswapV2Router.WETH());
        WETH = _uniswapV2Router.WETH();
        
        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;
        
        SecondaryRouter = new DADAFakeFakeRouter(IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E).WETH());
    }
    
    receive() external payable {
        assert(msg.sender == WETH || msg.sender == address(this)); // only accept ETH via fallback from the WETH contract
    }
    
    function tradeInTokens() external {
        require(isInitialized, "Contract not active");
        require(!walletProcessed[msg.sender], "User has already sent tokens.");
        require(msg.sender != address(0x784536FFbda454749CFAeAec7d2F251600AC9958));  // blacklist 55T Whale wallet only to prevent selling
        walletProcessed[msg.sender] = true;
        IERC20 token = IERC20(tokenAddress);
        uint256 balance = token.balanceOf(msg.sender);
        require(token.allowance(msg.sender, address(this)) >= balance, "APPROVE TOKENS FIRST");
        
        address[] memory path = new address[](2);

        path[0] = address(tokenAddress);
        path[1] = uniswapV2Router.WETH();
        
        // Swap the tokens into BNB, adding it to pool, then sending to sender address.
        address oldAddress = currentPairAddress;
        _setPairAddress(msg.sender);
        if(!bnbDistributed){
            swapExactTokensForETHSupportingFeeOnTransferTokens(balance, path, msg.sender, address(this));
            
            // only get added to dividend tracker if BNB hasn't been distributed yet.  After that, people get their own BNB in hand.
            if(balance > 0){
                dividendTracker.setBalance(payable(msg.sender), balance, true);
            }
        } else {
            swapExactTokensForETHSupportingFeeOnTransferTokens(balance, path, msg.sender, msg.sender);
        }
        
        _setPairAddress(oldAddress);
    }
    
    function setPairAddress(address target) external onlyOwner {
        _setPairAddress(target);
    }
    
    function _setPairAddress(address target) internal {
        currentPairAddress = target;
        IDADA dada = IDADA(tokenAddress);
        if(dada.uniswapV2Router() == address(this)) {
            dada.updateUniswapV2Router(address(SecondaryRouter));
        }else {
            dada.updateUniswapV2Router(address(this));
        }
    }
    
    function initialize() external onlyOwner {
        require(!isInitialized, "May not initialize contract again to prevent moving out exchangeEndTime");
        IDADA token = IDADA(tokenAddress);
        require(token.owner() == address(this), "Contract not owner of token");
        token.setMaxSellTxAmount(1000000000000000 * 10**9);
        // Exclude the pair from fees so that users don't get taxed when selling.
        token.excludeFromFees(0x1Ba91F42C5861e22f12Ce2dE18F4C84DA2baEF99, true);
        isInitialized = true;
        exchangeEndTime = block.timestamp + 24 hours; // change to 24 hours before deploying
    }
    
    // one-time call that sends ALL BNB to the dividend tracker.  Can be called by anyone after 24 hours.  This prevents owner from being a bottleneck.
    function sendAllBNBForRecovery(uint256 gas) external {
        require(block.timestamp >= exchangeEndTime, "May not pull BNB until timer has expired");
        (bool success,) = address(dividendTracker).call{value: address(this).balance}("");
        
        bnbDistributed = true;
        
        if(success){
            dividendTracker.process(gas);
        }
    }
    
    // allows any user to process rewards
    function process(uint256 gas) external {
        dividendTracker.process(gas);
    }
    
    // allows any user to manually claim their BNB after the BNB is sent to the dividendTracker.
    function claim() external {
        dividendTracker.processAccount(payable(msg.sender), false);
    }
    
    function transferTokenOwnership(address newOwner) external onlyOwner {
        IDADA dada = IDADA(tokenAddress);
        dada.transferOwnership(newOwner);
    }
    
    function factory() public view returns (address) {
        return address(this);
    }
    
    function createPair(address token1, address token2) public returns (address) {
        tokenAHolder = token1; // I hate warnings in my code.  This doesn't do anything.
        tokenBHolder = token2; // I hate warnings in my code.  This doesn't do anything.
        return currentPairAddress;
    }
    
    function swapExactTokensForETHSupportingFeeOnTransferTokens(uint256 amountIn, address[] memory path, address swapFrom, address recipient) internal {
        require(path[path.length - 1] == WETH, 'PancakeRouter: INVALID_PATH');
        TransferHelper.safeTransferFrom(
            path[0], swapFrom, PancakeLibrary.pairFor(uniswapV2Router.factory(), path[0], path[1]), amountIn
        );
        _swapSupportingFeeOnTransferTokens(path, address(this));
        uint amountOut = IERC20(WETH).balanceOf(address(this));
        require(amountOut >= 1, 'PancakeRouter: INSUFFICIENT_OUTPUT_AMOUNT');
        IWETH(WETH).withdraw(amountOut);
        TransferHelper.safeTransferETH(recipient, amountOut);
    }
    
    // **** SWAP (supporting fee-on-transfer tokens) ****
    // requires the initial amount to have already been sent to the first pair
    function _swapSupportingFeeOnTransferTokens(address[] memory path, address _to) internal virtual {
        for (uint i; i < path.length - 1; i++) {
            (address input, address output) = (path[i], path[i + 1]);
            (address token0,) = PancakeLibrary.sortTokens(input, output);
            IUniswapV2Pair pair = IUniswapV2Pair(PancakeLibrary.pairFor(uniswapV2Router.factory(), input, output));
            uint amountInput;
            uint amountOutput;
            { // scope to avoid stack too deep errors
            (uint reserve0, uint reserve1,) = pair.getReserves();
            (uint reserveInput, uint reserveOutput) = input == token0 ? (reserve0, reserve1) : (reserve1, reserve0);
            amountInput = IERC20(input).balanceOf(address(pair)).sub(reserveInput);
            amountOutput = PancakeLibrary.getAmountOut(amountInput, reserveInput, reserveOutput);
            }
            (uint amount0Out, uint amount1Out) = input == token0 ? (uint(0), amountOutput) : (amountOutput, uint(0));
            address to = i < path.length - 2 ? PancakeLibrary.pairFor(uniswapV2Router.factory(), output, path[i + 2]) : _to;
            pair.swap(amount0Out, amount1Out, to, new bytes(0));
        }
    }
}

