        uint256 lp = ratio0 + ratio1;
        _reserve0 = r0;
        _reserve1 = r1;
        _price0 = price0;
        _price1 = price1;
        _ratio0 = ratio0;
        //_ratio1 = ratio1;
        _totalSupply = totalSupply;
        _lp = lp;
    }

