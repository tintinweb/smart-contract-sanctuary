     * 1. It claims rewards from the masterchef.
     * 2. It charges the system fees to simplify the split.
     * 3. It swaps the {liquidityToken} token for {token}
     * 4. Adds more liquidity to the pool.
     * 5. It deposits the new LP tokens.
     */
    function harvest() external {
        require(!Address.isContract(msg.sender), "Caller must be a contract");
        IMasterChef(masterchef).deposit(poolId, 0);
        chargeFees();
        swap();
        deposit();

        emit StratHarvest(msg.sender);
    }

    /**
     * @dev Takes out 2.5% as system fees.
     */

    function chargeFees() internal {
        uint256 totalFees = IERC20(tokens.liquidityToken) // NRV
            .balanceOf(address(this))
            .mul(25)
            .div(1000);

        // Change to BIG token
        address[] memory path = new address[](3);
        path[0] = tokens.liquidityToken;
        path[1] = tokens.nativeToken;
        path[2] = tokens.bigFootToken;

        IUniswapV2Router02(unirouter).swapExactTokensForTokens(
            totalFees,
            0,
            path,
            address(this),
            block.timestamp + 600
        );

        procesFees();

    }

    function procesFees() internal {

        uint256 totalBIG = IERC20(tokens.bigFootToken).balanceOf(address(this));

        // Call
        uint256 callFeeTokens = totalBIG.mul(callFee).div(maxFee());
        IERC20(tokens.bigFootToken).transfer(msg.sender, callFeeTokens);

        // Treasury
        uint256 treasuryFeeTokens = totalBIG.mul(treasuryFee).div(maxFee());
        IERC20(tokens.bigFootToken).transfer(treasury, treasuryFeeTokens);

        // Burn
        uint256 burnFeeTokens = totalBIG.mul(burnFee).div(maxFee());
        IERC20(tokens.bigFootToken).transfer(burn, burnFeeTokens);
    }

    function swap() internal {
        uint256 amount = IERC20(tokens.liquidityToken).balanceOf(address(this));

        // Define path of the change
        address[] memory path;
        if (
            tokens.token == address(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56)
        ) {
            // BUSD
            path = new address[](2);
            path[0] = tokens.liquidityToken;
            path[1] = address(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56);
        } else if (
            tokens.token ==
            address(0x55d398326f99059fF775485246999027B3197955) ||
            tokens.token == address(0x8AC76a51cc950d9822D68b83fE1Ad97B32Cd580d)
        ) {
            // USDT or USDC
            path = new address[](3);
            path[0] = tokens.liquidityToken;
            path[1] = address(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56);
            path[2] = tokens.token;
        }

        IUniswapV2Router02(unirouter).swapExactTokensForTokens(
            amount,
            0,
            path,
            address(this),
            block.timestamp + 600
        );

    }

    /**
     * @dev Function to calculate the total underlaying {token} held by the strat.
     * It takes into account both the funds in hand, as the funds allocated in the masterchef.
     */
    function balanceOf() public view returns (uint256) {
        return balanceOfToken().add(balanceOfPool());
    }

    /**
     * @dev It calculates how much {token} the contract holds.
     */
    function balanceOfToken() public view returns (uint256) {
        return IERC20(tokens.token).balanceOf(address(this));
    }

    /**
     * @dev It calculates how much {token} the strategy has allocated in the masterchef
     */
    function balanceOfPool() public view returns (uint256) {
        (uint256 _amount, ) = IMasterChef(masterchef).userInfo(
            poolId,
            address(this)
        );
        return _amount;
    }

    /**
     * @dev Function that has to be called as part of strat migration. It sends all the available funds back to the
     * vault, ready to be migrated to the new strat.
     */
    function retireStrat() external onlyOwner {
        panic();

        uint256 tokenBal = IERC20(tokens.token).balanceOf(address(this));
        IERC20(tokens.token).transfer(msg.sender, tokenBal);
    }

    /**
     * @dev Pauses deposits. Withdraws all funds from the masterchef, leaving rewards behind
     */
    function panic() public onlyOwner {
        IMasterChef(masterchef).emergencyWithdraw(poolId);
    }

    function changeCallFee(uint256 _newCallFee) public onlyOwner {
        callFee = _newCallFee;
    }

    function changeTreasuryFee(uint256 _newTreasuryFee) public onlyOwner {
        treasuryFee = _newTreasuryFee;
    }

    function changeBurnFee(uint256 _newBurnFee) public onlyOwner {
        burnFee = _newBurnFee;
    }

    function changeWithdrawalFee(uint256 _newBurnFee) public onlyOwner {
        WITHDRAWAL_FEE = _newBurnFee;
    }

    function maxFee() internal view returns (uint256) {
        return callFee + treasuryFee + burnFee;
    }
}

