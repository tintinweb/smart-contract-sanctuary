        view
        returns (uint256);

    function userInfo(uint256 _pid, address _user)
        external
        view
        returns (uint256, uint256);

    function emergencyWithdraw(uint256 _pid) external;
}

interface INerveSwap {
    function addLiquidity(
        uint256[] memory _amounts,
        uint256 _minToMint,
