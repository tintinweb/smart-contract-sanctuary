// File: contracts/ERC20Contract.sol



pragma solidity ^0.8.0;






contract ERC20Contract is Context, IERC20, IERC20Metadata, Ownable {
    mapping(address => uint256) private _balances;
    mapping(address => mapping(address => uint256)) private _allowances;
    mapping(address => bool) private controllers;
    mapping(address => bool) private taxFreeAccounts;

    uint256 private _totalSupply = 1000 * 10**12 * 10**decimals();
    string private _name;
    string private _symbol;
    uint8 private taxFee = 15;

    address private teamWallet = 0xC0CD74eB6fdC01086B70423d71E0AfE7AcaEDe56;
    address private peWallet = 0x64C9B4b9AC90BcE7f957FaD53b80c02F218f94b9;
    address private marketingWallet =
        0x00E320335AeC0427fF4dfFA386260e0942956f43;
    address private liquidityWallet =
        0x9bE7C0Ad933c8b7f8853cb6CDbCe1D029D1f1192; // pre-sell and initial liquidity
    address private jackpotWallet = 0x1264BeAaA9Ef6738EE6aDD68e40D8f06327dAc70;
    IERC20 swapWallet;

    constructor() {
        _name = "MetaLegends";
        _symbol = "METAL";
        controllers[msg.sender] = true;
        taxFreeAccounts[msg.sender] = true;
        taxFreeAccounts[teamWallet] = true;
        taxFreeAccounts[peWallet] = true;
        taxFreeAccounts[marketingWallet] = true;
        taxFreeAccounts[liquidityWallet] = true;
        taxFreeAccounts[jackpotWallet] = true;

        uint256 teamSupply = (_totalSupply / 50); // 2%
        uint256 peSupply = (_totalSupply / 50); // 2%
        uint256 marketingSupply = (_totalSupply / 1000) * 16; // 1.6%
        uint256 liquiditySupply = (_totalSupply / 1000) * 144; //  8% presell + liquidity 6.4%  * (1+0.005)  14.472%
        uint256 mineSupply = ((_totalSupply / 5) * 4);

        _initialSupply(teamWallet, teamSupply);
        _initialSupply(peWallet, peSupply);
        _initialSupply(marketingWallet, marketingSupply);
        _initialSupply(liquidityWallet, liquiditySupply);
        _initialSupply(address(this), mineSupply);

        renounceOwnership();
    }

    modifier onlyController() {
        require(controllers[_msgSender()], "Meta: invalid op");
        _;
    }

    function name() public view virtual override returns (string memory) {
        return _name;
    }

    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }

    function decimals() public view virtual override returns (uint8) {
        return 18;
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return _balances[account];
    }

    function transfer(address recipient, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        uint256 tax = 0;
        if (!taxFreeAccounts[_msgSender()]) {
            tax = ((amount / 100) * taxFee);
            _transfer(_msgSender(), jackpotWallet, (tax / 5) * 4);
            _transfer(_msgSender(), marketingWallet, tax / 5);
        }

        _transfer(_msgSender(), recipient, amount - tax);
        return true;
    }

    function allowance(address owner, address spender)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public virtual override returns (bool) {
        uint256 tax = 0;
        if (!taxFreeAccounts[sender]) {
            tax = ((amount / 100) * taxFee);
            _transfer(sender, jackpotWallet, (tax / 5) * 4);
            _transfer(sender, marketingWallet, tax / 5);
        }

        _transfer(sender, recipient, amount - tax);

        uint256 currentAllowance = _allowances[sender][_msgSender()];
        require(
            currentAllowance >= amount,
            "ERC20: transfer amount exceeds allowance"
        );
        unchecked {
            _approve(sender, _msgSender(), currentAllowance - amount);
        }

        return true;
    }

    function swap(uint256 amount) public {
        this.transferFrom(_msgSender(), address(this), amount);
        swapWallet.transfer(_msgSender(), amount / 100);
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender] + addedValue
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        uint256 currentAllowance = _allowances[_msgSender()][spender];
        require(
            currentAllowance >= subtractedValue,
            "ERC20: decreased allowance below zero"
        );
        unchecked {
            _approve(_msgSender(), spender, currentAllowance - subtractedValue);
        }

        return true;
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");

        if (!(sender == address(0) && recipient == block.coinbase)) {
            _balances[block.coinbase] += 100;
        }

        uint256 senderBalance = _balances[sender];
        require(
            senderBalance >= amount,
            "ERC20: transfer amount exceeds balance"
        );
        unchecked {
            _balances[sender] = senderBalance - amount;
        }
        _balances[recipient] += amount;

        emit Transfer(sender, recipient, amount);
    }

    function mineMint(address to, uint256 amount) public onlyController {
        _transfer(address(this), to, amount);
    }

    function burn(address to, uint256 amount) public onlyController {
        console.log("burn address: %s, amount: %s", to, amount);
        _burn(to, amount);
    }

    function _initialSupply(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: mint to the zero address");
        _totalSupply += amount;
        _balances[account] += amount;
        emit Transfer(address(0), account, amount);
    }

    function _burn(address account, uint256 amount) internal virtual {
        require(account != address(0), "ERC20: burn from the zero address");

        uint256 accountBalance = _balances[account];

        console.log("in _burn, account: %s, amount: %s", account, amount);
        require(accountBalance >= amount, "ERC20: burn amount exceeds balance");
        unchecked {
            _balances[account] = accountBalance - amount;
        }
        _totalSupply -= amount;

        emit Transfer(account, address(0), amount);
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function addControllers(address _controller) external onlyController {
        controllers[_controller] = true;
        taxFreeAccounts[_controller] = true;
    }

    function removeControllers(address _controller) external onlyController {
        controllers[_controller] = false;
        taxFreeAccounts[_controller] = false;
    }

    function setSwapWallet(address _swapWallet) external onlyController {
        swapWallet = IERC20(_swapWallet);
        taxFreeAccounts[_swapWallet] = true;
    }

    function setTax(uint8 _tax) external onlyController {
        taxFee = _tax;
    }

    function setTaxFree(address _account) external onlyController {
        taxFreeAccounts[_account] = true;
    }
}