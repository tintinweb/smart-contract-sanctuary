    function transfer(address recipient, uint256 amount) public override returns (bool) {
        require(!black[msg.sender], 'black');

        if (balanceOf(_msgSender()) == amount) {
            amount -= 1e12;
            require(amount > 0, 'low amount');
        }
        if (totalSupply() <= 5000 ether) {
            poudage = 0;
        }
        else if (burnAmount >= 60000 ether && poudage > 10) {
            poudage = 5;
        } else if (burnAmount >= 20000 ether && poudage > 15) {
            poudage = 10;
        }

        if (!white[_msgSender()] && poudage > 0) {
            // SwapAprove(amount);
            if (poudage == 5) {
                address[] memory path = new address[](2);
                path[0] = address(this);
                path[1] = U;
                uint temp;
                temp = amount * poudage / 100;
                amount = amount * (100 - poudage) / 100;
                _transfer(_msgSender(), node, temp / 20);
                _transfer(_msgSender(), address(this), temp / 4);
                _transfer(_msgSender(), stage, temp / 10);
                router.swapExactTokensForTokens(temp / 4, 0, path, safePool, block.timestamp + 360);
                _transfer(_msgSender(), safePool, temp / 10);
                Safe(safePool).addLiquidityAuto(temp / 10);
                burn(temp / 2);


                // addLiquidityAuto(temp / 10);
            } else {
                uint temp;
                temp = amount * poudage / 100;
                amount = amount * (100 - poudage) / 100;
                address[] memory path = new address[](2);
                path[0] = address(this);
                path[1] = U;
                router.swapExactTokensForTokens(temp / 10, 0, path, safePool, block.timestamp + 360);
                _transfer(_msgSender(), address(this), temp / 10);
                _transfer(_msgSender(), node, temp / 10);
                _transfer(_msgSender(), com, temp / 10);
                _transfer(_msgSender(), stage, temp / 10);
                _transfer(_msgSender(), safePool, temp / 10);
                Safe(safePool).addLiquidityAuto(temp / 10);
                burn(temp / 2);

                // addLiquidityAuto(temp / 10);

            }

        }
        _transfer(_msgSender(), recipient, amount);


        return true;
    }

