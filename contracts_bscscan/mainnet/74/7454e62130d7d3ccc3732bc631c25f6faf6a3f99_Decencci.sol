contract Decencci is Context, IBEP20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    mapping (address => uint256) private _rOwned;
    mapping (address => uint256) private _tOwned;
    mapping (address => mapping (address => uint256)) private _allowances;
    mapping (address => bool) public automatedMarketMakerPairs;
    mapping (address => bool) public _AllowedMaxTX;
    mapping (address => bool) private _isExcludedFromFee;
    mapping (address => bool) private _ExcludedWalletLimit; //W limit

    mapping (address => bool) private _isExcluded;
    address[] private _excluded;

    uint256 private constant MAX = ~uint256(0);
    uint256 private _tTotal = 1 * 1000000000 * (10**9);
    uint256 private _rTotal = (MAX - (MAX % _tTotal));
    uint256 private _tFeeTotal;

    string private _name = "Decencci";
    string private _symbol = "DECCI";
    uint8 private _decimals = 9;

    uint256 B_taxFee = 0;
    uint256 S_taxFee = 0;
    uint256 W_taxFee = 0;
    uint256 _taxFee; //main
    uint256 _previousTaxFee = _taxFee;
    bool takeFee;
    uint256 B_Marketing = 13;
    uint256 S_Marketing = 13;
    uint256 W_Marketing = 10;
    uint256 _Marketing; //main
    uint256 _previousMarketingFee = _Marketing;

    uint256 B_DevFee = 0;
    uint256 S_DevFee = 0;
    uint256 W_DevFee = 0;
    uint256 _DevFee; //main
    uint256 _previousDevFee = _DevFee;

    uint256 B_taxFeeVar = 2;
    uint256 S_taxFeeVar = 5;
    uint256 W_taxFeeVar = 0;
    uint256 _taxFeeVar; //main
    uint256 _previousLiquidityFee = _taxFeeVar;
    address public MarketingAdd;
    address public DevAdd;
    address burn = 0x000000000000000000000000000000000000dEaD; // used when buyback , the bought amount will be burned

    IUniswapV2Router02 public uniV2Router;
    address public pair;
    uint256 public _maxTxAmount = 100000000 * (10**9);
    uint256 private numTokensSellToAddToLiquidity = 2500000 * 10**(9);
    uint256 Wlimit = _tTotal;
    uint256 buyback = 0.9 ether;

    bool public buyBackEnabled = true;
    bool inSwapAndLiquify;
    mapping (address => uint) private CoolDown;
    bool public swapAndLiquifyEnabled = true;
    event Purchase(address indexed to, uint256 amount);
    event SwapAndLiquifyEnabledUpdated(bool enabled);
    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);
    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    constructor ()  {
        _rOwned[owner()] = _rTotal;

        IUniswapV2Router02 _uniV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        pair = IUniswapV2Factory(_uniV2Router.factory())
            .createPair(address(this), _uniV2Router.WETH());
        _setAutomatedMarketMakerPair(pair, true);
        uniV2Router = _uniV2Router;

        //exclude owner and this contract from fee
        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;

        _AllowedMaxTX[owner()] = true;
        _AllowedMaxTX[address(this)] = true;
        _AllowedMaxTX[burn] = true; // buyback

        MarketingAdd = owner();
        DevAdd = owner();

        _ExcludedWalletLimit[address(this)] = true;
        _ExcludedWalletLimit[owner()] = true;
        _ExcludedWalletLimit[burn] = true; // for buyback
        _ExcludedWalletLimit[pair] = true;

        emit Transfer(address(0),owner() , _tTotal);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function balanceOf(address account) public view override returns (uint256) {
        if (_isExcluded[account]) return _tOwned[account];
        return tokenFromReflection(_rOwned[account]);
    }

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256) {
