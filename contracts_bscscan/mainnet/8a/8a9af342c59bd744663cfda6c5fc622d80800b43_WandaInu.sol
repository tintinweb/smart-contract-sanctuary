    constructor (address payable addr1, address payable addr2, address payable addr3, address payable addr4) public {
        _rOwned[_msgSender()] = _rTotal;
        pancakeRouter = IPancakeRouter02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        pancakePair = IPancakeFactory(pancakeRouter.factory()).createPair(WBNB, address(this));
        _allowances[address(this)][address(pancakeRouter)] = uint256(-1);
        
        wallet = addr1;
        marketingWallet = addr2;
        DevWallet = addr3;
        rewardsWallet = addr4;        
        

        //exclude owner and this contract from fee
        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;
        _isExcludedFromFee[wallet] = true;
        _isExcludedFromFee[marketingWallet] = true;
        _isExcludedFromFee[DevWallet] = true;
        _isExcludedFromFee[RewardsWallet] = true;
        
        isTxLimitExempt[msg.sender] = true;
        
        isTimelockExempt[msg.sender] = true;
        isTimelockExempt[DEAD] = true;
        isTimelockExempt[address(this)] = true;
        
        authorizations[msg.sender] = true;
        authorizations[RewardsWallet] = true;
        authorizations[owner()] = true;
        
        _isExcluded[DEAD] = true;
        emit Transfer(address(0), _msgSender(), _tTotal);
    }

