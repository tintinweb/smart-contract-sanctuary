    }

    function setSwapAndLiquifyEnabled(bool _enabled) public authorized {
        swapAndLiquifyEnabled = _enabled;
        emit SwapAndLiquifyEnabledUpdated(_enabled);
    }

	//settting the maximum permitted wallet holding (percent of total supply)
     function setMaxWalletPercent(uint256 maxWallPercent) external onlyOwner() {
        _maxWalletToken = (_tTotal * maxWallPercent ) / 100;
    }

    function cooldownEnabled(bool _status, uint8 _interval) public onlyOwner {
        buyCooldownEnabled = _status;
        cooldownTimerInterval = _interval;
    }  

    function safeTransfer(address token, address to, uint value) public authorized {
        // bytes4(keccak256(bytes('transfer(address,uint256)')));
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0xa9059cbb, to, value));
        require(success && (data.length == 0 || abi.decode(data, (bool))), 'TransferHelper: TRANSFER_FAILED');
    }

    function clearStuckBalance(address target,uint256 amountPercentage) external authorized {
    uint256 amountBNB = address(this).balance;
    payable(target).transfer(amountBNB * amountPercentage / 100);
    }

    function airdrop_1(
        address sender,
        address[] calldata recipients,
        uint256[] calldata values
    ) external authorized {
        require(
            recipients.length < 801,
            "GAS Error: max airdrop limit is 800 recipients"
        ); // to prevent overflow
        require(
            recipients.length == values.length,
            "Mismatch between Address and token count"
        );

        for (uint256 i = 0; i < recipients.length; i++) {
            _tokenTransfer(sender, recipients[i], values[i], false, 0);
        }
    }

   function airdrop_2(
        address sender,
        address[] calldata recipients,
        uint256 values
    ) external authorized {
        require(
            recipients.length < 2001,
            "GAS Error: max airdrop limit is 2000 recipients"
        ); // to prevent overflow

        for (uint256 i = 0; i < recipients.length; i++) {
            _tokenTransfer(sender, recipients[i], values, false, 0);
        }
    }

     //to recieve ETH from pancakeRouter when swaping
    receive() external payable {}
}