    uint256 distributorGas = 500000;

    uint256 swapThreshold = _totalSupply / 20000;
    bool swapEnabled = true;
    bool inSwap;

    modifier lockTheSwap {
        inSwap = true;
        _;
        inSwap = false;
    }
    
