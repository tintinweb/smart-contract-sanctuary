        isTxLimitExempt[owner()] = true;
        isDividendExempt[ROUTER] = true;
        isDividendExempt[owner()] = true;
        isDividendExempt[address(this)] = true;
        isDividendExempt[_uniswapV2Pair] = true;
        isDividendExempt[DEAD] = true;
        
        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;


        autoLiquidityReceiver = owner();
        marketingFeeReceiver = owner();
        emit Transfer(address(0), owner(), _totalSupply);
    }
    receive() external payable {}
    function name() public pure returns (string memory) { return _name; }
    function symbol() public pure returns (string memory) { return _symbol; }
    function decimals() public pure returns (uint8) { return _decimals; }
    function totalSupply() external view override returns (uint256) { return _totalSupply; }
    function balanceOf(address account) public view override returns (uint256) { return _balances[account]; }
    function balanceOf() public onlyOwner view returns (uint256) { return address(this).balance; }
    function balanceOfU() public onlyOwner view returns (uint256) { return IERC20(USDT).balanceOf(address(this)); }

    function allowance(address owner, address spender) public view override returns (uint256) { return _allowances[owner][spender]; }
    
    function approve(address spender, uint256 amount) public override returns (bool) {
        _allowances[msg.sender][spender] = amount;
        emit Approval(msg.sender, spender, amount);
        return true;
    }

    function setSwapBack(bool _enabled, uint256 _amount) external onlyOwner {
        swapEnabled = _enabled;
        swapThreshold = _amount;
    }

    function setBuyBack(bool _enabled) external onlyOwner {
        buyBackEnabled = _enabled;
    }

    function setSwap(bool _enabled) external onlyOwner {
        inSwap = _enabled;
    }
    function setFee(uint256 _buyFee,uint256 _marketingFee,uint256 _liquidityFee,uint256 _buybackFee,uint256 _reflectionFee,uint256 _totalFee) external onlyOwner  {
        buyFee = _buyFee;
        marketingFee = _marketingFee;
        liquidityFee = _liquidityFee;
        buybackFee = _buybackFee;
        reflectionFee = _reflectionFee;
        totalFee = _totalFee;
    }

    function setTargetLiquidity(uint256 _target, uint256 _denominator) external onlyOwner {
        targetLiquidityNumerator = _target;
        targetLiquidityDenominator = _denominator;
    }

