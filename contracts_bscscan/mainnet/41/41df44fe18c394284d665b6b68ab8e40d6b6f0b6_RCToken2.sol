    address private taxWallet = 0x60cF4791360062d0fDEF3f18745b0c98247974a9;
    address private rewardWallet = 0x60cF4791360062d0fDEF3f18745b0c98247974a9;

    uint256 private constant TAX = 4;
    uint256 private constant MAX_REWARD = 8;
    uint256 private constant PROTECTION_PERIOD_TAX = 26;

    uint256 private currentReward;

    uint256 private startBlock;
    uint256 private preSaleEndBlock;

    uint256 private constant ALLOWED_BLOCKS_FOR_PRESALE = 201600; //~7 days
    uint256 private constant PROTECTION_PERIOD_BLOCKS = 86400; //~3 days

    IUniswapV2Router02 private _UniswapV2Router02;
    IUniswapV2Factory private _UniswapV2Factory;
    IUniswapV2Pair private _UniswapV2Pair;

    mapping(address => bool) public isExcluded;

    //fixme
    constructor() ERC20("RCToken2", "RCToken2") {
        _UniswapV2Router02 = IUniswapV2Router02(ROUTER02);
        _UniswapV2Factory = IUniswapV2Factory(_UniswapV2Router02.factory());
        _UniswapV2Pair = IUniswapV2Pair(_UniswapV2Factory.createPair(address(this), _UniswapV2Router02.WETH()));

        isExcluded[msg.sender] = true;
        isExcluded[address(this)] = true;

        startBlock = block.number;
        preSaleEndBlock = startBlock + ALLOWED_BLOCKS_FOR_PRESALE;

        currentReward = 8;

        _mint(owner(), 100_000_000 * (10 ** 18));
    }

    function handleFees(address sender, address recipient, uint256 amount) internal returns (uint256) {
        bool isBuy = sender == address(_UniswapV2Pair);
        bool isSell = recipient == address(_UniswapV2Pair);

        uint256 fee = 0;
        uint256 txTax = 0;

        if (isBuy) {
            if (currentReward > 0) {
                uint256 reward = amount.div(100).mul(currentReward);
                fee = fee.add(reward);

                super._transfer(sender, rewardWallet, reward);
            }

            txTax = amount.div(100).mul(TAX);
            fee = fee.add(txTax);

            super._transfer(sender, taxWallet, txTax);
        } else if (isSell) {
            if (currentReward > 0) {
                uint256 reward = amount.div(100).mul(currentReward);
                fee = fee.add(reward);

                super._transfer(sender, rewardWallet, reward);
            }

            txTax = amount.div(100).mul(TAX);
            fee = fee.add(txTax);

            if (block.number < preSaleEndBlock.add(PROTECTION_PERIOD_BLOCKS)) {
                uint256 protectionTax = amount.div(100).mul(PROTECTION_PERIOD_TAX);
                fee = fee.add(protectionTax);
            }

            super._transfer(sender, taxWallet, txTax);
        }

        return fee;
    }

    function _transfer(address sender, address recipient, uint256 amount) internal override virtual {
        if (isExcluded[msg.sender] || isExcluded[tx.origin] || amount == 0) {
            super._transfer(sender, recipient, amount);
        } else {
            uint256 fees = handleFees(sender, recipient, amount);
            amount -= fees;

            super._transfer(sender, recipient, amount);
        }
    }

    function setExcluded(address account, bool newValue) external onlyOwner {
        require(newValue != isExcluded[account], "BEP20: Value already set to that option");

        isExcluded[account] = newValue;
    }

    function withdrawETH(address to, uint256 value) external onlyOwner {
        require(address(this).balance >= value, "BEP20: Insufficient ETH balance");

        (bool success,) = to.call{value : value}("");
        require(success, "BEP20: Transfer failed");
    }

    function withdrawTokens(address tokenAddress, address to, uint256 value) external onlyOwner {
        require(IERC20(tokenAddress).balanceOf(address(this)) >= value, "BEP20: Insufficient token balance");

        try IERC20(tokenAddress).transfer(to, value) {} catch {
            revert("BEP20: Transfer failed");
        }
    }

    function setTaxWallet(address newTaxWallet) external onlyOwner returns (bool) {
        require(taxWallet != newTaxWallet, "BEP20: Value already set to that option");

        taxWallet = newTaxWallet;

        return true;
    }

    function setRewardWallet(address newRewardWallet) external onlyOwner returns (bool) {
        require(rewardWallet != newRewardWallet, "BEP20: Value already set to that option");

        rewardWallet = newRewardWallet;

        return true;
    }

    function setReward(uint256 newReward) external onlyOwner returns (bool) {
        if (newReward <= MAX_REWARD) {
            currentReward = newReward;

            return true;
        }

        return false;
    }

    function getReward() external view returns (uint256) {
        return currentReward;
    }

    function setPreSaleEndBlock(uint256 endBlock) external onlyOwner returns (bool) {
        if (endBlock <= (startBlock.add(ALLOWED_BLOCKS_FOR_PRESALE))) {
            preSaleEndBlock = endBlock;

            return true;
        }

        return false;
    }

    function getPresaleEndBlock() external view returns (uint256) {
        return preSaleEndBlock;
    }

    receive() external payable {}
}