interface INFT {
    function transferFrom(address from_, address to_, uint256 tokenId_) external;
    function totalShares() view external returns (uint256);
    function balanceOf(address owner) external view returns (uint256 balance);
    function shareOf(uint256 _tokenId) external view returns (uint256);
}

contract StratNFT is OwnableUpgradeable, ReentrancyGuardUpgradeable, PausableUpgradeable {
    // Maximises yields in pancakeswap

    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    event SetGov(address _govAddress);
    event SetOnlyGov(bool _onlyGov);

    // Used to record part of the user's NFT data
    struct NftOwner {
        address addr;                   // user's address
        uint64  index;                  //  _stakingMomoArray index of the array
    }

    INFT public NFT;
    address public autoFarmAddress;
    address public govAddress; // timelock contract
    bool public onlyGov;// = true;

    uint256 public totalHashrate;

    // ERC721(V4-V6) NFT, user address => ERC721 tokenId Array
    mapping (address => uint256[]) _stakingMomoArray;
    // NFT owner's info, ERC721 tokend => NftOwner        
    mapping (uint256 => NftOwner) _momoIdToOwnerAndIndex;

    modifier onlyAllowGov() {
        require(msg.sender == govAddress, "!gov");
        _;
    }

    function sharesTotal() view external returns (uint256){
        // if(totalHashrate==0) return 0;
        // return NFT.totalShares(); // fixed value
        return totalHashrate;
    }

    function wantLockedTotal() view external returns (uint256){
        //return totalHashrate;
        return NFT.balanceOf(address(this));
    }

    function initialize(
        address _nftAddress,
        address _autoFarmAddress,
        address _govAddress
    ) public initializer {
        __Ownable_init();
        __ReentrancyGuard_init();
        __Pausable_init();

        NFT = INFT(_nftAddress);
        autoFarmAddress = _autoFarmAddress;
        govAddress = _govAddress;
        onlyGov = true;

        transferOwnership(autoFarmAddress);
    }

    function getStaking(address _userAddress) external view returns (uint256[] memory) {
        return _stakingMomoArray[_userAddress];
    }
    
    // Receives new deposits from user
    function deposit(address _userAddress, uint256[] memory _tokenIds)
        public
        virtual
        onlyOwner
        nonReentrant
        whenNotPaused
        returns (uint256)
    {
        uint256[] storage momos = _stakingMomoArray[_userAddress];
        uint256 hashrateFixed;
        for(uint i = 0; i < _tokenIds.length; i++)
        {
            NFT.transferFrom(msg.sender, address(this), _tokenIds[i]);
            hashrateFixed = hashrateFixed.add(NFT.shareOf(_tokenIds[i]));

            _momoIdToOwnerAndIndex[_tokenIds[i]] = NftOwner(_userAddress, SafeMathExt.safe64(momos.length));
            momos.push(_tokenIds[i]);
        }
        totalHashrate = totalHashrate.add(hashrateFixed);

        return hashrateFixed;

    }


    function withdraw(address _userAddress, uint256[] memory _tokenIds)
        public
        virtual
        onlyOwner
        nonReentrant
        returns (uint256)
    {
        uint256[] storage momos = _stakingMomoArray[_userAddress];
        uint256 hashrateFixed;
        for(uint i =0; i< _tokenIds.length;i++)
        {
            NftOwner storage nfto = _momoIdToOwnerAndIndex[_tokenIds[i]];
            require(_userAddress == address(nfto.addr), "invalid 721");

            hashrateFixed = hashrateFixed.add(NFT.shareOf(_tokenIds[i]));

            if (uint256(nfto.index) != momos.length.sub(1)) {
                momos[uint256(nfto.index)] = momos[momos.length.sub(1)]; 
                _momoIdToOwnerAndIndex[momos[uint256(nfto.index)]].index = nfto.index;
            }
            momos.pop();
            
            delete _momoIdToOwnerAndIndex[_tokenIds[i]];
            
            NFT.transferFrom(address(this), msg.sender, _tokenIds[i]);
        }

        totalHashrate = totalHashrate.sub(hashrateFixed);

        return hashrateFixed;

    }

    function setGov(address _govAddress) public virtual onlyAllowGov {
        govAddress = _govAddress;
        emit SetGov(_govAddress);
    }

    function setOnlyGov(bool _onlyGov) public virtual onlyAllowGov {
        onlyGov = _onlyGov;
        emit SetOnlyGov(_onlyGov);
    }

    function pause() public virtual onlyAllowGov {
        _pause();
    }

    function unpause() public virtual onlyAllowGov {
        _unpause();
    }

    function onERC721Received(address operator, address from, uint256 tokenId, bytes calldata data) external returns (bytes4){
        return 0x150b7a02;
    }

}

