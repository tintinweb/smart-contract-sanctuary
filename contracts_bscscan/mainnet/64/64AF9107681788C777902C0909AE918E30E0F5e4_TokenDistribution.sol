contract TokenDistribution is Ownable {
    using SafeMath for uint256;

    address payable public mainWallet;
    uint256 public arrayLimit;
    uint256 public fee1;
    uint256 public fee2;
    
    struct userInfo{
        address userAddr;
        address tokenAddr;
        uint256 tokenAmount;
    }
    mapping(address => userInfo[]) public userList;
    mapping(address => address[]) public tokenOwner;
    
    mapping(address => uint256) private vipLevel;
    mapping(address => uint256) private vipStartDate;
    
    event Multisended(uint256 total, address tokenAddress);
    event releasedTokens(address token, address owner, uint256 balance);

    receive() external payable {}
    
    constructor() public {
        mainWallet = 0xB425dc48b4cac24ef67a2feAE6d648a9A2b3A9fb;
        setArrayLimit(1000);
        setFee1(0.2 ether);
        setFee2(0.4 ether);
    }

    function setArrayLimit(uint256 _newLimit) public onlyOwner {
        require(_newLimit != 0);
        arrayLimit = _newLimit;
    }

    function setFee1(uint256 _newStep) public onlyOwner {
        require(_newStep != 0);
        fee1 = _newStep;
    }
    
    function setFee2(uint256 _newStep) public onlyOwner {
        require(_newStep != 0);
        fee2 = _newStep;
    }
    
    function setVipLevel() public payable {
        require(msg.value >= 1 * 1e18, "Unsuitable Amount");
        
        if (msg.value < 2 * 1e18) {
            vipLevel[msg.sender] = 1;
        } else if (msg.value < 3 * 1e18) {
            vipLevel[msg.sender] = 2;
        } else if (msg.value < 6 * 1e18) {
            vipLevel[msg.sender] = 3;
        } else {
            vipLevel[msg.sender] = 4;
        }
        
        vipStartDate[msg.sender] = now;
    }
    
    function vipLevelOfCustomer(address customer) public view returns(uint) {
        uint dateNow = now;
        uint expireDays = BokkyPooBahsDateTimeLibrary.diffDays(vipStartDate[customer], dateNow);
        if (vipLevel[msg.sender] == 1 && expireDays < 1) {
            return 1;
        } else if (vipLevel[msg.sender] == 2 && expireDays < 3) {
            return 2;
        } else if (vipLevel[msg.sender] == 3 && expireDays < 7) {
            return 3;
        } else if (vipLevel[msg.sender] == 4) {
            return 4;
        }
        return 0;
    }


    function uploadList(address tokenAddr, address payable[] memory _contributors, uint256[] memory _balances) public payable {
        uint customLev = vipLevelOfCustomer(msg.sender);
        if (customLev <= 0) {
            if (_contributors.length <= 500) {
                require(msg.value >= fee1, "Unsuitable Fee For Uploading List");
            } else {
                require(msg.value >= fee2, "Unsuitable Fee For Uploading List");
            }
        }
        
        require(_contributors.length <= arrayLimit);
        uint8 i = 0;
        for (i; i < _contributors.length; i++) {
            userList[msg.sender].push(userInfo(_contributors[i], tokenAddr, _balances[i]));
        }
        tokenOwner[tokenAddr].push(msg.sender);
    }
    
    function claimTokens(address tokenAddr) public {
        ERC20 erc20token = ERC20(tokenAddr);
        for (uint i = 0; i < tokenOwner[tokenAddr].length; i++) {
            address owner = tokenOwner[tokenAddr][i];
            for (uint j = 0; j < userList[owner].length; j++) {
                if (userList[owner][j].tokenAddr == tokenAddr && userList[owner][j].userAddr == msg.sender) {
                    erc20token.transferFrom(owner, msg.sender, userList[owner][j].tokenAmount);
                }
            }
        }
    }
    
    function getClaimable(address tokenAddr) public view returns(uint256) {
        uint256 count = 0;
        for (uint i = 0; i < tokenOwner[tokenAddr].length; i++) {
            address owner = tokenOwner[tokenAddr][i];
            for (uint j = 0; j < userList[owner].length; j++) {
                if (userList[owner][j].tokenAddr == tokenAddr && userList[owner][j].userAddr == msg.sender) {
                    count = count + userList[owner][j].tokenAmount;
                }
            }
        }
        return count;
    }
    
    function getTokenAddrByUser(address owner) public view returns(address[] memory) {
        address[] memory tokenList = new address[](userList[owner].length);
        uint count = 0;
        for (uint i = 0; i < userList[owner].length; i++) {
            bool already = false;
            for (uint j = 0; j <= tokenList.length - 1; j++) {
                if (userList[owner][i].tokenAddr == tokenList[j]) {
                  already = true;
                  break;
                }
            }
            if (already == false) {
                tokenList[count] = userList[owner][i].tokenAddr;
                count = count + 1;
            }
        }
        
        address[] memory retList = new address[](count);
        for (uint i = 0; i < count; i++) {
            retList[i] = tokenList[i];
        }
        
        return retList;
    }
    
    function releaseFunds() external onlyOwner {
        mainWallet.transfer(address(this).balance);
        return;
    }

    function releaseTokens(address _token) public onlyOwner {
        ERC20 erc20token = ERC20(_token);
        uint256 balance = erc20token.balanceOf(address(this));
        erc20token.transfer(mainWallet, balance);
        releasedTokens(_token, mainWallet, balance);
    }
    
}