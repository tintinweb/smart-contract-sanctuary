contract FryingDutchManLandingBestBusd is Ownable, ReentrancyGuard, FryingDutchMan {
    using SafeToken for address;

    IUniswapV2Factory public factory;
    IPancakeRouter02 public router;
    address public busd = 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56;
    address public wbnb;
    INarBestRateQueryV2 public immutable narBestQuery;
    INarBestRateSwap public immutable narBestSwap;
    address public constant wbnbNar = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;
    uint256 [] public routerIndex = [0,1,32,33,35,36,37];
   
    /// @dev Create a new liquidate strategy instance.
    /// @param _router The Uniswap router smart contract.
    constructor(IPancakeRouter02 _router,
        INarBestRateQueryV2 _narBestQuery,
        INarBestRateSwap _narBestSwap
    ) public {
        factory = IUniswapV2Factory(_router.factory());
        router = _router;
        wbnb = _router.WETH();
        narBestQuery = _narBestQuery;
        narBestSwap = _narBestSwap;
        busd.safeApprove(address(router), uint256(-1)); // 100% trust in the router
        busd.safeApprove(address(_narBestSwap), uint256(-1));
    }

    function execute(address paidtoken , address wantToken, address, uint256, bool) 
        external
        override
        payable
        nonReentrant
    {
        IUniswapV2Pair lpToken = IUniswapV2Pair(wantToken);
        address fToken = lpToken.token0() == busd ? lpToken.token1() : lpToken.token0();
        // 1. Remove all liquidity back to BUSD and farming tokens.
        lpToken.approve(address(router), uint256(-1));
        router.removeLiquidity(busd , fToken, lpToken.balanceOf(address(this)), 0, 0, address(this), now);
        // 2. Convert farming tokens to BUSD.
        address[] memory path = new address[](2);
        path[0] = fToken;
        path[1] = busd;
        fToken.safeApprove(address(router), 0);
        fToken.safeApprove(address(router), uint256(-1));
        router.swapExactTokensForTokens(fToken.myBalance(), 0, path, address(this), now);
        // 3. Return all back to the original caller.
        if (paidtoken == busd) {
            busd.safeTransfer(msg.sender, busd.balanceOf(address(this)));
        }
        else if (paidtoken == address(0x0)) {
            //paid == BNB            
            (uint256 route,)  = narBestQuery.oneRoute(busd,wbnbNar,busd.myBalance(),routerIndex);
            narBestSwap.trade(route,busd,busd.myBalance(),wbnbNar,0,0);
            uint256 balance = address(this).balance;
            SafeToken.safeTransferBNB(msg.sender, balance);
        }
        else {
            //busd to paidtoken
            (uint256 route,)  = narBestQuery.oneRoute(busd,paidtoken,busd.myBalance(),routerIndex);
            narBestSwap.trade(route,busd,busd.myBalance(),paidtoken,0,0);
            paidtoken.safeTransfer(msg.sender, paidtoken.balanceOf(address(this)));
        }
       
    }

    /// @dev Recover ERC20 tokens that were accidentally sent to this smart contract.
    /// @param token The token contract. Can be anything. This contract should not hold ERC20 tokens.
    /// @param to The address to send the tokens to.
    /// @param value The number of tokens to transfer to `to`.
    function recover(address token, address to, uint256 value) external onlyOwner nonReentrant {
        token.safeTransfer(to, value);
    }

    /// @dev modifyRoute for BestRate
    /// @param newRoutes NewRoutes.
    function modifyRoute(uint256 [] memory newRoutes) external onlyOwner nonReentrant {
        routerIndex = newRoutes;
    }

    function inCaseBNBGetStuck(uint256 _amount)
        external
        onlyOwner
    {
        SafeToken.safeTransferBNB(msg.sender, _amount);
    }

    receive() external payable {}
}