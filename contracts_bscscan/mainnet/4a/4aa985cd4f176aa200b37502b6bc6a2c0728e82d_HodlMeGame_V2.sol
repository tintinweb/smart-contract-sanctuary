// File contracts/HodlMeGame_V2.sol

// Unlicensed

pragma solidity ^0.8.0;




contract HodlMeGame_V2 is Initializable, OwnableUpgradeable {
    struct Game {
        uint256 id;
        address player1;
        address player2;
        uint256 gameState;
        uint256 amount;
        uint256 duration;
        uint256 creationTime;
        uint256 lastModifiedTime;
        uint256 startTime;
        uint256 endTime;
        uint256 player1InitialBalance;
        uint256 player2InitialBalance;
        address winner;
    }

    mapping(uint256 => Game) public games;
    mapping(address => uint256) public userGamesCount;
    mapping(address => mapping(uint256 => uint256)) public userGames;
    address public gameToken;
    uint256 public manualGameEndingFeePercentage;

    uint256 private constant GAME_CREATED = 1;
    uint256 private constant GAME_STARTED = 2;
    uint256 private constant GAME_ENDED = 3;
    uint256 private constant GAME_MINIMUM_PLAY_TIME_IN_SECONDS = 300;
    uint256 private gameIdCount;

    event GameCreated(
        address indexed player,
        uint256 indexed amount,
        uint256 gameId
    );

    event GameStarted(
        address indexed player1,
        address indexed player2,
        uint256 indexed amount,
        uint256 gameId
    );

    event GameEnded(
        address indexed winner,
        uint256 indexed amount,
        uint256 gameId
    );

    event LeftGame(
        address indexed player,
        uint256 indexed amount,
        uint256 gameId
    );

    modifier onlyGameToken() {
        require(
            gameToken == _msgSender(),
            "ERR_HODLME_TOKEN_ONLY"
        );
        _;
    }

    function initialize(address _gameToken) public initializer {
        __Ownable_init();
        gameToken = _gameToken;
        gameIdCount = 1;
        manualGameEndingFeePercentage = 1;
    }

    // create new game
    function newGame(uint256 _amount, uint256 _duration) public {
        require(_duration >= GAME_MINIMUM_PLAY_TIME_IN_SECONDS, "ERR_GAME_MINIMUM_TIME");
        require(_amount > 0, "ERR_GAME_AMOUNT_TOO_LOW");

        // increase game count
        uint256 gameId = gameIdCount++;

        // create and initialize new game
        Game storage game = games[gameId];
        game.id = gameId;
        game.player1 = _msgSender();
        game.amount = _amount;
        game.gameState = GAME_CREATED;
        game.creationTime = block.timestamp;
        game.lastModifiedTime = block.timestamp;
        game.duration = _duration;

        // update user personal games information
        userGames[_msgSender()][userGamesCount[_msgSender()]] = gameId;
        userGamesCount[_msgSender()]++;

        emit GameCreated(_msgSender(), _amount, gameId);

        // transfer tokens from sender into this contract
        IERC20(gameToken).transferFrom(_msgSender(), address(this), _amount);
    }

    // join game if the game has not started yet
    function joinGame(uint256 gameId) public {
        Game storage game = games[gameId];
        require(
            game.gameState == GAME_CREATED,
            "ERR_GAME_STATE_INVALID"
        );
        // game might be already started / ended
        require(
            _msgSender() != game.player1,
            "ERR_GAME_PLAYER_INVALID"
        );
        // sender is also the creator of the game

        // update game details
        game.player2 = _msgSender();
        game.gameState = GAME_STARTED;
        game.startTime = block.timestamp;
        game.endTime = block.timestamp + game.duration;
        game.lastModifiedTime = block.timestamp;
        game.player1InitialBalance = IERC20(gameToken).balanceOf(game.player1);
        game.player2InitialBalance = IERC20(gameToken).balanceOf(_msgSender()) - game.amount;

        // update user personal games information
        userGames[_msgSender()][userGamesCount[_msgSender()]] = gameId;
        userGamesCount[_msgSender()]++;

        emit GameStarted(game.player1, _msgSender(), game.amount, gameId);

        // transfer tokens from sender into this contract
        IERC20(gameToken).transferFrom(
            _msgSender(),
            address(this),
            game.amount
        );
    }

    // Owner of the game can leave game before anyone is joining
    function leaveGame(uint256 gameId) public {
        Game storage game = games[gameId];
        require(
            game.player1 == _msgSender(),
            "ERR_PLAYER1_NOT_OWNER"
        );
        require(
            game.gameState == GAME_CREATED,
            "ERR_GAME_STATE_INVALID"
        );
        require(
            block.timestamp - game.creationTime >= GAME_MINIMUM_PLAY_TIME_IN_SECONDS,
            "ERR_GAME_LEAVE_TOO_EARLY"
        );

        // update game state
        game.gameState = GAME_ENDED;
        game.lastModifiedTime = block.timestamp;

        emit LeftGame(_msgSender(), game.amount, gameId);

        // send tokens back to player
        IERC20(gameToken).transfer(_msgSender(), game.amount);
    }

    // Anyone can end a game manually if ending time has passed
    function endGameManually(uint gameId) public {
        Game storage game = games[gameId];

        require(game.gameState == GAME_STARTED, "ERR_GAME_STATE_INVALID");
        require(game.endTime < block.timestamp, "ERR_GAME_END_TOO_EARLY");

        // check user scores and decide the winner
        uint256 player1Score = 0;
        uint256 player2Score = 0;
        if (IERC20(gameToken).balanceOf(game.player1) > game.player1InitialBalance) {
            player1Score = IERC20(gameToken).balanceOf(game.player1) - game.player1InitialBalance;
        }

        if (IERC20(gameToken).balanceOf(game.player2) > game.player2InitialBalance) {
            player2Score = IERC20(gameToken).balanceOf(game.player2) - game.player2InitialBalance;
        }

        bool negative = false;
        if (player1Score == 0 && player2Score == 0) {
            negative = true;
            player1Score = game.player1InitialBalance - IERC20(gameToken).balanceOf(game.player1);
            player2Score = game.player2InitialBalance - IERC20(gameToken).balanceOf(game.player2);
        }

        require(player1Score != player2Score, "ERR_GAME_EQUALITY");

        if (negative == false) {
            if (player1Score > player2Score) {
                game.winner = game.player1;
            } else {
                game.winner = game.player2;
            }
        } else {
            if (player2Score > player1Score) {
                game.winner = game.player1;
            } else {
                game.winner = game.player2;
            }
        }

        // update game state
        game.gameState = GAME_ENDED;
        game.lastModifiedTime = block.timestamp;

        emit GameEnded(game.winner, game.amount * 2, gameId);

        // ending game fee is considered incentive for anyone who ends the game manually imediatly after endtime period
        uint256 endingFee = game.amount * 2 * manualGameEndingFeePercentage / 100;
        IERC20(gameToken).transfer(game.winner, game.amount * 2 - endingFee);
        IERC20(gameToken).transfer(_msgSender(), endingFee);
    }

    // If a token holder sells tokens, HODLME token contract will call this function and all seller games will be ended as loser
    function endUserGames(address loser) external onlyGameToken {
        for (uint256 i = 0; i < userGamesCount[loser]; i++) {
            uint256 gameId = userGames[loser][i];
            Game storage game = games[gameId];

            // end any started game of the loser
            if (game.gameState == GAME_STARTED) {
                game.gameState = GAME_ENDED;
                game.lastModifiedTime = block.timestamp;
                uint256 loserBalance = 0;
                if (loser == game.player1) {
                    loserBalance = IERC20(gameToken).balanceOf(game.player1);
                } else {
                    loserBalance = IERC20(gameToken).balanceOf(game.player2);
                }

                emit GameEnded(game.winner, game.amount * 2, gameId);

                IERC20(gameToken).transfer(game.winner, game.amount * 2);
            }
        }
    }

    // total number of games ever created
    function gamesCount() public view returns (uint256) {
        return gameIdCount - 1;
    }

    function setManualEndingFee(uint256 _percentage) public onlyOwner {
        require(_percentage < 30, "ERR_PERCENTAGE_TOO_HIGH");
        manualGameEndingFeePercentage = _percentage;
    }
}