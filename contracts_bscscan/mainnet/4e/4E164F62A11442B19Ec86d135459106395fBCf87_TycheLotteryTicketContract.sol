pragma solidity 0.8.6;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import './TycheMinterContract.sol';

contract TycheERC721Contract is Ownable, ERC721, TycheMinterContract {
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.UintSet;

    Counters.Counter private _currentAssetId;
    mapping(uint256 => address) private _assetMinters;
    mapping(address => EnumerableSet.UintSet) private _ownerAssets;
    
    constructor(string memory name_, string memory symbol_) ERC721(name_, symbol_) {
    }
    
    function getOwnerAssets(address owner) public view returns (uint256[] memory) {
        uint256 _total = _ownerAssets[owner].length();
        uint256[] memory _result = new uint256[](_total);
        for (uint256 i = 0; i < _total; i++) {
            uint256 id = _ownerAssets[owner].at(i);
            _result[i] = id;
        }
        return _result;
    }
    
    function getAssetMinter(uint256 assetId) public view returns (address) {
        return _assetMinters[assetId];
    }     
    
    /**
     * @dev See {IERC721-transferFrom}.
     */
    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override {
        //solhint-disable-next-line max-line-length
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");

        _transfer(from, to, tokenId);
        
        _ownerAssets[from].remove(tokenId);
        _ownerAssets[to].add(tokenId);
    }  
    
    /**
     * @dev See {IERC721-safeTransferFrom}.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) public virtual override {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: transfer caller is not owner nor approved");
        _safeTransfer(from, to, tokenId, _data);
        
        _ownerAssets[from].remove(tokenId);
        _ownerAssets[to].add(tokenId);        
    }
    
    function _mintAsset(address owner) internal onlyMinter returns (uint256) {
        require(owner != address(0), "TycheERC721Contract: Owner must not be a zero address");

        _currentAssetId.increment();
        uint256 newId = _currentAssetId.current();
        _mint(owner, newId);
        
        _ownerAssets[owner].add(newId);
        _assetMinters[newId] = _msgSender();
        return newId;
    }
    
    modifier onlyAssetMinter(uint256 assetId) {
        require(_assetMinters[assetId] == _msgSender(), "TycheERC721Contract: caller is not the asset minter");
        _;
    }      
        
}

