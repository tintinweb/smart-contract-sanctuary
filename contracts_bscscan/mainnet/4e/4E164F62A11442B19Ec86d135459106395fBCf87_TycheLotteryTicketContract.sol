pragma solidity 0.8.6;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

library LotteryStruct {
    enum LotteryStatus {
        START,
        CLOSED,
        CONCLUDED
    }
    struct Lottery {
        uint256 lotteryId;
        uint256 beginDate;
        uint256 endDate;
        uint256 number; // the winning number
        address fundCurrency;
        uint256 fund;
        address ticketCurrency;
        uint256 ticketPrice;
        uint256 ticketFund; // profit from selling tickets
        uint256 totalCommissionAmount; // total commission for all vendors
        uint256 totalReward; // total rewards need to be paid. It always <= fund
        uint256 totalNumbers; //the maximum number of selectable combinations begin with 01 => 99
        uint256 totalRandomNumbers; //the maximum number of combinations for the winning tickets. Always = the highest reward (jackpot) and must less than or equal totalNumbers
        LotteryStatus status;
    }
    
    struct Ticket {
        uint256 ticketId;
        uint256 lotteryId;
        uint256 number;
        bool isClaim;
        uint256 reward;
    }

    struct LotteryMatchingResult {
        uint256 matchedGroups;
        uint256 totalTickets;
    }
    
    struct RewardCondition {
        uint256 matchedGroups;
        uint256 reward;
    }
    
    struct CommissionCondition {
        uint256 totalTickets;
        uint256 percentCommission;
    }
    
    function getMatchedGroups(uint256 guessNumber, uint256 winningNumber, uint256 totalNumbers) internal pure returns(uint256) {
        uint256 intersection = guessNumber & winningNumber;
        if(intersection == 0) {
            return 0;
        }
        uint256 result = 0;
        for (uint256 i = 1; i <= totalNumbers; i++) {
            uint256 powerOfTwo = 2 ** i;
            if((powerOfTwo & intersection) == powerOfTwo){
                result += 1;
            }
        } 

        return result;
    }    
    
    function calculateFundByRewardConditions(LotteryStruct.RewardCondition[] memory rewardConditions) internal pure returns(uint256){
        uint256 fund = 0;
        uint256 total = rewardConditions.length;
        for (uint256 i = 0; i < total; i++) {
            fund += rewardConditions[i].reward;
        }        
        
        return fund;
    }  
    
    function ceil(uint256 a, uint256 m) internal pure returns (uint256) {
        uint256 c = SafeMath.add(a,m);
        uint256 d = SafeMath.sub(c,1);
        return SafeMath.mul(SafeMath.div(d,m),m);
    }        
    
    /**
     * Percent have to mul with 100
     * Example: 1% = 100, 0.1% = 10, 0.01% = 1
     */    
    function calculatePercent(uint256 _value, uint256 percent) internal pure returns (uint256)  {
        uint256 BASEPERCENT = 100;
        uint256 roundValue = ceil(_value, BASEPERCENT);
        uint256 mulRoundValue = SafeMath.mul(roundValue, BASEPERCENT);
        uint256 result = SafeMath.div(SafeMath.mul(mulRoundValue, percent), 1000000);
        return result;
    }
}

