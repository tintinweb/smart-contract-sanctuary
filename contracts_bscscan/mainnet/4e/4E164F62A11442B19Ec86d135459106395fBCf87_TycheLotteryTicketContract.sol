pragma solidity 0.8.6;

import "./TycheERC721Contract.sol";
import { LotteryStruct } from './TycheLotteryLibrary.sol';

import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

contract TycheLotteryTicketContract is TycheERC721Contract {

    using EnumerableSet for EnumerableSet.UintSet;

    mapping(uint256 => LotteryStruct.Ticket) private _tickets;
    mapping(uint256 => EnumerableSet.UintSet) private _lotteryTickets;
    
    constructor() TycheERC721Contract("TycheLotteryTicket", "TYLT") {}
    
    function createTicket(address owner, uint256 lotteryId, uint256 number) public onlyMinter returns (uint256) {
        uint256 _newId = _mintAsset(owner);
        LotteryStruct.Ticket memory newTicket = LotteryStruct.Ticket({
            ticketId: _newId,
            lotteryId: lotteryId,
            number: number,
            isClaim: false,
            reward: 0
        });
        
        _tickets[_newId] = newTicket;
        _lotteryTickets[lotteryId].add(_newId);

        return _newId;
    }
    
    function claimTicket(uint256 assetId) public onlyAssetMinter(assetId) returns (address, uint256) {
        
        require(isTicketMatched(assetId), "TycheLotteryTicketContract: Ticket is not matched or lottery has not been concluded");
        require(isTicketClaimed(assetId) == false, "TycheLotteryTicketContract:Ticket was claimed");
        
        address owner = getTicketOwner(assetId);
        uint256 reward = _tickets[assetId].reward;

        _tickets[assetId].isClaim = true;
        
        return (owner, reward);
    }    
    
    function setTicketReward(uint256 assetId, uint256 reward) public onlyAssetMinter(assetId) {
         _tickets[assetId].reward = reward;
    }
    
    function getTicket(uint ticketId) public view returns (LotteryStruct.Ticket memory){
        return _tickets[ticketId];
    }
    
    function getOwnerTickets(address owner) public view returns (LotteryStruct.Ticket[] memory) {
        uint256[] memory ticketIds = getOwnerAssets(owner);
        uint256 _total = ticketIds.length;
        LotteryStruct.Ticket[] memory _result = new LotteryStruct.Ticket[](_total);
        for (uint256 i = 0; i < _total; i++) {
            uint256 id = ticketIds[i];
            _result[i] = _tickets[id];
        }
        return _result;
    }
    
    function getLotteryTickets(uint lotteryId) public view returns (LotteryStruct.Ticket[] memory) {
        uint256 _total = _lotteryTickets[lotteryId].length();
        LotteryStruct.Ticket[] memory _result = new LotteryStruct.Ticket[](_total);
        for (uint256 i = 0; i < _total; i++) {
            uint256 id = _lotteryTickets[lotteryId].at(i);
            _result[i] = _tickets[id];
        }
        return _result;
    }
    
    function getTicketOwner(uint256 ticketId) public view returns(address){
        return ownerOf(ticketId);
    }
    
    function isTicketClaimed(uint256 ticketId) public view returns(bool){
        return _tickets[ticketId].isClaim;
    }
    
    function isTicketMatched(uint256 ticketId) public view returns(bool){
        return _tickets[ticketId].reward > 0;
    }
}

