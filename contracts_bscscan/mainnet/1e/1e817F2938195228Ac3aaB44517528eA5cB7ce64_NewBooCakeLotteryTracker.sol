  /// @dev Internal function that mints tokens to an account.
  /// Update magnifiedDividendCorrections to keep dividends unchanged.
  /// @param account The account that will receive the created tokens.
  /// @param value The amount that will be created.
  function _mint(address account, uint256 value) internal override {
    super._mint(account, value);

    magnifiedDividendCorrections[account] = magnifiedDividendCorrections[account]
      .sub( (magnifiedDividendPerShare.mul(value)).toInt256Safe() );
  }

  /// @dev Internal function that burns an amount of the token of a given account.
  /// Update magnifiedDividendCorrections to keep dividends unchanged.
  /// @param account The account whose tokens will be burnt.
  /// @param value The amount that will be burnt.
  function _burn(address account, uint256 value) internal override {
    super._burn(account, value);

    magnifiedDividendCorrections[account] = magnifiedDividendCorrections[account]
      .add( (magnifiedDividendPerShare.mul(value)).toInt256Safe() );
  }

  function _setBalance(address account, uint256 newBalance) internal {
    uint256 currentBalance = balanceOf(account);

    if(newBalance > currentBalance) {
      uint256 mintAmount = newBalance.sub(currentBalance);
      _mint(account, mintAmount);
    } else if(newBalance < currentBalance) {
      uint256 burnAmount = currentBalance.sub(newBalance);
      _burn(account, burnAmount);
    }
  }
}

contract NewBooCakeLotteryTracker is Ownable {
    using SafeMath for uint256;
    using IterableMapping for IterableMapping.Map;

    struct Winner {
        uint256 winningTicket;
        address winner;
        uint256 prize;
        bool claimed;
    }
    
    address public immutable CAKE = address(0x0E09FaBB73Bd3Ade0a17ECC321fD13a19e81cE82);

    uint256 public tokensPerTicket = 5 * (10**5) * (10**18);
    IterableMapping.Map private participantsMap;
    mapping(address => bool) public excludedFromLottery;

    uint256 public totalParticipations;
    uint256 public winningTicket;
    address public winner = address(0);
    Winner public mainWinner;
    Winner public subWinner1;
    Winner public subWinner2;
    Winner public subWinner3;
    Winner public subWinner4;
    Winner public subWinner5;
    Winner public subWinner6;
    Winner public subWinner7;
    Winner public subWinner8;
    Winner public subWinner9;
    Winner public subWinner10;
    uint256 winnersFound = 0;
    bool public stopTicketsIssuing;
    uint256 public prizeTransferred;
    uint256 public counter;

    bool public winnerInProgress;
    uint256 private partialRes;
    uint256 private currentIndex;


    constructor() public {
        counter = block.timestamp;
    }

    function stopTicketing(bool newValue) external onlyOwner {
        stopTicketsIssuing = newValue;
    }

    function isClaimed() external view returns (bool) {
        return prizeTransferred != 0;
    }

    function setBalance(address account, uint256 newBalance) external onlyOwner {
        require (!stopTicketsIssuing);
        require (!excludedFromLottery[account]);

        uint256 newParticipations = newBalance.div(tokensPerTicket);
        uint256 oldParticipations = participantsMap.get(account);

        if (newParticipations > oldParticipations) {
            // It is receiving funds
            uint256 addedParticipations = newParticipations.sub(oldParticipations);
            totalParticipations = totalParticipations.add(addedParticipations);
            participantsMap.set(account, newParticipations);
        }
        else if (newParticipations < oldParticipations) {
            // It is moving funds out
            totalParticipations = totalParticipations.sub(oldParticipations);
            participantsMap.remove(account);
        }
    }

    function calculateWinningTicket() external onlyOwner {
        uint256 accPrize = accumulatedPrice();

        winningTicket = getRandom();
        mainWinner.winningTicket = winningTicket;
        mainWinner.prize = accPrize.div(2);

        subWinner1.winningTicket = getRandom();
        subWinner1.prize = accPrize.div(20);

        subWinner2.winningTicket = getRandom();
        subWinner2.prize = accPrize.div(20);

        subWinner3.winningTicket = getRandom();
        subWinner3.prize = accPrize.div(20);

        subWinner4.winningTicket = getRandom();
        subWinner4.prize = accPrize.div(20);

        subWinner5.winningTicket = getRandom();
        subWinner5.prize = accPrize.div(20);

        subWinner6.winningTicket = getRandom();
        subWinner6.prize = accPrize.div(20);

        subWinner7.winningTicket = getRandom();
        subWinner7.prize = accPrize.div(20);

        subWinner8.winningTicket = getRandom();
        subWinner8.prize = accPrize.div(20);

        subWinner9.winningTicket = getRandom();
        subWinner9.prize = accPrize.div(20);

        subWinner10.winningTicket = getRandom();
        subWinner10.prize = accPrize.div(20);
    }

    function findWinnerParticipant(uint256 iterations) external onlyOwner returns (bool) {
        require (stopTicketsIssuing, "stopTicketsIssuing");
        require (winningTicket > 0, "winningTicket");

        // Skip if there is a winner already
        if (winner != address(0)) return true;

        uint256 totalParticipants = participantsMap.size();

        if (!winnerInProgress) {
            winnerInProgress = true;
            currentIndex = 0;
            partialRes = 0;
            winnersFound = 0;
        }

        while (currentIndex < totalParticipants && iterations > 0 && winnersFound < 11) {
            address participant = participantsMap.getKeyAtIndex(currentIndex);
            uint256 participantTickets = participantsMap.get(participant);

            partialRes = partialRes.add(participantTickets);
            if (partialRes > winningTicket && winner == address(0)) {
                winner = participant;
                mainWinner.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner1.winningTicket && subWinner1.winner == address(0)) {
                subWinner1.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner2.winningTicket && subWinner2.winner == address(0)) {
                subWinner2.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner3.winningTicket && subWinner3.winner == address(0)) {
                subWinner3.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner4.winningTicket && subWinner4.winner == address(0)) {
                subWinner4.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner5.winningTicket && subWinner5.winner == address(0)) {
                subWinner5.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner6.winningTicket && subWinner6.winner == address(0)) {
                subWinner6.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner7.winningTicket && subWinner7.winner == address(0)) {
                subWinner7.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner8.winningTicket && subWinner8.winner == address(0)) {
                subWinner8.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner9.winningTicket && subWinner9.winner == address(0)) {
                subWinner9.winner = participant;
                winnersFound++;
            }
            if (partialRes > subWinner10.winningTicket && subWinner10.winner == address(0)) {
                subWinner10.winner = participant;
                winnersFound++;
            }

            winnerInProgress = winnersFound < 11;

            currentIndex++;
            iterations--;
        }

        return !winnerInProgress;
    }

    function getParticipations(address account) external view returns (uint256) {
        return participantsMap.get(account);
    }

    function getParticipants() external view returns (uint256) {
        return participantsMap.size();
    }

    function accumulatedPrice() public view onlyOwner returns (uint256) {
        return IERC20(CAKE).balanceOf(address(this));
    }

    function claimPrice(address potentialWinner) external onlyOwner returns (bool) {
        uint256 prize = 0;

        if (winnersFound < 11) return false;

        if (potentialWinner == mainWinner.winner && !mainWinner.claimed) {
            prize += mainWinner.prize;
            mainWinner.claimed = true;
        }
        else if (potentialWinner == subWinner1.winner && !subWinner1.claimed) {
            prize += subWinner1.prize;
            subWinner1.claimed = true;
        }
        else if (potentialWinner == subWinner2.winner && !subWinner2.claimed) {
            prize += subWinner2.prize;
            subWinner2.claimed = true;
        }
        else if (potentialWinner == subWinner3.winner && !subWinner3.claimed) {
            prize += subWinner3.prize;
            subWinner3.claimed = true;
        }
        else if (potentialWinner == subWinner4.winner && !subWinner4.claimed) {
            prize += subWinner4.prize;
            subWinner4.claimed = true;
        }
        else if (potentialWinner == subWinner5.winner && !subWinner5.claimed) {
            prize += subWinner5.prize;
            subWinner5.claimed = true;
        }
        else if (potentialWinner == subWinner6.winner && !subWinner6.claimed) {
            prize += subWinner6.prize;
            subWinner6.claimed = true;
        }
        else if (potentialWinner == subWinner7.winner && !subWinner7.claimed) {
            prize += subWinner7.prize;
            subWinner7.claimed = true;
        }
        else if (potentialWinner == subWinner8.winner && !subWinner8.claimed) {
            prize += subWinner8.prize;
            subWinner8.claimed = true;
        }
        else if (potentialWinner == subWinner9.winner && !subWinner9.claimed) {
            prize += subWinner9.prize;
            subWinner9.claimed = true;
        }
        else if (potentialWinner == subWinner10.winner && !subWinner10.claimed) {
            prize += subWinner10.prize;
            subWinner10.claimed = true;
        }

        if (prize > 0) {
            bool success = IERC20(CAKE).transfer(potentialWinner, prize);

            if (success) {
                prizeTransferred += prize;
                return true;
            }
        }

        return false;
    }

    function setClaimerEmergency(address claimer) external onlyOwner {
        winner = claimer;
    }

    function excludeFromLottery(address account) external onlyOwner {
        require (!winnerInProgress);

        if (!excludedFromLottery[account]) {
            excludedFromLottery[account] = true;    	
            totalParticipations = totalParticipations.sub(participantsMap.get(account));
            participantsMap.remove(account);
        }
    }

    function getRandom() private returns (uint256) {
        // Generate random number between 1 and totalParticipations;
        uint256 totalParticipants = participantsMap.size();
        return (uint(keccak256(abi.encodePacked(
            block.timestamp,
            participantsMap.keys[0],
            participantsMap.keys[totalParticipants-1],
            counter++))) % totalParticipations) + 1;
    }
}