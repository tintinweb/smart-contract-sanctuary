        pancakeV2Pair = IpancakeV2Factory(pancakeV2Router.factory())
            .createPair(address(this), pancakeV2Router.WETH());

        // set the rest of the contract variables

        updateFee(_feeDecimals, _feePercentage);
        updateMinTokensBeforeAddToLP(_minTokensBeforeAddToLP);
        updateSwapAndAbsorbEnabled(_swapAndAddLiquidityEnabled);
        emit Transfer(address(0), _msgSender(), _tTotal);
    }



