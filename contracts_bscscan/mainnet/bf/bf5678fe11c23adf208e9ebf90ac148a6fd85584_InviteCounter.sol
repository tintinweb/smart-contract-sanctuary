pragma solidity 0.7.5;

contract InviteCounter is Ownable {

    EnumerableSet.AddressSet private _callers;
    using SafeMath for uint256;

    mapping (address => bool) public intvitedUsers;
    mapping (address => uint256) public users;
    mapping (uint => uint256) public countForLevel;
    mapping (uint => uint256) public amountForType;

    bool public started;

    constructor( ) {        
        started = false;
    }

    function userStateInPool(address _user,uint _targetLevel) external view returns(bool){
        if(!started){
            return true;
        }
        return users[_user] >= countForLevel[_targetLevel];
    }

    function updateUserInPool(address _user,bool,uint) external onlyCaller() {
        users[_user] = 0;
    }

    function inviteUser(address invite, address inviter, uint256 _amount, uint inviteType) external onlyCaller() returns(bool){
        if(intvitedUsers[invite] == true || inviteType == 2 || inviteType == 7){
            return false;
        }
        bool state = _amount>= amountForType[inviteType]?true:false;
        if(state && inviter != address(0)){
            users[inviter] = users[inviter].add(1);
        }
        intvitedUsers[invite] = true;
        return true;
    }

    function setCount(uint _level,uint256 _count) external onlyManager() returns (bool) {
        require(_level > 0 && _level < 6);
        countForLevel[_level] = _count;
        return true;
    }

    function setAmount(uint _type,uint256 _amount) external onlyManager() returns (bool) {
        require(_type > 0,"inviteCount: invalid type");
        amountForType[_type] = _amount;
        return true;
    }

    function setState(bool _state) external onlyManager() returns (bool) {
        started = _state;
        return true;
    }

    function addCaller(address _newCaller) public onlyManager() returns (bool) {
        require(_newCaller != address(0), "CowStaking: address is zero");
        return EnumerableSet.add(_callers, _newCaller);
    }

    function delCaller(address _delCaller) public onlyManager() returns (bool) {
        require(_delCaller != address(0), "CowStaking: address is zero");
        return EnumerableSet.remove(_callers, _delCaller);
    }

    function isCaller(address _caller) public view returns (bool) {
        return EnumerableSet.contains(_callers, _caller);
    }

    function getCaller(uint256 _index) public view returns (address) {
        require(_index <= getCallerLength() - 1, "CowStaking: index out of bounds");
        return EnumerableSet.at(_callers, _index);
    }

    function getCallerLength() public view returns (uint256) {
        return EnumerableSet.length(_callers);
    }
    
    modifier onlyCaller() {
        require(isCaller(msg.sender), "CowStaking: not the caller");
        _;
    }
}