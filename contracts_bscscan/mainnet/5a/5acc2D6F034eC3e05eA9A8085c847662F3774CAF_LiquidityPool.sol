        _moveDelegates(currentDelegate, delegate_, delegatorBalance);

        emit DelegateChanged(delegator, currentDelegate, delegate_);
    }

    function _moveDelegates(
        address srcRep,
        address dstRep,
        uint256 amount
    ) internal {
        if (srcRep != dstRep && amount > 0) {
            if (srcRep != address(0)) {
