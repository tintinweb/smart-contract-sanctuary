pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "base64-sol/base64.sol";

contract Riddler is ERC721Enumerable {
    using SafeMath for uint256;

    // Total supply of 10,000
    uint256 private MAX_SUPPLY = 10000;

    // Each riddle is assigned 25% of the total supply
    uint256 private TOKENS_PER_RIDDLE = 2500;

    // Each token can be minted for 0.06 each
    uint256 private PRICE = 60000000000000000;

    // 0.04 from each mint goes to the riddle prize pool
    uint256 private RIDDLE_PORTION = 40000000000000000;

    // 0.01 from each mint goes to charity address
    uint256 private CHARITY_PORTION = 10000000000000000;

    // 0.01 from each mint goes to developer address
    uint256 private DEV_PORTION = 10000000000000000;

    // 0.01 must be sacrificed for the prize pool when you burn
    uint256 private BURN_SACRIFICE_PORTION = 10000000000000000;

    // Charitable donations go to https://www.coincenter.org/
    // Coin Center is the leading non-profit focused on the policy issues facing cryptocurrencies
    address private CHARITY_WALLET = 0x15322B546e31F5Bfe144C4ae133A9Db6F0059fe3;

    // Compensation for the developer
    address private DEV_WALLET;

    // total tokens minted
    uint256 private _minted = 0;

    // Mapping from token ID to attempts
    mapping(uint256 => uint) private _tokenAttempts;

    // Track the prize pool for each riddle
    uint256[4] private _riddlePrizePools = [0, 0, 0, 0];

    // Track the latest riddle solved
    uint private _riddleSolved = 0;

    // Riddle answers
    bytes32 private _answer1 = 0xa3e6da19015a23800df55c11acdb943639a2eee145a54a56ceac012fd8ede6b9;
    bytes32 private _answer2 = 0x883206ad0410b9b1dab442d1760512dfeafe40073766cf512619763a55d4e123;
    bytes32 private _answer3 = 0x32f8c40390a533a5fde85051cfcbbf66da0f08d3c6047a0986d3de06b5d059e8;
    bytes32 private _answer4 = 0x005919c47dea8f1ae55c460258c3ff5503be7bd05a4de45ca8b733d45d31d18b;

    // Supply must be divisible by 4
    constructor(uint256 supply, address dev) ERC721("Solve The Riddle", "STR") {
        MAX_SUPPLY = supply;
        TOKENS_PER_RIDDLE = MAX_SUPPLY / 4;
        DEV_WALLET = dev;
    }

    // Riddle stats
    function stats() external view returns(string memory) {
        uint256 pool = 0;
        if (_riddleSolved < 1) {
            pool = _riddlePrizePools[0];
        } else if (_riddleSolved < 2) {
            pool = _riddlePrizePools[1];
        } else if (_riddleSolved < 3) {
            pool = _riddlePrizePools[2];
        } else if (_riddleSolved < 4) {
            pool = _riddlePrizePools[3];
        } else {
            pool = 0;
        }
        return string(abi.encodePacked('{"minted":', Strings.toString(_minted),',"reward":"',Strings.toString(pool),'","riddle":',Strings.toString(_riddleSolved+1),'}'));
    }

    // Generates a data uri svg containing the riddle, the hint that reacts to the number of attempts remaining for the token
    function _getSVG(uint attempts, string memory riddle, string memory hint) private pure returns (string memory) {
        string[10] memory colors = ["EAECEE", "D5D8DC", "ABB2B9", "808B96", "566573", "2C3E50", "273746", "212F3D", "1C2833", "17202A"];
        string[7] memory p = [
        '<svg xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMinYMin" viewBox="0 0 600 600"><defs><style>.a{fill:#',
        colors[attempts - 1],
        ';}.t1,.t2{font-family:Monospace;}.t1{font-size:40px;font-weight: bold;}.t2{font-size:30px;}</style></defs><polygon class="a" points="229.08 337.6 300 207.91 370.92 337.6 229.08 337.6"/><text class="t1 a" x="50%" y="65%" dominant-baseline="middle" text-anchor="middle">',
        riddle,
        '</text><text class="t2 a" x="50%" y="75%" dominant-baseline="middle" text-anchor="middle">',
        hint,
        '</text></svg>'];
        return string(abi.encodePacked("data:image/svg+xml;base64,", Base64.encode(abi.encodePacked(p[0], p[1], p[2], p[3], p[4], p[5], p[6]))));
    }

    function tokenURI(uint256 tokenId) public view override returns (string memory) {
        require(_exists(tokenId), "Token must exist");

        string memory image = "";
        if (_riddleSolved < 1) {
            image = _getSVG(_tokenAttempts[tokenId], "Genesis", "10 characters");
        } else if (_riddleSolved < 2) {
            image = _getSVG(_tokenAttempts[tokenId], "Dapp^", "hex");
        } else if (_riddleSolved < 3) {
            image = _getSVG(_tokenAttempts[tokenId], "CryptoKitties", "!punctuation");
        } else {
            image = _getSVG(_tokenAttempts[tokenId], "Duck, Duck", "10/2");
        }

        string[11] memory j = ['{"name":"',
        name(),' #',Strings.toString(tokenId),'","description":"STR is a degenerative, burnable, and charitable NFT collection.","tokenId":',Strings.toString(tokenId),
        ',"attributes":[{"trait_type":"Remaining Attempts","max_value":10,"value":',Strings.toString(_tokenAttempts[tokenId]),
        '}],"image":"',image,'"}'];

        return string(abi.encodePacked("data:application/json;base64,", Base64.encode(abi.encodePacked(j[0], j[1], j[2], j[3], j[4], j[5], j[6], j[7], j[8], j[9], j[10]))));
    }

    // Generates a sudo random number between 1 and 10.
    function _generateAttempts(uint inc) private view returns(uint) {
        uint attempts = (uint(keccak256(abi.encodePacked(block.difficulty, block.timestamp, inc))) % 10);
        if (attempts == 0) {
            return 1;
        }
        return attempts;
    }

    // You can mint between 1-10 tokens per transaction, each token is assigned a sudo random number of attempts.
    // Each attempt reduces the attempts, when the last attempt is used the token is burnt.
    function mint(uint256 qty) external payable {
        require(qty <= 10, "Max 10 per tx");
        require(PRICE * qty <= msg.value, "Incorrect eth");
        require(_minted + qty <= MAX_SUPPLY, "Exceeds max supply");

        for (uint i = 0; i < qty; i++) {
            _minted += 1;
            _safeMint(msg.sender, _minted);
            _tokenAttempts[_minted] = _generateAttempts(i);
            if (_minted <= TOKENS_PER_RIDDLE) {
                _riddlePrizePools[0] += RIDDLE_PORTION;
            } else if (_minted <= (TOKENS_PER_RIDDLE * 2)) {
                _riddlePrizePools[1] += RIDDLE_PORTION;
            } else if (_minted <= (TOKENS_PER_RIDDLE * 3)) {
                _riddlePrizePools[2] += RIDDLE_PORTION;
            } else {
                _riddlePrizePools[3] += RIDDLE_PORTION;
            }
        }
        Address.sendValue(payable(CHARITY_WALLET), qty * CHARITY_PORTION);
        Address.sendValue(payable(DEV_WALLET), qty * DEV_PORTION);
    }

    // Burn a token by id which reimburses 0.03 to your wallet, retaining 0.1 for the prize pool
    // IMPORTANT! You can only burn a token if the riddle associated with its id is still active. EG:
    // tokenId less than 2500 can only be burnt if riddle 1 hasn't been solved, 2501 - 5000 if riddle 2 hasn't been solved,
    // 5001 - 7500 if riddle 3 hasn't been solved, and 7501 - 10000 if riddle 4 hasn't been solved.
    function burn(uint256 tokenId) external {
        require(ERC721.ownerOf(tokenId) == msg.sender, "Only owners can burn");
        uint256 reimbursement = RIDDLE_PORTION - BURN_SACRIFICE_PORTION;

        if (tokenId <= TOKENS_PER_RIDDLE) {
            require(_riddleSolved < 1, "Cannot burn, riddle 1 solved");
            _riddlePrizePools[0] -= reimbursement;
        } else if (tokenId <= (TOKENS_PER_RIDDLE * 2)) {
            require(_riddleSolved < 2, "Cannot burn, riddle 2 solved");
            _riddlePrizePools[1] -= reimbursement;
        } else if (tokenId <= (TOKENS_PER_RIDDLE * 3)) {
            require(_riddleSolved < 3, "Cannot burn, riddle 3 solved");
            _riddlePrizePools[2] -= reimbursement;
        } else {
            require(_riddleSolved < 4, "Cannot burn, riddle 4 solved");
            _riddlePrizePools[3] -= reimbursement;
        }

        _burn(tokenId);
        delete _tokenAttempts[tokenId];
        Address.sendValue(payable(msg.sender), reimbursement);
    }

    function answerRiddle(uint256 tokenId, string memory answer) external returns (string memory) {
        require(ERC721.ownerOf(tokenId) == msg.sender, "Only owner can submit");
        require(_riddleSolved < 5, "Riddles solved");

        bytes32 riddle;
        uint256 pool;
        string memory minted = " must be minted";
        if (_riddleSolved < 1) {
            require(_minted > TOKENS_PER_RIDDLE, string(abi.encodePacked(TOKENS_PER_RIDDLE, minted)));
            pool = _riddlePrizePools[0];
            riddle = _answer1;
        } else if (_riddleSolved < 2) {
            require(_minted > (TOKENS_PER_RIDDLE * 2), string(abi.encodePacked((TOKENS_PER_RIDDLE * 2), minted)));
            pool = _riddlePrizePools[1];
            riddle = _answer2;
        } else if (_riddleSolved < 3) {
            require(_minted > (TOKENS_PER_RIDDLE * 3), string(abi.encodePacked((TOKENS_PER_RIDDLE * 3), minted)));
            pool = _riddlePrizePools[2];
            riddle = _answer3;
        } else {
            require(_minted == MAX_SUPPLY, string(abi.encodePacked(MAX_SUPPLY, minted)));
            pool = _riddlePrizePools[3];
            riddle = _answer4;
        }

        _tokenAttempts[tokenId] -= 1;

        if (keccak256(abi.encodePacked(answer)) == riddle) {
            _riddleSolved += 1;
            Address.sendValue(payable(msg.sender), pool);
            return "Congratulations! Check your wallet";
        }

        if (_tokenAttempts[tokenId] == 0) {
            _burn(tokenId);
            delete _tokenAttempts[tokenId];
            return "Incorrect, it was your last attempt and your token has been burned";
        }

        return string(abi.encodePacked("Incorrect, you have ", Strings.toString(_tokenAttempts[tokenId]), " remaining"));
    }
}

