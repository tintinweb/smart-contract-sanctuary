    );

    constructor() ERC20("LibertyETH", "LibertyETH") {

        uint256 totalSupply = 500 * 1e9 * 1e18;
        
        maxTransactionAmount = totalSupply * 5 / 1000; // 0.5% maxTransactionAmountTxn
        swapTokensAtAmount = totalSupply * 5 / 10000; // 0.05% swap tokens amount
        maxWallet = totalSupply * 1 / 100; // 1% Max wallet

        rewardsBuyFee = 50;
        operationsBuyFee = 20;
        liquidityBuyFee = 20;
        totalBuyFees = rewardsBuyFee + operationsBuyFee + liquidityBuyFee;
        
        rewardsSellFee = 50;
        operationsSellFee = 20;
        liquiditySellFee = 20;
        totalSellFees = rewardsSellFee + operationsSellFee + liquiditySellFee;

    	dividendTracker = new DividendTracker();
    	
    	operationsWallet = address(0x5b8ae20b67510D9Ab44f318d34d50643b7B85cbD); // set as operations wallet

    	IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
    	// testnet PCS router: 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3
