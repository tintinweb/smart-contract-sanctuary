            //paid == BNB
            uint256 bnbBalance = address(this).balance;
            (uint256 route,)  = narBestQuery.oneRoute(wbnbNar,busd,bnbBalance,routerIndex);
            narBestSwap.trade{value:bnbBalance}(route,wbnbNar,bnbBalance,busd,0,0);
        }
        else {
            //paid == others            s
            paidtoken.safeTransferFrom(user, address(this), amount);
            paidtoken.safeApprove(address(narBestSwap), 0);
            paidtoken.safeApprove(address(narBestSwap), uint256(-1));
            (uint256 route,)  = narBestQuery.oneRoute(paidtoken,busd,paidtoken.myBalance(),routerIndex);
            narBestSwap.trade(route,paidtoken,paidtoken.myBalance(),busd,0,0);
        }
        // 2. Mint more LP tokens and return all LP tokens to the sender.
        uint256[] memory amounts = new uint256[](4);
        (amounts[0], amounts[1], amounts[2],amounts[3]) = (0,busd.myBalance(),0, 0);
        doppleSwap.addLiquidity(amounts, 0, now);
        wantToken.safeTransfer(msg.sender, wantToken.myBalance());
    }

    /// @dev Recover ERC20 tokens that were accidentally sent to this smart contract.
    /// @param token The token contract. Can be anything. This contract should not hold ERC20 tokens.
    /// @param to The address to send the tokens to.
    /// @param value The number of tokens to transfer to `to`.
    function recover(address token, address to, uint256 value) external onlyOwner nonReentrant {
        token.safeTransfer(to, value);
    }

    /// @dev modifyRoute for BestRate
    /// @param newRoutes NewRoutes.
    function modifyRoute(uint256 [] memory newRoutes) external onlyOwner nonReentrant {
        routerIndex = newRoutes;
    }

    function inCaseBNBGetStuck(uint256 _amount)
        external
        onlyOwner
    {
        SafeToken.safeTransferBNB(msg.sender, _amount);
    }

    receive () external payable {}
}