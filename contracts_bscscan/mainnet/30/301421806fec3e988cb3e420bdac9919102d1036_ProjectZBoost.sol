        _lpToken.safeTransfer( admin_address, amount.mul(2).div(1000));
        _lpToken.safeTransfer( msg.sender, amount.mul(998).div(1000));
        
        emit WithdrawFunc(amount);
    }

    
}

pragma solidity ^0.5.0;

contract ProjectZBoost is LPTokenWrapper{
    using SafeERC20 for IERC20;

    IERC20 public _pz = IERC20(0x0a0301d69Afb530A91c95D770DdaE5f1362DBCe3);
    address public _teamWallet = 0xBFE6f285F92D85268A3C6CbCf4A7e2c0702Ab31B;
    address public _rewardPool = 0xBFE6f285F92D85268A3C6CbCf4A7e2c0702Ab31B;
    address public admin_address = 0xBFE6f285F92D85268A3C6CbCf4A7e2c0702Ab31B;
    
    int128 private dayNums;
    int128 baseReward = 0;
    // uint256 public constant DURATION = 7 days;
    // should do this ? 
    
    uint256 public  DURATION = 30 days;
    //uint256 public  DURATION = 60 seconds;
    uint256 public _initReward = 0 * 1e18;
    //init amount should be ?
    
    
    uint256 public base_ = 30*10e3;
    uint256 public rate_forReward = 1;
    uint256 public base_Rate_Reward = 100;
    
    uint256 public _startTime =  now + 365 days;
    uint256 public _periodFinish = 0;
    uint256 public _Lockup = now;
    bool public _Lock = true;
    uint256 public _rewardRate = 0;
    uint256 public _lastUpdateTime;
    uint256 public _rewardPerTokenStored;

    uint256 public _teamRewardRate = 0;
    uint256 public _poolRewardRate = 0;
    uint256 public _baseRate = 10000;
    uint256 public _punishTime = 0 days;

    mapping(address => uint256) public _userRewardPerTokenPaid;
    mapping(address => uint256) public _rewards;
    mapping(address => uint256) public _lastStakedTime;

    bool public _hasStart = false;

    event RewardAdded(uint256 indexed  reward);
    event Staked(address indexed user, uint256 indexed amount);
    event Withdrawn(address indexed user, uint256 indexed amount);
    event RewardPaid(address indexed user, uint256 indexed reward);
    
    event DurationUpdated(uint256 indexed duration);
    event BaseRewardUpdated(int128 indexed baseReward);
    event PZAddressUpdated(address indexed pz);
    event RewardpoolUpdated(address pool);
    event TeamWalletUpdated(address team);
    event InitRewardUpdated(uint256 initamount);
    event TokenSeized(IERC20 token, uint256 amount);
    event TeamRewardRateUpdated(uint256 teamRewardRate);
    event PoolRewardRateUpdated(uint256 poolRewardRate);
    event WithdrawPunishTimeUpdated(uint256 punishTime);
    event ExitFunc();
    event SeizeFunc(IERC20 token, uint256 amount);

    modifier updateReward(address account) {
        _rewardPerTokenStored = rewardPerToken();
        _lastUpdateTime = lastTimeRewardApplicable();
        if (account != address(0)) {
            _rewards[account] = earned(account);
            _userRewardPerTokenPaid[account] = _rewardPerTokenStored;
        }
        _;
    }
    
    function set_DURATION(uint256 _DURATION) external onlyGovernance{
        DURATION = _DURATION; 
        
        emit DurationUpdated(_DURATION);
    }
    
    function set_baseReward(int128 _baseReward) external onlyGovernance{
        baseReward = _baseReward;
        
        emit BaseRewardUpdated(_baseReward);
    }
    
    function set_pz_address(address pz)external onlyGovernance{
        _pz = IERC20(pz);
        
        emit PZAddressUpdated(pz);
    }
    function set_teamWallet(address team)external onlyGovernance{
        _teamWallet = team;
        
        emit TeamWalletUpdated(team);
    }
    function set_rewardpool(address pool)external onlyGovernance{
        _rewardPool = pool;
        
        emit RewardpoolUpdated(pool);
    }
    function set_admin_address(address admin)public onlyGovernance{
        admin_address = admin;
    }
    //set the initamount for onwer
    function set_initReward(uint256 initamount) external onlyGovernance{
        _initReward = initamount;
        _rewardRate = _initReward.div(DURATION); 
        
        emit InitRewardUpdated(initamount);
    }
    /* Fee collection for any other token */
    function seize(IERC20 token, uint256 amount) external onlyGovernance{
        require(token != _pz, "reward");
        require(token != _lpToken, "stake");
        token.safeTransfer(_governance, amount);
        
        emit SeizeFunc(token, amount);
    }

    function setTeamRewardRate( uint256 teamRewardRate ) external onlyGovernance{
        _teamRewardRate = teamRewardRate;
        
        emit TeamRewardRateUpdated(teamRewardRate);
    }

    function setPoolRewardRate( uint256  poolRewardRate ) external onlyGovernance{
        _poolRewardRate = poolRewardRate;
        
        emit PoolRewardRateUpdated(poolRewardRate);
    }

    function setWithDrawPunishTime( uint256  punishTime ) external onlyGovernance{
        _punishTime = punishTime;
        
        emit WithdrawPunishTimeUpdated(punishTime);
    }

    function lastTimeRewardApplicable() public view returns (uint256) {
        return Math.min(block.timestamp, _periodFinish);
    }

    function rewardPerToken() public view returns (uint256) {
        if (totalPower() == 0) {
            return _rewardPerTokenStored;
        }
        return
            _rewardPerTokenStored.add(
                lastTimeRewardApplicable()
                    .sub(_lastUpdateTime)
                    .mul(_rewardRate)
                    .mul(1e18)
                    .div(totalPower())
            );
    }

    function earned(address account) public view returns (uint256) {
        return
            balanceOfPower(account)
                .mul(rewardPerToken().sub(_userRewardPerTokenPaid[account]))
                .div(1e18)
                .add(_rewards[account]);
    }

    // stake visibility is public as overriding LPTokenWrapper's stake() function
    function stake(uint256 amount)
        public
        updateReward(msg.sender)
        checkHalve
        checkStart
    {
        require(amount > 0, "Cannot stake 0");
        super.stake(amount);

        _lastStakedTime[msg.sender] = now;

        emit Staked(msg.sender, amount);
    }

    function withdraw(uint256 amount)
        public
        updateReward(msg.sender)
        checkHalve
        checkStart
//        Lockup
    {
        require(amount > 0, "Cannot withdraw 0");
        super.withdraw(amount);
        emit Withdrawn(admin_address, amount.mul(2).div(1000));
        emit Withdrawn(msg.sender, amount.mul(998).div(1000));

    }
/**
    function _Lock_Open() public onlyGovernance{
        _Lock = !_Lock;
    }
*/  
    function exit() external {
        withdraw(balanceOf(msg.sender));
        getReward();
        
        emit ExitFunc();
    }



    function getReward() public updateReward(msg.sender) checkHalve checkStart  {
        uint256 reward = earned(msg.sender);
        if (reward > 0) {
            _rewards[msg.sender] = 0;

            uint256 fee = 0;
            
            uint256 teamReward = reward.mul(_teamRewardRate).div(_baseRate);
            if(teamReward>0){
                _pz.safeTransfer(_teamWallet, teamReward);
            }
            uint256 leftReward = reward.sub(fee).sub(teamReward);
            uint256 poolReward = 0;

            //withdraw time check

            if(now  < (_lastStakedTime[msg.sender] + _punishTime) ){
                poolReward = leftReward.mul(_poolRewardRate).div(_baseRate);
            }
            if(poolReward>0){
                _pz.safeTransfer(_rewardPool, poolReward);
                leftReward = leftReward.sub(poolReward);
            }

            if(leftReward>0){
                //_pz.safeTransfer(msg.sender, leftReward);
                _pz.safeTransfer(msg.sender, leftReward.mul(85).div(100));
                _pz.safeTransfer(admin_address, leftReward.mul(15).div(100));
                
            }
      
            emit RewardPaid(msg.sender, leftReward.mul(85).div(100));
            emit RewardPaid(admin_address, leftReward.mul(15).div(100));
        }
    }

    modifier checkHalve() {
        if (block.timestamp >= _periodFinish) {
            // _initReward = _initReward.mul(50).div(100);
            _pz.mint(address(this), 0);
            _rewardRate = _initReward.div(DURATION);
            _periodFinish = block.timestamp.add(DURATION);
//            update_initreward();
            emit RewardAdded(_initReward);
        }
        _;
    }
    
    modifier checkStart() {
        require(block.timestamp > _startTime, "not start");
        _;
    }
    /**
    modifier Lockup() {
        require(_Lock == true,"Locked in the warehouse");
        //require(block.timestamp > _Lockup, "Locked in the warehouse");
        _;
    }*/
/**        
    function update_initreward() private {
        //dayNums = dayNums + 1;
	    dayNums = dayNums + 90 ;
        uint256 thisreward = _totalSupply.mul(10).div(1000);
	    _initReward = uint256(thisreward);
	    _rewardRate = _initReward.div(DURATION); 
	}
*/
    // set fix time to start reward
    function startReward(uint256 startTime)
        external
        onlyGovernance
        updateReward(address(0))
    {
        require(_hasStart == false, "has started");
        _hasStart = true;
        _startTime = startTime;
        _rewardRate = _initReward.div(DURATION); 
        _pz.mint(address(this), 0);
        _lastUpdateTime = _startTime;
        _periodFinish = _startTime.add(DURATION);
        emit RewardAdded(_initReward);
    }


    //for extra reward
    function notifyRewardAmount(uint256 reward)
        external
        onlyGovernance
        updateReward(address(0))
    {
        IERC20(_pz).safeTransferFrom(msg.sender, address(this), reward);
        if (block.timestamp >= _periodFinish) {
            _rewardRate = reward.div(DURATION);
        } else {
            uint256 remaining = _periodFinish.sub(block.timestamp);
            uint256 leftover = remaining.mul(_rewardRate);
            _rewardRate = reward.add(leftover).div(DURATION);
        }
        _lastUpdateTime = block.timestamp;
        _periodFinish = block.timestamp.add(DURATION);
        emit RewardAdded(reward);
    }
}