pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/utils/Multicall.sol";

contract LiveTradeNFTPlatinum is
  ERC721,
  ERC721Enumerable,
  ERC721URIStorage,
  Pausable,
  Ownable,
  ERC721Burnable,
  Multicall
{
  using Counters for Counters.Counter;

  // total NFT for user claim
  uint256 public totalPublicClaim;

  // total NFT mint per user
  uint256 public totalPerUser;

  // Use LTD to check claim NFT
  ERC20 public addressTokenCheck;

  address public devAddress;

  uint256 public balanceStaked;

  mapping(address => uint256) public claimed; // Claimmed number <= totalPerUser

  mapping(address => uint256) public staked; // Staked bool <= totalPerUser

  // address => tokenId => boolean
  mapping(address => mapping(uint256 => bool)) public userNFTBountyClaimed;

  event eveStaked(address userStaked, uint256 balanceStaked);

  event eveReturnNFT(address userReturn, uint256 tokenId);

  string public baseURI =
    "https://nft.kshark.io/api/event/airdrops/nft-for-spaceship.json";

  Counters.Counter private _tokenIdCounter;

  constructor() ERC721("LiveTrade NFT Platinum", "Platinum") {}

  function initBounty(
    address _devAddress,
    ERC20 _addressTokenCheck,
    uint256 _totalPublicClaim,
    uint256 _balanceStaked,
    uint256 _totalPerUser
  ) public onlyOwner {
    devAddress = _devAddress;
    addressTokenCheck = ERC20(_addressTokenCheck);
    totalPublicClaim = _totalPublicClaim;
    balanceStaked = _balanceStaked;
    totalPerUser = _totalPerUser;
  }

  function setTotalPublicClaim(uint256 _newBalance) public onlyOwner {
    totalPublicClaim = _newBalance;
  }

  function _baseURI() internal view override returns (string memory) {
    return baseURI;
  }

  function setBaseURI(string memory baseURI_) public onlyOwner {
    baseURI = baseURI_;
  }

  function setBalanceStaked(uint256 _newBalance) public onlyOwner {
    balanceStaked = _newBalance;
  }

  function safeMint(address to) public onlyOwner {
    _safeMint(to, _tokenIdCounter.current());
    _tokenIdCounter.increment();
  }

  function safeMintToAccountMulti(uint256 num, address to) public onlyOwner {
    for (uint64 i = 0; i < num; i++) {
      safeMint(to);
    }
  }

  function safeMintToAllAccount(address[] calldata to) public onlyOwner {
    for (uint64 i = 0; i < to.length; i++) {
      safeMint(to[i]);
    }
  }

  // @dev: Approve this contract to token contract
  // Mission staked LTD for claim NFT bounty
  function stakeForBounty() public {
    require(staked[msg.sender] + 1 <= totalPerUser, "Token already max staked");
    require(claimed[msg.sender] + 1 <= totalPerUser, "NFT already max claimed");

    uint256 totalNFTnow = totalSupply();
    require(totalNFTnow + 1 <= totalPublicClaim, "Had limit NFT to claim");

    // send token to contract
    addressTokenCheck.transferFrom(msg.sender, address(this), balanceStaked);

    staked[msg.sender]++;

    claimNFTBounty();

    emit eveStaked(msg.sender, balanceStaked);
  }

  // User claim NFT bounty
  function claimNFTBounty() internal {
    require(claimed[msg.sender] + 1 <= totalPerUser, "NFT already max claimed");

    // Required number stake > number claim ( because stake first then claim NFT )
    require(
      staked[msg.sender] > claimed[msg.sender],
      "You're not staked enought"
    );

    claimed[msg.sender]++;

    // mint NFT for user
    uint256 totalNFTnow = totalSupply();
    require(totalNFTnow + 1 <= totalPublicClaim, "Had limit NFT to claim");

    _safeMint(msg.sender, _tokenIdCounter.current());

    userNFTBountyClaimed[msg.sender][_tokenIdCounter.current()] = true;

    _tokenIdCounter.increment();
  }

  // @dev: ApproveForAll this contract to this contract by account return NFT
  // User return NFT
  function returnNFT(uint256 tokenId) public {
    require(claimed[msg.sender] > 0, "Not claimed NFT before!");
    require(staked[msg.sender] > 0, "You're not staked KSC");

    require(
      userNFTBountyClaimed[msg.sender][tokenId],
      "This NFT is not NFT you claimed before!"
    );

    // Send NFT from user to contract
    IERC721(address(this)).safeTransferFrom(msg.sender, address(this), tokenId);

    // Burn NFT
    _burn(tokenId);

    uint256 feeReturn = (balanceStaked * 125) / 10000; // Fee 1.25%
    uint256 balanceReturn = balanceStaked - feeReturn;

    // Send token fee
    addressTokenCheck.transfer(devAddress, feeReturn);
    // Send token back to user staked
    addressTokenCheck.transfer(msg.sender, balanceReturn);

    // Setup details
    claimed[msg.sender]--;
    staked[msg.sender]--;
    // set = false
    userNFTBountyClaimed[msg.sender][tokenId] = false;

    emit eveReturnNFT(msg.sender, tokenId);
  }

  function setTokenToCheck(address _newAddressToken) public onlyOwner {
    addressTokenCheck = ERC20(_newAddressToken);
  }

  function setDevAddress(address newDevAddress) public onlyOwner {
    devAddress = newDevAddress;
  }

  function pause() public onlyOwner {
    _pause();
  }

  function unpause() public onlyOwner {
    _unpause();
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256 tokenId
  ) internal override(ERC721, ERC721Enumerable) whenNotPaused {
    super._beforeTokenTransfer(from, to, tokenId);
  }

  function _burn(uint256 tokenId) internal override(ERC721, ERC721URIStorage) {
    super._burn(tokenId);
  }

  function tokenURI(uint256 tokenId)
    public
    view
    override(ERC721, ERC721URIStorage)
    returns (string memory)
  {
    return super.tokenURI(tokenId);
  }

  function supportsInterface(bytes4 interfaceId)
    public
    view
    override(ERC721, ERC721Enumerable)
    returns (bool)
  {
    return super.supportsInterface(interfaceId);
  }

  // Required function to allow receiving ERC-721 - When safeTransferFrom called auto implement this func if (to) is contract address
  function onERC721Received(
    address, /*operator*/
    address, /*from*/
    uint256, /*id*/
    bytes calldata /*data*/
  ) external pure returns (bytes4) {
    return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
  }
}

