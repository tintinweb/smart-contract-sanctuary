pragma solidity 0.8.2;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/SafeCast.sol";
import "@openzeppelin/contracts/utils/Address.sol";

import "@openzeppelin/contracts/security/Pausable.sol";

import "@openzeppelin/contracts/access/AccessControl.sol";

import "../Interface/iTagTokenDirect.sol";

contract TagFLiquidity is AccessControl, Pausable {
    using SafeMath for uint256;

    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    iTagTokenDirect FT;

    address private Address;

    uint256 private Share;

    uint256 private currentBlock;

    uint256 private startBlock;

    uint256 private endBlock;

    constructor(
        address _FTAddress,
        uint256 _setShare,
        uint256 _startBlock,
        uint256 _endBlock
    ) public {
        FT = iTagTokenDirect(_FTAddress);

        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setRoleAdmin(ADMIN_ROLE, DEFAULT_ADMIN_ROLE);

        startBlock = _startBlock;
        endBlock = _endBlock;
        currentBlock = _startBlock;
        Share = _setShare;
    }

    function setAddress(address _addr) public virtual onlyRole(DEFAULT_ADMIN_ROLE) returns (bool) {
        Address = _addr;
        return true;
    }

    function fetchStartBlock() public view virtual returns (uint256) {
        return startBlock;
    }

    function fetchEndBlock() public view virtual returns (uint256) {
        return endBlock;
    }

    function fetchCurrentBlock() public view virtual returns (uint256) {
        return currentBlock;
    }

    function fetchShare() public view virtual returns (uint256) {
        return Share;
    }

    function pending() public view virtual returns (uint256) {
        uint256 diff;
        if (block.number > endBlock) {
            diff = endBlock.sub(currentBlock);
        } else {
            diff = block.number.sub(currentBlock);
        }
        return diff.mul(Share);
    }

    function iWithdraw() internal virtual returns (bool) {
        uint256 diff;
        if (block.number > endBlock) {
            diff = endBlock.sub(currentBlock);
            currentBlock = endBlock;
        } else {
            diff = block.number.sub(currentBlock);
            currentBlock = block.number;
        }
        uint256 amt = diff.mul(Share);
        mintTokens(Address, amt);
        return true;
    }

    function withdraw() public virtual onlyRole(ADMIN_ROLE) returns (bool) {
        return iWithdraw();
    }

    function mintTokens(address _to, uint256 _amt) internal virtual {
        FT.mint(_to, _amt);
    }
}

