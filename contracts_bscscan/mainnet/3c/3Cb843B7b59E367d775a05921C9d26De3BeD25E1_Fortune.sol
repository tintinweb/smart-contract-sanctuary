contract Fortune is Divine {

  using SafeMath for uint256;
  using SafeERC20 for IERC20;

    bool public presaleOpen;
    uint public price_Q = 200; // For qualified accounts
    uint public price_unQ = 150; // For unqualified accounts
    uint public startTime;
    uint public timeForQuilifiedBuyer = 3600; 
    uint public buyAmountLimit = 10; //upper limit of buyAmountLimit BNB
    uint public _floorbuyAmount = 1*1e16; // 0.01 BNB
    uint public _upperbuyAmount =  10*1e18; // 10 BNB
    uint public totalSaleAmount;
    mapping( address => bool ) public isQuilifiedBuyer;
    mapping( address => uint ) buyAmoutRec;
    
    constructor() Divine("Fortune", "FTN", 9) {
    }

    function addQuilifiedBuyer( address _QuilifiedBuyer ) external onlyOwner() returns ( bool ) {
        _addQuilifiedBuyer( _QuilifiedBuyer );
        return isQuilifiedBuyer[_QuilifiedBuyer];
    }

    function addQuilifiedBuyers( address[] calldata _QuilifiedBuyers ) external onlyOwner() returns ( bool ) {

        for( uint256 iteration_; _QuilifiedBuyers.length > iteration_; iteration_++ ) {
            _addQuilifiedBuyer( _QuilifiedBuyers[iteration_] );
        }
        return true;
    }

    function _addQuilifiedBuyer( address _QuilifiedBuyer ) internal {
        isQuilifiedBuyer[_QuilifiedBuyer] = true;
    }

    function removeQuilifiedPreBuyers( address[] calldata _QuilifiedBuyers ) external onlyOwner() returns ( bool ) {

        for( uint256 iteration_; _QuilifiedBuyers.length > iteration_; iteration_++ ) {
            if ( isQuilifiedBuyer[ _QuilifiedBuyers[iteration_] ] ){
                isQuilifiedBuyer[ _QuilifiedBuyers[iteration_] ] = false;
            }
        }
        return true;
    }

    function StartPresale () external onlyOwner() {
        startTime = block.timestamp;
        presaleOpen = true;
    }

    function EndPresale () external onlyOwner() {
        presaleOpen = false;
        require(_vault!=address(0),"Should set treasure vault contract before start");
        _mint(_vault,totalSaleAmount); //used for adding liquidity
    }

    function buy() payable public returns(uint) {
        require(presaleOpen,"Presale not opened");
        require(msg.value >= _floorbuyAmount && msg.value <= _upperbuyAmount ,"Transaction recovery");
        if ( block.timestamp-startTime<timeForQuilifiedBuyer ){
            require( isQuilifiedBuyer[msg.sender] == true, "unqulified buyers should buy latter" );
        }

        // calc payout for FTN tokens
        uint _payAmount = msg.value;
        uint _buyPrice;
        if (isQuilifiedBuyer[msg.sender] == true){
          _buyPrice = price_Q;
        }else {
          _buyPrice = price_unQ;
        }
        uint _tokenPayoutAmount = _payAmount.mul(_buyPrice).mul(1e9).div(1e18); 
        require(_tokenPayoutAmount+buyAmoutRec[msg.sender] <= buyAmountLimit.mul(_buyPrice).mul(1e9),"Buying limit is passed");       
        buyAmoutRec[msg.sender] += _tokenPayoutAmount;
        totalSaleAmount += _tokenPayoutAmount;
      
        return _tokenPayoutAmount.div(1e9); //return tokenbought without decimals
    }

    function ClaimAfterSale() external {
      // mint to users,after the end of presale
      require(presaleOpen == false,"plz wait until the presale ends");
      if (buyAmoutRec[msg.sender]>0){
        _mint(msg.sender, buyAmoutRec[msg.sender]);
        buyAmoutRec[msg.sender] = 0;
      }
    }

    function GetAmoutBought(address _accountadd) external view returns(uint) {
      return buyAmoutRec[_accountadd].div(1e9); // remove decimals
    }

    function withdraw(address payable _owner) public onlyOwner {
        require(!presaleOpen,"wait until presale ended, and sent to vault or creat liquidity");
        _owner.transfer(address(this).balance);                      
    }

    function mint(address account_, uint256 amount_) external onlyVault() {
        _mint(account_, amount_);
    }

    /**
     *  @notice allow anyone to send lost tokens to the vault
     *  @return bool
     */
    function recoverLostToken( address _token ) external returns ( bool ) {
        require(_vault!=address(0),"Vault is not set yet");
        IERC20( _token ).safeTransfer( _vault, IERC20( _token ).balanceOf( address(this) ) );
        return true;
    }

    /**
     * @dev Destroys `amount` tokens from the caller.
     *
     * See {ERC20-_burn}.
     */
    function burn(uint256 amount) public virtual {
        _burn(msg.sender, amount);
    }
   
    function burnFrom(address account_, uint256 amount_) public virtual {
        _burnFrom(account_, amount_);
    }

    function _burnFrom(address account_, uint256 amount_) public virtual {
        uint256 decreasedAllowance_ =
            allowance(account_, msg.sender).sub(
                amount_,
                "ERC20: burn amount exceeds allowance"
            );

        _approve(account_, msg.sender, decreasedAllowance_);
        _burn(account_, amount_);
    }
}