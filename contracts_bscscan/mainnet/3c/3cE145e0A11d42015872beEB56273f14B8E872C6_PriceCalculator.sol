contract ChainLinkPriceOracleBSC is IMarketOracle, Initializable, OwnableUpgradeable {
    using SafeMath for uint256;

    mapping (address => address) tokenToOraclePair;

    function initialize() external initializer {
        __Ownable_init();
        tokenToOraclePair[KnownTokens.AUTOv2] = ChainLinkPriceOraclePairsBSC.AUTO_USD;
        tokenToOraclePair[KnownTokens.BUSD] = ChainLinkPriceOraclePairsBSC.BUSD_BNB;
        tokenToOraclePair[KnownTokens.USDT] = ChainLinkPriceOraclePairsBSC.USDT_BNB;
        tokenToOraclePair[KnownTokens.BTCB] = ChainLinkPriceOraclePairsBSC.BTC_BNB;
        tokenToOraclePair[KnownTokens.ETH] = ChainLinkPriceOraclePairsBSC.ETH_BNB;
        tokenToOraclePair[KnownTokens.CAKE] = ChainLinkPriceOraclePairsBSC.CAKE_BNB;
    }

    function getData(address tokenAddress) external view override returns (uint256 priceInBNB) {
        address oraclePair = tokenToOraclePair[tokenAddress];
        require(oraclePair != address(0), "no_data");

        uint8 decimal = IAggregatorV3Interface(oraclePair).decimals();
        if (decimal == 18) { // trust the pair is price in bnb, but please double check when use it
            (, int price, , ,) = IAggregatorV3Interface(oraclePair).latestRoundData();
            priceInBNB = uint256(price);
        } else if (decimal == 8) { // trust the pair is price in usd, but please double check when use it
            (, int price, , ,) = IAggregatorV3Interface(oraclePair).latestRoundData();
            (, int bnbUSDPrice, , ,) = IAggregatorV3Interface(ChainLinkPriceOraclePairsBSC.AUTO_USD).latestRoundData();

            // scale to 1e18 base
            priceInBNB = uint256(price).mul(1e10).div(uint256(bnbUSDPrice).mul(1e10));
        }
    }
}

contract PriceLPPancakeBSC is IMarketOracle, Ownable {
    using SafeMath for uint256;
    using HomoraMath for uint;

    address public priceCalculatorAddress;

    constructor(address _addr) {
        priceCalculatorAddress = _addr;
    }

    function setPriceCalculator(address _addr) external onlyOwner {
        priceCalculatorAddress = _addr;
    }

    function getData(address pair) external view override returns (uint256) {
        address token0 = IPancakePair(pair).token0();
        address token1 = IPancakePair(pair).token1();
        uint totalSupply = IPancakePair(pair).totalSupply();
        (uint r0, uint r1, ) = IPancakePair(pair).getReserves();

        uint sqrtK = HomoraMath.sqrt(r0.mul(r1)).fdiv(totalSupply);
        uint px0 = PriceCalculator(priceCalculatorAddress).tokenPriceInBNB(token0, 1e18);
        uint px1 = PriceCalculator(priceCalculatorAddress).tokenPriceInBNB(token1, 1e18);
        uint fairPriceInBNB = sqrtK.mul(2).mul(HomoraMath.sqrt(px0)).div(2**56).mul(HomoraMath.sqrt(px1)).div(2**56);

        return fairPriceInBNB.div(1e18);
    }
}

interface IPriceFeed {
    function getData(address tokenAddress) external view returns (uint256);
}

contract PriceCalculator is OwnableUpgradeable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    uint256 constant _DECIMAL = 1e18;

    // WBNB
    address public constant WRAPPED_NATIVE_TOKEN = 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;

    // Pancake factory
    address public constant PANCAKE_FACTORY = 0xcA143Ce32Fe78f1f7019d7d551a6402fC5350c73;

    // Ape factory
    address public constant APE_FACTORY = 0x0841BD0B734E4F5853f0dD8d7Ea041c241fb0Da6;

    struct PriceFeed {
        address oracleAddress;
        address fallbackFacotry;
    }

    mapping(address => PriceFeed) public tokenPriceFeed;

    function initialize() external initializer {
        __Ownable_init();
    }

    function addFeeds(address[] memory args) external onlyOwner {
        uint256 len = args.length;
        require(len % 3 == 0, "bad_args");

        for (uint256 i = 0; i < len; i += 3) {
            tokenPriceFeed[args[i]] = PriceFeed(args[i + 1], args[i + 2]);
        }
    }

    function oracleValueOf(address oracleAddress, address tokenAddress, uint amount) internal view returns (uint256 valueInBNB) {
        uint256 price = IPriceFeed(oracleAddress).getData(tokenAddress);
        valueInBNB = price.mul(amount);
    }

    function getTokenPriceInBNBFromPancakePair(address factory, address token, uint256 amount) view internal returns(uint256) {
        address pair = IPancakeFactory(factory).getPair(token, address(WRAPPED_NATIVE_TOKEN));
        if (pair == address(0)) return 0;
        if (IPancakePair(pair).totalSupply() == 0) return 0;

        (uint reserve0, uint reserve1, ) = IPancakePair(pair).getReserves();

        if (IPancakePair(pair).token0() == address(WRAPPED_NATIVE_TOKEN)) {
            return amount.mul(_DECIMAL).mul(reserve0).div(reserve1).div(_DECIMAL);
        } else {
            return amount.mul(_DECIMAL).mul(reserve1).div(reserve0).div(_DECIMAL);
        }
    }

    function tokenPriceInBNB(address tokenAddress, uint amount) view external returns (uint256) {
        PriceFeed storage feed = tokenPriceFeed[tokenAddress];

        if (tokenAddress == address(0) || tokenAddress == WRAPPED_NATIVE_TOKEN) {
            return amount;
        }

        require(feed.oracleAddress != address(0) || feed.fallbackFacotry != address(0), "no_price_feed");

        uint256 pairPrice = 0;
        uint256 oraclePrice = 0;

        if (feed.fallbackFacotry != address(0)) {
            pairPrice = getTokenPriceInBNBFromPancakePair(feed.fallbackFacotry, tokenAddress, amount);
        }

        if (feed.oracleAddress != address(0)) {
            oraclePrice = oracleValueOf(feed.oracleAddress, tokenAddress, amount);
        }

        if (feed.fallbackFacotry == address(0)) {
            return oraclePrice;
        }

        if (feed.oracleAddress == address(0)) {
            return pairPrice;
        }

        return Math.min(oraclePrice, pairPrice);
    }
}