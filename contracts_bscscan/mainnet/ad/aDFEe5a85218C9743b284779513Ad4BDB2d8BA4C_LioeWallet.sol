contract LioeWallet is Ownable{
    using SafeMath for uint;

    IERC20 public token;
    IPancakeRouter public pancakeRouter;
    address public bnbAddress;
    address public usdAddress;
    address public lioeAddress;
    uint public startTime;
    uint256 public priceLimit;

    bool public paused;
    mapping(address => uint256) public total;
    mapping(address => uint256) public totalWithdraw;
    mapping(address => mapping(uint256 => mapping(uint256 => uint256))) public withdrawAmount;

    constructor() public {
        IPancakeRouter _router = IPancakeRouter(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        address _bnbAddress = 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c;
        address _usdAddress = 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56;
        address _lioeAddress = 0x6A2e100BB4e6b9A12386483Ef717Ff388151C0C8;

        token = IERC20(_lioeAddress);
        pancakeRouter = _router;
        bnbAddress = _bnbAddress;
        usdAddress = _usdAddress;
        lioeAddress = _lioeAddress;
        startTime = block.timestamp;
        priceLimit = 10**19;
        paused = false;
    }

    function addTeam(address account, uint value) public onlyOwner {
        require(!paused, "Contract is paused by Owner");
        total[account] = value;
        totalWithdraw[account] = 0;
        withdrawAmount[account][BokkyPooBahsDateTimeLibrary.getYear(block.timestamp)][BokkyPooBahsDateTimeLibrary.getMonth(block.timestamp)] = 0;
    }

    function setPriceLimit(uint256 value) public onlyOwner {
        priceLimit = value;
    }

    function withdraw(uint256 amount) public {
        require(!paused, "Contract is paused by Owner");
        require(total[msg.sender] > 0, "Unregistered address");
        if (checkPrice() || checkTime()) {
            lioeTransfer(msg.sender, amount);
        }
    }

    function lioeTransfer(address account, uint256 amount) internal {
        require(withdrawAmount[account][BokkyPooBahsDateTimeLibrary.getYear(block.timestamp)][BokkyPooBahsDateTimeLibrary.getMonth(block.timestamp)].add(amount) < total[account].mul(10).div(100), "Withdraw amount is too much");
        token.transfer(msg.sender, amount);
        totalWithdraw[account] = totalWithdraw[account] + amount;
        withdrawAmount[account][BokkyPooBahsDateTimeLibrary.getYear(block.timestamp)][BokkyPooBahsDateTimeLibrary.getMonth(block.timestamp)] = withdrawAmount[account][BokkyPooBahsDateTimeLibrary.getYear(block.timestamp)][BokkyPooBahsDateTimeLibrary.getMonth(block.timestamp)].add(amount);
    }

    function setPause(bool pause) public onlyOwner {
        paused = pause;
    }

    function getLioeToUsd() internal view returns(uint) {
        address[] memory path = new address[](2);
        path[0] = bnbAddress;
        path[1] = usdAddress;
        uint[] memory amounts = new uint[](2);
        amounts = pancakeRouter.getAmountsOut(1000000000000000000, path);
        uint bnbPrice = amounts[1].div(1000000000000000000);

        path[0] = lioeAddress;
        path[1] = bnbAddress;
        amounts = pancakeRouter.getAmountsOut(1000000000, path);
        uint lioePrice = amounts[1].mul(bnbPrice);
        return lioePrice;
    }

    function checkPrice() internal view returns(bool) {
        uint lioePrice = getLioeToUsd();
        if (lioePrice >= priceLimit) {
            return true;
        }
        return false;
    }

    function getLioePrice() public view returns(uint256) {
        return getLioeToUsd();
    }

    function checkTime() internal view returns(bool) {
        require(block.timestamp > startTime, "Wrong timestamp");
        uint endTime = BokkyPooBahsDateTimeLibrary.addMonths(startTime, 6);
        if(block.timestamp > endTime) {
            return true;
        }
        return false;
    }
}