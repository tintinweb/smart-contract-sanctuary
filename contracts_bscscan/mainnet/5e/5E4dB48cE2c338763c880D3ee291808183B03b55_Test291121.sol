    address public uniswapV2Pair;
    address public routerAddress;
    
    // Pre-sale variables
    uint256 public totalPreSaleSold;
    bool public PreSale = true;
    uint256 public dateEndPresale;
    
    event Sell(address sender, uint256 amountTokens);
    
    modifier inPreSale {
        require(PreSale == true, "Pre-sale of tokens is over.");
        _;
    }

    bool public WeAreWaitingForDayK = false;
    uint256 public lastCallForWaitingForDayK;
    uint256 public dailyWait;

    modifier inWaitingForDayK {
        require(WeAreWaitingForDayK == true, "Day K is long over.");
        _;
    }
    
    struct investmentRound {
        uint8 number;
        uint256 max;
        uint256 price;
    }
    
    mapping (uint256 => investmentRound) investmentRounds;
    
    AggregatorV3Interface internal priceFeed;
    
    constructor (address payable devsWalletAddress) {
        _devsWalletAddress = devsWalletAddress;
        
        _balances[address(this)] = _tTotal;
        emit Transfer(address(0), address(this), _tTotal);
        
        // Investment rounds (price in USD - 8 decimals)
        investmentRounds[0] = investmentRound(1, 4 * 10**8 * 10**18, 150000); // Round 1
        investmentRounds[1] = investmentRound(2, 8 * 10**8 * 10**18, 165000); // Round 2
        investmentRounds[2] = investmentRound(3, 16 * 10**8 * 10**18, 181500); // Round 3
        investmentRounds[3] = investmentRound(4, 24 * 10**8 * 10**18, 199650); // Round 4
        investmentRounds[4] = investmentRound(5, 40 * 10**8 * 10**18, 219615); // Round 5
        investmentRounds[5] = investmentRound(6, 56 * 10**8 * 10**18, 241577); // Round 6
        investmentRounds[6] = investmentRound(7, 64 * 10**8 * 10**18, 265734); // Round 7
        investmentRounds[7] = investmentRound(8, 72 * 10**8 * 10**18, 292308); // Round 8
        investmentRounds[8] = investmentRound(9, 76 * 10**8 * 10**18, 321538); // Round 9
        investmentRounds[9] = investmentRound(10, 80 * 10**8 * 10**18, 353692); // Round 10
        
        //priceFeed = AggregatorV3Interface(0x2514895c72f50D8bd4B4F9b1110F0D6bD2c97526); // Testnet
        priceFeed = AggregatorV3Interface(0x0567F2323251f0Aab15c8dFb1967E4e8A7D42aeE); // Mainnet
        
    }
    
    function setRouterAddress(address newRouter) public onlyOwner() {
        routerAddress = newRouter;
        IUniswapV2Router02 _newUniswapRouter = IUniswapV2Router02(routerAddress);
        uniswapV2Pair = IUniswapV2Factory(_newUniswapRouter.factory()).createPair(address(this), _newUniswapRouter.WETH());
        uniswapV2Router = _newUniswapRouter;
    }

    // Get price of BNB in USD (8 decimals) https://docs.chain.link/docs/get-the-latest-price/
    function getLatestPrice() public view inPreSale returns (uint256) {
        (
            uint80 roundID, 
            int price,
            uint startedAt,
            uint timeStamp,
            uint80 answeredInRound
        ) = priceFeed.latestRoundData();
        return uint256(price);
    }

    function getTokenPrice() public view inPreSale returns (uint256) {
        for (uint i = 0; i <= 10; i++) {
            if (totalPreSaleSold <= investmentRounds[i].max) {
                return investmentRounds[i].price;
            }
        }

        return investmentRounds[10].price;

    }

     // Pre-sale
    function buyTokens() public payable inPreSale {
        require(msg.value > 0, "You have to send more than 0 BNB.");

        // Calculating amount of token to buy
        uint256 amountTokens = ((msg.value).mul(getLatestPrice()).div(getTokenPrice()));
        
        // Check if the contract has enough tokens to sell
        require((totalPreSaleSold + amountTokens) <= (8 * 10**9 * 10**18), "The smart contract doesn't hold enough tokens.");

        // Send 20% of send amount to Devs
        _devsWalletAddress.transfer((msg.value).mul(20).div(100));
        
        // Transfer Tokens to the Sender
        _balances[address(this)] = _balances[address(this)].sub(amountTokens);
        _balances[_msgSender()] = _balances[_msgSender()].add(amountTokens);
        emit Transfer(address(this), _msgSender(), amountTokens);
        
        // Increase the token Pre-sale sold
        totalPreSaleSold = totalPreSaleSold.add(amountTokens);
        
        // Emit sell
        emit Sell(_msgSender(), amountTokens);
    }
    
    // End Pre-sale
    function endPreSale() public onlyOwner() inPreSale {
        
        // Transfer 20 % of total pre-sold Tokens to Devs
        uint256 amountTokens = totalPreSaleSold.mul(20).div(100);
        _balances[address(this)] = _balances[address(this)].sub(amountTokens);
        _balances[_devsWalletAddress] = _balances[_devsWalletAddress].add(amountTokens);
        emit Transfer(address(this), _devsWalletAddress, amountTokens);
        
        // Transfer 40 % BNB and Tokens to the LP of Pancake to have price 0.09 USD/token
        uint256 amountBNBToLiquidity = (address(this).balance).mul(40).div(100);
        uint256 startPriceInPancakeSwap = 900000;
        uint256 amountTokenToLiquidity = ((amountBNBToLiquidity).mul(getLatestPrice()).div(startPriceInPancakeSwap));

        // Setting Pancake router
        setRouterAddress(0x10ED43C718714eb63d5aA57B78B54704E256024E); // Pancake MainNet
        //setRouterAddress(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3); // Pancake TestNet
        
        // Adding 40% of amount to liquidity on Pancake
        addLiquidity(amountTokenToLiquidity, amountBNBToLiquidity);
        
        // Burn all remaining tokens
        _burn(address(this), _balances[address(this)]);
        
        // End of the pre-sale
        dateEndPresale = block.timestamp;
        PreSale = false;
        WeAreWaitingForDayK = true;
        dailyWait = (address(this).balance).div(3650);
    }

    function waitingForDayK() public inWaitingForDayK {
        require((lastCallForWaitingForDayK + 1 days) <= block.timestamp, "This function can only be triggered once every 24 hours.");
        require(address(this).balance > 0, "The contract no longer has any BNB.");

        // Adding dailyWait in BNB to liquidity on Pancake
        addLiquidity(0, dailyWait);

        // Save current time
        lastCallForWaitingForDayK = block.timestamp;
    }

    function dayK() public onlyOwner() inWaitingForDayK {
        require(address(this).balance > 0, "The contract no longer has any BNB.");

        // Adding remaining amount to liquidity on Pancake
        addLiquidity(0, address(this).balance);

        WeAreWaitingForDayK = false;
    }
    
    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    function approve(address spender, uint256 value) public returns (bool) {
        require(spender != address(0));

        _allowed[msg.sender][spender] = value;
        emit Approval(msg.sender, spender, value);
        return true;
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        approve(address(uniswapV2Router), tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp
        );
    }

    function balanceOf(address account) public view virtual override returns (uint256) {
        return _balances[account];
    }

    function allowance(address owner, address spender) public view returns (uint256) {
        return _allowed[owner][spender];
    }

    function transferFrom(address from, address to, uint256 value) public returns (bool) {
        require(value <= _balances[from]);
        require(value <= _allowed[from][msg.sender]);
        require(to != address(0));

        _balances[from] = _balances[from].sub(value);
        _balances[to] = _balances[to].add(value);
        _allowed[from][msg.sender] = _allowed[from][msg.sender].sub(value);
        emit Transfer(from, to, value);
        return true;
    }

    function transfer(address to, uint256 value) public returns (bool) {
        require(value <= _balances[msg.sender]);
        require(to != address(0));
        if (msg.sender == _devsWalletAddress) {
            require((dateEndPresale + 180 days) <= block.timestamp, "Developer tokens are locked for 6 months after the end of the presale.");
        }
        

        _balances[msg.sender] = _balances[msg.sender].sub(value);
        _balances[to] = _balances[to].add(value);
        emit Transfer(msg.sender, to, value);
        return true;
    }

    function _burn(address account, uint256 amount) internal {
        require(account != address(0));
        require(amount <= _balances[account]);

        _tTotal = _tTotal.sub(amount);
        _balances[account] = _balances[account].sub(amount);
        emit Transfer(account, address(0), amount);
    }
    
}