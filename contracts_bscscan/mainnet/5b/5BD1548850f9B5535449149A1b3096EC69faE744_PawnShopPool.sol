interface ICryptoWorldBuilding {
    function ownerOf(uint256 tokenId) external returns (address);
    function getClassByID(uint id) external view returns (uint);
}

interface INftc is IERC20 {
    function mint(address to, uint256 amount) external returns (bool);
}

contract PawnShopPool is
    ERC721HolderUpgradeable,
    ReentrancyGuard,
    Ownable
{
    using SafeMath for uint256;
    address public _nftToken;
    ICryptoWorldBuilding public cryptoWorldBuilding;
    // The Nftc Token!
    INftc public Nftc;
    address public nftcAddress;

    //
    uint256 commonPrice = 100e18;
    //
    uint256 iconicPrice = 1000e18;
    //
    uint256 featurePrice = 10000e18;

    //
    //
    uint256 commonExtend;
    //
    uint256 iconicExtend;
    //
    uint256 featureExtend;

    //
    uint256 commonRate = 2;
    uint256 iconicRate = 4;
    uint256 featureRate = 8;

    //
    uint256 minCycle = 15;

    uint256 constant DAY = 1 days;

    //user address => tokenid => PawnInfo
    mapping(address => mapping(uint256 => PawnInfo)) public pawnShopPool;

    struct PawnInfo {
        //
        uint256 stackStartTime;
        //   30   36122%4%8%NFTC
        uint256 cycle;
        //
        uint256 rewardAmount;
        //
        uint256 class;
    }

    //NFTC
    function setNftcAddress(address _nftcAddress) public onlyOwner {
        require(
            _nftcAddress != address(0),
            "NFTCToken: _nftcAddress is the zero address"
        );
        Nftc = INftc(_nftcAddress);
        nftcAddress = _nftcAddress;
    }

    //
    function setPawnBuildingPrice(
        uint256 _commonPrice,
        uint256 _iconicPrice,
        uint256 _featurePrice
    ) public onlyOwner {
        commonPrice = _commonPrice;
        iconicPrice = _iconicPrice;
        featurePrice = _featurePrice;

        commonExtend = _commonPrice.mul(commonRate).div(100);
        iconicExtend = _iconicPrice.mul(iconicRate).div(100);
        featureExtend = _featurePrice.mul(featureRate).div(100);
    }

    //
    function setMinCycle(uint256 _cycle) public onlyOwner {
        minCycle = _cycle;
    }

    //
    function setExtendRate(
        uint256 _commonRate,
        uint256 _iconicRate,
        uint256 _featureRate
    ) public onlyOwner {
        commonRate = _commonRate;
        iconicRate = _iconicRate;
        featureRate = _featureRate;

        commonExtend = commonPrice.mul(commonRate).div(100);
        iconicExtend = iconicPrice.mul(iconicRate).div(100);
        featureExtend = featurePrice.mul(featureRate).div(100);
    }

    //set NFTC token contract address
    function setNftToken(address _nfttoken_) public onlyOwner {
        require(
            _nfttoken_ != address(0),
            "NFTCToken: _nftcAddress is the zero address"
        );
        _nftToken = _nfttoken_;
        cryptoWorldBuilding = ICryptoWorldBuilding(_nfttoken_);
    }

    function getClassByID(uint id) public view returns(uint){
        return cryptoWorldBuilding.getClassByID(id);
    }

    function stake(
        uint256 _tokenId
    ) public {
        require(
            cryptoWorldBuilding.ownerOf(_tokenId) == _msgSender(),
            "Only Token Owner can add"
        );
        uint _class = getClassByID(_tokenId);
        require(_class == 1 || _class == 2 || _class == 3, "class error");
        uint256 _rewardAmount = getRewardAmountByClass(_class);
        pawnShopPool[msg.sender][_tokenId] = PawnInfo({
            stackStartTime: block.timestamp,
            class: _class,
            cycle: minCycle,
            rewardAmount: _rewardAmount
        });
        IERC721Upgradeable(_nftToken).safeTransferFrom(
            _msgSender(),
            address(this),
            _tokenId
        );
        //nftc
        IERC20(Nftc).transfer(msg.sender, _rewardAmount);
    }

    //
    function extendCycle(
        uint256 _tokenId,
        uint256 _cycle
    ) public {
        uint _class = getClassByID(_tokenId);
        PawnInfo storage _pawnInfo = pawnShopPool[msg.sender][_tokenId];
        // require(_pawnInfo, "please pawn first");
        require(_class == 1 || _class == 2 || _class == 3, "class error");
        require(_class == _pawnInfo.class, "class match error");
        require(_cycle == 90 || _cycle == 180 || _cycle == 360, "cycle error");

        _pawnInfo.cycle = _pawnInfo.cycle.add(_cycle);

        //
        uint256 currentExtend = getExtendRateByClass(_class);
        require(
            Nftc.balanceOf(msg.sender) >= currentExtend,
            "Not Enough  Balance"
        );

        //NFTC
        IERC20(Nftc).transfer(address(this), currentExtend);
    }

    function getExtendRateByClass(uint256 _class)
        public
        view
        returns (uint256)
    {
        if (_class == 1) {
            return commonExtend;
        } else if (_class == 2) {
            return iconicExtend;
        } else if (_class == 3) {
            return featureExtend;
        }else{
            return 0;
        }
    }

    function getRewardAmountByClass(uint256 _class)
        public
        view
        returns (uint256)
    {
        if (_class == 1) {
            return commonPrice;
        } else if (_class == 2) {
            return iconicPrice;
        } else if (_class == 3) {
            return featurePrice;
        }else{
            return 0;
        }
    }

    function claimNft(uint256 _tokenId) public returns(uint256) {
        //NFTCNFT
        //
        PawnInfo storage _pawnInfo = pawnShopPool[msg.sender][_tokenId];
        require(
            _pawnInfo.stackStartTime.add(DAY * _pawnInfo.cycle) >
                block.timestamp,
            "over-time"
        );

        //NFT
        IERC721Upgradeable(_nftToken).safeTransferFrom(
            address(this),
            _msgSender(),
            _tokenId
        );
        //NFTC
        uint256 currentExtend = _pawnInfo.rewardAmount;
        require(
            Nftc.balanceOf(msg.sender) >= currentExtend,
            "Not Enough  Balance"
        );
        TransferHelper.safeTransferFrom(
            address(Nftc),
            msg.sender,
            address(this),
            currentExtend
        );
        //
        delete pawnShopPool[msg.sender][_tokenId];

        return _tokenId;
    }

    function toString(uint256 value) internal pure returns (string memory) {
        // Inspired by OraclizeAPI's implementation - MIT license
        // https://github.com/oraclize/ethereum-api/blob/b42146b063c7d6ee1358846c198246239e9360e8/oraclizeAPI_0.4.25.sol

        if (value == 0) {
            return "0";
        }
        uint256 temp = value;
        uint256 digits;
        while (temp != 0) {
            digits++;
            temp /= 10;
        }
        bytes memory buffer = new bytes(digits);
        while (value != 0) {
            digits -= 1;
            buffer[digits] = bytes1(uint8(48 + uint256(value % 10)));
            value /= 10;
        }
        return string(buffer);
    }
}