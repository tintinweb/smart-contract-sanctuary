// Contract Token
contract Token is ERC20 {
    using SafeMath for uint256;

    address payable owner;

    mapping(address => bool) airDropMapping;

    mapping(address => bool) exclidedFromTx;

    mapping(address => bool) preSaleMapping;

    uint256 TAX_FEE = 3;
    uint256 BURN_FEE = 5;
    uint256 REFER_RATE = 30;

    uint256 airDropAmout = 100000000 * 10**18;
    uint256 preSaleBaseAmount = 10000000000;

    constructor() ERC20("SuperDoge", "SuperDoge") {
        _mint(msg.sender, 1000000000000000 * 10**18);
        owner = payable(msg.sender);
        exclidedFromTx[msg.sender] = true;
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        if (exclidedFromTx[msg.sender] == true) {
            _transfer(_msgSender(), recipient, amount);
        } else {
            uint256 burnAmount = amount.mul(BURN_FEE) / 100;
            uint256 ownerAmount = amount.mul(TAX_FEE) / 100;
            _transfer(
                _msgSender(),
                recipient,
                amount.sub(burnAmount).sub(ownerAmount)
            );
            _burn(_msgSender(), burnAmount);
        }
        return true;
    }

    function claimAirDrop() public payable {
        require(msg.value >= 0.01 ether, "Wrong ether value.");

        if (airDropMapping[msg.sender] == false) {
            _transfer(owner, msg.sender, airDropAmout);
            airDropMapping[msg.sender] = true;
        }
        owner.transfer(msg.value);
    }

    function claimAirDropWithRefer(address refer) public payable {
        require(msg.value >= 0.01 ether, "Wrong ether value.");

        if (airDropMapping[msg.sender] == false) {
            _transfer(owner, msg.sender, airDropAmout);
            _transfer(owner, refer, airDropAmout);
            airDropMapping[msg.sender] = true;
        }
        owner.transfer(msg.value);
    }

    function preSaleBuyWithoutRefer() public payable {
        require(msg.value >= 0.03 ether, "Minimum 0.03.");
        uint256 amount = msg.value * preSaleBaseAmount;
        _transfer(owner, msg.sender, amount);
        owner.transfer(msg.value);
        preSaleMapping[msg.sender] = true;
    }

    function preSaleBuyWithRefer(address refer) public payable {
        require(msg.value >= 0.03 ether, "Minimum 0.03.");
        uint256 amount = msg.value * preSaleBaseAmount;
        uint256 referAmount = amount.mul(REFER_RATE) / 100;
        _transfer(owner, msg.sender, amount);
        _transfer(owner, refer, referAmount);
        owner.transfer(msg.value);
        preSaleMapping[msg.sender] = true;
    }

    function isInvestor(address acc) public view returns (bool) {
        return preSaleMapping[acc];
    }

    function hasClaimedAirDrop(address acc) public view returns (bool) {
        return airDropMapping[acc];
    }
}