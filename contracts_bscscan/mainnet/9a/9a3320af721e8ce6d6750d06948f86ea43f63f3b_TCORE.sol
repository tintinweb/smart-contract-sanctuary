    mapping (address => uint)  public BNBContributed;
    // Possible ways this could break addressed
    // 1) No agreement to terms - added require
    // 2) Adding liquidity after LGE is over - added require
    // 3) Overflow from uint - impossible there is not enough BNB available
    // 4) Depositing 0 - not an issue it will just add 0 totally
    function addLiquidity(bool agreesToTermsOutlinedInLiquidityGenerationParticipationAgreement)
        public
        payable
    {
        require(liquidityGenerationOngoing(), "Liquidity Generation Event over");
        require(agreesToTermsOutlinedInLiquidityGenerationParticipationAgreement, "No agreement provided");
        BNBContributed[msg.sender] += msg.value; // Overflow protection from safemath is not neded here
        totalBNBContributed = totalBNBContributed.add(msg.value); // For front end display during LGE. This resets with definitive correct balance while calling pair.
        emit LiquidityAddition(msg.sender, msg.value);
    }

    // Sends all available balances and mints LP tokens
    // Possible ways this could break addressed
    // 1) Multiple calls and resetting amounts - addressed with boolean
    // 2) Failed WBNB wrapping/unwrapping addressed with checks
    // 3) Failure to create LP tokens, addressed with checks
    // 4) Unacceptable division errors. Addressed with multiplications by 1e18
    // 5) Pair not set - impossible since its set in constructor
    function addLiquidityToBakerySwapTCORExWBNBPair() public {
        require(liquidityGenerationOngoing() == false, "TCORE LGE ongoing");
        require(LPGenerationCompleted == false, "TCORE LGE already finished");
        totalBNBContributed = address(this).balance;
        IBakerySwapPair pair = IBakerySwapPair(tokenBakerySwapPair);
        console.log("Balance of this", totalBNBContributed / 1e18);
        //Wrap BNB
        address WBNB = BakerySwapRouter02.WBNB();
        IWBNB(WBNB).deposit{value : totalBNBContributed}();
        require(address(this).balance == 0 , "Transfer Failed");
        IWBNB(WBNB).transfer(address(pair),totalBNBContributed);
        emit Transfer(address(this), address(pair), _balances[address(this)]);
        _balances[address(pair)] = _balances[address(this)];
        _balances[address(this)] = 0;
        pair.mint(address(this));
        totalLPTokensMinted = pair.balanceOf(address(this));
        console.log("Total tokens minted",totalLPTokensMinted);
        require(totalLPTokensMinted != 0 , "LP creation failed");
        LPperBNBUnit = totalLPTokensMinted.mul(1e18).div(totalBNBContributed); // 1e18x for  change
        console.log("Total per LP token", LPperBNBUnit);
        require(LPperBNBUnit != 0 , "LP creation failed");
        LPGenerationCompleted = true;
    }

    // Possible ways this could break addressed
    // 1) Accessing before event is over and resetting bnb contributed -- added require
    // 2) No BakerySwap pair - impossible at this moment because of the LPGenerationCompleted bool
    // 3) LP per unit is 0 - impossible checked at generation function
    function claimLPTokens() public {
        require(LPGenerationCompleted, "Event not over yet");
        require(BNBContributed[msg.sender] > 0 , "Nothing to claim, move along tornado");
        IBakerySwapPair pair = IBakerySwapPair(tokenBakerySwapPair);
        uint256 amountLPToTransfer = BNBContributed[msg.sender].mul(LPperBNBUnit).div(1e18);
        pair.transfer(msg.sender, amountLPToTransfer); // stored as 1e18x value for change
        BNBContributed[msg.sender] = 0;
        emit LPTokenClaimed(msg.sender, amountLPToTransfer);
    }

