    {
        require(liquidityGenerationOngoing(), "Event over");
        console.log("0 days since start is", contractStartTimestamp.add(0 days), "Time now is", block.timestamp);
        return contractStartTimestamp.add(0 days).sub(block.timestamp);
    }

    function liquidityGenerationOngoing()
        public
        view
        returns (bool)
    {
        console.log("0 days since start is", contractStartTimestamp.add(0 days), "Time now is", block.timestamp);
        console.log("liquidity generation ongoing", contractStartTimestamp.add(0 days) < block.timestamp);
        return contractStartTimestamp.add(0 days) > block.timestamp;
    }

    uint256 public totalLPTokensMinted;
