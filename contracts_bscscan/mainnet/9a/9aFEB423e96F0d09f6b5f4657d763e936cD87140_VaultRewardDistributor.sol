pragma solidity 0.7.4;

import "./openzeppelin/TokensRecoverable.sol";
import "./openzeppelin/Owned.sol";
import "./interfaces/IERC1155Pi.sol";
import "./openzeppelin/ReentrancyGuard.sol";
import "./openzeppelin/Context.sol";

contract VaultRewardDistributor is Owned, TokensRecoverable, ReentrancyGuard, Context
{
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    struct TransferParams
    {
        address feeAddress;
        uint256 feePercent;
    }

    TransferParams[] public transferParams;
    address public stakingContract;
    uint256 public totalParams;
    event Transfer(address token, address from, address to, uint256 amount);
    event StakingContractSet(address stakingContract);

    constructor(address _stakingContract)
    {
        require(_stakingContract!=address(0),"_stakingContract cannot be zero address");
        stakingContract = _stakingContract;
        totalParams = 0;
    }

    function setStakingContract(address _stakingContract) external ownerOnly{
        require(_stakingContract!=address(0),"_stakingContract cannot be zero address");
        stakingContract = _stakingContract;
        emit StakingContractSet(stakingContract);
    }

    function addParams(address _feeAddress, uint256 _feePercent) external ownerOnly{
        require(_feeAddress!=address(0),"_feeAddress cannot be zero address");
        transferParams.push(
            TransferParams(_feeAddress,_feePercent)
        );
        totalParams++;
    }


    function addParamsBulk(address[] memory _feeAddresses, uint256[] memory _feePercents) external ownerOnly{
        require(_feeAddresses.length==_feePercents.length, "_feeAddresses & _feePercents should be equal array size");
        for(uint i=0;i<_feeAddresses.length;i++){
        require(_feeAddresses[i]!=address(0),"_feeAddress cannot be zero address");
            transferParams.push(
                TransferParams(_feeAddresses[i],_feePercents[i])
            );
            totalParams++;
        }
    }

    function resetParams() external ownerOnly{
        delete transferParams;
        totalParams=0;
    }

    function transfer(address token, address from, address to, uint256 amount) external onlyStakingContract nonReentrant returns (bool){
        IERC20(token).transferFrom(stakingContract, address(this), amount);
        uint256 totalFeePercent=0;
        for(uint i=0;i<transferParams.length;i++){
             totalFeePercent = totalFeePercent.add(transferParams[i].feePercent);
        }
        require(totalFeePercent==100000,"totalFeePercent should be 100%");

        for(uint i=0;i<transferParams.length;i++){
            uint256 fee = transferParams[i].feePercent.mul(amount).div(100000);
            IERC20(token).safeTransfer(transferParams[i].feeAddress, fee);
        }

        emit Transfer(token, from, to, amount);

        return true;
    }

    /**
     * @dev Throws if called by any account other than the minter.
     */
    modifier onlyStakingContract() {
        require(_msgSender() == stakingContract, "RewardDistributor: caller is not the stakingContract");
        _;
    }



}

