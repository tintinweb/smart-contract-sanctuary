    uint256 maxLiquidity = 10000 * 10**8 * 10**9;
    uint256 maxAmount = 200000 * 10**8 * 10**9;
    uint256 maxTXAmount = 30000 * 10**8 * 10**9;
    uint256 maxBuyAmount = 10000 * 10**8 * 10**9;
    uint256 maxBuyTime = 0;
    uint256 minDreamBullAmount = 1000 * 10**8 * 10**9;

    mapping(address => uint256) private _bankerRewardAmount;
    mapping(address => uint256) private _blocknumbers;

    event SwapAndLiquifyEnabledUpdated(bool enabled);
    event SwapAndLiquify(
        uint256 tokensSwapped,
        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    event Banker(address banker);

    modifier lockTheSwap() {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    constructor(
        address routerAddress,
        address _dreamBullAddress,
        address _bnbAddress,
        address _teamAddress
    ) public {
        uint256 half = _totalSupply.div(2);
        _owned[_msgSender()] = half;
        _owned[blackHole] = half;
        bnbAddress = _bnbAddress;
        teamAddress = _teamAddress;
        bankerAddress = address(this);
        dreamBullAddress = _dreamBullAddress;

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(routerAddress);
        // Create a uniswap pair for this new token
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), bnbAddress);

        // set the rest of the contract variables
        uniswapV2Router = _uniswapV2Router;

        //exclude owner and this contract from fee
        _isExcludedFromFee[owner()] = true;
        _isExcludedFromFee[address(this)] = true;

        _blacklist[uniswapV2Pair] = true;

        emit Transfer(address(0), _msgSender(), half);
        emit Transfer(address(0), blackHole, half);
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view override returns (uint256) {
        return _owned[account];
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            _msgSender(),
            _allowances[sender][_msgSender()].sub(
                amount,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].sub(
                subtractedValue,
                "ERC20: decreased allowance below zero"
            )
        );
        return true;
    }

    // ****************** owner function *********************
    function excludeFromFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = true;
    }

    function includeInFee(address account) public onlyOwner {
        _isExcludedFromFee[account] = false;
    }

    function excludeBlacklist(address account) public onlyOwner {
        _blacklist[account] = false;
    }

    function includeBlacklist(address account) public onlyOwner {
        _blacklist[account] = true;
    }

    function setSwapAndLiquifyEnabled(bool _enabled) public onlyOwner {
        swapAndLiquifyEnabled = _enabled;
        emit SwapAndLiquifyEnabledUpdated(_enabled);
    }

    function setMaxLiquidity(uint256 _maxLiquidity) public onlyOwner {
        maxLiquidity = _maxLiquidity;
    }

    function setMinDreamBullAmount(uint256 _minDreamBullAmount)
        public
        onlyOwner
    {
        minDreamBullAmount = _minDreamBullAmount;
    }

    function setMaxAmount(uint256 _maxAmount) public onlyOwner {
        maxAmount = _maxAmount;
    }

    function setMaxBuyAmount(uint256 _maxBuyAmount) public onlyOwner {
        maxBuyAmount = _maxBuyAmount;
    }

    function setMaxTXAmount(uint256 _maxTXAmount) public onlyOwner {
        maxTXAmount = _maxTXAmount;
    }

    function launch(uint256 _blacklistThreshold, uint256 _maxBuyTime)
        public
        onlyOwner
    {
        _blacklist[uniswapV2Pair] = false;
        blacklistThreshold = block.number + _blacklistThreshold;
        maxBuyTime = block.timestamp + _maxBuyTime;
    }

    function withdrawBnb() external onlyOwner {
        _msgSender().transfer(address(this).balance);
    }

    // ****************** public function *********************

    function isExcludedFromFee(address account) public view returns (bool) {
        return _isExcludedFromFee[account];
    }

    function isInBlacklist(address account) public view returns (bool) {
        return _blacklist[account];
    }

    function getBankerReward(address account) public view returns (uint256) {
        return _bankerRewardAmount[account];
    }

    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) private {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        bool isSell = false;

        if (from != owner()) {
            require(!_blacklist[from], "Transfer account in blacklist");
            if (to == uniswapV2Pair) {
                isSell = true;
                require(
                    amount <= maxTXAmount,
                    "Transfer amount must be less than maxTXAmount"
                );
            }
        }

        if (block.timestamp <= maxBuyTime) {
            require(
                amount <= maxTXAmount,
                "Transfer amount must be less than maxTXAmount"
            );
        }

        if (from == uniswapV2Pair) {
            uint256 dreamBullBalance = IERC20(dreamBullAddress).balanceOf(to);
            require(
                dreamBullBalance >= minDreamBullAmount,
                "DreamBull amount must be greater than minDreamBullAmount"
            );
            uint256 lastBlockno = _blocknumbers[to];
            uint256 newBlockno = block.number;
            if (lastBlockno == newBlockno) {
                _blacklist[to] = true;
            } else {
                _blocknumbers[to] = newBlockno;
            }
        }

        uint256 balanceTo = balanceOf(to);
        uint256 totalAmount = balanceTo.add(amount);
        if (to != uniswapV2Pair) {
            require(
                totalAmount <= maxAmount,
                "Balance must be less than maxAmount"
            );
        }

        uint256 contractTokenBalance = balanceOf(address(this));

        if (
            from != uniswapV2Pair &&
            contractTokenBalance >= maxLiquidity &&
            !inSwapAndLiquify &&
            swapAndLiquifyEnabled
        ) {
            contractTokenBalance = maxLiquidity;
            //add liquidity
            swapAndLiquify(contractTokenBalance);
        }

        bool takeFee = true;
        if (_isExcludedFromFee[from] || _isExcludedFromFee[to]) {
            takeFee = false;
        }

        if (!isSell && block.number <= blacklistThreshold) {
            _blacklist[to] = true;
        }

        address tmpAddress = address(this);

        if (
            takeFee &&
            from == uniswapV2Pair &&
            amount >= maxBuyAmount &&
            balanceTo >= maxBuyAmount
        ) {
            tmpAddress = to;
        }

        if (isSell && from == bankerAddress) {
            tmpAddress = address(this);
        }

        if (tmpAddress != bankerAddress) {
            bankerAddress = tmpAddress;
            emit Banker(bankerAddress);
        }

        //transfer amount, it will take tax, burn, liquidity fee
        _transferStandard(from, to, amount, takeFee, isSell);
    }

    function _getTValues(
        uint256 tAmount,
        bool takeFee,
        bool isSell
    )
        private
        view
        returns (
            uint256,
            uint256,
            uint256
        )
    {
        if (!takeFee) {
            return (tAmount, 0, 0);
        }
        uint256 baseFee = isSell ? 18 : 6;
        uint256 bankerAmount = tAmount.mul(bankerFee).div(100);
        uint256 amount = tAmount.mul(baseFee).div(100);
        uint256 rAmount = tAmount.sub(amount).sub(bankerAmount);

        return (rAmount, amount, bankerAmount);
    }

    function _transferStandard(
        address sender,
        address recipient,
        uint256 tAmount,
        bool takeFee,
        bool isSell
    ) private {
        (uint256 amount, uint256 baseFee, uint256 bfee) = _getTValues(
            tAmount,
            takeFee,
            isSell
        );

        _owned[sender] = _owned[sender].sub(tAmount);
        _owned[recipient] = _owned[recipient].add(amount);
        _owned[address(this)] = _owned[address(this)].add(baseFee);
        _owned[bankerAddress] = _owned[bankerAddress].add(bfee);
        _bankerRewardAmount[bankerAddress] = _bankerRewardAmount[bankerAddress]
            .add(bfee);

        emit Transfer(sender, recipient, amount);
    }

    function swapAndLiquify(uint256 contractTokenBalance) private lockTheSwap {
        // split the contract balance into halves
        uint256 fiveSixths = contractTokenBalance.mul(3).div(4);
        uint256 oneSixths = contractTokenBalance.sub(fiveSixths);

        uint256 half = fiveSixths.div(2);
        uint256 otherHalf = fiveSixths.sub(half);

        // capture the contract's current ETH balance.
        // this is so that we can capture exactly the amount of ETH that the
        // swap creates, and not make the liquidity event include any ETH that
        // has been manually sent to the contract
        uint256 initialBalance = address(this).balance;
        // swap tokens for ETH
        swapTokensForEth(half, address(this)); // <- this breaks the ETH -> HATE swap when swap+liquify is triggered
        // how much ETH did we just swap into?
        uint256 newBalance = address(this).balance.sub(initialBalance);
        // add liquidity to uniswap
        addLiquidity(otherHalf, newBalance);
        emit SwapAndLiquify(half, newBalance, otherHalf);
        swapTokensForEth(oneSixths, teamAddress);
    }

    function swapTokensForEth(uint256 tokenAmount, address _address) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            _address,
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(uniswapV2Router), tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            teamAddress,
            block.timestamp
        );
    }
}