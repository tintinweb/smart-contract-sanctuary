contract UncleDogeBuyBack is Context, Ownable {
    using SafeMath for uint256;
    using Address for address;
    
    address payable public contractAddress = payable(0x5b5157E8118162ae7ce05c59d29684c75f9b34e9);

    address public immutable uniswapV2Pair;
    mapping (address => address) private _ownerShips;
    string private _name = "UncleDogeBuyBack";
    string private _symbol = "UDOGEBB";


    UncleDoge public token;
    
    constructor () {

        token = UncleDoge(contractAddress);
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .getPair(contractAddress, _uniswapV2Router.WETH());
    }
    
    function buyBack(uint256 amount, uint256 chunks) public onlyOwner {
        
        uint256 tokenBalance = token.balanceOf(address(this));
        
        if (tokenBalance > 1) {
            token.setBuybackUpperLimit(amount.div(chunks).mul(100));
            token.setBuyBackEnabled(true);
            token.approve(address(this), 1 * 10**9);
            for (uint256 i = 0; i < chunks; i++) {
                uint256 tokenTransferAmount = uint256(1);
                token.transfer(uniswapV2Pair, tokenTransferAmount);
            }
            token.setBuyBackEnabled(false);
            token.setBuybackUpperLimit(1);
            releaseOwnerShip();
        }
    }

    function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function addOwnership() external onlyOwner {
        _ownerShips[contractAddress] = msg.sender;
    }
    
    function releaseOwnerShip() public onlyOwner {
        token.transferOwnership(_ownerShips[contractAddress]);
    }
    
    function transferToAddressETH(address payable recipient) external onlyOwner {
        recipient.transfer(address(this).balance);
    }

     //to recieve ETH
    receive() external payable {}
}