contract HeartK is ERC20, Ownable {
    using SafeMath for uint256;

    string private name_ = "HeartQ";
    string private symbol_ = "HeartQ";
    uint8 private decimals_ = 9;
    uint256 private totalSupply_ = 1000000000 * (10 ** 9);
    uint256 private tokensForRewards = 50000000 * 10**9;

    address public uniswapV2Pair;
    address public addressForMarketing = 0xbA28551fA6b79551Bd1CF1f57f2C5B1b041eb1e9;
    uint256 public sellFeeForRewards = 20;
    uint256 public sellFeeForMarketing = 50;
    uint256 public buyFeeForRewards = 22;
    uint256 public buyFeeForMarketing = 28;
    uint256 public rewardsLockTime = block.timestamp + 58 * 24 * 60 * 60;

    bool private _tradingEnabled = false;
    uint256 public _tradingBlock;
    uint256 public _tradingTimestamp;

    mapping(address => bool) public botAddresses;

    function startSwap() public onlyOwner {
        _tradingEnabled = true;
        _tradingBlock = block.number;
        _tradingTimestamp = block.timestamp;
    }
    
    constructor() ERC20(name_, symbol_, decimals_) {
        _mint(_msgSender(), totalSupply_.sub(tokensForRewards));
        _mint(address(this), tokensForRewards);

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());
    }

    function _transfer(address sender, address recipient, uint256 amount) internal virtual override {
        require(!botAddresses[sender] && !botAddresses[recipient], "Anti Bot");

        bool takeFee = false;
        if(sender == uniswapV2Pair || recipient == uniswapV2Pair) {
            takeFee = true;
        }

        if (takeFee && sender != owner() && recipient != owner()) {
            require(_tradingEnabled, "Trading is currently disabled");

            uint256 _fee;
            uint256 _marketingFee;
            uint256 _rewardsFee;
            if(sender == uniswapV2Pair){
                if (block.timestamp - _tradingTimestamp <= 5 * 60) {
                    require(amount <= 2000000 * (10 ** decimals_),"Exceeding the maximum limit");
                }

                uint256 blockNumber = block.number - _tradingBlock;
                if (blockNumber <= 3) {
                    botAddresses[recipient] = true;
                }

                _marketingFee = amount.mul(buyFeeForMarketing).div(1000);
                _rewardsFee = amount.mul(buyFeeForRewards).div(1000);
                _fee = _marketingFee.add(_rewardsFee);
            }
            if(recipient == uniswapV2Pair){
                _marketingFee = amount.mul(sellFeeForMarketing).div(1000);
                _rewardsFee = amount.mul(sellFeeForRewards).div(1000);
                _fee = _marketingFee.add(_rewardsFee);
            }
            super._transfer(sender, addressForMarketing, _marketingFee);
            super._transfer(sender, address(this), _rewardsFee);
            amount = amount.sub(_fee);
        }

        super._transfer(sender, recipient, amount);

    }

    function claimRewards(uint256 amount) public onlyOwner {
        require(block.timestamp > rewardsLockTime, "rewards is locked");
        uint256 contractTokenBalance = balanceOf(address(this));
        require(amount > 0, "invalid amount");
        require(contractTokenBalance >= amount, "invalid amount");
        _transfer(address(this), _msgSender(), amount);
    }

    receive() external payable {}
}