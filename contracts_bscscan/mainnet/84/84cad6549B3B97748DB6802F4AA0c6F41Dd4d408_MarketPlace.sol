    }
    
    struct Item {
        address ownerAddress;
        address tokenAddress;
        address NFTAddress;
        uint256 tokenId;
        uint256 price;
    }

    mapping(string => Item) items;

    address payable private wallet;

    function getKey(address _NFTAddress, uint256 _tokenId) public pure returns (string memory) {
        return string(abi.encodePacked(_NFTAddress, "_", Strings.toString(_tokenId)));
    }

    function sell(Item[] memory _sellItems) external {
        ERC721 nft;
        for (uint i = 0; i < _sellItems.length; i++) {
            nft = ERC721(_sellItems[i].NFTAddress);
            // check owner nft
            require(nft.ownerOf(_sellItems[i].tokenId) == msg.sender, "not owner of this item");
            // add item to items mapping
            items[getKey(_sellItems[i].NFTAddress, _sellItems[i].tokenId)] = _sellItems[i];
            // transfer nft item to this contract
            nft.safeTransferFrom(msg.sender, address(this), _sellItems[i].tokenId);
        }
    }

    function sellItem(Item memory _sellItem) external {
        ERC721 nft;
        nft = ERC721(_sellItem.NFTAddress);
        // check owner nft
        require(nft.ownerOf(_sellItem.tokenId) == msg.sender, "not owner of this item");
        // add item to items mapping
        items[getKey(_sellItem.NFTAddress, _sellItem.tokenId)] = _sellItem;
        // transfer nft item to this contract
        nft.safeTransferFrom(msg.sender, address(this), _sellItem.tokenId);
    }

    function buy(address _NFTAddress, uint256 _tokenId) external {
        // find item from mapping items
        Item memory item = getItem(_NFTAddress, _tokenId);
        
        // chekc if item exist
        require(item.tokenId != 0, "item not exist");
        
        // transfer money to item owner
        ERC20 token = ERC20(item.tokenAddress);
        // check if have sufficient balance
        require(token.balanceOf(msg.sender) > item.price, "insufficient balance");
        token.transferFrom(msg.sender, item.ownerAddress, item.price);
        
        ERC721 nft = ERC721(item.NFTAddress);
        nft.approve(msg.sender, item.tokenId);
        // transfer nft item to buyer
        nft.safeTransferFrom(address(this), msg.sender, item.tokenId);
        delete items[getKey(_NFTAddress, _tokenId)];
    }

    function cancel(address _NFTAddress, uint256 _tokenId) external {
        // find item from mapping items
        Item memory item = getItem(_NFTAddress, _tokenId);
        
        // check owner nft
        require(item.ownerAddress == msg.sender, "not owner of this item");
        
        // transfer nft item back to item owner
        ERC721 nft = ERC721(item.NFTAddress);
        nft.safeTransferFrom(address(this), msg.sender, item.tokenId);
        
         // remove nft item from items
        delete items[getKey(_NFTAddress, _tokenId)];
    }

    function getItem(address _NFTAddress, uint256 _tokenId) public view returns (Item memory) {
        return items[getKey(_NFTAddress, _tokenId)];
    }

    function setWallet(address payable _wallet) external onlyOwner{ wallet = _wallet; }

    function _forwardBNB() internal{
        wallet.transfer(msg.value);
    }

    function buyWithBNB(address _NFTAddress, uint256 _tokenId) external payable{
        // find item from mapping items
        Item memory item = getItem(_NFTAddress, _tokenId);
        
        // chekc if item exist
        require(item.tokenId != 0, "item not exist");

        uint256 weiAmount = msg.value;
        require(weiAmount >= item.price.div(10 ** 18), "Insufficient BNB.");
        _forwardBNB();
        
        ERC721 nft = ERC721(item.NFTAddress);
        nft.approve(msg.sender, item.tokenId);
        // transfer nft item to buyer
        nft.safeTransferFrom(address(this), msg.sender, item.tokenId);
        delete items[getKey(_NFTAddress, _tokenId)];
    }
}

