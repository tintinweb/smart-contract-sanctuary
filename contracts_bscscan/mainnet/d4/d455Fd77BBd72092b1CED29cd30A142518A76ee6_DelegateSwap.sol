contract AccessController {
    address public ownerAddress;
    address public workerAddress;

    bool public paused = false;

    // mapping (address => enumRoles) accessRoles; // multiple operators idea

    event OwnerSet(address newOwner);
    event WorkerSet(address newWorker);

    event Paused();
    event Unpaused();

    constructor() {
        ownerAddress = msg.sender;
        workerAddress = msg.sender;
        emit OwnerSet(ownerAddress);
        emit WorkerSet(workerAddress);
    }

    modifier onlyOwner() {
        require(msg.sender == ownerAddress, "AccessControl: Owner access denied");
        _;
    }

    modifier onlyWorker() {
        require(msg.sender == workerAddress, "AccessControl: worker access denied");
        _;
    }

    modifier whenNotPaused() {
        require(!paused, "AccessControl: currently paused");
        _;
    }

    modifier whenPaused {
        require(paused, "AccessControl: currenlty not paused");
        _;
    }

    function setOwner(address _newOwner) public onlyOwner {
        require(_newOwner != address(0x0), "AccessControl: invalid Owner address");
        ownerAddress = _newOwner;
        emit OwnerSet(ownerAddress);
    }

    function setWorker(address _newWorker) external {
        require(_newWorker != address(0x0), "AccessControl: invalid worker address");
        require(msg.sender == ownerAddress || msg.sender == workerAddress, "AccessControl: invalid worker address");
        workerAddress = _newWorker;
        emit WorkerSet(workerAddress);
    }

    function pause() external onlyWorker whenNotPaused {
        paused = true;
        emit Paused();
    }

    function unpause() external onlyOwner whenPaused {
        paused = false;
        emit Unpaused();
    }
}

contract RoutingManagement is AccessController {
    struct Route {
        string name;
        bool enable;
        IPancakeRouter02 route;
    }

    event AddedTradingRoute(
        address indexed addedBy,
        string name,
        IPancakeRouter02 indexed routingAddress,
        uint256 indexed index
    );

    event EnabledTradingRoute(
        address indexed enabledBy,
        string name,
        IPancakeRouter02 indexed routingAddress,
        uint256 indexed index
    );

    event DisabledTradingRoute(
        address indexed disabledBy,
        string name,
        IPancakeRouter02 indexed routingAddress,
        uint256 indexed index
    );

    Route[] public tradingRoutes; // list of trading routes

    modifier onlyTradingRouteEnabled(uint256 _index) {
        require(tradingRoutes[_index].enable == true, "This trading route is disabled");
        _;
    }

    modifier onlyTradingRouteDisabled(uint256 _index) {
        require(tradingRoutes[_index].enable == false, "This trading route is enabled");
        _;
    }

    function addTradingRoute(string memory _name, IPancakeRouter02 _routingAddress) public onlyOwner {
        tradingRoutes.push(Route({ name: _name, enable: true, route: _routingAddress }));
        emit AddedTradingRoute(msg.sender, _name, _routingAddress, tradingRoutes.length - 1);
    }

    function disableTradingRoute(uint256 _index) public onlyOwner onlyTradingRouteEnabled(_index) {
        tradingRoutes[_index].enable = false;
        emit DisabledTradingRoute(msg.sender, tradingRoutes[_index].name, tradingRoutes[_index].route, _index);
    }

    function enableTradingRoute(uint256 _index) public onlyOwner onlyTradingRouteDisabled(_index) {
        tradingRoutes[_index].enable = true;
        emit EnabledTradingRoute(msg.sender, tradingRoutes[_index].name, tradingRoutes[_index].route, _index);
    }

    function allRoutesLength() public view returns (uint256) {
        return tradingRoutes.length;
    }

    function isTradingRouteEnabled(uint256 _index) public view returns (bool) {
        return tradingRoutes[_index].enable;
    }
}

contract DelegateSwap is AccessController, ReentrancyGuard, RoutingManagement {
    using SafeERC20 for IERC20;

    event DelegateBuy(address srcToken, address desToken, address user, uint256 amount);

    address public deployer;
    mapping(address => uint256) public ethDeposit;

    constructor(
        string memory _routerName,
        IPancakeRouter02 _router
    ) {
        deployer = msg.sender;
        addTradingRoute(_routerName, _router);
    }

    function deposit() external payable nonReentrant {
        ethDeposit[msg.sender] += msg.value;
    }

    function withdraw() external nonReentrant {
        uint256 _amount = ethDeposit[msg.sender];
        ethDeposit[msg.sender] = 0;
        payable(msg.sender).transfer(_amount);
    }

    function withdrawExcessTokens(address _token) external onlyOwner {
        IERC20 token = IERC20(_token);
        uint256 amount = IERC20(token).balanceOf(address(this));
        if (amount > 0) {
            IERC20(token).transfer(msg.sender, amount);
        }
    }

    function delegateTokensForTokens(
        uint256 routerIndex,
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address user,
        uint256 deadline
    ) external onlyWorker onlyTradingRouteEnabled(routerIndex) {
        console.log("===delegates tokens for tokens ===");
        IERC20 _srctoken = IERC20(path[0]);

        require(_srctoken.balanceOf(user) >= amountIn, "Insufficient balance.");
        require(_srctoken.allowance(user, address(this)) >= amountIn, "Need Approval balance.");

        _srctoken.safeTransferFrom(user, address(this), amountIn);
        console.log("transfer token1 to contract");
        IPancakeRouter02 _route = tradingRoutes[routerIndex].route;
        console.log(tradingRoutes[routerIndex].name);
        _srctoken.safeApprove(address(_route), amountIn);
        console.log("approved to router");
        _route.swapExactTokensForTokensSupportingFeeOnTransferTokens(amountIn, amountOutMin, path, user, deadline);
        console.log("=== done swapping=== ");
    }

    function delegateETHForTokens(
        uint256 routerIndex,
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address user,
        uint256 deadline
    ) external payable onlyWorker onlyTradingRouteEnabled(routerIndex) {
        console.log("===delegates eth for tokens ===");

        require(ethDeposit[user] >= amountIn, "Insufficient balance.");

        ethDeposit[user] -= amountIn;

        IPancakeRouter02 _route = tradingRoutes[routerIndex].route;
        console.log(tradingRoutes[routerIndex].name);
        _route.swapExactETHForTokensSupportingFeeOnTransferTokens{ value: amountIn }(
            amountOutMin,
            path,
            user,
            deadline
        );
        console.log("=== done swapping=== ");
    }

    function delegateTokensForEth(
        uint256 routerIndex,
        uint256 amountIn,
        uint256 amountOutMin,
        address[] calldata path,
        address user,
        uint256 deadline
    ) external onlyWorker onlyTradingRouteEnabled(routerIndex) {
        console.log("===delegates token for Eth ===");
        IERC20 _srctoken = IERC20(path[0]);

        require(_srctoken.balanceOf(user) >= amountIn, "Insufficient balance.");
        require(_srctoken.allowance(user, address(this)) >= amountIn, "Need Approval balance.");

        _srctoken.safeTransferFrom(user, address(this), amountIn);

        IPancakeRouter02 _route = tradingRoutes[routerIndex].route;
        console.log(tradingRoutes[routerIndex].name);
        _srctoken.safeApprove(address(_route), amountIn);
        console.log("approved to router");
        _route.swapExactTokensForETHSupportingFeeOnTransferTokens(amountIn, amountOutMin, path, user, deadline);
        console.log("=== done swapping=== ");
    }
}

