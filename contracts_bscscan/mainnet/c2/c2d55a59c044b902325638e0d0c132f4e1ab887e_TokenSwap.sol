contract TokenSwap {
    
    using SafeMath for uint256;
     
    mapping (address => uint256) public holders;
    
    uint256 public countholders;
    
    address public owner;
    
    IBEP20 public naut;
    IBEP20 public newtoken;
    uint256 public newtokendecimal;
    
    bool public tokenInitialize;
    
    bool public claimstart;
    
    constructor() public {
        
        owner = msg.sender;
        IBEP20 _nauttoken = IBEP20(0x05B339B0A346bF01f851ddE47a5d485c34FE220c);
        naut = _nauttoken;
        
    }
    
    
        modifier onlyOwner() {
            require(msg.sender == owner);
            _;
    }  
    
    
      modifier isclaimStart() {
        require(claimstart == true, "sorry, Claim not Start yet");
        _;
    }


    function submitToken(uint256 _amount) public {
        
    require (_amount > 0, "Min token 1");    
        
    if (holders[msg.sender] == 0 ) {
        
    countholders += 1;    
    }   
    
    holders[msg.sender] = holders[msg.sender].add(_amount);   

    naut.transferFrom(msg.sender,address(this),_amount);   
        
    }
    
    
    function withdrawnaut(address _add, uint256 _amount) public onlyOwner {
        
    require(naut.balanceOf(address(this)) >= _amount, "Insufficient token");    

    naut.transfer(_add,_amount);
        
    }
    
    
    function withdrawnnewtoken(address _add, uint256 _amount) public onlyOwner {
        
    require(newtoken.balanceOf(address(this)) >= _amount, "Insufficient token");    

    newtoken.transfer(_add,_amount);
        
    }
    
    
    
    function Initializetoken(address _tokenadd, uint256 decimal) public onlyOwner {
        
        IBEP20 _newtoken = IBEP20(_tokenadd);
        newtoken = _newtoken;
        newtokendecimal = decimal;
        tokenInitialize = true;
        
    }
    
    
    function claimnewToken() public isclaimStart {
        
        require(tokenInitialize == true, "New token not initialize");
        require (holders[msg.sender] > 0, "No previous deposit");
        
        uint256 tamount = holders[msg.sender] * 10 ** 10;
        
        require(newtoken.balanceOf(address(this)) >= tamount, "Insufficient token for claim");
        
         newtoken.transfer(msg.sender,tamount);
         
         holders[msg.sender] = 0;
        
    }
    
    function startclaim() public onlyOwner {
        
        claimstart = true;
    }
    
    function stopclaim() public onlyOwner {
        
        claimstart = false;
    }
}