pragma solidity 0.7.6;
pragma abicoder v2;

import '@openzeppelin/contracts/token/ERC20/SafeERC20.sol';
import '@openzeppelin/contracts/token/ERC20/IERC20.sol';
import '@openzeppelin/contracts/token/ERC20/ERC20.sol';
import '@openzeppelin/contracts/math/SafeMath.sol';
import '../interfaces/ITokenLockerV2.sol';
import '../interfaces/IP2EToken.sol';
import '../core/SafeOwnable.sol';
import '../invite/Invite.sol';
import 'hardhat/console.sol';
import './TokenLocker.sol';

contract TokenLockerV2 is ERC20, SafeOwnable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    event NewReceiver(address receiver, uint sendAmount, uint totalReleaseAmount, uint lastReleaseAt);
    event ReleaseToken(address receiver, uint releaseAmount, uint nextReleaseAmount, uint nextReleaseBlockNum);
    event InviterReward(address user, IERC20 _token, address invitee, uint relation, uint amount);
    event ClaimReward(address user, IERC20 token, uint amount);

    struct ReleaseInfo {
        address receiver;               //who will receive the release token
        uint256 totalAmount;            //total receive amount
        uint256 remainAmount;           //remain to unlock
        uint256 releaseAmount;          //release amount every period
        uint256 loop;                   //which loop
        uint256 lastReleaseAt;          //the last seconds the the receiver get the released token
        uint256 pending;                //remain to claim
        uint256 inviterTotalAmount;     //receive total amount as inviter
        uint256 inviterPending;         //remain to claim as inviter
    }
    mapping(address => ReleaseInfo) public receivers;

    Invite immutable public invite;
    TokenLocker public tokenLockerV1;
    uint256 public immutable LOCK_PERIOD;
    uint256 public immutable LOCK_PERIOD_NUM;
    IERC20 public immutable token;
    uint256 public totalLockAmount;
    
    uint256 constant VALUE_FEE_BASE = 10000;
    uint256 public returnRelationNum;
    mapping(uint => uint) public returnPercent;

    mapping(address => bool) public minters;

    function setReturnRelationNum(uint _returnRelationNum) external onlyOwner {
        returnRelationNum = _returnRelationNum;
    }

    function setReturnPercent(uint _relation, uint _percent) external onlyOwner {
        require(_relation <= returnRelationNum, "illegal relation");
        require(_percent <= VALUE_FEE_BASE, "illegal percent");
        returnPercent[_relation] = _percent;
    }

    function addMinter(address _minter) external onlyOwner {
        minters[_minter] = true;
    }

    function delMinter(address _minter) external onlyOwner {
        delete minters[_minter];
    }

    modifier onlyOwnerOrMinter() {
        require(owner() == _msgSender() || minters[_msgSender()], "caller is not the minter");
        _;
    }

    function setTokenLockerV1(TokenLocker _tokenLockerV1) external onlyOwner {
        tokenLockerV1 = _tokenLockerV1;
    }

    constructor(
        string memory _name, string memory _symbol, 
        IERC20 _token, uint256 _lockPeriod, uint256 _lockPeriodNum, 
        Invite _invite, TokenLocker _tokenLockerV1
    ) ERC20(_name, _symbol) SafeOwnable(msg.sender) {
        require(address(_token) != address(0), "token address is zero");
        token = _token;
        LOCK_PERIOD = _lockPeriod;
        LOCK_PERIOD_NUM = _lockPeriodNum;
        invite = _invite;
        tokenLockerV1 = _tokenLockerV1;
        returnRelationNum = 3;
        returnPercent[1] = 1000;
        returnPercent[2] = 300;
        returnPercent[3] = 200;
    }

    function claimInternal(address _receiver) internal returns(uint) {
        ReleaseInfo storage releaseInfo = receivers[_receiver];
        if (releaseInfo.receiver == address(0) || block.timestamp <= releaseInfo.lastReleaseAt) {
            return 0;
        }
        uint interval = block.timestamp.sub(releaseInfo.lastReleaseAt);
        uint loop = interval.div(LOCK_PERIOD);
        if (loop == 0) {
            return 0;
        }
        uint releaseAmount = releaseInfo.releaseAmount.mul(loop);
        if (releaseAmount > releaseInfo.remainAmount) {
            releaseAmount = releaseInfo.remainAmount;
        }
        if (releaseAmount > 0) {
            releaseInfo.pending = releaseInfo.pending.add(releaseAmount);
            releaseInfo.remainAmount = releaseInfo.remainAmount.sub(releaseAmount);
            releaseInfo.lastReleaseAt = releaseInfo.lastReleaseAt.add(loop.mul(LOCK_PERIOD));
        }
        releaseInfo.loop = releaseInfo.loop.add(loop);
    }

    function addReceiver(address _receiver, uint256 _amount) external onlyOwnerOrMinter {
        claimInternal(_receiver);
        require(_receiver != address(0), "receiver address is zero");
        require(_amount > 0, "release amount is zero");
        totalLockAmount = totalLockAmount.add(_amount);
        ReleaseInfo storage releaseInfo = receivers[_receiver];
        releaseInfo.totalAmount = releaseInfo.totalAmount.add(_amount);
        uint releaseAmount = _amount.div(LOCK_PERIOD_NUM);
        uint lockAmount = _amount.sub(releaseAmount);
        releaseInfo.receiver = _receiver;
        releaseInfo.remainAmount = releaseInfo.remainAmount.add(lockAmount);
        releaseInfo.releaseAmount = releaseInfo.remainAmount.div(LOCK_PERIOD_NUM - 1);
        releaseInfo.lastReleaseAt = block.timestamp;
        releaseInfo.loop = 1;
        releaseInfo.pending = releaseInfo.pending.add(releaseAmount);
        token.safeTransferFrom(msg.sender, address(this), _amount);
        emit NewReceiver(_receiver, _amount, releaseInfo.remainAmount, releaseInfo.lastReleaseAt);
    }

    function pending(address _user) external view returns (uint lastClaim, uint amount, uint remain) {
        uint v1Pending = 0;
        uint v1Remain = 0;
        if (address(tokenLockerV1) != address(0)) {
            (, v1Pending, v1Remain) = tokenLockerV1.pending(_user);
        }
        ReleaseInfo storage releaseInfo = receivers[_user];
        amount = amount.add(releaseInfo.pending);
        uint interval = block.timestamp.sub(releaseInfo.lastReleaseAt);
        uint loop = interval.div(LOCK_PERIOD);
        uint releaseAmount;
        if (loop > 0) {
            releaseAmount = releaseInfo.releaseAmount.mul(loop);
            if (releaseAmount > releaseInfo.remainAmount) {
                releaseAmount = releaseInfo.remainAmount;
            }
            amount = amount.add(releaseAmount);
        }
        address[] memory inviters = invite.inviterTree(_user, returnRelationNum);
        uint totalPercent = 0;
        for (uint i = 0; i < inviters.length; i ++) {
            if (inviters[i] != address(0)) {
                totalPercent = totalPercent.add(returnPercent[i + 1]);
            }
        }
        amount = amount.sub(amount.mul(totalPercent).div(VALUE_FEE_BASE));
        remain = remain.add(releaseInfo.remainAmount.sub(releaseAmount));
        if (releaseInfo.lastReleaseAt == 0) {
            lastClaim = block.timestamp;
        } else {
            lastClaim = releaseInfo.lastReleaseAt;
        }
        amount = amount.add(v1Pending);
        remain = remain.add(v1Remain);
    }

    function claim(address _user) external {
        if (address(tokenLockerV1) != address(0)) {
            tokenLockerV1.claim(_user);
        }
        claimInternal(_user);
        ReleaseInfo storage releaseInfo = receivers[_user];
        uint pending = releaseInfo.pending;
        address[] memory inviters = invite.inviterTree(_user, returnRelationNum);
        uint invitetAmount = 0;
        for (uint i = 0; i < inviters.length; i ++) {
            if (inviters[i] == address(0)) {
                continue;
            }
            uint amount = pending.mul(returnPercent[i + 1]).div(VALUE_FEE_BASE);
            invitetAmount = invitetAmount.add(amount);
            if (amount > 0) {
                ReleaseInfo storage inviterReleaseInfo = receivers[inviters[i]]; 
                inviterReleaseInfo.receiver = inviters[i];
                inviterReleaseInfo.inviterTotalAmount = inviterReleaseInfo.inviterTotalAmount.add(amount);
                inviterReleaseInfo.inviterPending = inviterReleaseInfo.inviterPending.add(amount);
                emit InviterReward(inviters[i], token, _user, i, amount);
            }
        }
        pending = pending.sub(invitetAmount);
        releaseInfo.pending = 0;
        if (pending > 0) {
            token.safeTransfer(_user, pending);
            totalLockAmount = totalLockAmount.sub(pending);
        }
    }

    function inviterClaim(address _user) external {
        ReleaseInfo storage releaseInfo = receivers[_user]; 
        if (releaseInfo.inviterPending > 0) {
            token.safeTransfer(_user, releaseInfo.inviterPending);
            totalLockAmount = totalLockAmount.sub(releaseInfo.inviterPending);
            emit ClaimReward(_user, token, releaseInfo.inviterPending);
            releaseInfo.inviterPending = 0;
        }
    }

    function inviterPending(address _user) external view returns (ITokenLockerV2.RewardInfo[] memory rewards) {
        rewards = new ITokenLockerV2.RewardInfo[](1); 
        ReleaseInfo storage releaseInfo = receivers[_user];
        rewards[0].token = token;
        rewards[0].totalAmount = rewards[0].totalAmount.add(releaseInfo.inviterTotalAmount);
        rewards[0].pendingAmount = rewards[0].pendingAmount.add(releaseInfo.inviterPending);
    }
}

