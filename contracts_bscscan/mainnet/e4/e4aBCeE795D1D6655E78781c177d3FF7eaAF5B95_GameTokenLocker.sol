import '../interfaces/IP2EToken.sol';
import '../core/SafeOwnable.sol';
import '../invite/Invite.sol';
import 'hardhat/console.sol';
import './TokenLockerV2.sol';

contract GameTokenLocker is SafeOwnable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    IERC20[] public defaultListTokens;
    mapping(IERC20 => uint) defaultListTokenMap;
    Invite public invite;

    function addDefaultListToken(IERC20 _token) public onlyOwner {
        for (uint i = 0; i < defaultListTokens.length; i ++) {
            require(defaultListTokens[i] != _token, "illegal token");
        }
        defaultListTokens.push(_token);
        defaultListTokenMap[_token] = defaultListTokens.length;
    }

    function delDefaultListToken(uint i) external onlyOwner {
        IERC20 token = defaultListTokens[i];
        delete defaultListTokens[i];
        delete defaultListTokenMap[token];
    }

    function defaultListTokensLength() external view returns (uint) {
        return defaultListTokens.length;
    }

    function setInvite(Invite _invite) external onlyOwner {
        invite = _invite;
    }

    function inviterClaim(address _user) external {
        if (address(invite) != address(0)) {
            invite.claim(_user, defaultListTokens);
        }
    }

    function inviterPending(address _user) external view returns (ITokenLockerV2.RewardInfo[] memory rewards) {
        rewards = new ITokenLockerV2.RewardInfo[](defaultListTokens.length);
        for (uint i = 0; i < defaultListTokens.length; i ++) {
            rewards[i].token = defaultListTokens[i];
        }
        if (address(invite) != address(0)) {
            uint[] memory currentRewards = invite.pending(_user, defaultListTokens);
            for (uint i = 0; i < defaultListTokens.length; i ++) {
                rewards[i].pendingAmount = rewards[i].pendingAmount.add(currentRewards[i]);
            }
        }
    }

    constructor(Invite _invite, IERC20[] memory _defaultListTokens) SafeOwnable(msg.sender) {
        invite = _invite;
        for (uint i = 0; i < _defaultListTokens.length; i ++) {
            addDefaultListToken(_defaultListTokens[i]);
        }
    }
}

