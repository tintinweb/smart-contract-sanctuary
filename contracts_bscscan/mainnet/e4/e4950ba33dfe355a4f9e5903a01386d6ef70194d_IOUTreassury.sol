    }
     modifier onlyOperator {
        require(operators[msg.sender], "Operator: caller is not the operator");
        _;
    }
    function _swapbnb()external payable{
        
        IWETH(WETH).deposit{value: msg.value}();
                    address[] memory path = new address[](2);
        path[0] = WETH;
        path[1] =BUSD;
        _approveTokenIfNeeded(path[0], address(router));
        _approveTokenIfNeeded(path[1], address(router));
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(IERC20(WETH).balanceOf(address(this)), 0, path, address(this), block.timestamp);
    }
    function _swap(address swapToken, address desiredToken) external {
            address[] memory path = new address[](2);
        path[0] = swapToken;
        path[1] = desiredToken;
          _approveTokenIfNeeded(path[0], address(router));
    router.swapExactTokensForTokensSupportingFeeOnTransferTokens(IERC20(swapToken).balanceOf(address(this)), 0, path, address(this), block.timestamp);

    }
    function withdrawbnb(address payable bnbaddress) public onlyOwner{
        bnbaddress.transfer(address(this).balance);
    }
    function withdrawbusd(uint256 amount,address withdraw) public  onlyOwner{
          IERC20(BUSD).transfer(address(withdraw), amount);
    }
    function autowithdrawbusd(uint256 _amount,address withdraw) public  onlyOwner{
         depositamount[withdraw]=depositamount[withdraw].sub(_amount);
          IERC20(BUSD).transfer(address(withdraw), _amount);
    }
    function withdrawtoken(address token, uint256 amount,address withdraw) public  onlyOwner{
          IERC20(token).transfer(address(withdraw), amount);
    }
    function withdrawiou(uint256 amount,address withdraw) public onlyOwner{
          
          IERC20(IOU).transfer(address(withdraw), amount);
    }
    
    function changetechinical(address _address) public onlyOwner{
        require(_address!=address(0),"address must valid");
        technical=address(_address);
    }
    function changeIOU(address _IOU) public onlyOwner{
        require(_IOU!=address(0),"address must valid");
        IOU=address(_IOU);
    }
    
    function changeBUSD(address _BUSD) public onlyOwner{
        require(_BUSD!=address(0),"address must valid");
        BUSD=address(_BUSD);
    }
    
     function changemarketing(address _address) public onlyOwner{
        require(_address!=address(0),"address must valid");
        marketting=address(_address);
    }
     function changeinvestor(address _address) public onlyOwner{
        require(_address!=address(0),"address must valid");
        investor=address(_address);
    }
     function changecompany(address _address) public onlyOwner{
        require(_address!=address(0),"address must valid");
        company=address(_address);
    }
     function changerefferral(address _address) public onlyOwner{
        require(_address!=address(0),"address must valid");
        refferal=address(_address);
    }
    function changestakingaddress(address _stakingaddress) public onlyOwner{
        stakingaddress=address(_stakingaddress);
    }
    //distribution 
    function changetechinicalpercentage(uint256 _percentage) public onlyOwner{
        technicalfee=_percentage;
    }
    function changemarketingpercentage(uint256 _percentage) public onlyOwner{
        marketingfee=_percentage;
    }
    function changecompanypercentage(uint256 _percentage) public onlyOwner{
        companyfee=_percentage;
    }
    function changeinvestorpercentage(uint256 _percentage) public onlyOwner{
        investorfee=_percentage;
    }
    function changestakingpercentage(uint256 _percentage) public onlyOwner{
        stakingfee=_percentage;
    }
    function changedivisionpercentage(uint256 _percentage) public onlyOwner{
        division=_percentage;
    }
    function depositbusd(uint _amount) external payable{ 
          id++;
          registerTime[msg.sender] = block.timestamp;
          depositamount[msg.sender]=depositamount[msg.sender]+_amount;
          depositlist[id]=msg.sender;
          require(IERC20(BUSD).allowance(msg.sender, address(this)) >= _amount, 'BUSD: Input token is not approved');
          uint256 bal;
          IWETH(WETH).deposit{value: msg.value}();
          bal= IERC20(WETH).balanceOf(address(this));
          IERC20(WETH).safeTransfer(address(technical), bal);
          IERC20(BUSD).safeTransferFrom(msg.sender, address(this), _amount);
    }
    function depositiou(uint _amount) external payable{ 
          id++;
          registerTime[msg.sender] = block.timestamp;
          depositamount[msg.sender]=depositamount[msg.sender]+_amount;
          depositlist[id]=msg.sender;
          require(IERC20(IOU).allowance(msg.sender, address(this)) >= _amount, 'IOU: Input token is not approved');
          uint256 bal;
          IWETH(WETH).deposit{value: msg.value}();
          bal= IERC20(WETH).balanceOf(address(this));
          IERC20(WETH).safeTransfer(address(technical), bal);
          IERC20(IOU).safeTransferFrom(msg.sender, address(this), _amount);
    }
     function _returnAssets(address[] memory tokens) private {
        uint256 balance;
        for (uint256 i; i < tokens.length; i++) {
            balance = IERC20(tokens[i]).balanceOf(address(this));
            if (balance > 0) {
                if (tokens[i] == WETH) {
                    IWETH(WETH).withdraw(balance);
                    (bool success,) = msg.sender.call{value: balance}(new bytes(0));
                    require(success, 'IOU: BNB transfer failed');
                }
            }
        }
    }
  
    function distributionfee(address pancakeVault,uint256 _amount,address tokenIn) public onlyOperator{
         IERC20(BUSD).transfer(address(technical), _amount.mul(technicalfee).div(division));
         IERC20(BUSD).transfer(address(marketting), _amount.mul(marketingfee).div(division));
         IERC20(BUSD).transfer(address(investor), _amount.mul(investorfee).div(division));
         IERC20(BUSD).transfer(address(company), _amount.mul(companyfee).div(division));
         stakingsinglepair(pancakeVault,(_amount.mul(stakingfee).div(division)),tokenIn);
    }
    
    function distributionfeeOnly(uint256 _amount) public onlyOperator{
         IERC20(BUSD).transfer(address(technical), _amount.mul(technicalfee).div(division));
         IERC20(BUSD).transfer(address(marketting), _amount.mul(marketingfee).div(division));
         IERC20(BUSD).transfer(address(investor), _amount.mul(investorfee).div(division));
         IERC20(BUSD).transfer(address(company), _amount.mul(companyfee).div(division));
    }
    function stakingsinglepair (address pancakeVault, uint256 stakingamountBUSD, address tokenIn)  public onlyOperator  {
        IVault vault = IVault(pancakeVault);
        address[] memory path = new address[](2);
        path[0] = BUSD;
        path[1] = tokenIn;
        _approveTokenIfNeeded(path[1], address(pancakeVault));
        _approveTokenIfNeeded(path[0], address(router));
        _approveTokenIfNeeded(path[1], address(router));
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(stakingamountBUSD.sub(callfee),0, path, address(this), block.timestamp);
        path[0] = BUSD;
        path[1] = WETH;
        _approveTokenIfNeeded(path[0], address(router));
        _approveTokenIfNeeded(path[1], address(router));
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(callfee,0, path, address(this), block.timestamp);
        _returnAssets(path);
        vault.deposit(IERC20(tokenIn).balanceOf(address(this)));
        vault.safeTransfer(address(stakingaddress), vault.balanceOf(address(this)));
     
    }
 
    function _removeLiqudity(address pair, address to) private {
        IERC20(pair).safeTransfer(pair, IERC20(pair).balanceOf(address(this)));
        (uint256 amount0, uint256 amount1) = IUniswapV2Pair(pair).burn(to);
        require(amount0 >= minimumAmount, 'UniswapV2Router: INSUFFICIENT_A_AMOUNT');
        require(amount1 >= minimumAmount, 'UniswapV2Router: INSUFFICIENT_B_AMOUNT');
    }
    function _getVaultPair (address pancakeVault) private view returns (IVault vault, IUniswapV2Pair pair) {
        vault = IVault(pancakeVault);
        pair = IUniswapV2Pair(vault.want());
        require(pair.factory() == router.factory(), 'Incompatible liquidity pair factory');
    }
    function checkWETH() public view returns (bool isValid) {
        isValid = WETH == router.WETH();
        require(isValid, 'IOU: WETH address not matching Router.WETH()');
    }
    function _getSwapAmount(uint256 investmentA, uint256 reserveA, uint256 reserveB) private view returns (uint256 swapAmount) {
        uint256 halfInvestment = investmentA / 2;
        uint256 nominator = router.getAmountOut(halfInvestment, reserveA, reserveB);
        uint256 denominator = router.quote(halfInvestment, reserveA.add(halfInvestment), reserveB.sub(nominator));
        swapAmount = investmentA.sub(Babylonian.sqrt(halfInvestment * halfInvestment * nominator / denominator));
    }
    function _approveTokenIfNeeded(address token, address spender) private {
        if (IERC20(token).allowance(address(this), spender) == 0) {
            IERC20(token).safeApprove(spender, uint256(~0));
        }
    }
     function updateOperator(address _operator, bool _status) external onlyOperator {
        operators[_operator] = _status;
    }
}