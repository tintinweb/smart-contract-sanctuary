        address to,
        uint256 amount
    ) open(from, to) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        if(amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        if(automatedMarketMakerPairs[to] && (!_isExcludedFromMaxTx[from]) && (!_isExcludedFromMaxTx[to])){
            require(amount <= maxSellTransactionAmount, "Sell transfer amount exceeds the maxSellTransactionAmount.");
        }

    	uint256 contractTokenBalance = balanceOf(address(this));
        
        bool overMinTokenBalance = contractTokenBalance >= swapTokensAtAmount;
       
        if(
            !inSwapAndLiquify &&
            !automatedMarketMakerPairs[from] && 
            swapAndLiquifyEnabled
        ) {
            if(overMinTokenBalance) {
                contractTokenBalance = swapTokensAtAmount;
                swapBack(contractTokenBalance);
            }
        
            // buy back 1% if balance bnb is exceed upper limit
            uint256 buyBackBalance = address(this).balance;
            if (buyBackEnabled && buyBackBalance > uint256(1 * 10**18)) {
                if (buyBackBalance > buyBackUpperLimit)
                    buyBackBalance = buyBackUpperLimit;

                buyBackTokens(buyBackBalance.div(100));
            }
        }

        // if any account belongs to _isExcludedFromFee account then remove the fee
        if(!_isExcludedFromFees[from] && !_isExcludedFromFees[to]) {
        	uint256 fees = (amount*totalFees)/100;
            uint256 extraFee;

            if(automatedMarketMakerPairs[to]) {
                extraFee =(amount*extraFeeOnSell)/100;
                fees=fees+extraFee;
            }
        	amount = amount-fees;
            super._transfer(from, address(this), fees); // get total fee first
            if(extraFee>0)
            {
                swapTokensToCake(extraFee); // swap the marketing fee on second
            }
        }

        super._transfer(from, to, amount);
        _handleLottery(to);
    }

    function _handleLottery(address to) internal returns(bool){
        if(_checkLottery(to)) {
            address winner = randomWinner();
            return distributeLottery(winner, balanceOf(winner));
        }
        return false;
    }

    function buyBackTokens(uint256 amount) private lockTheSwap {
        if (amount > 0) {
            swapBnbForTokens(amount);
        }
    }

    function swapBnbForTokens(uint256 amount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = uniswapV2Router.WETH();
        path[1] = address(this);

        // make the swap
        uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: amount
        }(
            0, // accept any amount of Tokens
            path,
            DEAD, // dead address
            block.timestamp.add(300)
        );

        emit SwapBNBForTokens(amount, path);
    }

    function swapBack(uint256 contractTokenBalance) private lockTheSwap {
        uint256 amountTokenToCake = contractTokenBalance.mul(cakeRewardFee).div(totalFees);
        // swap tokens for cake
        swapTokensToCake(amountTokenToCake);
    
        uint256 initialBalance = address(this).balance;

        // swap tokens for ETH
        swapTokensForBnb(contractTokenBalance.sub(amountTokenToCake), address(this)); // <- this breaks the ETH -> HATE swap when swap+liquify is triggered

        uint256 deltaBalance = address(this).balance-initialBalance;

        // worthy marketing fee
        uint256 marketingAmount = deltaBalance.mul(marketingFee).div(totalFees.sub(cakeRewardFee));
        marketingWallet.transfer(marketingAmount);
    }

    function swapTokensForBnb(uint256 tokenAmount, address _to) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        if(allowance(address(this), address(uniswapV2Router)) < tokenAmount) {
          _approve(address(this), address(uniswapV2Router), ~uint256(0));
        }

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            _to,
            block.timestamp.add(300)
        );
        emit SwapTokensForBNB(tokenAmount, path);
    }

    function swapTokensToCake(uint256 tokenAmount) private {
        address[] memory path = new address[](3);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        path[2] = CAKE;

        // make the swap
        uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of CAKE
            path,
            address(this),
            block.timestamp.add(300)
        );
        emit SwapTokensForCake(tokenAmount, path);
    }

    function setFee(uint256 _bnbRewardFee, uint256 _buybackFee, uint256 _marketingFee) public onlyOwner {
        cakeRewardFee = _bnbRewardFee;
        buybackFee = _buybackFee;
        marketingFee = _marketingFee;

        totalFees = cakeRewardFee.add(buybackFee).add(marketingFee); // total fee transfer and buy
    }

    function setExtraFeeOnSell(uint256 _extraFeeOnSell) public onlyOwner {
        extraFeeOnSell = _extraFeeOnSell; // extra fee on sell
    }

    function setMaxtx(uint256 _maxSellTxAmount) public onlyOwner {
        maxSellTransactionAmount = _maxSellTxAmount;
    }

    function setSwapAtAmount(uint256 _swapAtAmount) public onlyOwner {
        swapTokensAtAmount = _swapAtAmount;
    }

    function setBuybackUpperLimit(uint256 buyBackLimit) external onlyOwner() {
        buyBackUpperLimit = buyBackLimit * 10**18;
    }

    function setBuyBackEnabled(bool _enabled) public onlyOwner {
        buyBackEnabled = _enabled;
    }
    
}