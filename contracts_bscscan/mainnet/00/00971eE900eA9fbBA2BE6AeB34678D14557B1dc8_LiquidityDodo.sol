        IERC20(liquidity).safeTransfer(msg.sender, amount);
        if (_amountA < amountA) {
            IERC20(tokenA).safeTransfer(msg.sender, amountA.sub(_amountA));
        }
        if (_amountB < amountB) {
            IERC20(tokenB).safeTransfer(msg.sender, amountB.sub(_amountB));
        }
        return (amount, _amountA, _amountB);
    }
    
    function remove(address dvm, address tokenA, address tokenB, uint256 amount, uint256 deadLine) override external lock onlyCaller returns (uint256, uint256) {
        IERC20(dvm).safeTransferFrom(msg.sender, address(this), amount);
        (uint256 amountA, uint256 amountB) = IDVM(dvm).sellShares(amount, address(this), 0, 0, "", deadLine);
        require(0 < amountA, "publics:remove_liquidity_amounta_is_zero");
        require(0 < amountB, "publics:remove_liquidity_amountb_is_zero");
        IERC20(tokenA).safeTransfer(msg.sender, amountA);
        IERC20(tokenB).safeTransfer(msg.sender, amountB);
        return (amountA, amountB);
    }
    
    function addSingle(address liquidity, address token, uint256 amount, uint256 deadLine) override external lock onlyCaller returns (uint256) {
        IDODO dodo = IDODO(liquidity);
        if (dodo._BASE_TOKEN_() == token) {
            return addSingleBase(liquidity, amount, deadLine);
        }else if (dodo._QUOTE_TOKEN_() == token) {
            return addSingleQuote(liquidity, amount, deadLine);
        }else {
            require(false, "publics:add_liquidity_single_token_invalid");
        }
        return 0;
    }
    
    function addSingleBase(address liquidity, uint256 amount, uint256 deadLine) internal returns (uint256) {
        (uint256 baseShares, ) = proxy.addLiquidityToV1(liquidity, amount, 0, 1, 0, 0, deadLine);
        return baseShares;
    }
    
    function addSingleQuote(address liquidity, uint256 amount, uint256 deadLine) internal returns (uint256) {
        (, uint256 quoteShares) = proxy.addLiquidityToV1(liquidity, 0, amount, 0, 1, 0, deadLine);
        return quoteShares;
    }

    
