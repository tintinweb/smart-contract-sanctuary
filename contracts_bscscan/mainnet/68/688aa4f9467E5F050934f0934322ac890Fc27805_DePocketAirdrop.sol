pragma solidity 0.8.4;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./IBEP20.sol";

contract DePocketAirdrop is AccessControl, ReentrancyGuard {
    using SafeMath for uint256;
    mapping(address => uint256) public userBalances;
    mapping(address => bool) public blacklists;
    bool private locked;
    IBEP20 private _depocket = IBEP20(address(0x7d99eda556388Ad7743A1B658b9C4FC67D7A9d74));
    uint256 private constant DECIMAL_MULTIPLIER = 10**18;

    event UserClaimed(address account, uint256 balance);
    event BlackListAdded(address account);

    modifier onlyAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "DePocket: ADMIN role required");
        _;
    }

    constructor(address multiSigAccount) {
        require(multiSigAccount != address(0), "Zero address");
        _setupRole(DEFAULT_ADMIN_ROLE, multiSigAccount);
        locked = false;
    }

    function addWhitelist(address[] memory whitelist, uint256[] memory balances) external onlyAdmin returns (bool){
        require(whitelist.length > 0, "User whitelist should not be blank");
        require(whitelist.length == balances.length, "Account numbers and balance numbers not equal");
        
        for (uint i=0; i< whitelist.length; i++) {
            userBalances[whitelist[i]] = balances[i];
        }

        return true;
    }

    function addBlacklist(address[] memory blacklist) external onlyAdmin returns (bool){
        require(blacklist.length > 0, "User blacklist should not be blank");
        
        for (uint i=0; i< blacklist.length; i++) {
            blacklists[blacklist[i]] = true;
            emit BlackListAdded(blacklist[i]);
        }

        return true;
    }

    function claim() external nonReentrant{
        require(!locked, "Claim temporary locked!");
        require(!blacklists[_msgSender()], "This account listed in blocked list!");
        require(_msgSender() != address(0), "Zero address");
        require(userBalances[_msgSender()] > 0, "Your account claimed");
        uint256 claimableAmount = userBalances[_msgSender()];
        _depocket.transfer(_msgSender(), claimableAmount.mul(DECIMAL_MULTIPLIER));
        userBalances[_msgSender()] = 0;
        emit UserClaimed(_msgSender(), claimableAmount);
    }

    function lockClaim(bool lock) external onlyAdmin returns (bool){
        locked = lock;
        return true;
    }
}

