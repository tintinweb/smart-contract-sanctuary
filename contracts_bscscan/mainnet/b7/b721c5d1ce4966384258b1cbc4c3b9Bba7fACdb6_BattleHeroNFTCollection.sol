pragma solidity 0.8.9;

import "../../node_modules/@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "../../node_modules/@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "../../node_modules/@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721PausableUpgradeable.sol";
import "../../node_modules/@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";

contract BattleHeroNFTCollection is
    AccessControlEnumerableUpgradeable,
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    ERC721URIStorageUpgradeable,
    ERC721PausableUpgradeable
{
        bytes32 public PAUSER_ROLE;
        bytes32 public UPGRADER_ROLE;
        bytes32 public DESIGNER_ROLE;

        mapping (uint256 => string) public collectionIdToName;

        uint256 public lastCollectionId;
        uint256 public tokenCount;

        mapping (uint256 => uint256) public tokenIdToRarity;
        mapping (uint256 => uint256) public tokenIdToCollectionId;

        function initialize() public initializer {
                __ERC721_init("BattleHero Collections", "BHERO");
                __ERC721URIStorage_init();
                __ERC721Pausable_init();
                __ERC721Enumerable_init();
                
                __AccessControlEnumerable_init();
                lastCollectionId = 0;
                tokenCount       = 0;
                PAUSER_ROLE      = keccak256("PAUSER_ROLE");
                UPGRADER_ROLE    = keccak256("UPGRADER_ROLE");
                DESIGNER_ROLE    = keccak256("DESIGNER_ROLE");
                _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
                _setupRole(PAUSER_ROLE, _msgSender());
                _setupRole(UPGRADER_ROLE, _msgSender());
                _setupRole(DESIGNER_ROLE, _msgSender());
        }

        function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
        }

        function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
        }

        function addCollection(string memory collectionName) public onlyRole(DESIGNER_ROLE) {
                require(bytes(collectionName).length != 0, "Call this function with a name of a new collection as a parameter." );
                lastCollectionId += 1;
                collectionIdToName[lastCollectionId] = collectionName;
        }

        function changeCollectionName(uint256 collectionId, string memory collectionName) public onlyRole(DESIGNER_ROLE) {
                require(bytes(collectionName).length != 0, "Call this function with a different name to change for collectionId." );
                require(bytes(collectionIdToName[collectionId]).length != 0, "that collection hasn't been set yet." );
                collectionIdToName[collectionId] = collectionName;
        }

        function _mint( uint256 tokenId, address to, uint256 collectionId, uint256 rarity ) internal {                
                _safeMint(to, tokenId);
                tokenIdToCollectionId[tokenId] = collectionId;
                tokenIdToRarity[tokenId] = rarity;
                tokenCount = tokenCount+1;
        }

        function mint(address to,  uint256 collectionId, uint256[] memory tokenId, uint256[] memory rarity ) public onlyRole(DESIGNER_ROLE) {
                require(bytes(collectionIdToName[collectionId]).length != 0, "That collection hasn't been set yet." );
                require((tokenId.length == rarity.length), "Inconsistent input info.");                
                for(uint i = 0; i < tokenId.length; i++){
                        _mint(tokenId[i], to, collectionId, rarity[i]);
                }
        }
       function mint(address to,  uint256 collectionId, uint256 tokenId, uint256 rarity ) public onlyRole(DESIGNER_ROLE) {
                require(bytes(collectionIdToName[collectionId]).length != 0, "That collection hasn't been set yet." );
                _mint(tokenId, to, collectionId, rarity);
        }
        function editTokenIdAdmin( uint256 tokenId, uint256 collectionId, uint256 rarity ) public onlyRole(DESIGNER_ROLE) {
                require(bytes(collectionIdToName[collectionId]).length != 0, "That collection hasn't been set yet." );
                require(!_exists(tokenId) , "That TokenId is already in use, use other TokenId to mint");
                tokenIdToCollectionId[tokenId] = collectionId;
                tokenIdToRarity[tokenId] = rarity;
        }

        function tokenIdToCollectionString(uint256 tokenId) public view returns(string memory) {
                uint256 collectionId = tokenIdToCollectionId[tokenId];
                return collectionIdToName[collectionId];
        }

        function tokenIdToRarityString(uint256 tokenId) public view returns(string memory) {
                uint256 rarity = tokenIdToRarity[tokenId];
                if (rarity == 0) {
                        return("Common");
                }
                if (rarity == 1) {
                        return("Low Rare");
                }
                if (rarity == 2) {
                        return("Rare");
                }
                if (rarity == 3) {
                        return("Epic");
                }
                if (rarity == 4) {
                        return("Legendary");
                }
                if (rarity == 5) {
                        return("Mitic");
                }
                return("");
        }


        function _beforeTokenTransfer(
                address from,
                address to,
                uint256 tokenId
        ) internal virtual override(ERC721PausableUpgradeable, ERC721EnumerableUpgradeable, ERC721Upgradeable) {
                super._beforeTokenTransfer(from, to, tokenId);
        }

        function burn(uint256 tokenId) public onlyRole(DESIGNER_ROLE){
                _burn(tokenId);
        }

        function _burn(uint256 tokenId)  internal virtual override(ERC721URIStorageUpgradeable, ERC721Upgradeable) {
                super._burn(tokenId);
        }

        function supportsInterface(bytes4 interfaceId) public view virtual override( AccessControlEnumerableUpgradeable, ERC721EnumerableUpgradeable, ERC721Upgradeable) returns (bool) {
                return super.supportsInterface(interfaceId);
        }

        function tokenURI(uint256 tokenId) public view virtual override(ERC721URIStorageUpgradeable, ERC721Upgradeable) returns (string memory) {
                return super.tokenURI(tokenId);
        }
        /**
        * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each
        * token will be the concatenation of the `baseURI` and the `tokenId`. Empty
        * by default, can be overriden in child contracts.
        */
        function _baseURI() internal view virtual override returns (string memory) {
                return "https://battlehero.io/avatar/";
        }

        function tokenInfo(uint256 tokenId) public view returns (string memory, string memory, string memory, uint256){
                string memory _collectionName = tokenIdToCollectionString(tokenId);
                string memory _uri = tokenURI(tokenId);
                string memory _rarity = tokenIdToRarityString(tokenId);
                return (_collectionName, _rarity, _uri, tokenId);
        }
}

