// File: contracts/Workers.sol

contract Workers is IWorkers, ERC721Enumerable
{
    //========================
    // LIBS
    //========================

    using SafeMath for uint256;
    using SafeERC20 for IGameToken;

    //========================
    // STRUCT
    //========================

    struct Stats
    {
        uint256 id;
        uint256 seed;
        uint256 mintBlock;
        uint256 burnValue;
        uint16 level;
        uint16 stamina;
        uint32 power;
    }

    struct Cosmetics
    {        
        uint16 head;
        uint16 body;
    }

    //========================
    // ATTRIBUTES V1.0
    //========================
    
    IGameManager public manager;
    Stats[] public stats;
    Cosmetics[] public cosmetics;
    mapping(uint256 => mapping(uint256 => uint256)) public nftVars;

    //========================
    // EVENTS
    //========================

    event NewWorker(address indexed _minter, uint256 indexed _id);

    //========================
    // CREATE
    //========================

    constructor()
    ERC721("Bomb Worker", "BSCIENCE")
    {

    }

    //========================
    // CONFIG FUNCTIONS
    //========================

    function setGameManager(IGameManager _manager) external
    {
        if (address(manager) != address(0))
        {
            manager.requireAdmin(msg.sender);
        }
        manager = _manager;
    }

    //========================
    // INFO FUNCTIONS
    //========================

    function getInfo(uint256 _id) public view returns (Stats memory, Cosmetics memory)
    {
        Stats memory s = stats[_id];
        Cosmetics memory c = cosmetics[_id];        
        return (s, c);
    }    

    function getNftVar(uint256 _id, uint256 _var) public view returns (uint256)
    {
        return nftVars[_id][_var];
    }

    function totalMinted() public view returns (uint256)
    {
        return stats.length;
    }

    function getPower(uint256 _id) public override view returns (uint256)
    {
        Stats memory s = stats[_id];
        return s.power;
    }

    //========================
    // USER FUNCTIONS
    //========================

    function getWorkersOwnedBy(address _user) public override view returns (uint256[] memory workers)
    {
        uint256 count = balanceOf(_user);
        workers = new uint256[](count);
        for(uint256 n = 0; n < count; n++)
        {
            workers[n] = tokenOfOwnerByIndex(_user, n);
        }
    }

    //========================
    // GAME FUNCTIONS
    //========================

    function setNftVar(uint256 _id, uint256 _var, uint256 _value) public
    {
        manager.requireGameManager(msg.sender);
        nftVars[_id][_var] = _value;
    }

    function useStamina(uint256 _id, uint16 _stamina) public
    {
        manager.requireGameManager(msg.sender);
        Stats storage s = stats[_id];
        s.stamina = uint16(uint256(s.stamina).sub(_stamina));
    }

    function addStamina(uint256 _id, uint16 _stamina) public
    {
        manager.requireGameManager(msg.sender);
        Stats storage s = stats[_id];
        s.stamina = uint16(uint256(s.stamina).add(_stamina));
    }

    //========================
    // MINT
    //========================   

    function mint(address _minter, uint256 _seed) public returns (uint256)
    {
        //generate
        uint16 level = generateLevel(_seed.add(1));
        return customMint(
            _minter,
            Stats(
            {
                id: 0,
                mintBlock: 0,
                stamina: 0,
                burnValue: 0,
                seed: _seed,
                level: level,
                power: generatePower(level, _seed.add(2))                
            }),
            Cosmetics(
            {
                head: level,
                body: uint16(manager.randoms().randomSeededMinMax(0, 3, _seed.add(3)))
            }));
    } 

    function customMint(address _minter, Stats memory _stats, Cosmetics memory _cosmetics) public returns (uint256)
    {
        //check
        manager.requireNFTMinter(msg.sender);

        //default
        _stats.id = totalMinted();      
        _stats.mintBlock = block.number;
        _stats.stamina = 0;
        _stats.burnValue = manager.workerBurnValue();

        //get burn value
        manager.token().safeTransferFrom(msg.sender, address(this), _stats.burnValue);

        //mint
        stats.push(_stats);
        cosmetics.push(_cosmetics);
        _mint(_minter, _stats.id);

        emit NewWorker(_minter, _stats.id);
        return _stats.id;
    }

    function generateLevel(uint256 _seed) private view returns (uint16)
    {
        uint256[] memory rarities = manager.getWorkerRarities();
        uint256 total = 0;

        //get total
        for (uint256 n = 0; n < rarities.length; n++)
        {
            total = total.add(rarities[n]);
        }

        //get level
        uint256 rand = manager.randoms().randomSeededMinMax(0, total, _seed);
        uint256 levelBorder = 0;
        for (uint256 n = 0; n < rarities.length; n++)
        {
            levelBorder = levelBorder.add(rarities[n]);
            if (rand < levelBorder)
            {
                return uint16(n);
            }
        } 
        return uint16(rarities.length.sub(1));
    }

    function generatePower(uint16 _level, uint256 _seed) private view returns (uint32)
    {
        uint256 from = uint256(_level).mul(30).add(30);
        uint256 to = from.add(30);
        return uint32(manager.randoms().randomSeededMinMax(
            from,
            to,
            _seed));
    }

    //========================
    // BURN
    //========================

    function burn(uint256 _id) external
    {
        //check
        manager.requireNFTBurner(msg.sender);

        //burn value
        Stats memory s = stats[_id];
        manager.token().safeTransfer(ownerOf(_id), s.burnValue);

        //burn
        _burn(_id);
    }
}