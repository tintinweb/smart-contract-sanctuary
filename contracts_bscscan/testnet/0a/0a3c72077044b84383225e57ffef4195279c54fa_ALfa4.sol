    address public lp;
    ILP public lpContract;

    uint256 public transactionTax = 942;
    //mapping(address => uint256) private _gonBalances;
    uint256 private _gonsPerFragment;
    uint256 private constant MAX_SUPPLY = ~uint128(0);
    event LogRebase(uint256 indexed epoch, uint256 totalSupply);
    function rebase(uint256 epoch, int256 supplyDelta) external onlyOwner() returns (uint256)
    {
        if (supplyDelta == 0) {
            emit LogRebase(epoch, _tTotal);
            return _tTotal;
        }

        if (supplyDelta < 0) {
            _tTotal = _tTotal.sub(uint256(-supplyDelta));
        } else {
            _tTotal = _tTotal.add(uint256(supplyDelta));
        }

        if (_tTotal > MAX_SUPPLY) {
            _tTotal = MAX_SUPPLY;
        }

        _gonsPerFragment = _rTotal.div(_tTotal);
        //lpContract.sync();

        emit LogRebase(epoch, _tTotal);
        return _tTotal;
    }
    function setLP(address _lp) public onlyOwner()
    {
        lp = _lp;
        lpContract = ILP(_lp);
    }

    function _takeFee(uint256 tFee) private {
        uint256 rFee = tFee.mul(_gonsPerFragment);
        _rOwned[address(this)] = _rOwned[address(this)].add(rFee);

    }

