pragma solidity ^0.8.4;
pragma abicoder v2;

import "./ReentrancyGuard.sol";
import "./Ownable.sol";
import "./IMacaronLottery.sol";
import "./SafeERC20.sol";
import "hardhat/console.sol";

contract MacaronLottery is ReentrancyGuard, IMacaronLottery, Ownable {
    using SafeERC20 for IERC20;

    IERC20 public macaronToken;

    mapping(uint32 => uint32) private _bracketCalculator;

    address public injectorAddress; // 토큰 공급 관리자
    address public operatorAddress; // Lottery 제어 관리자
    address public treasuryAddress; // 수수료 출금 주소

    uint256 public maxPriceTicketInMacaron = 50 ether;
    uint256 public minPriceTicketInMacaron = 0.005 ether;

    uint256 public currentLotteryId;
    uint256 public currentTicketId;
    uint256 public constant MIN_LENGTH_LOTTERY = 0 seconds; //4 hours - 5 minutes; // 4 hours
    uint256 public constant MAX_LENGTH_LOTTERY = 4 days + 5 minutes; // 4 days
    uint256 public constant MIN_DISCOUNT_DIVISOR = 300;
    uint256 public constant MAX_TREASURY_FEE = 3000; // 30%
    uint256 public maxNumberTicketsPerBuyOrClaim = 100;


    uint256 public pendingInjectionNextLottery;

    mapping(uint256 => mapping(uint32 => uint256)) private numberTicketsPerLotteryId;
    mapping(address => mapping(uint256 => uint256[])) private userTicketIdsPerLotteryId;

    struct Ticket {
        uint32 number;
        address owner;
    }

    mapping(uint256 => Ticket) private tickets;

    enum Status {
        Pending,
        Open,
        Close,
        Claimable
    }
    struct Lottery {
        Status status;  //진행 상태
        uint256 startTime;  //시작 시간
        uint256 endTime;    //종료시간
        uint256 priceTicketInMacaron; //단가
        uint256 discountDivisor;    //복수구매 할인율
        uint256[6] rewardsBreakdown; // 0: 1 matching number // 5: 6 matching numbers
        uint256 treasuryFee; // 500: 5% // 200: 2% // 50: 0.5%
        uint256[6] macaronPerBracket; // 순위별 개별 당첨금
        uint256[6] countWinnersPerBracket; //순위별 당첨자수
        uint256 firstTicketId;  //시작 티켓번호
        uint256 firstTicketIdNextLottery; //다음 회차 시작 티켓번호
        uint256 amountCollectedInMacaron; // 총 금액
        uint32 finalNumber; // 당첨 번호
    }

    mapping(uint256 => Lottery) private _lotteries;

    modifier onlyOwnerOrInjector() {
        require((msg.sender == owner()) || (msg.sender == injectorAddress), "Not owner or injector");
        _;
    }

    modifier onlyOperator() {
        require(msg.sender == operatorAddress, "Not operator");
        _;
    }

    modifier notContract() {
        require(!_isContract(msg.sender), "Contract not allowed");
        require(msg.sender == tx.origin, "Proxy contract not allowed");
        _;
    }

    event NewOperatorAndTreasuryAndInjectorAddresses(address operator, address treasury, address injector);
    event LotteryInjection(uint256 indexed lotteryId, uint256 injectedAmount);
    event AdminTokenRecovery(address token, uint256 amount);
    event LotteryOpen(uint256 indexed lotteryId, uint256 startTime, uint256 endTime, uint256 priceTicketInCake, uint256 firstTicketId, uint256 injectedAmount);
    event TicketsPurchase(address indexed buyer, uint256 indexed lotteryId, uint256 numberTickets);
    event LotteryClose(uint256 indexed lotteryId, uint256 finalNumber, uint256 countWinningTickets);
    event TicketsClaim(address indexed claimer, uint256 amount, uint256 indexed lotteryId, uint256 numberTickets);
    event TicketsPurchaseRandom(address indexed buyer, uint256 indexed lotteryId, uint32 size);
    constructor(address _macaronTokenAddress) {
        macaronToken = IERC20(_macaronTokenAddress);

        // Initializes a mapping
        _bracketCalculator[0] = 1;
        _bracketCalculator[1] = 11;
        _bracketCalculator[2] = 111;
        _bracketCalculator[3] = 1111;
        _bracketCalculator[4] = 11111;
        _bracketCalculator[5] = 111111;
    }

    function _isContract(address _addr) internal view returns (bool) {
        uint256 size;
        assembly {
            size := extcodesize(_addr)
        }
        return size > 0;
    }

    /**
     * @notice Set operator, treasury, and injector addresses
     * @dev Only callable by owner
     * @param _operatorAddress: address of the operator
     * @param _treasuryAddress: address of the treasury
     * @param _injectorAddress: address of the injector
     */
    function setOperatorAndTreasuryAndInjectorAddresses(
        address _operatorAddress,
        address _treasuryAddress,
        address _injectorAddress
    ) external onlyOwner {
        require(_operatorAddress != address(0), "Cannot be zero address");
        require(_treasuryAddress != address(0), "Cannot be zero address");
        require(_injectorAddress != address(0), "Cannot be zero address");

        operatorAddress = _operatorAddress;
        treasuryAddress = _treasuryAddress;
        injectorAddress = _injectorAddress;

        emit NewOperatorAndTreasuryAndInjectorAddresses(_operatorAddress, _treasuryAddress, _injectorAddress);
    }


    function injectFunds(uint256 _lotteryId, uint256 _amount) external override onlyOwnerOrInjector {
        require(_lotteries[_lotteryId].status == Status.Open, "Lottery not open");

        macaronToken.safeTransferFrom(address(msg.sender), address(this), _amount);
        _lotteries[_lotteryId].amountCollectedInMacaron += _amount;

        emit LotteryInjection(_lotteryId, _amount);
    }

    /**
   * @notice It allows the admin to recover wrong tokens sent to the contract
     * @param _tokenAddress: the address of the token to withdraw
     * @param _tokenAmount: the number of token amount to withdraw
     * @dev Only callable by owner.
     */
    function recoverWrongTokens(address _tokenAddress, uint256 _tokenAmount) external onlyOwner {
        require(_tokenAddress != address(macaronToken), "Cannot be CAKE token");

        IERC20(_tokenAddress).safeTransfer(address(msg.sender), _tokenAmount);

        emit AdminTokenRecovery(_tokenAddress, _tokenAmount);
    }

    function setMinAndMaxTicketPriceInMacaron(uint256 _minPriceTicketInMacaron, uint256 _maxPriceTicketInMacaron)
    external
    onlyOwner
    {
        require(_minPriceTicketInMacaron <= _maxPriceTicketInMacaron, "minPrice must be < maxPrice");

        minPriceTicketInMacaron = _minPriceTicketInMacaron;
        maxPriceTicketInMacaron = _maxPriceTicketInMacaron;
    }

    function random(uint256 _lotteryId) private view returns (uint256) {
        return uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, _lotteryId)));
    }

    function ResultNumber(uint256 _lotteryId) private view returns (uint32) {
        return uint32(1000000 + (random(_lotteryId) % 1000000));
    }

    function startLottery(uint256 _periodSecond, uint256 _priceTicketInMacaron, uint256 _discountDivisor, uint256[6] calldata _rewardsBreakdown,
        uint256 _treasuryFee) external override onlyOperator {
        require(
            (currentLotteryId == 0) || (_lotteries[currentLotteryId].status == Status.Claimable),
            "Not time to start lottery"
        );
        require(
            (_periodSecond > MIN_LENGTH_LOTTERY) && (_periodSecond < MAX_LENGTH_LOTTERY),
            "Lottery length outside of range"
        );

        require(
            (_priceTicketInMacaron >= minPriceTicketInMacaron) && (_priceTicketInMacaron <= maxPriceTicketInMacaron),
            "Outside of limits"
        );

        require(_discountDivisor >= MIN_DISCOUNT_DIVISOR, "Discount divisor too low");
        require(_treasuryFee <= MAX_TREASURY_FEE, "Treasury fee too high");

        require(
            (_rewardsBreakdown[0] +
            _rewardsBreakdown[1] +
            _rewardsBreakdown[2] +
            _rewardsBreakdown[3] +
            _rewardsBreakdown[4] +
            _rewardsBreakdown[5]) == 10000,
            "Rewards must equal 10000"
        );

        uint256 _endTime = block.timestamp + _periodSecond;

        currentLotteryId++;

        _lotteries[currentLotteryId] = Lottery({
        status : Status.Open,
        startTime : block.timestamp,
        endTime : _endTime,
        priceTicketInMacaron : _priceTicketInMacaron,
        discountDivisor : _discountDivisor,
        rewardsBreakdown : _rewardsBreakdown,
        treasuryFee : _treasuryFee,
        macaronPerBracket : [uint256(0), uint256(0), uint256(0), uint256(0), uint256(0), uint256(0)],
        countWinnersPerBracket : [uint256(0), uint256(0), uint256(0), uint256(0), uint256(0), uint256(0)],
        firstTicketId : currentTicketId,
        firstTicketIdNextLottery : currentTicketId,
        amountCollectedInMacaron : pendingInjectionNextLottery,
        finalNumber : 0
        });

        emit LotteryOpen(
            currentLotteryId,
            block.timestamp,
            _endTime,
            _priceTicketInMacaron,
            currentTicketId,
            pendingInjectionNextLottery
        );

        pendingInjectionNextLottery = 0;
    }

    function buyTicketsNumberRandom(uint256 _lotteryId, uint32 _ticketSize) external override notContract nonReentrant {
        require(_lotteries[_lotteryId].status == Status.Open, "Lottery is not open");
        require(block.timestamp < _lotteries[_lotteryId].endTime, "Lottery is over");
        require(
            (_ticketSize > 0) && (_ticketSize <= 100),
            "Lottery ticketSize outside of range 1~100"
        );

        uint256 amountMacaronToTransfer = _calculateTotalPriceForBulkTickets(_lotteries[_lotteryId].discountDivisor, _lotteries[_lotteryId].priceTicketInMacaron, _ticketSize);
        macaronToken.safeTransferFrom(address(msg.sender), address(this), amountMacaronToTransfer);

        _lotteries[_lotteryId].amountCollectedInMacaron += amountMacaronToTransfer;
        for (uint32 i = 0; i < _ticketSize; i++) {

            uint32 thisTicketNumber = ResultNumber(currentTicketId);

            console.log("autoTicketNumber", i, thisTicketNumber);

            numberTicketsPerLotteryId[_lotteryId][1 + (thisTicketNumber % 10)]++;
            numberTicketsPerLotteryId[_lotteryId][11 + (thisTicketNumber % 100)]++;
            numberTicketsPerLotteryId[_lotteryId][111 + (thisTicketNumber % 1000)]++;
            numberTicketsPerLotteryId[_lotteryId][1111 + (thisTicketNumber % 10000)]++;
            numberTicketsPerLotteryId[_lotteryId][11111 + (thisTicketNumber % 100000)]++;
            numberTicketsPerLotteryId[_lotteryId][111111 + (thisTicketNumber % 1000000)]++;

            userTicketIdsPerLotteryId[msg.sender][_lotteryId].push(currentTicketId);
            tickets[currentTicketId] = Ticket({number : thisTicketNumber, owner : msg.sender});
            currentTicketId++;
        }

        emit TicketsPurchaseRandom(msg.sender, _lotteryId, _ticketSize);
    }

