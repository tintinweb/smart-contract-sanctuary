    function getPriceTokenBnb(address _token) external view returns (uint256);
    function getBnbToToken(address _token, uint _amount) external view returns (uint256);
    function getBnbPriceBusd() external view returns(uint256);
    function getTokenPriceBnbToBusd(address _token) external view returns(uint256);
