}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.4;

import "./IUniswapV2Router01.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

abstract contract ContractsLibrary is  Initializable,  AccessControlUpgradeable, UUPSUpgradeable {
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");
    bytes32 public constant UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
    address public BUSD_MAIN ;
    address public BUSD_TEST ;
    address public WBNB_MAIN ;
    address public WBNB_TEST ;
    address public ROUTER_MAIN;
    address public ROUTER_TEST;   

    modifier onlyOwner() {
        require(hasRole(OWNER_ROLE, _msgSender()), "Only owner");
        _;
    }    

 function _authorizeUpgrade(address newImplementation)
        internal
        virtual
        onlyRole(UPGRADER_ROLE)
        override
    {}
 
    function __ContractsLibrary_init() initializer public {    
     BUSD_MAIN = address(0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56);
     BUSD_TEST = address(0x8301F2213c0eeD49a7E28Ae4c3e91722919B8B47);
     WBNB_MAIN = address(0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c);    
     WBNB_TEST = address(0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd);
     ROUTER_MAIN = address(0x10ED43C718714eb63d5aA57B78B54704E256024E);
     ROUTER_TEST = address(0xD99D1c33F9fC3444f8101754aBC46c52416550D1);
    _grantRole(OWNER_ROLE, msg.sender);
    _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);                
    _grantRole(UPGRADER_ROLE, msg.sender);
    }




    function setBusdAddress(address _address, bool _isTest) external virtual onlyOwner {
        if(_isTest) {
            BUSD_TEST = _address;
        } else {
            BUSD_MAIN = _address;
        }
    }

    function setWbnbAddress(address _address, bool _isTest) external virtual onlyOwner {
        if(_isTest) {
            WBNB_TEST = _address;
        } else {
            WBNB_MAIN = _address;
        }
    }

    function setRouterAddress(address _address, bool _isTest) external onlyOwner {
        if(_isTest) {
            ROUTER_TEST = _address;
        } else {
            ROUTER_MAIN = _address;
        }
    }

    function getChainId() external view returns (uint256) {
        return block.chainid;
    }

    function BUSD() public view returns (address) {
        if(block.chainid == 56)
            return BUSD_MAIN;
        else
            return BUSD_TEST;
    }

    function WBNB() public view returns (address) {
        if(block.chainid == 56)
            return WBNB_MAIN;
        else
            return WBNB_TEST;
    }

    function ROUTER() public view returns (IUniswapV2Router01) {
        if(block.chainid == 56)
            return IUniswapV2Router01(ROUTER_MAIN);
        else
            return IUniswapV2Router01(ROUTER_TEST);
    }

}

