pragma solidity ^0.8.4;

import "./IOracle.sol";
import "./ContractsLibrary.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";


contract Oracle is Initializable, IOracle, ContractsLibrary {
    using SafeMath for uint256;    
    uint public b;

    function setB(uint b_)   public {
     b=b_;
    }
    
    function initialize() initializer  public {
     __ContractsLibrary_init();
    }
     

    function getPriceTokenBnb(address _token) public view override returns(uint256) {
        address[] memory _addressArray = new address[](2);
        _addressArray[0] = _token;
        _addressArray[1] = WBNB();
        uint[] memory _amounts = ROUTER().getAmountsOut(1 ether, _addressArray);
        return _amounts[_amounts.length.sub(1)];
    }

    function getTokenPriceBnbToBusd(address _token) public view override returns(uint256) {
        address[] memory _addressArray = new address[](3);
        _addressArray[0] = _token;
        _addressArray[1] = WBNB();
        _addressArray[2] = BUSD();
        uint[] memory _amounts = ROUTER().getAmountsOut(1 ether, _addressArray);
        return _amounts[_amounts.length.sub(1)];
    }
    
    function getPriceBUSD(address _token) public view override returns(uint256) {
        address[] memory _addressArray = new address[](2);
        _addressArray[0] = _token;
        _addressArray[1] = BUSD();
        uint[] memory _amounts = ROUTER().getAmountsOut(1 ether, _addressArray);
        return _amounts[_amounts.length.sub(1)];
    }

    function getBnbToToken(address _token, uint _amount) public view override returns(uint256) {
        address[] memory _addressArray = new address[](2);
        _addressArray[0] = WBNB();
        _addressArray[1] = _token;
        uint[] memory _amounts = ROUTER().getAmountsOut(_amount, _addressArray);
        return _amounts[_amounts.length.sub(1)];
    }

    function getBusdToBNBToToken(address _token, uint _amount) public view override returns(uint256) {
        address[] memory _addressArray = new address[](3);
        _addressArray[0] = BUSD();
        _addressArray[1] = WBNB();
        _addressArray[2] = _token;
        uint[] memory _amounts = ROUTER().getAmountsOut(_amount, _addressArray);
        return _amounts[_amounts.length.sub(1)];
    }

    function getBUSDToToken(address _token, uint _amount) public view override returns(uint256) {
        address[] memory _addressArray = new address[](2);
        _addressArray[0] = BUSD();
        _addressArray[1] = _token;
        uint[] memory _amounts = ROUTER().getAmountsOut(_amount, _addressArray);
        return _amounts[_amounts.length.sub(1)];
    }

    function getBnbPriceBusd() public view override returns(uint256) {
        address[] memory _addressArray = new address[](2);
        _addressArray[0] = WBNB();
        _addressArray[1] = BUSD();
        uint[] memory _amounts = ROUTER().getAmountsOut(1 ether, _addressArray);
        return _amounts[_amounts.length.sub(1)];
    }

}

