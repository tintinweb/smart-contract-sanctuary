pragma solidity 0.8.4;

contract TimeLock is Ownable, Pausable{
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;
    using Address for address payable;
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;

    struct saleStore {
        IBEP20 saleToken;
        uint256 saleAmount;
        uint256 lockTime;
        uint256 unlockTime;
        uint256 instantRoi;
        uint256 installmentRoi;
        EnumerableSet.AddressSet accStore;
    }

    struct userStore {
        uint256 lockedAmount;
        uint256 lockedTime;
        uint256 lastClaimed;
        uint256 unlockCount;
        uint256 reservedAmount;
        uint256 releasedAmount;
        bool instantClaim;
    }
    
    struct userLockStore {
        address owner;
        address tokenAddress;
        uint256 lockAmount;
        uint256 lockTime;
        uint256 unlockTime;
        uint256 claimTime;
    }

    uint256 private internalTicket;
    uint256 public saleLength;
    uint256 public monthDuration = 86400 * 30;

    mapping (uint256 => saleStore) private saleInfo;
    mapping (address => mapping (uint256 => userStore)) public userInfo;
    mapping (uint256 => userLockStore) public userLockInfo;
    mapping (address => EnumerableSet.UintSet) private userTicketInfo;

    event lockEvent(
        address indexed user,
        uint256 amount,
        uint256 ticketId,
        uint256 time
    );
    event unlockEvent(
        address indexed user,
        uint256 amount,
        uint256 ticketId,
        uint256 time
    );
    event relockEvent(
        address indexed user,
        uint256 oldTime,
        uint256 newTime,
        uint256 time
    );

    receive() external payable {}
        
    constructor() {}

    function pause() public onlyOwner{
      _pause();
    }
    
    function unpause() public onlyOwner{
      _unpause();
    }

    function adminEmergency(address addr,address account,uint256 amount) public onlyOwner {
        if(addr == address(0)){
            payable(account).transfer(amount);
        }else {
            IBEP20(addr).transfer(account,amount);
        }
    }

    function createPublicSale(
        IBEP20 saleToken,
        uint256 saleAmount,
        uint256 unlockTime,
        uint256 instantRoi,
        uint256 installmentRoi,
        address[] memory wallets,
        uint256[] memory amounts
    ) public 
      whenNotPaused {

        require(wallets.length == amounts.length, "account data mismatched");  
        require(block.timestamp < unlockTime, "time is invalid");
        require(installmentRoi.add(instantRoi) == 100, "invalid roi percentage");
        
        saleLength++;
        saleInfo[saleLength].saleToken = saleToken;
        saleInfo[saleLength].saleAmount = saleAmount;
        saleInfo[saleLength].lockTime = block.timestamp;
        saleInfo[saleLength].unlockTime = unlockTime;
        saleInfo[saleLength].instantRoi = instantRoi;
        saleInfo[saleLength].installmentRoi = installmentRoi;

        uint256 requiredAmount;
        for(uint256 k; k<amounts.length; k++) {
            requiredAmount = requiredAmount.add(amounts[k]);
        }

        require(saleAmount >= requiredAmount, "sale amount should be valid");

        saleToken.safeTransferFrom(_msgSender(),address(this),saleAmount);
    }

    function withdraw(
        uint256 saleId
    ) public 
      whenNotPaused  {
        userStore storage user = userInfo[_msgSender()][saleId];
        require(saleInfo[saleId].accStore.contains(_msgSender()), 
                "Unable to access");
        require(saleInfo[saleId].unlockTime < block.timestamp, "sale still not yet over");
        require(user.instantClaim, "already claimed");
        
        uint256 amountOut = user.lockedAmount.mul(saleInfo[saleId].instantRoi).div(1e2);
        saleInfo[saleId].saleToken.safeTransfer(_msgSender(),amountOut);
        user.instantClaim = true;
        user.releasedAmount = user.releasedAmount.add(amountOut);
        user.lockedTime = saleInfo[saleId].unlockTime;
        user.lastClaimed = saleInfo[saleId].unlockTime;
    }

    function claim(
         uint256 saleId
    ) public 
      whenNotPaused 
      onlyOwner {
        userStore storage user = userInfo[_msgSender()][saleId];

        require(user.lastClaimed.add(monthDuration) < block.timestamp, "unable to claim now");
        require(user.releasedAmount != user.lockedAmount, "amount exceed");

        require(saleInfo[saleId].accStore.contains(_msgSender()), 
                "Unable to access");
        require(user.instantClaim, "already claimed");     
        
        uint256 reward = user.lockedAmount.mul(saleInfo[saleId].installmentRoi).div(1e2);
        uint given = user.unlockCount;
        user.unlockCount = 0;
        uint256 stakeTime = user.lockedTime;
        while(stakeTime.add(monthDuration) < block.timestamp) {
            stakeTime = stakeTime.add(monthDuration);
            user.lastClaimed = user.lastClaimed.add(monthDuration);
            user.unlockCount = user.unlockCount + 1;         
        }        
        
        uint256 amountOut = reward * (user.unlockCount - given);        
        if(user.releasedAmount.add(amountOut) > user.lockedAmount) {
            amountOut = user.lockedAmount.sub(user.releasedAmount);
            user.releasedAmount = user.lockedAmount;
        }
        saleInfo[saleId].saleToken.safeTransfer(_msgSender(),amountOut);
    }

    function saleAccountsInfo(uint256 saleId) public view returns (address[] memory){
       return (
           saleInfo[saleId].accStore.values()
       );
    }

        function lock(
        address user,
        address token,
        uint256 amount,
        uint256 unlockTime
    ) public whenNotPaused {
        require(_msgSender() == user, "Invalid user");
        require(block.timestamp < unlockTime, "Time Expired");
        
        internalTicket++;
        IBEP20(token).safeTransferFrom(user,address(this),amount);
        userLockInfo[internalTicket] = userLockStore(user,token,amount,block.timestamp,unlockTime, 0);
        userTicketInfo[user].add(internalTicket);
        emit lockEvent(user,amount,internalTicket,block.timestamp);
    }
    
    function unlock(uint256 lockTicketId) public whenNotPaused {
        userLockStore storage store = userLockInfo[lockTicketId];
        require(store.owner == _msgSender(), "Invalid user");
        require(userTicketInfo[store.owner].contains(lockTicketId), "Invalid ticket id");
        require(store.unlockTime < block.timestamp, "time invalid");
        
        IBEP20(store.tokenAddress).safeTransfer(store.owner,store.lockAmount);
        userTicketInfo[store.owner].remove(lockTicketId);
        store.claimTime = block.timestamp;
        emit unlockEvent(store.owner,store.lockAmount,lockTicketId,block.timestamp);
    }

    function relock(uint256 lockTicketId,uint256 duration) public whenNotPaused {
        userLockStore storage store = userLockInfo[lockTicketId];
        require(store.owner == _msgSender(), "Invalid user");
        require(userTicketInfo[store.owner].contains(lockTicketId), "Invalid ticket id");

        emit relockEvent(store.owner,store.lockTime,store.lockTime + duration,block.timestamp);
        store.lockTime = store.lockTime + duration;        
    }
    
    function transferLockOwnership(address newOwner,uint256 lockTicketId) public whenNotPaused {
        userLockStore storage store = userLockInfo[lockTicketId];
        require(store.owner == _msgSender(), "Invalid user");
        require(userTicketInfo[store.owner].contains(lockTicketId), "Invalid ticket id");
        
        store.owner = newOwner;
        userTicketInfo[store.owner].remove(lockTicketId);
        userTicketInfo[newOwner].add(lockTicketId);
    }
    
    function multiUnlock() public whenNotPaused {
        address user = _msgSender();
        uint256[] memory tickets = userTicketInfo[user].values();
        
        uint256 len = tickets.length;
        for(uint256 i; i<len; i++){
            if((userLockInfo[tickets[i]].unlockTime < block.timestamp) && userTicketInfo[user].contains(tickets[i])) {
                IBEP20(userLockInfo[tickets[i]].tokenAddress).safeTransfer(user,userLockInfo[tickets[i]].lockAmount);
                userTicketInfo[user].remove(tickets[i]);
                userLockInfo[tickets[i]].claimTime = block.timestamp;
                emit unlockEvent(user,userLockInfo[tickets[i]].lockAmount,tickets[i],block.timestamp);
            }
        }
    }
    
    struct response {
        address token;
        uint256 id;
    }
    
    function userLockedTokens(address user) public view returns (response[] memory result) {
        uint256[] memory tickets = userTicketInfo[user].values();
        
        uint256 len = tickets.length;
        result = new response[](len);
        for(uint256 i; i<len; i++){
            result[i].token = userLockInfo[tickets[i]].tokenAddress;
            result[i].id = tickets[i];
        }
    }
    
    function userLockedLength(address user) public view returns (uint256) {
         return userTicketInfo[user].length();
    }
    
    function userLockedAt(address user,uint256 index) public view returns (uint256) {
        return userTicketInfo[user].at(index);
    }

    function userLockedIds(address user) public view returns (uint256[] memory) {
        return userTicketInfo[user].values();
    }
    
    function userLockedContains(address user,uint256 ticket) public view returns (bool) {
        return userTicketInfo[user].contains(ticket);
    }    
}