// File: contracts/ZeelLoanInfo.sol


pragma solidity ^0.8.11;
pragma experimental ABIEncoderV2;




contract ZeelLoanInfo is Ownable {
    using SafeMath for uint256;
    using Strings for string;

    struct User {
        string username;
        string loanScore;
        string scoreDate;
        string registerNumber;
        string lastName;
        string firstName;
        bool exists;
    }

    struct Loan {
        uint256 userId;
        uint256 loanId;
        string requestDate;
        string issuedDate;
        string loanAmt;
        string rateAmt;
        string loanTerm;
        string loanContractPath;
        bool exists;
    }

    event UserAdded(uint256 indexed userId);
    event LoanAdded(uint256 indexed loanId);

    Loan[] list;
    mapping(uint256 => User) userList;
    mapping(uint256 => Loan) loanList;

    constructor() {}

    function addUser(uint256 userId, User memory user)
        public
        onlyOwner
        returns (bool)
    {
        require(!userList[userId].exists);
        userList[userId] = user;
        emit UserAdded(userId);
        return true;
    }

    function addLoan(uint256 loanId, Loan memory loan)
        public
        onlyOwner
        returns (bool)
    {
        require(!loanList[loanId].exists);
        loanList[loanId] = loan;
        list.push(loan);
        emit LoanAdded(loanId);
        return true;
    }

    function getUser(uint256 userId) public view returns (User memory user) {
        return userList[userId];
    }

    function getLoanList() public view returns (Loan[] memory loan) {
        return list;
    }

    function getLoan(uint256 loanId) public view returns (Loan memory loan) {
        return loanList[loanId];
    }
}