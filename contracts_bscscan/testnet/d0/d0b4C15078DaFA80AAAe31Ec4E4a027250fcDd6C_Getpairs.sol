contract Getpairs {

    address public grv_bnb;
    address public wbnb_busd;

    address public grv = address(0x16Daf659Afa05ABAcbC2d8c2CBda373ed1D3Fe97);
    address public busd = address(0x78867BbEeF44f2326bF8DDd1941a4439382EF2A7);
    address public wbnb = address(0xae13d989daC2f0dEbFf460aC112a837C89BAa7cd);

    constructor () public {}
    //IUniswapV2Router02 _pancakeV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E); // Mainnet
    IUniswapV2Router02 _pancakeV2Router = IUniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);//0xD99D1c33F9fC3444f8101754aBC46c52416550D1); // testnet // https://twitter.com/UniSwap/status/1369547285160370182

    // Create a uniswap pair for this new token UniSwap testnet factory: 0x6725F303b657a9451d8BA641348b6761A6CC7a17
    function getGrvBnb () public returns (address){
        grv_bnb = IUniswapV2Factory(_pancakeV2Router.factory())
        .getPair(grv,wbnb);
        //console.log("The Pair address for grv/wbnb is: ", grv_bnb);
        return grv_bnb;
    }

    function getWbnbBusd() public returns (address){
        wbnb_busd = IUniswapV2Factory(_pancakeV2Router.factory())
        .getPair(busd,wbnb);
        //console.log("The Pair address for busd/wbnb is: ", wbnb_busd)
        return wbnb_busd;
    }
}