pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "contracts/GodfatherStorage.sol";
import "contracts/CouponToken.sol";

contract AirDropUtil is Ownable, ReentrancyGuard {
    CouponToken public coupon;
    GodfatherStorage public godfatherStorage;
    uint256 public couponUserCounter;
    uint256 public normalUserCounter;
    uint256 public minimumCoupon;
    uint256 public maxAirDropQuota;
    mapping(string => bool) private ipAddresses;
    mapping(address => bool) public gotAirDrop;

    constructor(GodfatherStorage _godfatherStorage, CouponToken _coupon) {
        godfatherStorage = _godfatherStorage;
        coupon = _coupon;
        minimumCoupon = 200;
        maxAirDropQuota = 500;
    }

    function setGodfatherStorageAddress(GodfatherStorage _godfatherStorage)
        public
        onlyOwner
    {
        godfatherStorage = _godfatherStorage;
    }

    function setMinimumCoupon(uint256 value) external onlyOwner {
        minimumCoupon = value;
    }

    function setQuota(uint256 value) external onlyOwner {
        maxAirDropQuota = value;
    }

    function getAirDropCouponUser(string calldata ipAddress)
        public
        returns (uint256[] memory)
    {
        require(
            keccak256(bytes(ipAddress)) != keccak256(bytes("")),
            "AirDropUtil: Invalid parameter"
        );
        require(
            gotAirDrop[msg.sender] == false,
            "AirDropUtil: You have got the reward before"
        );
        require(
            ipAddresses[ipAddress] == false,
            "AirDropUtil: You have got the reward before"
        );

        uint256 _couponUserCounter = couponUserCounter;
        require(
            _couponUserCounter < maxAirDropQuota,
            "AirDropUtil: No Quota Lefts"
        );

        // Check ERC-20 Coupon > 200 ???
        // To be Add
        require(
            coupon.balanceOf(msg.sender) >= minimumCoupon,
            "AirDropUtil: Not enough Coupon"
        );

        _couponUserCounter++;
        uint256[] memory tokenIds = mintGodfather(0, 1);

        // Update Storage
        gotAirDrop[msg.sender] = true;
        ipAddresses[ipAddress] = true;
        couponUserCounter = _couponUserCounter;

        return tokenIds;
    }

    function getAirDropNormalUser(string memory ipAddress)
        public
        returns (uint256[] memory)
    {
        require(
            keccak256(bytes(ipAddress)) != keccak256(bytes("")),
            "AirDropUtil: Invalid parameter"
        );
        require(
            gotAirDrop[msg.sender] == false,
            "AirDropUtil: You have got the reward before"
        );
        require(
            ipAddresses[ipAddress] == false,
            "AirDropUtil: You have got the reward before"
        );

        uint256 _normalUserCounter = normalUserCounter;
        require(
            _normalUserCounter < maxAirDropQuota,
            "AirDropUtil: No Quota Lefts"
        );

        _normalUserCounter++;
        uint256[] memory tokenIds = mintGodfather(0, 1);

        // Update Storage
        gotAirDrop[msg.sender] = true;
        ipAddresses[ipAddress] = true;
        normalUserCounter = _normalUserCounter;

        return tokenIds;
    }

    function mintGodfather(uint256 rarity, uint256 quantity)
        public
        nonReentrant
        returns (uint256[] memory)
    {
        uint256[] memory tokenIds = godfatherStorage.bulkMint(
            msg.sender,
            rarity,
            quantity
        );
        return tokenIds;
    }
}

