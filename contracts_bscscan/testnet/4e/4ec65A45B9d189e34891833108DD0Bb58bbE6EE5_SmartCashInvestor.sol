/**
 * @dev Contract module which provides functions to swap/zap tokens
 *
 * This module is used through inheritance.
 */
abstract contract ZapperModule {
	using SafeMath for uint256;
	using SafeERC20 for IERC20;

	struct ZapInfo {
		address token0;
		address token1;
		uint256 token0Amount;
		uint256 token1Amount;
		address[] token0Path;
		address[] token1Path;
	}

	function zapTokenToLP(
		address router,
		uint256 amount,
		address lpToken,
		address[] memory fromToLPToken0Path,
		address[] memory fromToLPToken1Path,
		address recipient
	) internal returns (uint256) {
		require(
			fromToLPToken0Path.length > 0,
			'Zapper -> zapTokenToLP: fromToLPToken0Path empty'
		);
		require(
			fromToLPToken1Path.length > 0,
			'Zapper -> zapTokenToLP: fromToLPToken1Path empty'
		);
		require(amount > 0, 'Zapper -> zapTokenToLP: amount = 0');

		ZapInfo memory zapInfo;

		zapInfo.token0 = fromToLPToken0Path[fromToLPToken0Path.length - 1];
		zapInfo.token1 = fromToLPToken1Path[fromToLPToken1Path.length - 1];
		zapInfo.token0Amount = 0;
		zapInfo.token1Amount = 0;
		zapInfo.token0Path = fromToLPToken0Path;
		zapInfo.token1Path = fromToLPToken1Path;

		uint256 halfAmount = amount.div(2);

		if (fromToLPToken0Path[0] == zapInfo.token0) {
			zapInfo.token0Amount = halfAmount;
			zapInfo.token1Amount = swapTokens(
				router,
				amount.sub(halfAmount),
				zapInfo.token1Path,
				address(this)
			);
		} else if (fromToLPToken0Path[0] == zapInfo.token1) {
			zapInfo.token0Amount = swapTokens(
				router,
				halfAmount,
				zapInfo.token0Path,
				address(this)
			);
			zapInfo.token1Amount = amount.sub(halfAmount);
		} else {
			zapInfo.token0Amount = swapTokens(
				router,
				halfAmount,
				zapInfo.token0Path,
				address(this)
			);
			zapInfo.token1Amount = swapTokens(
				router,
				amount.sub(halfAmount),
				zapInfo.token1Path,
				address(this)
			);
		}

		uint256 toBalance = tokenBalanceOf(lpToken, recipient);
		IPancakeRouter02(router).addLiquidity(
			zapInfo.token0,
			zapInfo.token1,
			zapInfo.token0Amount,
			zapInfo.token1Amount,
			0,
			0,
			recipient,
			block.timestamp + 600
		);
		return tokenBalanceOf(lpToken, recipient).sub(toBalance);
	}

	function swapTokens(
		address router,
		uint256 amount,
		address[] memory path,
		address recipient
	) internal returns (uint256) {
		require(path.length > 0, 'Zapper -> swapTokens: path empty');
		require(amount > 0, 'Zapper -> swapTokens: amount = 0');

		uint256 swappedAmt = 0;
		address to = path[path.length - 1];
		uint256 toBalance = tokenBalanceOf(to, recipient);

		IPancakeRouter02(router)
			.swapExactTokensForTokensSupportingFeeOnTransferTokens(
				amount,
				0,
				path,
				recipient,
				block.timestamp + 600
			);

		swappedAmt = tokenBalanceOf(to, recipient).sub(toBalance);

		return swappedAmt;
	}

	function swapGasToToken(
		address router,
		uint256 amount,
		address[] memory path,
		address recipient
	) internal returns (uint256) {
		require(path.length > 0, 'Zapper -> swapGasToToken: path empty');
		require(amount > 0, 'Zapper -> swapGasToToken: amount = 0');

		uint256 swappedAmt = 0;
		address to = path[path.length - 1];
		uint256 toBalance = tokenBalanceOf(to, recipient);

		IPancakeRouter02(router)
			.swapExactETHForTokensSupportingFeeOnTransferTokens{value: amount}(
			0,
			path,
			recipient,
			block.timestamp + 600
		);

		swappedAmt = tokenBalanceOf(to, recipient).sub(toBalance);

		return swappedAmt;
	}

	function swapTokenToGas(
		address router,
		uint256 amount,
		address[] memory path,
		address recipient
	) internal returns (uint256) {
		require(path.length > 0, 'Zapper -> swapTokenToGas: path empty');
		require(amount > 0, 'Zapper -> swapTokenToGas: amount = 0');

		uint256 swappedAmt = 0;
		uint256 toBalance = recipient.balance;

		IPancakeRouter02(router)
			.swapExactTokensForETHSupportingFeeOnTransferTokens(
				amount,
				0,
				path,
				recipient,
				block.timestamp + 600
			);

		swappedAmt = recipient.balance.sub(toBalance);

		return swappedAmt;
	}

	function tokenBalance(address token) internal view returns (uint256) {
		return IERC20(token).balanceOf(address(this));
	}

	function tokenBalanceOf(address token, address owner)
		internal
		view
		returns (uint256)
	{
		return IERC20(token).balanceOf(owner);
	}
}

