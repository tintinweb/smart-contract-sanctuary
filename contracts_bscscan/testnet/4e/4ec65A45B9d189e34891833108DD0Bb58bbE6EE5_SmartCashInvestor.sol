/*
 * @dev Contract that helps invest cash smart
 *
 */
contract SmartCashInvestor is
	AccessControlEnumerable,
	ReentrancyGuard,
	Pausable,
	ZapperModule
{
	using SafeMath for uint256;
	using SafeERC20 for IERC20;

	// The max amount to be spent in basis points
	uint256 private constant MAX_BP = 10000;

	// The amount of rewards to convert to shares in basis points
	uint256 public cashToShareBP;

	// The address to cashLP
	address public cashLP;

	// Router to swap/zap rewards
	IPancakeRouter02 public router;

	// The token that represents dollar rewards
	IERC20 public cashToken;

	// The token that represents share
	IERC20 public shareToken;

	// The vault that compounds cashLP
	IVault cashLPVault;

	// The vault that compounds shares
	IVault shareVault;

	// Path to convert cash to share
	address[] public cashToSharePath;

	// Path to convert cash to stable
	address[] public cashToStablePath;

	event UpdateSplit(uint256 cashToShareBP);
	event AdminTokenRecovery(address tokenRecovered, uint256 amount);

	constructor(
		uint256 _cashToShareBP,
		address _cashLP,
		IPancakeRouter02 _router,
		IERC20 _cashToken,
		IVault _cashLPVault,
		IVault _shareVault,
		address[] memory _cashToStablePath,
		address[] memory _cashToSharePath
	) {
		require(
			_cashToShareBP <= MAX_BP,
			'allocation is too high'
		);

		_setupRole(DEFAULT_ADMIN_ROLE, msg.sender);

		cashToShareBP = _cashToShareBP;
		cashLP = _cashLP;
		router = _router;
		cashToken = _cashToken;
		cashLPVault = _cashLPVault;
		shareVault = _shareVault;
		cashToStablePath = _cashToStablePath;
		cashToSharePath = _cashToSharePath;
		shareToken = IERC20(cashToSharePath[cashToSharePath.length - 1]);

		cashToken.safeApprove(address (router), type(uint256).max);
		IERC20(cashToStablePath[cashToStablePath.length - 1]).safeApprove(address (router), type(uint256).max);
		IERC20(cashLP).safeApprove(address (cashLPVault), type(uint256).max);
		shareToken.safeApprove(address (shareVault), type(uint256).max);
	}

	/**
	 * @notice Invests cash in a smart manner
	 * @dev This takes out cash rewards from share boardroom, and has options to hold some yield and reinvest back into
	 * share and cashLP boardrooms.
	 */
	function invest(uint256 amount_) external nonReentrant whenNotPaused {
		require(amount_ > 0, "Amount must be greater than 0");

		uint256 cashBefore = cashToken.balanceOf(address (this));
		cashToken.safeTransferFrom(msg.sender, address(this), amount_);
		uint256 cashAmount = cashToken.balanceOf(address (this)).sub(cashBefore);

		uint256 cashForShare = cashAmount.mul(cashToShareBP).div(MAX_BP);
		if (cashForShare > 0) {
			uint256 shareAmount = swapTokens(
				address(router),
				cashForShare,
				cashToSharePath,
				msg.sender
			);
			shareVault.depositFor(address(shareToken), shareAmount, msg.sender);
		}

		uint256 cashForCashLP = cashAmount.sub(cashForShare);

		if(cashForCashLP > 0) {
			address[] memory cashToCashLP0 = new address[](1);
			cashToCashLP0[0] = address(cashToken);
			uint256 amount = zapTokenToLP(
				address(router),
				cashForCashLP,
				cashLP,
				cashToCashLP0,
				cashToStablePath,
				address(this)
			);
			cashLPVault.depositFor(cashLP, amount, msg.sender);
		}
	}

	/**
	 * @notice Allows admin to update reward split
	 * @param _cashToShareBP: Allocation of cash rewards to invest in share boardroom in basis points
	 * @dev This function is only callable by admin
	 */
	function updateSplit(uint256 _cashToShareBP)
	external
	onlyRole(DEFAULT_ADMIN_ROLE)
	{
		require(
			_cashToShareBP != cashToShareBP, 'Split is the same'
		);
		require(
			_cashToShareBP <= MAX_BP,
			'allocation is too high'
		);

		cashToShareBP = _cashToShareBP;
	}



	/**
	 * @notice It allows the admin to tokens in the contract
	 * @param _tokenAddress: the address of the token to withdraw
	 * @param _tokenAmount: the number of tokens to withdraw
	 * @dev This function is only callable by admin.
	 */
	function recoverTokens(address _tokenAddress, uint256 _tokenAmount)
		external
		onlyRole(DEFAULT_ADMIN_ROLE)
	{
		IERC20 token = IERC20(_tokenAddress);
		_tokenAmount = Math.min(_tokenAmount, token.balanceOf(address(this)));
		token.safeTransfer(address(msg.sender), _tokenAmount);
		emit AdminTokenRecovery(_tokenAddress, _tokenAmount);
	}

	/**
	 * @notice It allows the compounder to be paused
	 * @dev This function is only callable by admin
	 */
	function pause() external onlyRole(DEFAULT_ADMIN_ROLE) {
		_pause();
	}

	/**
	 * @notice It allows the compounder to resumed
	 * @dev This function is only callable by admin
	 */
	function unpause() external onlyRole(DEFAULT_ADMIN_ROLE) {
		_unpause();
	}

}

