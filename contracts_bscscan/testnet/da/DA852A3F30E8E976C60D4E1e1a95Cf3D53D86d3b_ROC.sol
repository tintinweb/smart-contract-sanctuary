pragma solidity >=0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import '@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol';
import '@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol';
import "./RouterProxy.sol";
import "./ILottery.sol";
import "./IOracle.sol";
import "./Ledger.sol";


contract ROC is Context, Ownable, IERC20{
  using SafeMath for uint256;
    using Address for address; 
    RouterProxy public Proxy;
    string private _name = "Raffle Token";
    string private _symbol = "ROC";
    uint8 private _decimals = 18;
    uint256 private _total;
    mapping (address => mapping (address => uint256)) private _allowances;
    IUniswapV2Pair public uniswapV2Pair;
    IOracle public PriceOracle;
    uint256 public BuyFee;
    uint256 public SellFee;
    uint256 public FeesDecimals;
    ILottery public CurrentBuyLottery;
    ILottery public CurrentSellLottery;
    bool public BlockTicket;
    bool public Migrated;
    bool public LiquidityOperation;
    Ledger public CurrentLedger;
    bool public InitialBalanceSet;
    event TokenMigrated(address to);
    uint256 public TicketDiscountPrice = 10;
    
  
    
    constructor(RouterProxy proxy, Ledger ledger,uint256 buyFee,uint256 sellFee, uint256 feesDecimals, ILottery currentBuyLottery,ILottery currentSellLottery, IOracle priceOracle)
    { 
        ChangeFees(buyFee,sellFee);
        FeesDecimals = feesDecimals;
        PriceOracle = priceOracle;
        Proxy = proxy;
        ChangeCurrentLottery(currentBuyLottery,currentSellLottery);
        IUniswapV2Pair pair = IUniswapV2Pair(IUniswapV2Factory(IUniswapV2Router02(Proxy.GetRouter()).factory())
            .createPair(address(this), IUniswapV2Router02(Proxy.GetRouter()).WETH()));
       uniswapV2Pair = pair;
       CurrentLedger = ledger;
       BlockTicket = true;
       LiquidityOperation = true;
     
       
       
    }
    function ChangeDiscount(uint256 discount) public onlyOwner
    {
        TicketDiscountPrice = discount;
    }
    function SetInitialBalance() public onlyOwner
    {
        require(InitialBalanceSet == false,"Balance initialized");
        CurrentLedger.SetBalance(owner(),100000000 * 10 ** _decimals );
       _total = _total.add(100000000 * 10 ** _decimals);
       InitialBalanceSet = true;
       emit Transfer(address(0),owner() ,_total);

    }
    function SetLiquidityOperation(bool value) public onlyOwner
    {
        LiquidityOperation  = value;
    }
    function MigrateTo(IERC20 token) public onlyOwner
    {
        CurrentLedger.Migrate(token);
        Migrated = true;
        emit TokenMigrated(address(token));
    }
    function DenyTicket() public onlyOwner
    {
        BlockTicket = true;
    }
    
    function AllowTicket() public onlyOwner
    {
        BlockTicket = false;
    }
    function ChangeOracle(IOracle oracle) public onlyOwner
    {
        PriceOracle = oracle;
    }
    function ChangeCurrentLottery(ILottery buyLottery, ILottery sellLottery) public onlyOwner
    {
        ChangeBuyLottery(buyLottery);
        ChangeSellLottery(sellLottery);
    }

    function ChangeBuyLottery(ILottery buyLottery)  public onlyOwner
    {
        require(address(buyLottery) != address(0), " Lottery must be valid");
        CurrentBuyLottery = buyLottery;
        _approve(address(CurrentBuyLottery), address(this), 2**256 -1);
    }

    function ChangeSellLottery(ILottery sellLottery)  public onlyOwner
    {
        require(address(sellLottery) != address(0), " Lottery must be valid");
        CurrentSellLottery = sellLottery;
        _approve(address(CurrentSellLottery), address(this), 2**256 -1);
    }


    
    function ChangeFees(uint256 buyFee,uint256 sellFee) public onlyOwner
    {
        BuyFee = buyFee;
        SellFee = sellFee;
    }

   function name() public view returns (string memory) {
        return _name;
    }

    function symbol() public view returns (string memory) {
        return _symbol;
    }

    function decimals() public view returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _total;
    }

    function balanceOf(address account) public view override returns (uint256) {
        //return _balances[account];
        return CurrentLedger.GetBalance(account);
    }
    

    function transfer(address recipient, uint256 amount) public override returns (bool) {
        _transfer(_msgSender(),recipient,amount);
        return true;
    }

    function allowance(address owner, address spender) public view override returns (uint256) {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) public override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(address sender, address recipient, uint256 amount) public override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance"));
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].add(addedValue));
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].sub(subtractedValue, "ERC20: decreased allowance below zero"));
        return true;
    } 

    function _approve(address owner, address spender, uint256 amount) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(address from, address to, uint256 amount) private
    {
        require(Migrated == false, "Token has been migrated");
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");
        require(CurrentLedger.GetBalance(from)>= amount, "Insufficent Balance");
        require(LiquidityOperation == false 
                || to ==owner() 
                || from == owner()
                || to ==address(Proxy.GetRouter()) 
                || from == (Proxy.GetRouter())
                , "Liquidity operation in progress");
        
        if(BlockTicket== false && 
            (from != address(CurrentBuyLottery)|| from != address(CurrentSellLottery) ) )
            {
                if(to == address(uniswapV2Pair))
                {
                    uint256 sellTax = amount.mul(SellFee).div(10**FeesDecimals);
                    _simpleTransfer(from, address(CurrentSellLottery), sellTax);
                    amount = amount.sub(sellTax);
                    
                }
                if(from == address(uniswapV2Pair))
                {
                    require(address(PriceOracle) != address(0),"Oracle not set");
                    uint256 buyTaxFee = amount.mul(BuyFee).div(10**FeesDecimals);
                    _simpleTransfer(from, address(CurrentBuyLottery), buyTaxFee);
                    (uint256 claims)= PriceOracle.GetClaims(buyTaxFee, CurrentBuyLottery.GetTicketCost().mul(TicketDiscountPrice).div(100), uniswapV2Pair);
                    if( claims> 0) 
                        CurrentBuyLottery.SetClaim(to,claims);  
                    amount = amount.sub(buyTaxFee);
                }
            }
        _simpleTransfer(from, to, amount);
    }
    
    function ClaimTicket() public
    {
        CurrentBuyLottery.ClaimTo(_msgSender());
    } 

    function ClaimOf(address addr) public view returns (uint256)
    {
        return CurrentBuyLottery.ClaimOf(addr);
    }

    function _simpleTransfer(address from, address to, uint256 amount) private
    {
        CurrentLedger.SetBalance(from, CurrentLedger.GetBalance(from).sub(amount));
        CurrentLedger.SetBalance(to, CurrentLedger.GetBalance(to).add(amount));   
        emit Transfer(from,to ,amount);
    }




}

