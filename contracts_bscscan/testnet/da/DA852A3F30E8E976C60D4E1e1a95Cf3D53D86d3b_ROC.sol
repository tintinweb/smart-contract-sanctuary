pragma solidity >=0.8.0;
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract Ledger is Context, Ownable
{
    using SafeMath for uint256;
    using Address for address; 
    IERC20 public CurrentToken;
    mapping(address => uint256) public Balances;
    event LedgerMigration(address from, address to);

    modifier onlyCurrentTokenOrOwner
    {
        require(_msgSender() == address(CurrentToken) ||
                _msgSender() == owner());
        _;
    }
    modifier onlyCurrentToken
    {
        require(_msgSender() == address(CurrentToken));
        _;
    }
    constructor()
    {

    }
    
    function Migrate(IERC20 newToken) public onlyCurrentTokenOrOwner
    {
        emit LedgerMigration(address(CurrentToken), address(newToken));
        CurrentToken = newToken;
        
    }
    function SetBalance(address addr, uint256 amount) public onlyCurrentToken 
    {
        Balances[addr] = amount;
    }
    function GetBalance(address addr) public view returns(uint256)
    {
        return Balances[addr];
    }



}

