contract CollectNFT is Ownable, Pausable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    uint256 public NFTPrice = 50 ether;
    uint256 public BONUS_REF = 50;
    uint256 public constant PERCENT_DIVIDER = 1000; 

    // The token being sold
    PixelNFT public pHeroNFT;

    // Address where funds are collected
    address payable public wallet;

    event SaleStarted(uint256 block);
    event SaleEnded(uint256 block);
    event AdminWithdrawal(address token, uint256 amount);
    event NFTPurchased(address indexed purchaser, uint256 value, uint256 tokenId);
    event NFTPurchasedMultiple(address indexed purchaser, uint256 value, uint indexed firstTokenId, uint indexed lastTokenId);

    constructor(address _pixelhNFT) {
        require(_pixelhNFT != address(0));

        pHeroNFT = PixelNFT(_pixelhNFT);
        wallet = payable(pHeroNFT.owner());
    }

    function mintPackage(string memory _URI, address ref) public whenNotPaused returns(bool) {
        require(msg.sender != ref, "You cant send money to yourself");
        uint refBonus = NFTPrice.mul(BONUS_REF).div(PERCENT_DIVIDER);
       
        uint toWallet = NFTPrice.sub(refBonus);

        // Mint NFT
        pHeroNFT.safeMint(msg.sender, _URI);
        emit NFTPurchased(msg.sender, NFTPrice, pHeroNFT.currentId());
        return true;
    }

    function bachMyNFT(string[] memory _URIS, address user) public onlyOwner returns(bool) {
        uint amount = _URIS.length;
        // Mint NFT
        uint firstTokenId = pHeroNFT.currentId().add(1);
        for (uint i = 0; i < amount; i++) {
            pHeroNFT.safeMint(user, _URIS[i]);
        }

        return true;
    }

    function setBusdAddress(address _BUSD_address) public onlyOwner {
        require(_BUSD_address != address(0));

    }

    function setWalletAddress(address _wallet_address) public onlyOwner {
        require(_wallet_address != address(0));
        wallet = payable(_wallet_address);
    }

    function setBonusRef(uint256 _bonus_ref) public onlyOwner {
        require(_bonus_ref <= PERCENT_DIVIDER, "The bonus ref must be less than or equal to PERCENT_DIVIDER");
        BONUS_REF = _bonus_ref;
    }

    function setNFTPrice(uint256 _NFTPrice) public onlyOwner {
        require(_NFTPrice != 0);
        NFTPrice = _NFTPrice;
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function setpixelAddress(address _pixel_address) public onlyOwner {
        require(_pixel_address != address(0));
        pHeroNFT = PixelNFT(_pixel_address);
    }

}