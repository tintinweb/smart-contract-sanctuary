pragma solidity 0.8.0;
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./utils/IReferral.sol";
import "./TreatToken.sol";

contract TreatsFactory is AccessControl, ReentrancyGuard {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    bytes32 internal constant GOVERNANCE_ROLE = keccak256("GOVERNANCE_ROLE");
    // Info of each user.
    struct UserInfo {
        uint256 amount;
        uint256 rewardDebt;
    }

    struct DepositTracker {
        uint256 time;
        uint256 amount;
    }

    mapping(address => mapping(uint256 => DepositTracker)) public subjectToFee;

    struct PoolInfo {
        IERC20 lpToken; // Address of LP token contract.
        uint256 allocPoint; // How many allocation points assigned to this pool. TREATs to distribute per block.
        uint256 lastRewardBlock; // Last block number that TREATs distribution occurs.
        uint256 accTreatPerShare; // Accumulated TREATs per share, times 1e12. See below.
        uint256 minStake;
        uint256 minHoldTime;
        uint16 earlyWithdrawFee; // Deposit fee in basis points
    }

    // The TREAT TOKEN!
    GoldenTreat public treat;
    address public referralAddress;
    // TREAT tokens created per block.
    uint256 public treatPerBlock;
    uint256 public constant BONUS_MULTIPLIER = 1;
    // Deposit Fee address
    address public feeAddress;

    // Info of each pool.
    PoolInfo[] public poolInfo;
    // Info of each user that stakes LP tokens.
    mapping(uint256 => mapping(address => UserInfo)) public userInfo;
    // Total allocation points. Must be the sum of all allocation points in all pools.
    uint256 public totalAllocPoint;
    // The block number when TREAT mining starts.
    uint256 public startBlock;
    uint256 public referralMiningPercent;
    uint256 public referralRewardPercent;

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(
        address indexed user,
        uint256 indexed pid,
        uint256 amount
    );
    event SetFeeAddress(address indexed user, address indexed newAddress);
    event SetReferralAddress(address indexed user, address indexed newAddress);
    event SetReferralMiningPercent(address indexed user, uint256 newPercent);
    event UpdateEmissionRate(address indexed user, uint256 goosePerBlock);

    constructor(
        GoldenTreat _treat,
        address _feeAddress,
        uint256 _treatPerBlock,
        uint256 _startBlock,
        uint256 _referralMiningPercent,
        uint256 _referralRewardPercent,
        address _referralAddress
    ) {
        treat = _treat;
        referralAddress = _referralAddress;
        feeAddress = _feeAddress;
        treatPerBlock = _treatPerBlock;
        startBlock = _startBlock;
        referralMiningPercent = _referralMiningPercent;
        referralRewardPercent = _referralRewardPercent;

        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _setupRole(GOVERNANCE_ROLE, _msgSender());
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    mapping(IERC20 => bool) public poolExistence;
    modifier nonDuplicated(IERC20 _lpToken) {
        require(poolExistence[_lpToken] == false, "nonDuplicated: duplicated");
        _;
    }

    // Add a new lp to the pool. Can only be called by the owner.
    function add(
        uint256 _allocPoint,
        IERC20 _lpToken,
        uint256 _minStake,
        uint256 _minHoldTime,
        uint16 _earlyWithdrawFee,
        bool _withUpdate
    ) public onlyRole(GOVERNANCE_ROLE) nonDuplicated(_lpToken) {
        require(
            _earlyWithdrawFee <= 10000,
            "add: invalid deposit fee basis points"
        );
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 lastRewardBlock = block.number > startBlock
            ? block.number
            : startBlock;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolExistence[_lpToken] = true;
        poolInfo.push(
            PoolInfo({
                lpToken: _lpToken,
                allocPoint: _allocPoint,
                lastRewardBlock: lastRewardBlock,
                accTreatPerShare: 0,
                minStake: _minStake,
                minHoldTime: _minHoldTime,
                earlyWithdrawFee: _earlyWithdrawFee
            })
        );
    }

    // Update the given pool.
    function set(
        uint256 _pid,
        uint256 _allocPoint,
        uint256 _minStake,
        uint256 _minHoldTime,
        uint16 _earlyWithdrawFee,
        bool _withUpdate
    ) public onlyRole(GOVERNANCE_ROLE) {
        require(
            _earlyWithdrawFee <= 10000,
            "set: invalid deposit fee basis points"
        );
        if (_withUpdate) {
            massUpdatePools();
        }
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(
            _allocPoint
        );
        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].earlyWithdrawFee = _earlyWithdrawFee;
        poolInfo[_pid].minStake = _minStake;
        poolInfo[_pid].minHoldTime = _minHoldTime;
    }

    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to)
        public
        pure
        returns (uint256)
    {
        return _to.sub(_from).mul(BONUS_MULTIPLIER);
    }

    // View function to see pending TREATs on frontend.
    function pendingTreat(uint256 _pid, address _user)
        external
        view
        returns (uint256)
    {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accTreatPerShare = pool.accTreatPerShare;
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (block.number > pool.lastRewardBlock && lpSupply != 0) {
            uint256 multiplier = getMultiplier(
                pool.lastRewardBlock,
                block.number
            );
            uint256 treatReward = multiplier
                .mul(treatPerBlock)
                .mul(pool.allocPoint)
                .div(totalAllocPoint);
            accTreatPerShare = accTreatPerShare.add(
                treatReward.mul(1e12).div(lpSupply)
            );
        }
        return user.amount.mul(accTreatPerShare).div(1e12).sub(user.rewardDebt);
    }

    // Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        }
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.number <= pool.lastRewardBlock) {
            return;
        }
        uint256 lpSupply = pool.lpToken.balanceOf(address(this));
        if (lpSupply == 0 || pool.allocPoint == 0) {
            pool.lastRewardBlock = block.number;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardBlock, block.number);
        uint256 treatReward = multiplier
            .mul(treatPerBlock)
            .mul(pool.allocPoint)
            .div(totalAllocPoint);
        treat.mint(
            referralAddress,
            treatReward.mul(referralMiningPercent).div(10000)
        );
        treat.mint(address(this), treatReward);
        pool.accTreatPerShare = pool.accTreatPerShare.add(
            treatReward.mul(1e12).div(lpSupply)
        );
        pool.lastRewardBlock = block.number;
    }

    // Deposit LP tokens for TREAT allocation.
    function deposit(
        uint256 _pid,
        uint256 _amount,
        address _referrer,
        uint256 _percent
    ) public nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        updatePool(_pid);
        if (_amount > 0) {
            if (pool.minHoldTime > 0) {
                _tokenSubjectToFeeUpdate(
                    msg.sender,
                    _amount,
                    pool.minHoldTime,
                    _pid
                );
            }
        }

        if (IReferral(referralAddress).getStatus() && _referrer != address(0)) {
            if (
                IReferral(referralAddress).checkUserReferrer(msg.sender) ==
                address(0)
            ) {
                IReferral(referralAddress).addReferral(
                    _referrer,
                    msg.sender,
                    _percent
                );
            }
        }

        if (user.amount > 0) {
            uint256 pending = user
                .amount
                .mul(pool.accTreatPerShare)
                .div(1e12)
                .sub(user.rewardDebt);
            if (pending > 0) {
                treat.burn(pending);
                treat.mint(msg.sender, pending);
                if (
                    IReferral(referralAddress).checkUserReferrer(msg.sender) !=
                    address(0)
                ) {
                    uint256 amountOfReward = pending
                        .mul(referralRewardPercent)
                        .div(10000);
                    IReferral(referralAddress).addAmount(
                        msg.sender,
                        amountOfReward
                    );
                }
            }
        }
        if (_amount > 0) {
            pool.lpToken.safeTransferFrom(
                address(msg.sender),
                address(this),
                _amount
            );
            user.amount = user.amount.add(_amount);
        }
        if (user.amount > 0) {
            require(user.amount >= pool.minStake, "Minimum Stake Not reached!");
        }
        user.rewardDebt = user.amount.mul(pool.accTreatPerShare).div(1e12);
        emit Deposit(msg.sender, _pid, _amount);
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(uint256 _pid, uint256 _amount) public nonReentrant {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        DepositTracker storage subjectToFeeDataStorage = subjectToFee[
            msg.sender
        ][_pid];
        updatePool(_pid);
        uint256 sendToFee;
        if (_amount > 0) {
            if (
                subjectToFeeDataStorage.amount > 0 ||
                subjectToFeeDataStorage.time > 0
            ) {
                _tokenSubjectToFeeUpdate(msg.sender, 0, 0, _pid);
                if (user.amount.sub(subjectToFeeDataStorage.amount) < _amount) {
                    uint256 noFeeTaken = user.amount.sub(
                        subjectToFeeDataStorage.amount
                    );
                    uint256 forFeeDeduction = _amount.sub(noFeeTaken);
                    uint256 deductFee = forFeeDeduction
                        .mul(pool.earlyWithdrawFee)
                        .div(10000);
                    uint256 sendToUser = deductFee.add(noFeeTaken);
                    sendToFee = _amount.sub(sendToUser);
                    pool.lpToken.transfer(feeAddress, sendToFee);
                }
            }
        }
        require(user.amount >= _amount, "withdraw: not good");
        uint256 pending = user.amount.mul(pool.accTreatPerShare).div(1e12).sub(
            user.rewardDebt
        );
        if (pending > 0) {
            treat.burn(pending);
            treat.mint(msg.sender, pending);
            if (
                IReferral(referralAddress).checkUserReferrer(msg.sender) !=
                address(0)
            ) {
                uint256 amountOfReward = pending.mul(referralRewardPercent).div(
                    10000
                );
                IReferral(referralAddress).addAmount(
                    msg.sender,
                    amountOfReward
                );
            }
        }
        if (_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.lpToken.transfer(address(msg.sender), _amount.sub(sendToFee));
        }
        if (user.amount > 0) {
            require(user.amount >= pool.minStake, "Minimum Stake Not reached!");
        }
        user.rewardDebt = user.amount.mul(pool.accTreatPerShare).div(1e12);
        emit Withdraw(msg.sender, _pid, _amount);
    }

    // Safe treat transfer function, just in case if rounding error causes pool to not have enough TREATs.
    function _safeTreatTransfer(address _to, uint256 _amount) internal {
        uint256 treatBal = treat.balanceOf(address(this));
        bool transferSuccess = false;
        if (_amount > treatBal) {
            transferSuccess = treat.transfer(_to, treatBal);
        } else {
            transferSuccess = treat.transfer(_to, _amount);
        }
        require(transferSuccess, "safeTreatTransfer: transfer failed");
    }

    // Update dev address by the previous dev.
    function updateReferralAddress(address _referralAddress)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        referralAddress = _referralAddress;
        emit SetReferralAddress(msg.sender, _referralAddress);
    }

    // Update dev address by the previous dev.
    function updateReferralMiningPercent(uint256 _referralMiningPercent)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        referralMiningPercent = _referralMiningPercent;
        emit SetReferralMiningPercent(msg.sender, _referralMiningPercent);
    }

    function setFeeAddress(address _feeAddress) public {
        require(msg.sender == feeAddress, "setFeeAddress: FORBIDDEN");
        feeAddress = _feeAddress;
        emit SetFeeAddress(msg.sender, _feeAddress);
    }

    //Pancake has to add hidden dummy pools inorder to alter the emission, here we make it simple and transparent to all.
    function updateEmissionRate(uint256 _treatPerBlock)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        massUpdatePools();
        treatPerBlock = _treatPerBlock;
        emit UpdateEmissionRate(msg.sender, _treatPerBlock);
    }

    function _tokenSubjectToFeeUpdate(
        address _depositor,
        uint256 _amount,
        uint256 _freeWithdrawIn,
        uint256 _pid
    ) internal {
        DepositTracker storage subjectToFeeDataStorage = subjectToFee[
            _depositor
        ][_pid];
        PoolInfo storage pool = poolInfo[_pid];
        if (subjectToFeeDataStorage.time == 0) {
            if (_amount > 0) {
                subjectToFeeDataStorage.time = block.timestamp.add(
                    _freeWithdrawIn
                );
                subjectToFeeDataStorage.amount = _amount;
            }
        }
        // If wallet has prior transactions
        else {
            uint256 coolDownRemaining;
            // Calculate how much time is left to vest tokens
            if (subjectToFeeDataStorage.time < block.timestamp) {
                coolDownRemaining == 0;
            } else {
                coolDownRemaining = subjectToFeeDataStorage.time.sub(
                    block.timestamp
                );
            }
            // If there is no vesting, delete
            if (coolDownRemaining == 0) {
                delete subjectToFee[_depositor][_pid];
            }
            // If there is vesting calculate remaining tokens that need vesting.
            else {
                uint256 remainingPercentage = coolDownRemaining
                    .mul(10**9)
                    .div(pool.minHoldTime)
                    .mul(10000)
                    .div(10**9);
                subjectToFeeDataStorage.amount = subjectToFeeDataStorage
                    .amount
                    .mul(remainingPercentage)
                    .div(10000);
            }
            subjectToFeeDataStorage.time = block.timestamp.add(_freeWithdrawIn);
            subjectToFeeDataStorage.amount = subjectToFeeDataStorage.amount.add(
                _amount
            );
        }
    }
}

