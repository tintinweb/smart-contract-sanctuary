    {
        _AddLiquidity[_address] = _excluded;
    }

    /*
     * @title Set liquify trigger amount
     * @param amount
     * @dev The caller must have the `GOVERNANCE_ROLE`
     */
    function setLiquifyTriggerAmount(uint256 _liquifyTriggerAmount)
        public
        onlyRole(GOVERNANCE_ROLE)
    {
        liquifyTriggerAmount = _liquifyTriggerAmount;
    }

    /*
     * @title Set vesting settings
     * @param Vesting percentage
     * @param Vesting lock after purchase amount
     * @param Token vesting bool
     * @dev The caller must have the `GOVERNANCE_ROLE`
     */
    function setVestingSettings(
        uint256 _vestingPercentage,
        uint256 _vestingLockAfterPurchase,
        bool _tokenVesting
    ) public onlyRole(GOVERNANCE_ROLE) {
        vestingPercentage = _vestingPercentage;
        vestingLockAfterPurchase = _vestingLockAfterPurchase;
        tokenVesting = _tokenVesting;
    }

    function _tokenVestingUpdate(
        address _recipient,
        uint256 _amount,
        bool _liquidity
    ) internal {
        VestingData storage tokenVestingDataStorage;

        if (_liquidity) {
            tokenVestingDataStorage = liquidityLock[_recipient];
        } else {
            tokenVestingDataStorage = tokenVestingData[_recipient];
        }
        if (tokenVestingDataStorage.time == 0) {
            if (_amount > 0) {
                tokenVestingDataStorage.time = block.timestamp.add(
                    vestingLockAfterPurchase
                );
                tokenVestingDataStorage.amount = _amount
                    .mul(vestingPercentage)
                    .div(10000);
            }
        }
        // If wallet has prior transactions
        else {
            // Calculate how much time is left to vest tokens
            uint256 coolDownRemaining = tokenVestingDataStorage.time.sub(
                block.timestamp
            );
            // If there is no vesting, delete
            if (coolDownRemaining == 0) {
                delete tokenVestingData[_recipient];
            }
            // If there is vesting calculate remaining tokens that need vesting.
            else {
                if (coolDownRemaining > vestingLockAfterPurchase)
                    coolDownRemaining = vestingLockAfterPurchase;
                uint256 remainingPercentage = coolDownRemaining
                    .mul(10**18)
                    .div(vestingLockAfterPurchase)
                    .mul(10000)
                    .div(10**18);

                tokenVestingDataStorage.amount = tokenVestingDataStorage
                    .amount
                    .mul(remainingPercentage)
                    .div(10000);
            }

            tokenVestingDataStorage.time = block.timestamp.add(
                vestingLockAfterPurchase
            );
            tokenVestingDataStorage.amount = tokenVestingDataStorage.amount.add(
                _amount.mul(vestingPercentage).div(10000)
            );
        }
    }

    function viewNotVestedTokens(address recipient)
        public
        view
        returns (uint256 locked)
    {
        VestingData memory transactionlocked = tokenVestingData[recipient];

        if (transactionlocked.time == 0) {
            locked = 0;
        } else {
            uint256 coolDownRemaining = transactionlocked.time.sub(
                block.timestamp
            );
            if (coolDownRemaining == 0) {
                locked = 0;
            } else {
                uint256 remainingPercentage = coolDownRemaining
                    .mul(10**18)
                    .div(vestingLockAfterPurchase)
                    .mul(10000)
                    .div(10**18);

                locked = transactionlocked.amount.mul(remainingPercentage).div(
                    10000
                );
            }
        }
    }

    function recoverUnVested(address _from, uint256 _toRecover) internal {
        _balances[burnAddress] += _toRecover;
        emit Transfer(_from, burnAddress, _toRecover);
    }

    function standardTransfer(
        address from,
        address to,
        uint256 amount,
        bool fromVesting,
        bool toVesting
    ) internal returns (uint256 amountOut) {
        uint256 willSend;
        uint256 toRecover;
        uint256 senderBalance = _balances[from];
        if (fromVesting) {
            _tokenVestingUpdate(from, 0, false);
            // First we check how much of the tokens will be burned.
            VestingData storage transactionlockedFrom = tokenVestingData[from];
            uint256 vestedTokens = balanceOf(from).sub(
                transactionlockedFrom.amount
            );
            uint256 vestedPercent = vestedTokens
                .mul(10**18)
                .div(senderBalance)
                .mul(10000)
                .div(10**18);
            uint256 toRecoverPercent = transactionlockedFrom
                .amount
                .mul(10**18)
                .div(senderBalance)
                .mul(10000)
                .div(10**18);
            willSend = amount.mul(vestedPercent).div(10000);
            toRecover = amount.mul(toRecoverPercent).div(10000);
            // Find out what portion of the balance is locked and unlocked
            if (toRecover > 0) {
                if (exchangeAddress[to]) {
                    _balances[address(this)] += toRecover;
                    emit Transfer(from, address(this), toRecover);
                    _takeTax(toRecover);
                    if (
                        liquidityBalance.add(buyBackBalance) >
                        liquifyTriggerAmount
                    ) {
                        _swapAndLiquify();
                    }
                } else {
                    recoverUnVested(from, toRecover);
                }

                transactionlockedFrom.amount = transactionlockedFrom.amount.sub(
                    toRecover
                );
            }
        } else {
            willSend = amount;
            toRecover = 0;
        }

        // send to recover token portion that is made to be recovered
        require(senderBalance >= amount);
        unchecked {
            _balances[from] = senderBalance - amount;
        }

        _balances[to] += willSend;
        emit Transfer(from, to, willSend);
        if (toVesting) {
            _tokenVestingUpdate(to, willSend, false);
        }
        return amountOut = willSend;
    }

    function liquidityTransfer(
        address from,
        address to,
        uint256 amount
    ) internal returns (uint256 amountOut) {
        uint256 senderBalance = _balances[from];
        require(senderBalance >= amount);

        if (_AddLiquidity[to]) {
            _tokenVestingUpdate(from, 0, false);
            _tokenVestingUpdate(from, 0, true);
            VestingData storage transactionlocked = tokenVestingData[from];
            uint256 percentage = (((amount * 10**18) / senderBalance) * 10000) /
                10**18;
            uint256 toReduce = transactionlocked.amount.mul(percentage).div(
                10000
            );
            transactionlocked.amount = transactionlocked.amount.sub(toReduce);
            if (toReduce > 0) {
                _tokenVestingUpdate(from, toReduce, true);
            }
        } else {
            _tokenVestingUpdate(from, 0, false);
            _tokenVestingUpdate(from, 0, true);
            VestingData storage transactionlocked = liquidityLock[to];
            uint256 percentage = (((amount * 10**18) / senderBalance) * 10000) /
                10**18;
            uint256 toReduce = transactionlocked.amount.mul(percentage).div(
                10000
            );
            transactionlocked.amount = transactionlocked.amount.sub(toReduce);
        }

        unchecked {
            _balances[from] = senderBalance - amount;
        }
        _balances[to] += amount;
        emit Transfer(from, to, amount);
        return amountOut = amount;
    }

    function transferNoVesting(
        address from,
        address to,
        uint256 amount
    ) internal returns (uint256 amountOut) {
        uint256 senderBalance = _balances[from];
        require(senderBalance >= amount);
        unchecked {
            _balances[from] = senderBalance - amount;
        }
        delete tokenVestingData[from];
        delete tokenVestingData[to];
        _balances[to] += amount;
        emit Transfer(from, to, amount);
        return amountOut = amount;
    }

    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _amount
    ) internal virtual returns (uint256 amountOut) {
        if (_from != address(0)) {
            if (tokenVesting == false) {
                return amountOut = transferNoVesting(_from, _to, _amount);
            } else if (_AddLiquidity[_from] || _AddLiquidity[_to]) {
                return amountOut = liquidityTransfer(_from, _to, _amount);
            } else if (_isExcludedFromFee[_from]) {
                if (_isExcludedFromFee[_to]) {
                    //SENDER AND RECEIVER HAS NO VESTING
                    return
                        amountOut = standardTransfer(
                            _from,
                            _to,
                            _amount,
                            false,
                            false
                        );
                } else {
                    // RECEIVER HAS VESTING
                    return
                        amountOut = standardTransfer(
                            _from,
                            _to,
                            _amount,
                            false,
                            true
                        );
                }
            } else if (_isExcludedFromFee[_to]) {
                //NO VESTING FOR RECEIVER
                return
                    amountOut = standardTransfer(
                        _from,
                        _to,
                        _amount,
                        true,
                        false
                    );
            } else {
                amountOut = standardTransfer(_from, _to, _amount, true, true);
            }
        }
    }

    function _takeTax(uint256 _amount)
        internal
        returns (uint256 amountAfterFee)
    {
        ExchangeTax storage tax = SellTax;

        uint256 _marketingBalance = _amount.mul(tax.marketing).div(10000);
        uint256 _liquidityAmount = _amount.mul(tax.liquidity).div(10000);
        uint256 _buyBackAmount = _amount.mul(tax.buyBack).div(10000);
        uint256 _burnAmount = _amount.mul(tax.burned).div(10000);

        uint256 totalFeeAmount = _marketingBalance
            .add(_liquidityAmount)
            .add(_buyBackAmount)
            .add(_burnAmount);

        if (_marketingBalance != 0) {
            transferNoVesting(
                address(this),
                marketingWallet,
                _marketingBalance
            );
        }
        if (_liquidityAmount != 0) {
            liquidityBalance += _liquidityAmount;
        }
        if (_buyBackAmount != 0) {
            buyBackBalance += _buyBackAmount;
        }
        if (_burnAmount != 0) {
            transferNoVesting(address(this), burnAddress, _burnAmount);
        }
        return totalFeeAmount;
    }

    function _afterTokenTransfer() internal virtual {}

    //take amount of tokens for lp fee + buybackBabyDoge Fee
    function _swapAndLiquify() internal {
        address weth = IBabyDogeV2Router02(ROUTER).WETH();

        // Split the Token balance into halves
        uint256 halfForTokenLP = liquidityBalance.div(2);
        uint256 totalToSwap = halfForTokenLP.add(buyBackBalance);
        uint256 expectedBuyBackWETH = _getQuote(
            buyBackBalance,
            address(this),
            weth
        );
        buyBackBalance = 0;
        liquidityBalance = 0;

        uint256 ethBalanceBeforeSwap = address(this).balance;
        _swapTokens(address(this), weth, totalToSwap, address(this));

        uint256 ethReceivedFromSwap = address(this).balance.sub(
            ethBalanceBeforeSwap
        );
        uint256 ethToLiquidity = ethReceivedFromSwap.sub(expectedBuyBackWETH);
        _addLiquidity(halfForTokenLP, ethToLiquidity);
        if (expectedBuyBackWETH > 0)
            _swapTokens(
                weth,
                babyDogeAddress,
                expectedBuyBackWETH,
                address(this)
            );
        IERC20(babyDogeAddress).transfer(
            distributionContract,
            IERC20(babyDogeAddress).balanceOf(address(this))
        );
        emit SwapAndLiquify(
            halfForTokenLP,
            ethReceivedFromSwap,
            halfForTokenLP
        );
    }

    function _addLiquidity(uint256 _tokenAmount, uint256 _ethAmount) private {
        _approve(address(this), ROUTER, _tokenAmount);
        IBabyDogeV2Router02(ROUTER).addLiquidityETH{value: _ethAmount}(
            address(this),
            _tokenAmount,
            0,
            0,
            address(this),
            block.timestamp
        );
    }

    function _getQuote(
        uint256 _amountIn,
        address _fromTokenAddress,
        address _toTokenAddress
    ) internal view returns (uint256 amountOut) {
        address pair = IBabyDogeV2Factory(IBabyDogeV2Router02(ROUTER).factory())
            .getPair(_fromTokenAddress, _toTokenAddress);
        (uint256 reserve0, uint256 reserve1, ) = IBabyDogeV2Pair(pair)
            .getReserves();
        address token0 = IBabyDogeV2Pair(pair).token0();
        (uint256 reserveIn, uint256 reserveOut) = token0 == _fromTokenAddress
            ? (reserve0, reserve1)
            : (reserve1, reserve0);
        uint256 amountInWithFee = _amountIn.mul(997);
        uint256 numerator = amountInWithFee.mul(reserveOut);
        uint256 denominator = reserveIn.mul(1000).add(amountInWithFee);
        amountOut = numerator.div(denominator);
    }

    function _swapTokens(
        address _FromTokenContractAddress,
        address _ToTokenContractAddress,
        uint256 _amountIn,
        address _forWallet
    ) internal returns (uint256 tokenBought) {
        address[] memory path;
        address WETH = IBabyDogeV2Router02(ROUTER).WETH();
        uint256 amountOutMin;

        if (_ToTokenContractAddress == WETH) {
            path = new address[](2);
            path[0] = _FromTokenContractAddress;
            path[1] = WETH;
            IERC20(path[0]).approve(ROUTER, _amountIn);
            amountOutMin = _getQuote(_amountIn, path[0], path[1]);
            tokenBought = IBabyDogeV2Router02(ROUTER).swapExactTokensForETH(
                _amountIn,
                amountOutMin,
                path,
                _forWallet,
                block.timestamp + 1200
            )[path.length - 1];
        } else {
            path = new address[](2);
            path[0] = WETH;
            path[1] = _ToTokenContractAddress;
            amountOutMin = _getQuote(_amountIn, path[0], path[1]);
            amountOutMin = amountOutMin.mul(8000).div(10000);
            tokenBought = IBabyDogeV2Router02(ROUTER).swapExactETHForTokens{
                value: _amountIn
            }(amountOutMin, path, _forWallet, block.timestamp + 1200)[
                path.length - 1
            ];
        }
    }

    function getCurrentVotes(address _account) external view returns (uint256) {
        uint256 nCheckpoints = numCheckpoints[_account];
        return
            nCheckpoints > 0
                ? checkpoints[_account][nCheckpoints - 1].votes
                : 0;
    }

    function delegate(address _delegatee) public {
        return _delegate(msg.sender, _delegatee);
    }

    function _delegate(address _delegator, address _delegatee) internal {
        address currentDelegate = delegates[_delegator];
        uint256 delegatorBalance = _balances[_delegator];
        delegates[_delegator] = _delegatee;

        emit DelegateChanged(_delegator, currentDelegate, _delegatee);

        _moveDelegates(
            currentDelegate,
            _delegatee,
            delegatorBalance,
            delegatorBalance
        );
    }

    function _moveDelegates(
        address _srcRep,
        address _dstRep,
        uint256 _amountOut,
        uint256 _amountIn
    ) internal {
        if (_srcRep != _dstRep && _amountOut > 0) {
            if (_srcRep != address(0)) {
                uint256 srcRepNum = numCheckpoints[_srcRep];
                uint256 srcRepOld = srcRepNum > 0
                    ? checkpoints[_srcRep][srcRepNum - 1].votes
                    : 0;
                uint256 srcRepNew = srcRepOld.sub(_amountOut);
                _writeCheckpoint(_srcRep, srcRepNum, srcRepOld, srcRepNew);
            }

            if (_dstRep != address(0)) {
                uint256 dstRepNum = numCheckpoints[_dstRep];
                uint256 dstRepOld = dstRepNum > 0
                    ? checkpoints[_dstRep][dstRepNum - 1].votes
                    : 0;
                uint256 dstRepNew = dstRepOld.add(_amountIn);
                _writeCheckpoint(_dstRep, dstRepNum, dstRepOld, dstRepNew);
            }
        }
    }

    function _writeCheckpoint(
        address _delegatee,
        uint256 _nCheckpoints,
        uint256 _oldVotes,
        uint256 _newVotes
    ) internal {
        uint256 blockNumber = block.number;

        if (
            _nCheckpoints > 0 &&
            checkpoints[_delegatee][_nCheckpoints - 1].fromBlock == blockNumber
        ) {
            checkpoints[_delegatee][_nCheckpoints - 1].votes = _newVotes;
        } else {
            checkpoints[_delegatee][_nCheckpoints] = Checkpoint(
                blockNumber,
                _newVotes
            );
            numCheckpoints[_delegatee] = _nCheckpoints + 1;
        }

        emit DelegateVotesChanged(_delegatee, _oldVotes, _newVotes);
    }

    function getPriorVotes(address account, uint256 blockNumber)
        public
        view
        returns (uint256)
    {
        require(blockNumber < block.number);
        uint256 nCheckpoints = numCheckpoints[account];
        if (nCheckpoints == 0) {
            return 0;
        }
        if (checkpoints[account][nCheckpoints - 1].fromBlock <= blockNumber) {
            return checkpoints[account][nCheckpoints - 1].votes;
        }
        if (checkpoints[account][0].fromBlock > blockNumber) {
            return 0;
        }

        uint256 lower = 0;
        uint256 upper = nCheckpoints - 1;
        while (upper > lower) {
            uint256 center = upper - (upper - lower) / 2;
            Checkpoint memory cp = checkpoints[account][center];
            if (cp.fromBlock == blockNumber) {
                return cp.votes;
            } else if (cp.fromBlock < blockNumber) {
                lower = center;
            } else {
                upper = center - 1;
            }
        }
        return checkpoints[account][lower].votes;
    }
}

