        ERC20TransferProxy _erc20TransferProxy,
        ERC721SaleNonceHolder _nonceHolder,
        ERC1155SaleNonceHolder _nonceHolder1155,
        address payable _beneficiary
    ) AbstractSale(_beneficiary) {
        transferProxy = _transferProxy;
