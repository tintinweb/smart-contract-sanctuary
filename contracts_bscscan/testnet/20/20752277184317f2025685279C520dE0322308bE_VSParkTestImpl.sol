pragma solidity ^0.8.2;
import "./VSParkImpl.sol";

contract VSParkTestImpl is VSParkImpl {
    function __VSParkTest_init(
        uint256 initBoxPrice,
        string memory name,
        string memory symbol,
        string memory initBaseURI,
        address BUSDAddress,
        address initProxyRegistryAddress
    ) public initializer {
        __VSPark_init(initBoxPrice, name, symbol, initBaseURI, BUSDAddress, initProxyRegistryAddress);
    }

    function mint(uint256[] memory categoryIds) public {
        for (uint256 i = 0; i < categoryIds.length; i++) {
            uint256 newId = _awardItem(_msgSender(), categoryIds[i]);
            emit BoxAwarded(_msgSender(), newId, block.timestamp);
        }
    }
}

