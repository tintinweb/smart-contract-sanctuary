        revert("Target is not in range");
    }

    /* ================ VIEWS ================ */

    /**
     * @dev returns the number of remaining stock
     */
    function getStock() external view override returns (uint256) {
        return _stockCount;
    }

    function getStockPrefixSum() external view returns (uint32[] memory) {
        return _cumulativeStockByCategory;
    }

    /* ================ UTIL FUNCTIONS ================ */

    /**
     * @dev randomly pick an item from the remaining stock
     * @return returns the location of the picked item in the range of [0, stock)
     */
    function _pickRandLocation() private returns (uint256) {
        uint256 index = _random(_stockCount);
        uint256 location = _indexMapping[index];
        if (location == 0) {
            location = index;
        }
        uint256 lastIndexLocation = _indexMapping[_stockCount - 1];
        if (lastIndexLocation == 0) {
            lastIndexLocation = _stockCount - 1;
        }
        _indexMapping[index] = lastIndexLocation;
        _stockCount--;
        return location;
    }

    /**
     * @dev return a random integer from [0, range)
     */
    function _random(uint256 range) private view returns (uint256) {
        uint256 salt = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, _stockCount)));
        uint256 seed = IRandom(randomGeneratorAddress).generateSignature(salt);
        return uint256(keccak256(abi.encodePacked(seed))) % (range);
    }

    /* ================ ADMIN ACTIONS ================ */

    /**
     * @dev Set random generator contract address
     * @param newAddress new random generator address
