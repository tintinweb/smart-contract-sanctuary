pragma solidity ^0.8.2;
pragma abicoder v2;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/MathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "../interfaces/IVSPark.sol";
import "./VSParkStockImpl.sol";

contract OwnableDelegateProxy {}

contract ProxyRegistry {
    mapping(address => OwnableDelegateProxy) public proxies;
}

contract VSParkImpl is
    IVSPark,
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    AccessControlUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    UUPSUpgradeable,
    OwnableUpgradeable,
    VSParkStockImpl
{
    using StringsUpgradeable for uint256;
    using SafeMathUpgradeable for uint256;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;

    bytes32 public UPGRADER_ROLE;

    CountersUpgradeable.Counter public tokenCount;

    mapping(uint256 => uint256) private _tokenCategoryId;

    mapping(uint256 => uint256) private _categoryRecyclePrice;

    mapping(uint256 => Series) private _series;

    uint256 public boxPrice;

    string public baseURI;

    address public proxyRegistryAddress;

    bool public recycleLock;

    IERC20Upgradeable public BUSD;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function __VSPark_init(
        uint256 initBoxPrice,
        string memory name,
        string memory symbol,
        string memory initBaseURI,
        address BUSDAddress,
        address initProxyRegistryAddress
    ) public initializer {
        __Ownable_init();
        __Pausable_init();
        __AccessControl_init();
        __ReentrancyGuard_init();
        __ERC721_init(name, symbol);
        __UUPSUpgradeable_init();
        __VSParkStockImpl_init();
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
        _setupRole(UPGRADER_ROLE, msg.sender);
        _pause();
        BUSD=IERC20Upgradeable(BUSDAddress);
        recycleLock = true;
        boxPrice = initBoxPrice;
        baseURI = initBaseURI;
        proxyRegistryAddress = initProxyRegistryAddress;
    }

    /* ================ UTIL FUNCTIONS ================ */

    modifier _onlyAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, _msgSender()), "require admin permission");
        _;
    }

    modifier _notContract() {
        uint256 size;
        address addr = msg.sender;
        assembly {
            size := extcodesize(addr)
        }
        require(size == 0, "contract not allowed");
        require(msg.sender == tx.origin, "proxy contract not allowed");
        _;
    }

    modifier _existsToken(uint256 tokenId) {
        require(_exists(tokenId), "nonexistent token");
        _;
    }

    /* ================ VIEWS ================ */

    function series(uint256 seriesId) external view override returns (Series memory) {
        return _series[seriesId];
    }

    function categoryRecyclePrice(uint256 categoryId) public view override returns (uint256) {
        return _categoryRecyclePrice[categoryId];
    }

    function tokenDetail(uint256 tokenId) public view override _existsToken(tokenId) returns (uint256, uint256) {
        return (_tokenCategoryId[tokenId], _categoryRecyclePrice[_tokenCategoryId[tokenId]]);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable, AccessControlUpgradeable, IVSPark)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(IVSPark, ERC721Upgradeable)
        _existsToken(tokenId)
        returns (string memory)
    {
        string memory base = _baseURI();
        return bytes(base).length > 0 ? string(abi.encodePacked(base, tokenId.toString())) : "";
    }

    function _baseURI() internal view override(ERC721Upgradeable) returns (string memory) {
        return baseURI;
    }

    function implementationVersion() public pure override returns (string memory) {
        return "1.0.0";
    }

    function isApprovedForAll(address owner, address operator) public view override returns (bool) {
        if (proxyRegistryAddress != address(0)) {
            ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
            if (address(proxyRegistry.proxies(owner)) == operator) {
                return true;
            }
        }
        return super.isApprovedForAll(owner, operator);
    }

    function verifyRecycle(
        uint256[] memory tokenIds,
        uint256 seriesId,
        bool verifyOwner
    ) public view override returns (bool, uint256) {
        if (seriesId == 0) {
            uint256 recyclePrice = 0;
            for (uint256 i = 0; i < tokenIds.length; i++) {
                if (verifyOwner && ownerOf(tokenIds[i]) != msg.sender) return (false, 0);
                recyclePrice = recyclePrice.add(_categoryRecyclePrice[_tokenCategoryId[tokenIds[i]]]);
            }
            return (true, recyclePrice);
        }
        return verifySeries(tokenIds, seriesId, verifyOwner);
    }

    function verifySeries(
        uint256[] memory tokenIds,
        uint256 seriesId,
        bool verifyOwner
    ) internal view returns (bool, uint256) {
        if (tokenIds.length != _series[seriesId].tokenCategoryIds.length) return (false, 0);
        uint256 recyclePrice = 0;
        uint256 bucketSeries;
        uint256 bucketTokenIds;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            if (verifyOwner && ownerOf(tokenIds[i]) != msg.sender) return (false, 0);
            recyclePrice = recyclePrice.add(_categoryRecyclePrice[_tokenCategoryId[tokenIds[i]]]);
            bucketSeries |= 1 << _series[seriesId].tokenCategoryIds[i];
            bucketTokenIds |= 1 << _tokenCategoryId[tokenIds[i]];
        }
        if (bucketSeries != bucketTokenIds) return (false, 0);
        return (true, recyclePrice.mul(_series[seriesId].multiplier));
    }

    /* ================ INTERNAL FUNCTIONS ================ */

    function _awardItem(address receiver, uint256 categoryId) internal returns (uint256) {
        tokenCount.increment();
        uint256 newId = tokenCount.current();
        _tokenCategoryId[newId] = categoryId;
        _safeMint(receiver, newId);
        return newId;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721Upgradeable, ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    /* ================ TRANSACTIONS ================ */

    function recycle(uint256[] memory tokenIds, uint256 seriesId)
        external
        override
        _existsToken(tokenIds[0])
        whenNotPaused
        nonReentrant
    {
        require(!recycleLock, "recycle lock!");
        (bool success, uint256 recyclePrice) = verifyRecycle(tokenIds, seriesId, true);
        require(success, "recycle fail!");
        for (uint256 i = 0; i < tokenIds.length; i++) {
            _burn(tokenIds[i]);
        }
        BUSD.safeTransfer(_msgSender(), recyclePrice);
        emit RecycleSucceeded(_msgSender(), recyclePrice, block.timestamp);
    }

    function buy(uint256 quality) external override _notContract whenNotPaused nonReentrant {
        BUSD.safeTransferFrom(_msgSender(), address(this), boxPrice.mul(quality));
        for (uint256 i = 0; i < quality; i++) {
            uint256 categoryId = _pickTokenCategoryFromStock();
            uint256 newId = _awardItem(_msgSender(), categoryId);
            emit BoxAwarded(_msgSender(), newId, block.timestamp);
        }
    }

    /* ================ ADMIN ACTIONS ================ */
    function batchSetCategoryRecyclePrices(uint256[] memory categoryIds, uint256[] memory categoryRecyclePrices)
        external
        override
        _onlyAdmin
    {
        require(categoryIds.length == categoryRecyclePrices.length, "length error!");
        for (uint256 i = 0; i < categoryRecyclePrices.length; i++) {
            _categoryRecyclePrice[categoryIds[i]] = categoryRecyclePrices[i];
        }
    }

    function batchSetStock(uint32[] memory stock) external override _onlyAdmin {
        _setStock(stock);
    }

    function batchSetSeries(
        uint256[] memory seriesIds,
        uint256[][] memory tokenCategoryIds,
        uint256[] memory multipliers
    ) external override _onlyAdmin {
        require(
            seriesIds.length == tokenCategoryIds.length && tokenCategoryIds.length == multipliers.length,
            "length error!"
        );
        for (uint256 i = 0; i < seriesIds.length; i++) {
            _series[seriesIds[i]] = Series(tokenCategoryIds[i], multipliers[i]);
        }
    }

    function setRandomGenerator(address newAddress) external override _onlyAdmin {
        _setRandomGenerator(newAddress);
    }

    function _authorizeUpgrade(address) internal view override {
        require(hasRole(UPGRADER_ROLE, _msgSender()), "require upgrader permission");
    }

    function setBaseURI(string memory newBaseURI) external override _onlyAdmin {
        baseURI = newBaseURI;
    }

    function setRecycleLock(bool newrRecycleLock) external override _onlyAdmin {
        recycleLock = newrRecycleLock;
    }

    function pause() external override _onlyAdmin {
        _pause();
    }

    function unpause() external override _onlyAdmin {
        _unpause();
    }

    function setBoxPrice(uint256 newPrice) external override _onlyAdmin {
        boxPrice = newPrice;
        emit BoxPriceChanged(boxPrice, newPrice);
    }

    function withdraw(uint256 amount) external override _onlyAdmin {
        require(amount <= BUSD.balanceOf(address(this)), "over balance!");
        BUSD.safeTransfer(_msgSender(), amount);
    }
}

