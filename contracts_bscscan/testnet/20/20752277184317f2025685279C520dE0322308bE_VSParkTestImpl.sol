contract VSParkStockImpl is IVSParkStock, Initializable {
    uint256 private _stockCount; // the number of available tokens in the stock
    uint32[] private _cumulativeStockByCategory; // cumulative sum of the stock per category
    uint256[90000] private _indexMapping; // initialize as an array full of 0

    /* ================ GLOBAL CONFIGURATION ================ */
    address public randomGeneratorAddress;

    /**
     * @dev Initializes all the inherited contracts
     */
