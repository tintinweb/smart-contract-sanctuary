    /**
     * @dev initialize the _cumulativeStockByCategory array and set stockCount
     * @param stock an array contains the number of tokens for each token type
     */
    function _setStock(uint32[] memory stock) internal {
        uint32 sum = 0;
        for (uint32 i = 0; i < stock.length; i++) {
            sum += stock[i];
            _cumulativeStockByCategory.push(sum);
        }
        _stockCount = _cumulativeStockByCategory[_cumulativeStockByCategory.length - 1];
    }

    /**
     * @dev returns a random picked token category
     */
    function _pickTokenCategoryFromStock() internal returns (uint256) {
        require(_stockCount > 0, "Out of stock!");
        uint256 location = _pickRandLocation();
        uint256 category = _binarySearch(_cumulativeStockByCategory, location);
        emit PickToken(category);
        return category;
    }

    /**
     * @notice Searches the sorted array of ints for the specified value using the binary search algorithm
     * @dev Each item in the array is not duplicated and non-negative. There is only one answer for each value.
     * @param array The integer array to be searched. It should be sorted into ascending order.
     * @param target The value to be searched for.
