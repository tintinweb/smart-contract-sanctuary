    function _binarySearch(uint32[] memory array, uint256 target) internal pure returns (uint32) {
        uint32 left = 0;
        uint32 right = uint32(array.length) - 1;
        uint32 mid;
        while (left < right - 1) {
            mid = left + (right - left) / 2;
            if (array[mid] > target) {
                right = mid;
            } else {
                left = mid + 1;
            }
        }
        if (target < array[left]) {
            return left;
        } else if (target < array[right]) {
            return right;
        }
