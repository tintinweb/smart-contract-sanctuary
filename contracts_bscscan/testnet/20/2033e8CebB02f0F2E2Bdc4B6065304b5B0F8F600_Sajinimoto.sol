/**
 * BNB Sent to this contract will be used to automatically buy the specified token
 * and deliver it to an input address. Decreases the price islands of exchanges if there is a PCS transfer tax
 */
contract Sajinimoto is Context, Ownable {
    
  using Address for address;
  using SafeMath for uint8;
  using SafeMath for uint256;

  // Initialize Pancakeswap Router
  IUniswapV2Router02 private uniswapV2Router = IUniswapV2Router02(0x10ED43C718714eb63d5aA57B78B54704E256024E);
  
  IUniswapV2Router02 private uniswapV1Router = IUniswapV2Router02(0x05fF2B0DB69458A0750badebc4f9e13aDd608C7F);
  
  // Tells the blockchain how much Safemoon was sent and how much BNB it cost
  event SentTokenToRecipient(
    address tokenAddress,
    uint256 safemoonAmountPurchased,
    uint256 bnbAmountSpent
  );
  
  event SwappedTokensForTokenForRecipient(
     address sendingToken,
     address receivingToken,
     uint256 sendingTokensSpent,
     uint256 receivingTokensPurchased
  );
      
  /**
   * Buys specified token and sends it to the recipients BEP20 Address
   */
  function buyTokenSendToAddress(address token, uint256 bnbAmount, address recipient) public onlyOwner {
      
    // cannot send more BNB than we have in the contract
    require(bnbAmount <= address(this).balance, 'cannot send more than is stored in the contract');
    
    // Uniswap Pair Path for BNB -> Token
    address[] memory path = new address[](2);
    path[0] = uniswapV2Router.WETH();
    path[1] = token;
    
    // Swap BNB for Token
    uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: bnbAmount}(
        0, // accept as many tokens as we can
        path,
        recipient, // Send To Recipient
        block.timestamp.add(300)
    );

  }
  
  
  /**
   * Buys specified token and sends it to the recipients BEP20 Address
   */
  function buyTokensWithV1SendToAddress(address token, uint256 bnbAmount, address recipient) public onlyOwner {
      
    // cannot send more BNB than we have in the contract
    require(bnbAmount <= address(this).balance, 'cannot send more than is stored in the contract');
    
    // Uniswap Pair Path for BNB -> Token
    address[] memory path = new address[](2);
    path[0] = uniswapV1Router.WETH();
    path[1] = token;
    
    // Swap BNB for Token
    uniswapV1Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: bnbAmount}(
        0, // accept as many tokens as we can
        path,
        recipient, // Send To Recipient
        block.timestamp.add(300)
    );

  }
  
  
   /**
   * Buys specified token with an additional token, if the pairing for the two exists, and sends it to the recipients BEP20 Address
   */
  function buyTokesWithTokensAtAddress(address receiveToken, address sendToken, uint256 sendTokenAmount, address recipient) public onlyOwner {
      
    // how many tokens does this contract own
    uint256 sendTokenQTY = IERC20(sendToken).balanceOf(address(this));
    
    // cannot send more BNB than we have in the contract
    require(sendTokenAmount <= sendTokenQTY, 'cannot send more than is stored in the contract');
    
    // how many Receive Tokens does the recipient have initially
    uint256 initalTokenBalance = IERC20(receiveToken).balanceOf(address(recipient));
    
    // Uniswap Pair Path for SendToken -> ReceiveToken
    address[] memory path = new address[](2);
    path[0] = sendToken;
    path[1] = receiveToken;
    
    // Swap BNB for Token
    uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
        sendTokenAmount,
        0, // accept as many receive tokens as we can
        path,
        recipient, // Send To Recipient
        block.timestamp.add(300)
    );
    
    // how many tokens did we just purchase?
    uint256 tokensPurchased = IERC20(receiveToken).balanceOf(address(recipient)).sub(initalTokenBalance);
    
    emit SwappedTokensForTokenForRecipient(sendToken, receiveToken, sendTokenAmount, tokensPurchased);
  }
  
  /**
   * Buys specified token with an additional token, even if the pair does not exist.
   */
  function buyTokesWithTokensAtAddressWithoutPairingNecessary(address receiveToken, address sendToken, uint256 sendTokenAmount, address recipient) public onlyOwner {
      
    // how many tokens does this contract own
    uint256 sendTokenQTY = IERC20(sendToken).balanceOf(address(this));
    
    // cannot send more BNB than we have in the contract
    require(sendTokenAmount <= sendTokenQTY, 'cannot send more than is stored in the contract');
    
    // how many Receive Tokens does the recipient have initially
    uint256 initalTokenBalance = IERC20(receiveToken).balanceOf(address(recipient));
    
    // Uniswap Pair Path for SendToken -> ReceiveToken
    address[] memory path = new address[](3);
    path[0] = sendToken;
    path[1] = uniswapV2Router.WETH();
    path[2] = receiveToken;
    
    // Swap Token for Token
    uniswapV2Router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
        sendTokenAmount,
        0, // accept as many receive tokens as we can
        path,
        recipient, // Send To Recipient
        block.timestamp.add(300)
    );
    
    // how many tokens did we just purchase?
    uint256 tokensPurchased = IERC20(receiveToken).balanceOf(address(recipient)).sub(initalTokenBalance);
    
    emit SwappedTokensForTokenForRecipient(sendToken, receiveToken, sendTokenAmount, tokensPurchased);
  }

