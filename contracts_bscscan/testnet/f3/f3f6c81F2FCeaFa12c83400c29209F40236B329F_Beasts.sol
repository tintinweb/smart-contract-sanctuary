pragma solidity ^0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";

contract Supervisor is Ownable {
  address newOwner;
  address public supervisor;
 
  constructor(address _supervisor) {
    supervisor = _supervisor;
  }

  function proposeNewOwner(address _newOwner) public {
    require(_msgSender() == supervisor, "ONLY_SUPERVISOR");
    newOwner = _newOwner;
  }

  function transferOwnership(address _newOwner) public override onlyOwner {
    require(newOwner == _newOwner, "SUPERVISOR_PROPOSAL_REQUIRED");
    newOwner = address(0);
    super.transferOwnership(_newOwner);
  }
}

