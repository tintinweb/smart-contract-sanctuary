pragma solidity ^0.8.9;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";

contract Items is ERC1155, Ownable {

    using SafeMath for uint256;
    IERC20 token;
    uint public constant BeastBox = 1;
    uint public constant ItemBox = 2;
    uint public constant EKC = 9999;
    uint public constant Medicine15 = 10000;
    uint public constant Medicine30 = 10001;
    uint public constant Medicine45 = 10002;
    //Don't use ItemId to mint tokens
    // weapon [3, 4, 5, 6]
    // head armor [7, 8, 9, 10]
    // leg armor [11, 12, 13, 14]
    // eye [15, 16, 17, 18]
    uint256 private ItemId = 3;
    uint256 private ItemBoxRankCount = 0;
    uint256 private ItemTypeCount = 0;

    constructor(IERC20 _token) ERC1155("https://game.example/api/item/{id}.json") {
        token = _token;
        setUpItemBoxRank();
    }

    mapping(uint256 => uint256) public ItemBoxRankList;
    mapping(uint256 => uint256[]) public ItemIds;
    mapping(address => uint256[]) public ItemBelongUserList;

    function getEKCId() public pure returns (uint256) {
        return EKC;
    }

    function getMedicine15Id() public pure returns (uint256) {
        return Medicine15;
    }

    function getMedicine30Id() public pure returns (uint256) {
        return Medicine30;
    }

    function getMedicine45Id() public pure returns (uint256) {
        return Medicine45;
    }

    function setUpItemBoxRank() private {
        setItemBoxRank(1, 8550);
        setItemBoxRank(2, 1410);
        setItemBoxRank(3, 35);
        setItemBoxRank(4, 5);
        setItemId(1);
        setItemId(2);
        setItemId(3);
        setItemId(4);
    }

    function setItemBoxRank(uint256 rank, uint256 quality) public onlyOwner {
        ItemBoxRankCount = ItemBoxRankList[rank] > 0 ? ItemBoxRankCount : ItemBoxRankCount + 1;
        ItemBoxRankList[rank] = quality;
    }

    function getItemBoxRank(uint256 rank) public view returns(uint256) {
        return ItemBoxRankList[rank];
    }

    function setItemId(uint256 TypeItem) public onlyOwner {
        for (uint256 index = 0; index < ItemBoxRankCount; index++) {
           ItemIds[TypeItem].push(ItemId);
           ++ItemId;
        }
        ++ItemTypeCount;
    }

    function getItemId(uint256 TypeItem) public view returns(uint256[] memory) {
        return ItemIds[TypeItem];
    }

    function getItemBelongUser(address account) public view returns(uint256[] memory) {
        return ItemBelongUserList[account];
    }

    function buyBeastBox(address _to) public {
        uint256 price = 1;
        token.transferFrom(
            _to,
            address(this),
            price
        );
        _mint(_to, BeastBox, 1, '');
    }

    function buyItemBox(address _to) public {
        uint256 totalItem = getTotalBoxItem();
        require(totalItem > 0, "RUN_OUT_OF_BOX_ITEM");
        uint256 price = 1;
        token.transferFrom(
            _to,
            address(this),
            price
        );
        _mint(_to, ItemBox, 1, '');
    }

    function buyItem(address _to) public returns(uint256[] memory) {
        uint256 totalItem = getTotalBoxItem();
        require(totalItem > 0, "RUN_OUT_OF_BOX_ITEM");
        require(balanceOf(_to, ItemBox) > 0, "USER_HAVE_NO_BOX");

        uint amount = 1;

        (uint256 randRank, uint256 randItem) = randMod(totalItem, ItemTypeCount);
        uint256 rankItem = setRankItem(randRank, 1, 0);
        uint256 itemId = ItemIds[randItem + 1][rankItem];

        _mint(_to, itemId, amount, '');
        ItemBelongUserList[_to].push(itemId);

        burn(_to, ItemBox, amount);
        return ItemBelongUserList[_to];
    }

    function burn (address _to, uint256 _id, uint256 _amount) public {
        _burn(_to, _id, _amount);
    }

    function transferFrom(address _from, address _to, uint256 _id, uint256 _amount, bytes calldata _data) public {
        _safeTransferFrom(_from, _to, _id, _amount, _data);
    }

    function randMod(uint256 _modulusRank, uint256 _modulusItem) private view returns(uint256, uint256)
    {
        uint rand = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, ItemBoxRankCount, ItemId)));
        return (rand % _modulusRank, rand % _modulusItem);
    }

    function getTotalBoxItem() private view returns(uint256)
    {
        uint256 totalItem = 0;
        for (uint256 index = 1; index <= ItemBoxRankCount; index++) {
           totalItem += ItemBoxRankList[index];
        }
        return totalItem;
    }

    function setRankItem(uint256 randNum, uint256 index, uint256 offset) private returns(uint256)
    {
        if(index > ItemBoxRankCount){
            return 1;
        }

        if(randNum >= offset && randNum < offset + ItemBoxRankList[index]){
            ItemBoxRankList[index] = ItemBoxRankList[index] - 1;
            return index;
        }

        return setRankItem(randNum, index + 1, offset + ItemBoxRankList[index]);
    }

    function mintEKC(address _to, uint256 amount) public {
        _mint(_to, EKC, amount, '');
    }

    
    function buyMedicine15(address _to, uint256 amount ) public {
        uint256 price = 1;
        token.transferFrom(
            _to,
            address(this),
            price * amount
        );
        _mint(_to, Medicine15, amount, '');
    }

    function buyMedicine30(address _to, uint256 amount ) public {
        uint256 price = 1;
        token.transferFrom(
            _to,
            address(this),
            price * amount
        );
        _mint(_to, Medicine30, amount, '');
    }

    function buyMedicine45(address _to, uint256 amount ) public {
        uint256 price = 1;
        token.transferFrom(
            _to,
            address(this),
            price * amount
        );
        _mint(_to, Medicine45, amount, '');
    }

}

