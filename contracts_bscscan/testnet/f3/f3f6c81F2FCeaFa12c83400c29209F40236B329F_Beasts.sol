pragma solidity ^0.8.9;
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./IBeasts.sol";
import "../items/Items.sol";

/*
 * @dev Provides information about the current execution context, including the
 * sender of the transaction and its data. While these are generally available
 * via msg.sender and msg.data, they should not be accessed in such a direct
 * manner, since when dealing with GSN meta-transactions the account sending and
 * paying for execution may not be the actual sender (as far as an application
 * is concerned).
 *
 * This contract is only required for intermediate, library-like contracts.
 */

contract BeastsUpgrade is Ownable {
    using SafeMath for uint256;
    Items items;

    constructor(
        Items _items
    ) {
        items = _items;
        initBeastUpgradeConditions();
    }

    mapping(uint => IBeasts.BeastUpgradeCondition) public BeastUpgradeConditions;

    function initBeastUpgradeConditions() private {
        BeastUpgradeConditions[uint(IBeasts.Rank.Sergeant)] = IBeasts.BeastUpgradeCondition({
            numOfBeast: 2,
            ekc: 1,
            maxLvl: 2
        });
        BeastUpgradeConditions[uint(IBeasts.Rank.Lieutenant)] = IBeasts.BeastUpgradeCondition({
            numOfBeast: 3,
            ekc: 2,
            maxLvl: 3
        });
        BeastUpgradeConditions[uint(IBeasts.Rank.Colonel)] = IBeasts.BeastUpgradeCondition({
            numOfBeast: 4,
            ekc: 3,
            maxLvl: 4
        });
        BeastUpgradeConditions[uint(IBeasts.Rank.Major)] = IBeasts.BeastUpgradeCondition({
            numOfBeast: 5,
            ekc: 4,
            maxLvl: 5
        });
        BeastUpgradeConditions[uint(IBeasts.Rank.General)] = IBeasts.BeastUpgradeCondition({
            numOfBeast: 6,
            ekc: 5,
            maxLvl: 6
        });
    }

    function upgrade(address account, IBeasts.Beast[] memory allBeast) public returns(IBeasts.UpgradeResult, uint256){        
        ensureSame(allBeast);
        ensureEnoughBeats(allBeast);
        require(allBeast[0].level >= BeastUpgradeConditions[uint(allBeast[0].rank)].maxLvl, "LVL_IS_MAX");

        //get and burn EKC
        uint256 ekc = getEKC4Upgrade(account, allBeast[0].rank);
        items.burn(account, items.getEKCId(), ekc);
        
        //set upgrade result
        (uint256 percentFail, uint256 percentSuccess1, uint256 percentSuccess2, uint256 percentSuccess3) = setPercent4Upgrade(allBeast[0].rank, allBeast[0].level);
        uint256 rand = randMod4Upgrade(percentFail + percentSuccess1 + percentSuccess2 + percentSuccess3);
        IBeasts.UpgradeResult upgradeResult = setUpgradeResult(rand, percentSuccess1, percentSuccess2, percentSuccess3);

        //upgrade
        if(upgradeResult != IBeasts.UpgradeResult.FAIL){
            uint256 level = allBeast[0].level + 1;

            if(upgradeResult == IBeasts.UpgradeResult.INCREASE_2_LEVEL){
                level = allBeast[0].level + 2;
            }

            if(upgradeResult == IBeasts.UpgradeResult.INCREASE_3_LEVEL){
                level = allBeast[0].level + 3;
            }

            if(level > 6){
                level = 6;
            }
            return (upgradeResult, level);
        }
        return (upgradeResult, allBeast[0].level);
    }

    function ensureTokenIdsInAscendingOrder(uint256[] memory tokenIds) public pure {
        uint256 length = tokenIds.length;
        for (uint256 index = 1; index < length; index++) {
            require(tokenIds[index] > tokenIds[index - 1], "TOKEN_IDS_MUST_BE_IN_ASCENDING_ORDER");
        }
    }

    function ensureSame(IBeasts.Beast[] memory allBeast) private pure {
        uint256 length = allBeast.length;
        IBeasts.Classification class = allBeast[0].class;
        IBeasts.Rank rank = allBeast[0].rank;
        require(rank != IBeasts.Rank.Corporal, "CORPORAL_CAN_NOT_EVOL");
        uint256 BeastId = allBeast[0].BeastId;
        uint256 level = allBeast[0].level;
        
        for (uint256 index = 1; index < length; index++) {
            require(allBeast[index].class == class, "NOT_SAME_CLASS");
            require(allBeast[index].rank == rank, "NOT_SAME_RANK");
            require(allBeast[index].BeastId == BeastId, "NOT_SAME_BEAST");
            require(allBeast[index].level == level, "NOT_SAME_LVL");
        }
    }

    function ensureEnoughBeats(IBeasts.Beast[] memory allBeast) private view {
        uint256 length = allBeast.length;
        IBeasts.Rank rank = allBeast[0].rank;
        require(rank != IBeasts.Rank.Corporal, "CORPORAL_CAN_NOT_EVOL");
        require(length == BeastUpgradeConditions[uint(rank)].numOfBeast, "NOT_ENOUGH_BEAST");
    }

    function getEKC4Upgrade(address account,IBeasts.Rank rank) public view returns(uint256) {
        uint256 EKC = items.balanceOf(account, items.getEKCId() );
        require(EKC >= BeastUpgradeConditions[uint(rank)].ekc, "NOT_ENOUGH_EKC");
        return BeastUpgradeConditions[uint(rank)].ekc;
    }

    function setPercent4Upgrade(IBeasts.Rank rank, uint256 level ) private pure returns(uint256, uint256, uint256, uint256) {
        uint256 percentFail = 0;
        uint256 percentSuccess1 = 0;
        uint256 percentSuccess2 = 0;
        uint256 percentSuccess3 = 0;


        if(rank == IBeasts.Rank.Sergeant){
            percentSuccess1 = 7500;
            percentFail = 2500;
        }

        if(rank ==IBeasts.Rank.Lieutenant){
            if(level == 1){
                percentSuccess1 = 5400;
                percentSuccess2 = 1800;
                percentFail = 2800;
            }
            if(level == 2){
                percentSuccess1 = 5400;
                percentFail = 4600;
            }
        }

        if(rank == IBeasts.Rank.Colonel){
             if(level == 1){
                percentSuccess1 = 5520;
                percentSuccess2 = 1104;
                percentSuccess3 = 276;
                percentFail = 3100;
            }

            if(level == 2){
                percentSuccess1 = 5244;
                percentSuccess2 = 1311;
                percentFail = 3445;
            }

            if(level == 3){
                percentSuccess1 = 4981;
                percentFail = 5019;
            }
        }

        if(rank == IBeasts.Rank.Major){
            if(level == 1){
                percentSuccess1 = 5280;
                percentSuccess2 = 1056;
                percentSuccess3 = 264;
                percentFail = 3400;
            }

            if(level == 2){
                percentSuccess1 = 5016;
                percentSuccess2 = 1003;
                percentSuccess3 = 250;
                percentFail = 3731;
            }

            if(level == 3){
                percentSuccess1 = 4765;
                percentSuccess2 = 1191;
                percentFail = 4044;
            }

            if(level == 4){
                percentSuccess1 = 4526;
                percentFail = 5474;
            }
        }

        if(rank == IBeasts.Rank.General){
            if(level == 1){
                percentSuccess1 = 5040;
                percentSuccess2 = 1008;
                percentSuccess3 = 252;
                percentFail = 3700;
            }

            if(level == 2){
                percentSuccess1 = 4788;
                percentSuccess2 = 957;
                percentSuccess3 = 239;
                percentFail = 4016;
            }

            if(level == 3){
                percentSuccess1 = 4548;
                percentSuccess2 = 909;
                percentSuccess3 = 227;
                percentFail = 3416;
            }

            if(level == 4){
                percentSuccess1 = 4321;
                percentSuccess2 = 1080;
                percentFail = 4590;
            }

            if(level == 5){
                percentSuccess1 = 4105;
                percentFail = 5895;
            }
        }

        return (percentFail, percentSuccess1, percentSuccess2, percentSuccess3);
    }

    function randMod4Upgrade(uint256 _modulus) private view returns(uint256)
    {
        uint rand = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender)));
        return rand % _modulus;
    }

    function setUpgradeResult(uint256 rand, uint256 percentSuccess1, uint256 percentSuccess2, uint256 percentSuccess3 ) private pure returns(IBeasts.UpgradeResult) {
        uint256 totalSuccess = percentSuccess1 + percentSuccess2 + percentSuccess3;
        if(rand < totalSuccess){
            if(rand < percentSuccess1){
                return IBeasts.UpgradeResult.INCREASE_1_LEVEL;
            }
            if(rand >= percentSuccess1 && rand < percentSuccess1 + percentSuccess2){
                return IBeasts.UpgradeResult.INCREASE_2_LEVEL;
            }
            if(rand >= percentSuccess1 + percentSuccess1 && rand < percentSuccess1 + percentSuccess2 + percentSuccess3){
                return IBeasts.UpgradeResult.INCREASE_3_LEVEL;
            }
        }

        return IBeasts.UpgradeResult.FAIL;
    }
