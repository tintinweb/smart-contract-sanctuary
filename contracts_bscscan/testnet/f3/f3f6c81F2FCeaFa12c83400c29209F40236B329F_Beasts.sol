pragma solidity ^0.8.9;
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";

import "../common/0-supervisor.sol";
import "../items/Items.sol";
import "./IBeasts.sol";
import "./BeastsUpgrade.sol";

/*
 * @dev Provides information about the current execution context, including the
 * sender of the transaction and its data. While these are generally available
 * via msg.sender and msg.data, they should not be accessed in such a direct
 * manner, since when dealing with GSN meta-transactions the account sending and
 * paying for execution may not be the actual sender (as far as an application
 * is concerned).
 *
 * This contract is only required for intermediate, library-like contracts.
 */

contract Beasts is ERC721Enumerable,  Supervisor {
    using SafeMath for uint256;
    Items items;
    IERC20 EKToken;
    BeastsUpgrade beastsUpgrade;
    uint256 public createdTokenCount = 0;
    uint256 public boxIndex = 0;
    uint256 public beastInfoId = 0;
    uint public lastBeast = 0;

    constructor(
        IERC20 _EKToken,
        address supervisor,
        Items _items,
        BeastsUpgrade _beastsUpgrade
    ) 
    ERC721("Beast", "BeastNFT")
    Supervisor(supervisor) {
        EKToken = _EKToken;
        items = _items;
        beastsUpgrade = _beastsUpgrade;
        initBeastBox();
        setUpBeastInfo();
    }

    IBeasts.BeastBox[] public BeastBoxList;

    function setItemContact(Items _items) public onlyOwner {
        items = _items;
    }

    function setBeastsUpgradeContact(BeastsUpgrade _beastsUpgrade) public onlyOwner {
        beastsUpgrade = _beastsUpgrade;
    }

    function initBeastBox() private {
        createBeastBox( 5000, 3000, 1700, 250, 50, 0);
        createBeastBox( 3000, 4000, 2600, 300, 98, 2);
    }

    function createBeastBox(uint256 corporal, uint256 sgt, uint256 lt, uint256 col, uint256 major, uint256 general) public onlyOwner {
        BeastBoxList.push(IBeasts.BeastBox({
            id: boxIndex + 1,
            corporal: corporal,
            sgt:sgt,
            lt:lt,
            col:col,
            major:major,
            general:general
        }));
        ++boxIndex;
    }

    mapping(IBeasts.Classification => IBeasts.BeastInfo[]) public BeastInfolist;

    function setUpBeastInfo() private {
        setBeastInfo(IBeasts.Classification.GROUND);
        setBeastInfo(IBeasts.Classification.WATER);
        setBeastInfo(IBeasts.Classification.FLY);
        setBeastInfo(IBeasts.Classification.GROUND);
        setBeastInfo(IBeasts.Classification.WATER);
        setBeastInfo(IBeasts.Classification.FLY);
    }

    function setBeastInfo(IBeasts.Classification class) public onlyOwner {
        BeastInfolist[class].push(IBeasts.BeastInfo({
            id: beastInfoId + 1,
            class: class
        }));
        ++beastInfoId;
    }

    function getBeastInfoListByClass(IBeasts.Classification class) public view returns(IBeasts.BeastInfo[] memory) {
        return BeastInfolist[class];
    }

    mapping(uint256 => IBeasts.Beast) public beasts;

    function mint(address to, uint256 level, uint256 BeastId, IBeasts.Classification class, IBeasts.Rank rank) internal returns(IBeasts.Beast memory) {
        uint256 tokenId = ++createdTokenCount;
        beasts[tokenId] = IBeasts.Beast(
            tokenId,
            BeastId,
            level,
            class,
            rank
        );
        super._mint(to, tokenId);
        emit Buy(to, BeastId, level);
        return beasts[tokenId];
    }

    event Buy(address user, uint256 BeastId, uint256 level);

    function isContract(address account) internal view returns (bool) {
        bytes32 codehash;
        bytes32 accountHash = 0xc5d2460186f7233c927e7db2dcc703c0e500b653ca82273b7bfad8045d85a470;
        assembly { codehash := extcodehash(account) }
        return (codehash != accountHash && codehash != 0x0);
    }

    function buy(address _to, uint256 _id, uint boxId) public returns(IBeasts.Beast memory) {
        require(!isContract(_msgSender()), "NO_CONTRACT_CALLS");

        uint256 totalbox = BeastBoxList[boxId].corporal + BeastBoxList[boxId].sgt + BeastBoxList[boxId].lt + BeastBoxList[boxId].col + BeastBoxList[boxId].major + BeastBoxList[boxId].general;
        (uint256 randClass, uint256 randRank, uint256 randFly, uint256 randGround, uint256 randWater ) = randMod(3, totalbox, getBeastInfoListByClass(IBeasts.Classification.FLY).length, getBeastInfoListByClass(IBeasts.Classification.GROUND).length, getBeastInfoListByClass(IBeasts.Classification.WATER).length);
        
        IBeasts.Classification class = setClassByRand(randClass);
        IBeasts.Rank rank = setRankBeast(boxId, randRank, totalbox);
        uint256 beastId = setBeastByRand(class, randFly, randGround, randWater );

        items.burn(_to, _id, 1);
        return mint(_to, 1, beastId, class, rank);
    }

    function getBeastListByAccount(address account, uint256 limit, uint256 offset) public view returns(IBeasts.Beast[] memory result) {
        uint256 max = this.balanceOf(account);
        if (offset >= max) return result;
        uint256 count = (offset + limit <= max) ? limit : max - offset;
        result = new IBeasts.Beast[](count);
        for (uint256 index = 0; index < count; index++) {
            result[index] = beasts[this.tokenOfOwnerByIndex(account, index + offset)];
        }
        return result;
    }

    function randMod(uint256 _modulusClass, uint256 _modulusBox, uint256 _modulusFly, uint256 _modulusGround, uint256 _modulusWater) private view returns(uint256, uint256, uint256, uint256, uint256)
    {
        uint rand = uint(keccak256(abi.encodePacked(block.timestamp, msg.sender, createdTokenCount)));
        return (rand % _modulusClass, rand % _modulusBox, rand % _modulusFly, rand % _modulusGround, rand % _modulusWater);
    }

    function setClassByRand(uint256 randNum) private pure returns(IBeasts.Classification)
    {
        if(randNum == 0){
            return IBeasts.Classification.FLY;
        }
        if(randNum == 1){
            return IBeasts.Classification.GROUND;
        }
        return IBeasts.Classification.WATER;
    }

    function setRankBeast(uint256 boxId, uint256 randNum, uint256 totalbox) private returns(IBeasts.Rank)
    {
       
        require(totalbox > 0, "RUN_OUT_OF_BOX_1");
        if(randNum < BeastBoxList[boxId].corporal){
            BeastBoxList[boxId].corporal = --BeastBoxList[boxId].corporal;
            return IBeasts.Rank.Corporal;
        }

        if(randNum >= BeastBoxList[boxId].corporal && randNum < BeastBoxList[boxId].corporal + BeastBoxList[boxId].sgt){
            BeastBoxList[boxId].sgt = --BeastBoxList[boxId].sgt;
            return IBeasts.Rank.Sergeant;
        }

        if(randNum >= BeastBoxList[boxId].corporal + BeastBoxList[boxId].sgt && randNum < BeastBoxList[boxId].corporal + BeastBoxList[boxId].sgt + BeastBoxList[boxId].lt){
            BeastBoxList[boxId].lt = --BeastBoxList[boxId].lt;
            return IBeasts.Rank.Lieutenant;
        }
        
        if(randNum >= BeastBoxList[boxId].corporal + BeastBoxList[boxId].sgt + BeastBoxList[boxId].lt && randNum < BeastBoxList[boxId].corporal + BeastBoxList[boxId].sgt + BeastBoxList[boxId].lt + BeastBoxList[boxId].col){
            BeastBoxList[boxId].col = --BeastBoxList[boxId].col;
            return IBeasts.Rank.Colonel;
        }

        if(randNum >= BeastBoxList[boxId].corporal + BeastBoxList[boxId].sgt + BeastBoxList[boxId].lt + BeastBoxList[boxId].col && randNum < BeastBoxList[boxId].corporal + BeastBoxList[boxId].sgt + BeastBoxList[boxId].lt + BeastBoxList[boxId].col + BeastBoxList[boxId].major){
            BeastBoxList[boxId].major = --BeastBoxList[boxId].major;
            return IBeasts.Rank.Major;
        }

        if(randNum >= BeastBoxList[boxId].corporal + BeastBoxList[boxId].sgt + BeastBoxList[boxId].lt + BeastBoxList[boxId].col + BeastBoxList[boxId].major && randNum < totalbox){
            BeastBoxList[boxId].general = --BeastBoxList[boxId].general;
            return IBeasts.Rank.General;
        }

        BeastBoxList[boxId].corporal = --BeastBoxList[boxId].corporal;
        return IBeasts.Rank.Corporal;
    }

    function setBeastByRand(IBeasts.Classification class, uint256 randFly, uint256 randGround, uint256 randWater ) private view returns(uint256)
    {
        uint256 rand = 0 ;
        if(class == IBeasts.Classification.FLY){
            rand = randFly;
        }

        if(class == IBeasts.Classification.GROUND){
            rand = randGround;
        }

        if(class == IBeasts.Classification.WATER){
            rand = randWater;
        }
        return BeastInfolist[class][rand].id;
    }

    event UpgradeSuccess(uint256[] inputTokenIds, uint256 outputTokenId, IBeasts.UpgradeResult upgradeResult, address user);
    event UpgradeFail(uint256[] inputTokenIds, IBeasts.UpgradeResult upgradeResult, address user);

    function upgrade(uint256[] memory tokenIds) public {
        require(!isContract(_msgSender()), "NO_CONTRACT_CALLS");
        address account = _msgSender();
        beastsUpgrade.ensureTokenIdsInAscendingOrder(tokenIds);
        IBeasts.Beast[] memory allBeast = getBeastByIds(tokenIds);
        ensureAllTokensBelongToSender(allBeast);
        //pay EK token
        EKToken.transferFrom(
            account,
            address(this),
            1
        );
        (IBeasts.UpgradeResult upgradeResult, uint256 level) = beastsUpgrade.upgrade(account, allBeast);

        //upgrade
        if(upgradeResult != IBeasts.UpgradeResult.FAIL){
            for (uint256 index = 0; index < tokenIds.length; index++) _burn(tokenIds[index]);

            IBeasts.Beast memory _beast = mint(account, level, allBeast[0].BeastId, allBeast[0].class, allBeast[0].rank);
            emit UpgradeSuccess(tokenIds, _beast.id, upgradeResult, account);
            return;
        } 

        emit UpgradeFail(tokenIds, upgradeResult, account);
    }

    function getBeastByIds(uint256[] memory tokenIds) private view returns(IBeasts.Beast[] memory) {
        uint256 length = tokenIds.length;
        IBeasts.Beast[] memory result = new IBeasts.Beast[](length);
        for (uint256 index = 0; index < length; index++) {
            result[index] = beasts[tokenIds[index]];
        }
        return result;
    }

    function ensureAllTokensBelongToSender(IBeasts.Beast[] memory allBeast) private view {
        uint256 length = allBeast.length;
        address sender = _msgSender();
        for (uint256 index = 0; index < length; index++) {
        require(this.ownerOf(allBeast[index].id) == sender, "MUST_OWN_ALL_TOKENS");
        }
    }
}

