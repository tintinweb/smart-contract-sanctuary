pragma solidity ^0.8.9;

interface IBeasts {

  struct BeastBox {
    uint256 id;
    uint256 corporal;
    uint256 sgt;
    uint256 lt;
    uint256 col;
    uint256 major;
    uint256 general;
  }

  struct BeastInfo {
    uint256 id;
    Classification class;
  }

  enum Classification {
    FLY,
    GROUND,
    WATER
  }

  enum Rank {
    Corporal,
    Sergeant,
    Lieutenant,
    Major,
    Colonel,
    General
  }

  struct Beast {
    uint256 id;
    uint256 BeastId;
    uint256 level;
    IBeasts.Classification class;
    IBeasts.Rank rank;
  }

  enum UpgradeResult {
    FAIL,
    INCREASE_1_LEVEL,
    INCREASE_2_LEVEL,
    INCREASE_3_LEVEL
  }

  struct BeastUpgradeCondition {
    uint256 numOfBeast;
    uint256 ekc;
    uint256 maxLvl;
  }
}

