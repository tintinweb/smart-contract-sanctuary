pragma solidity >=0.8.0 <0.9.0;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

import "./Redeemable.sol";

contract GameCoin is ERC20, AccessControl, Redeemable {
  bytes32 public constant MINT_ROLE = keccak256("MINT_ROLE");
  bytes32 public constant BURN_ROLE = keccak256("BURN_ROLE");
  bytes32 public constant ISSUE_ROLE = keccak256("ISSUE_ROLE");

  constructor(uint256 initialSupply) ERC20("GameCoin", "GAM") {
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    _mint(msg.sender, initialSupply);
  }

  function mint(address account, uint amount) external onlyRole(MINT_ROLE) {
    _mint(account, amount);
  }

  function burn(address account, uint amount) external onlyRole(BURN_ROLE) {
    _burn(account, amount);
  }

  function burn(uint amount) external {
    _burn(msg.sender, amount);
  }

  //
  // REDEEM
  //

  event Redeem(address redemmer, uint amount, uint nonce, address issuer);
  event DisableRedeemNonce(address admin, uint nonce);

  function redeem(uint amount, uint nonce, address issuer, bytes memory signature) external {
    _checkRole(ISSUE_ROLE, issuer);
    _checkRedeem(abi.encodePacked(this, amount, nonce), nonce, issuer, signature);
    emit Redeem(msg.sender, amount, nonce, issuer);

    // _mint() never fail
    _mint(msg.sender, amount);
  }

  function disableRedeemFor(uint nonce) external onlyRole(DEFAULT_ADMIN_ROLE) {
    emit DisableRedeemNonce(msg.sender, nonce);

    _disableRedeemFor(nonce);
  }
}

