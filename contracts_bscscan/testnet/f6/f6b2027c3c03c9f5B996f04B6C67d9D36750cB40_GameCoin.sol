pragma solidity >=0.8.0 <0.9.0;

import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";

import "hardhat/console.sol";

abstract contract Redeemable {
  // used nonces
  mapping(uint => bool) private _nonces;

  function canRedeem(uint nonce) public view returns (bool) {
    return !_nonces[nonce];
  }

  function _checkRedeem(bytes memory message, uint nonce, address issuer, bytes memory signature) internal {
    require(!_nonces[nonce], "already redeemed");
    bytes32 h = ECDSA.toEthSignedMessageHash(keccak256(message));
    require(SignatureChecker.isValidSignatureNow(issuer, h, signature), "signature not match");
    _nonces[nonce] = true;
  }

  function _disableRedeemFor(uint nonce) internal {
    _nonces[nonce] = true;
  }
}

