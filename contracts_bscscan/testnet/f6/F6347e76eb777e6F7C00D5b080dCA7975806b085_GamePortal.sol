pragma solidity ^0.8.9;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";

contract SimplePool is ReentrancyGuard, AccessControl, ERC1155Holder {
    event EventDeposit(
        uint256 indexed poolID,
        address indexed _tokenAddress,
        uint256 indexed _id,
        uint256 _value
    );

    event EventWithdraw(
        uint256 indexed poolID,
        address indexed _tokenAddress,
        uint256 indexed _id,
        uint256 _value
    );

    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    struct TokenContract {
        address tokenAddress;
        uint256 amount;
        TokenType tokenType;
        mapping(uint256 => uint256) nftAmount;
    }

    struct TokenDetail {
        int256 amount;
    }

    enum TokenType {
        ERC20,
        ERC721,
        ERC1155
    }

    uint256 numPools;
    mapping(uint256 => TokenContract[]) pools;
    bytes32 public constant ADMIN_ROLE = keccak256("ADMIN_ROLE");

    constructor() public {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(ADMIN_ROLE, msg.sender);
    }

    // Add a new type of token to initial a new pool
    function createNewPool(address _tokenContractAddress, TokenType _tokenType)
        public
        returns (uint256 poolID)
    {
        poolID = numPools++;
        // We cannot use "pools[poolID] = TokenContract(address, amount)"
        // because the RHS creates a memory-struct "TokenContract" that contains a mapping.
        TokenContract[] storage tokenArray = pools[poolID];
        uint256 idx = tokenArray.length;
        tokenArray.push();
        TokenContract storage tc = tokenArray[idx];
        tc.tokenAddress = _tokenContractAddress;
        tc.amount = 0;
        tc.tokenType = _tokenType;
        return poolID;
    }

    function withdrawFromPool(
        uint256 _poolID,
        address _tokenAddress,
        uint256 _tokenAmount,
        uint256 _tokenID
    ) internal {
        require(_poolID < numPools, "Invalid Pool not found");
        bool isTokenContractExist = false;
        TokenContract[] storage tokenArray = pools[_poolID];
        for (uint256 i = 0; i < tokenArray.length; i++) {
            if (_tokenAddress == tokenArray[i].tokenAddress) {
                if (tokenArray[i].tokenType == TokenType.ERC20) {
                    require(
                        (_tokenAmount <= tokenArray[i].amount),
                        "Insufficient pool balance for  withdrawal"
                    );
                    tokenArray[i].amount -= _tokenAmount;
                } else {
                    require(
                        (tokenArray[i].nftAmount[_tokenID] <=
                            tokenArray[i].amount),
                        "Insufficient pool balance for  withdrawal"
                    );
                    tokenArray[i].nftAmount[_tokenID] -= _tokenAmount;
                }
                isTokenContractExist = true;
            }
        }
        require(isTokenContractExist, "Invalid token contract address");
        emit EventWithdraw(_poolID, _tokenAddress, _tokenID, _tokenAmount);
    }

    function contributeToPool(
        uint256 _poolID,
        address _tokenAddress,
        uint256 _tokenAmount,
        uint256 _tokenID,
        uint256 _tokenType
    ) internal {
        TokenType tokenType = TokenType(_tokenType);
        bool isTokenContractExist = false;
        TokenContract[] storage tokenArray = pools[_poolID];
        for (uint256 i = 0; i < tokenArray.length; i++) {
            if (_tokenAddress == tokenArray[i].tokenAddress) {
                if (tokenArray[i].tokenType == TokenType.ERC20) {
                    tokenArray[i].amount += _tokenAmount;
                } else {
                    tokenArray[i].nftAmount[_tokenID] += _tokenAmount;
                }
                isTokenContractExist = true;
            }
        }
        if (!isTokenContractExist) {
            uint256 idx = tokenArray.length;
            tokenArray.push();
            TokenContract storage tc = tokenArray[idx];
            tc.tokenAddress = _tokenAddress;
            tc.tokenType = tokenType;
            if (tokenType == TokenType.ERC20) {
                tc.amount = _tokenAmount;
            } else {
                tc.amount = 0;
                tc.nftAmount[_tokenID] = _tokenAmount;
            }
        }
        emit EventDeposit(_poolID, _tokenAddress, _tokenID, _tokenAmount);
    }

    function getTotalPoolContract(uint256 poolID)
        external
        view
        returns (uint256 quantity)
    {
        TokenContract[] storage tokenArray = pools[poolID];
        return tokenArray.length;
    }

    function getPoolTokenQuantity(
        uint256 poolID,
        address tokenAddress,
        uint256 tokenID
    ) public view returns (uint256 quantity) {
        require(poolID < numPools, "Invalid Pool not found");
        TokenContract[] storage tokenArray = pools[poolID];
        for (uint256 i = 0; i < tokenArray.length; i++) {
            if (tokenAddress == tokenArray[i].tokenAddress) {
                if (tokenArray[i].tokenType == TokenType.ERC20) {
                    return tokenArray[i].amount;
                } else {
                    return tokenArray[i].nftAmount[tokenID];
                }
            }
        }
        return 0;
    }

    function withdraw(
        uint256 _poolID,
        address _tokenAddress,
        uint256 _tokenType,
        uint256 _amount,
        uint256 _tokenID
    ) external nonReentrant onlyRole(ADMIN_ROLE) {
        require(_tokenAddress != address(0), "Invalid address format");
        require(_tokenType <= 2, "Invalid Token Type");
        TokenType tokenType = TokenType(_tokenType);
        withdrawFromPool(_poolID, _tokenAddress, _amount, _tokenID);
        if (tokenType == TokenType.ERC20) {
            IERC20(_tokenAddress).safeTransfer(msg.sender, _amount);
        } else if (tokenType == TokenType.ERC721) {
            IERC721(_tokenAddress).safeTransferFrom(
                address(this),
                msg.sender,
                _tokenID
            );
        } else {
            IERC1155(_tokenAddress).safeTransferFrom(
                address(this),
                msg.sender,
                _tokenID,
                _amount,
                ""
            );
        }
    }

    function withdrawPoolNonExist(
        address _tokenAddress,
        uint256 _tokenType,
        uint256 _amount,
        uint256 _tokenID
    ) private nonReentrant onlyRole(DEFAULT_ADMIN_ROLE) {
        require(_tokenAddress != address(0), "Invalid address format");
        require(_tokenType <= 2, "Invalid Token Type");
        TokenType tokenType = TokenType(_tokenType);
        if (tokenType == TokenType.ERC20) {
            IERC20(_tokenAddress).safeTransfer(msg.sender, _amount);
        } else if (tokenType == TokenType.ERC721) {
            IERC721(_tokenAddress).safeTransferFrom(
                address(this),
                msg.sender,
                _tokenID
            );
        } else {
            IERC1155(_tokenAddress).safeTransferFrom(
                address(this),
                msg.sender,
                _tokenID,
                _amount,
                ""
            );
        }
    }

    function transfer(
        uint256 _poolID,
        address _tokenAddress,
        uint256 _tokenType,
        address _receiver,
        uint256 _amount,
        uint256 _tokenID
    ) external nonReentrant onlyRole(ADMIN_ROLE) {
        require(
            _tokenAddress != address(0) && _receiver != address(0),
            "Invalid address format"
        );
        require(_tokenType <= 2, "Invalid Token Type");
        TokenType tokenType = TokenType(_tokenType);
        withdrawFromPool(_poolID, _tokenAddress, _amount, _tokenID);
        if (tokenType == TokenType.ERC20) {
            IERC20(_tokenAddress).safeTransfer(_receiver, _amount);
        } else if (tokenType == TokenType.ERC721) {
            IERC721(_tokenAddress).safeTransferFrom(
                address(this),
                _receiver,
                _tokenID
            );
        } else {
            IERC1155(_tokenAddress).safeTransferFrom(
                address(this),
                _receiver,
                _tokenID,
                _amount,
                ""
            );
        }
    }

    function deposit(
        uint256 _poolID,
        address _tokenAddress,
        uint256 _tokenType,
        uint256 _amount,
        uint256 _tokenID
    ) external nonReentrant onlyRole(ADMIN_ROLE) {
        if (_poolID < numPools) {
            contributeToPool(
                _poolID,
                _tokenAddress,
                _amount,
                _tokenID,
                _tokenType
            );
        } else {
            contributeToPool(0, _tokenAddress, _amount, _tokenID, _tokenType);
        }
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC1155Receiver, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

