pragma solidity 0.8.9;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "./SimpleToken.sol";
import "./SimplePool.sol";
import "./Simple1155.sol";

contract GamePortal is
    Initializable,
    OwnableUpgradeable,
    ERC1155HolderUpgradeable,
    ReentrancyGuardUpgradeable,
    UUPSUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    SimpleToken public simpleToken;
    Simple1155 public simple1155;
    SimplePool public simplePool;

    mapping(address => uint256) private tokenbalances;
    uint256[] private tokenIds;

    // tokenid => playerAddress => quantity
    mapping(uint256 => mapping(address => uint256)) private nft1155;

    event userUpgradeUnitEvent(
        uint256[] indexed _ids,
        uint256[] indexed _amounts,
        uint256 indexed _tokenAmount
    );

    event userDismantleUnitEvent(
        uint256[] indexed _dismantleIds,
        uint256[] indexed _dismantleAmounts
    );

    event userWithdrawUnitEvent(
        uint256[] indexed _ids,
        uint256[] indexed _amounts
    );

    event userWithdrawTokenEvent(uint256 indexed _tokenAmount);

    event userDepositUnitEvent(
        uint256[] indexed _ids,
        uint256[] indexed _amounts
    );

    event userDepositTokenEvent(uint256 indexed _tokenAmount);

    struct PlayerBalance {
        uint256 nftId;
        uint256 nftAmount;
        uint256 tokenAmount;
    }

    function initialize() public initializer {
        __Ownable_init();
        __UUPSUpgradeable_init();
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyOwner
    {}

    function setTokenAddress(address _simpleTokenAddress) external onlyOwner {
        simpleToken = SimpleToken(_simpleTokenAddress);
    }

    function set1155Address(address _simple1155Address) external onlyOwner {
        simple1155 = Simple1155(_simple1155Address);
    }

    function setPoolAddress(address _simplePoolAddress) external onlyOwner {
        simplePool = SimplePool(_simplePoolAddress);
    }

    function openChest(
        uint256[] memory _ids,
        uint256[] memory _amounts,
        uint256 _tokenAmount
    ) external nonReentrant {
        require(
            _ids.length > 0 || _tokenAmount > 0,
            "Token Id Array or Token Amount must be more than 0"
        );

        if (_tokenAmount > 0) {
            simpleToken.mint(address(this), _tokenAmount);
            tokenbalances[msg.sender] += _tokenAmount;
        }

        if (_ids.length > 0) {
            simple1155.mintBatch(address(this), _ids, _amounts, "");
            for (uint256 i = 0; i < _ids.length; i++) {
                nft1155[_ids[i]][msg.sender] += _amounts[i];

                bool isTokenIdExist = false;
                for (uint256 j = 0; j < tokenIds.length; ++j) {
                    if (_ids[i] == tokenIds[j]) {
                        isTokenIdExist = true;
                    }
                }

                if (isTokenIdExist == false) {
                    tokenIds.push(_ids[i]);
                }
            }
        }
    }

    function userUpgradeUnit(
        uint256 _poolId,
        uint256[] memory _ids,
        uint256[] memory _amounts,
        uint256 _tokenAmount
    ) external nonReentrant {
        if (_tokenAmount > 0) {
            require(
                tokenbalances[msg.sender] >= _tokenAmount,
                "Insufficient balance"
            );
        }

        if (_ids.length > 0) {
            for (uint256 i = 0; i < _ids.length; ++i) {
                uint256 id = _ids[i];
                uint256 amount = _amounts[i];

                uint256 fromBalance = nft1155[id][msg.sender];
                require(
                    fromBalance >= amount,
                    "ERC1155: insufficient balance for transfer"
                );
                unchecked {
                    nft1155[id][msg.sender] = fromBalance - amount;
                }
            }
        }

        if (_tokenAmount > 0) {
            tokenbalances[msg.sender] -= _tokenAmount;
            IERC20Upgradeable(simpleToken).safeTransfer(
                address(simplePool),
                _tokenAmount
            );
            simplePool.deposit(
                _poolId,
                address(simpleToken),
                0,
                _tokenAmount,
                0
            );
        }

        if (_ids.length > 0) {
            simple1155.safeBatchTransferFrom(
                address(this),
                address(simplePool),
                _ids,
                _amounts,
                ""
            );

            for (uint256 i = 0; i < _ids.length; ++i) {
                uint256 id = _ids[i];
                uint256 amount = _amounts[i];

                simplePool.deposit(_poolId, address(simple1155), 2, amount, id);
            }
        }

        emit userUpgradeUnitEvent(_ids, _amounts, _tokenAmount);
    }

    function userDismantleUnit(
        uint256 _poolId,
        uint256[] memory _dismantleIds,
        uint256[] memory _dismantleAmounts,
        uint256[] memory _ids,
        uint256[] memory _amounts
    ) external nonReentrant {
        if (_dismantleIds.length > 0) {
            for (uint256 i = 0; i < _dismantleIds.length; ++i) {
                uint256 id = _dismantleIds[i];
                uint256 amount = _dismantleAmounts[i];

                uint256 fromBalance = nft1155[id][msg.sender];
                require(
                    fromBalance >= amount,
                    "ERC1155: insufficient balance for transfer"
                );
                unchecked {
                    nft1155[id][msg.sender] = fromBalance - amount;
                }
            }
        }

        if (_dismantleIds.length > 0) {
            simple1155.safeBatchTransferFrom(
                address(this),
                address(simplePool),
                _dismantleIds,
                _dismantleAmounts,
                ""
            );

            for (uint256 i = 0; i < _dismantleIds.length; ++i) {
                uint256 id = _dismantleIds[i];
                uint256 amount = _dismantleAmounts[i];

                simplePool.deposit(_poolId, address(simple1155), 2, amount, id);
            }
        }

        if (_ids.length > 0) {
            simple1155.mintBatch(address(this), _ids, _amounts, "");
            for (uint256 i = 0; i < _ids.length; i++) {
                nft1155[_ids[i]][msg.sender] += _amounts[i];

                bool isTokenIdExist = false;
                for (uint256 j = 0; j < tokenIds.length; ++j) {
                    if (_ids[i] == tokenIds[j]) {
                        isTokenIdExist = true;
                    }
                }

                if (isTokenIdExist == false) {
                    tokenIds.push(_ids[i]);
                }
            }
        }

        emit userDismantleUnitEvent(_dismantleIds, _dismantleAmounts);
    }

    function userWithdrawUnit(uint256[] memory _ids, uint256[] memory _amounts)
        external
        nonReentrant
    {
        for (uint256 i = 0; i < _ids.length; ++i) {
            uint256 id = _ids[i];
            uint256 amount = _amounts[i];

            uint256 fromBalance = nft1155[id][msg.sender];
            require(
                fromBalance >= amount,
                "ERC1155: insufficient balance for transfer"
            );
            unchecked {
                nft1155[id][msg.sender] = fromBalance - amount;
            }
        }

        if (_ids.length > 0) {
            simple1155.safeBatchTransferFrom(
                address(this),
                msg.sender,
                _ids,
                _amounts,
                ""
            );
        }

        emit userWithdrawUnitEvent(_ids, _amounts);
    }

    function userWithdrawToken(uint256 _tokenAmount) external nonReentrant {
        if (_tokenAmount > 0) {
            require(
                tokenbalances[msg.sender] >= _tokenAmount,
                "Insufficient balance"
            );
        }

        if (_tokenAmount > 0) {
            tokenbalances[msg.sender] -= _tokenAmount;
            IERC20Upgradeable(simpleToken).safeTransfer(
                msg.sender,
                _tokenAmount
            );
        }
        emit userWithdrawTokenEvent(_tokenAmount);
    }

    function userDepositUnit(
        uint256[] memory _ids,
        uint256[] memory _amounts
    ) external nonReentrant {
        simple1155.safeBatchTransferFrom(
            msg.sender,
            address(this),
            _ids,
            _amounts,
            ""
        );

        for (uint256 i = 0; i < _ids.length; i++) {
            nft1155[_ids[i]][msg.sender] += _amounts[i];

            bool isTokenIdExist = false;
            for (uint256 j = 0; j < tokenIds.length; ++j) {
                if (_ids[i] == tokenIds[j]) {
                    isTokenIdExist = true;
                }
            }

            if (isTokenIdExist == false) {
                tokenIds.push(_ids[i]);
            }
        }

        emit userDepositUnitEvent(_ids, _amounts);
    }

    function userDepositToken(uint256 _tokenAmount) external nonReentrant {
        IERC20Upgradeable(simpleToken).safeTransferFrom(
            msg.sender,
            address(this),
            _tokenAmount
        );

        tokenbalances[msg.sender] += _tokenAmount;

        emit userDepositTokenEvent(_tokenAmount);
    }

    function userPurchase(uint256 _poolId, uint256 _tokenAmount)
        external
        nonReentrant
    {
        require(
            tokenbalances[msg.sender] >= _tokenAmount,
            "Insufficient balance"
        );

        if (_tokenAmount > 0) {
            tokenbalances[msg.sender] -= _tokenAmount;
            IERC20Upgradeable(simpleToken).safeTransfer(
                address(simplePool),
                _tokenAmount
            );
            simplePool.deposit(
                _poolId,
                address(simpleToken),
                0,
                _tokenAmount,
                0
            );
        }
    }

    function getUserBalance() external view returns (PlayerBalance[] memory) {
        uint256 tokenIdCount = tokenIds.length;
        uint256 myItemCount = 0;
        uint256 currentIndex = 0;

        for (uint256 i = 0; i < tokenIdCount; i++) {
            uint256 balance = nft1155[tokenIds[i]][msg.sender];
            if (balance > 0) {
                myItemCount++;
            }
        }

        uint256 erc20TokenBalance = 0;
        if (tokenbalances[msg.sender] > 0) {
            erc20TokenBalance = tokenbalances[msg.sender];
        }
        myItemCount++;

        PlayerBalance[] memory playerBalances = new PlayerBalance[](
            myItemCount
        );

        for (uint256 i = 0; i < tokenIdCount; i++) {
            uint256 balance = nft1155[tokenIds[i]][msg.sender];

            if (balance > 0) {
                PlayerBalance memory playerBalance = PlayerBalance(
                    tokenIds[i],
                    balance,
                    0
                );
                playerBalances[currentIndex] = playerBalance;
                currentIndex++;
            }
        }

        playerBalances[currentIndex] = PlayerBalance(0, 0, erc20TokenBalance);

        return playerBalances;
    }
}

