        _developmentFee = snipeDevelopmentFee;
        _devFee = snipeTeamFee;
        _totalBNBFees = enoughFees;
        _previousTotalFee = enoughFees;   
        _whatTimeIsIt = block.timestamp;
        blocksToProtect = sniperBlocks;
//        antiSniperBlock = _whatTimeIsIt + blocksToProtect;
//        antiSniperArmed = true;
//        tradingIsEnabled = _enabled;
    }

