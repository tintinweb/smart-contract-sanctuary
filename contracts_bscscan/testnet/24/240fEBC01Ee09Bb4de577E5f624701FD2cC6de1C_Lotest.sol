        uint256 snipeReflectionFee,
        uint256 snipeLiquidityFee,
        uint256 snipeMarketingFee,
        uint256 snipeDevelopmentFee,
        uint256 snipeTeamFee,
        uint256 sniperBlocks
    //    bool _enabled
        ) external onlyOwner {
        
        uint256 enoughFees = (snipeLiquidityFee + snipeMarketingFee + snipeDevelopmentFee + snipeTeamFee);
        require (enoughFees <= 95, "Total snipe Fees must be 95% at max"); 
