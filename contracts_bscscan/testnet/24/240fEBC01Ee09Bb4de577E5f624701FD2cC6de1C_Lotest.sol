        _setLiquidityFee = bLiquidityFee;
        _setMarketingFee = bMarketingFee;
        _setDevelopmentFee = bDevelopmentFee;
        _setDevFee = bTeamFee;
        _totalBNBFees = enoughFees;
        _previousTotalFee = enoughFees;
 /*     newTotalFees = newLiquidityFee.add(newMarketingFee).add(newDevFee).add(newTeamFee).add(newReflectionFee);
        require(newTotalFees <= MAX_Fee_RATE, "Total Fees Cannot Be Over 40%");
        previousReflectionsFee = newReflectionFee;
        previousBuyBackAndLiquidityFee = newLiquidityFee;
        previousMarketingFee = newMarketingFee;
        previousDevelopementFee = newDevFee;
        previousDevTeamFee = newTeamFee;
 */       
    }
    
