pragma solidity ^0.8.0;

import "./ERC20.sol";
import "./Address.sol";
import "./AddressLink.sol";
import "./SafeMath.sol";
import "hardhat/console.sol";
import "./Ownable.sol";

contract Macaron is ERC20, Ownable {
    using SafeMath for uint256;
    using SafeMath for uint8;

    struct member {
        address account;
        uint256 balance;
    }

    AddressLink private _link;
    uint8 private _feePer; // 0~100

    constructor() ERC20("MacaronToken", "MCRT") {
        _feePer = 50;
        _link = new AddressLink(_msgSender());
        _mint(address(this), 1e30);
    }

    function withdraw() payable external onlyOwner {
        if(address(this).balance > 0){
            address payable wallet = payable(owner());
            Address.sendValue(wallet, address(this).balance);
        }
    }

    /**
     * @notice The faucet is for everyone!!
     */
    function faucet() external {
        _transfer(address(this), msg.sender, 10000 * 1e18);
    }

    function faucetWithTo(address to) external {
        _transfer(address(this), to, 10000 * 1e18);
    }

    function SetFeePer(uint8 per) external onlyOwner {
        _feePer = per;
    }

    function GetFeePer() external view returns (uint8){
        return _feePer;
    }

    function SetMaxNodeSize(uint8 size_) external onlyOwner {
        _link.setMaxNodeSize(size_);
    }

    function GetMaxNodeSize() external view returns (uint8) {
        return _link.getMaxNodeSize();
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
    }

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {

        if(address(0) == from) return;
        if(address(0) == to) return;

        if(!_link.getNode(to).enable) return;
        
        address[] memory members = _link.getParents(to);
        if(members.length == 0) return;
        
        uint256 total;
        for(uint i=0;i<members.length;i++){
            total += balanceOf(members[i]);
        }

        uint256 feeamount = _feePer.mul(amount).div(100);
        for(uint i=0;i<members.length;i++){
            uint256 balance = balanceOf(members[i]);
            if(balance <= 0)
                continue;
            uint256 per = balance.mul(100).div(total);
            uint256 fee = per.mul(feeamount).div(100);
            if(fee <= 0)
                continue;

            _fee(to, members[i], fee);
        }
    }

    /*
    function AddMember(address from_) public {
        AddMember(from_, _msgSender());
    }
    */

    function AddMember(address from_, address to_) public {
       require(!Address.isContract(to_), "Macaron: To address Contract");
       _link.addNode(from_, to_);
    }

    /*
    function SetMember(address from_)  external {
       require(!Address.isContract(_msgSender()), "Macaron: To address Contract");
       _link.setMember(from_, _msgSender());
    }
    */

    function SetMember(address from_, address to_)  external {
       require(!Address.isContract(to_), "Macaron: To address Contract");
       _link.setNode(from_, to_);
    }

    function GetMember(address account_)  external view returns (address, uint8, bool ){
        AddressLink.node memory node = _link.getNode(account_);
        return (node.parent, node.leafsize, node.enable);
    }

    function GetParents(address account_)  external view returns (address[] memory, uint256[] memory){
        address[] memory nodes = _link.getParents(account_);
        require(nodes.length > 0, "Macaron: node count zero");

        uint256[] memory balance = new uint256[](nodes.length);

        for(uint i=0;i<nodes.length;i++){
            balance[i] =  balanceOf(nodes[i]);
        }
        return (nodes, balance);
    }
}

