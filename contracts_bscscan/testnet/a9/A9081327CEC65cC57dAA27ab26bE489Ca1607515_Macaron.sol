pragma solidity ^0.8.0;

import "./Ownable.sol";
import "hardhat/console.sol";

contract AddressLink is Ownable {
    
    struct node {
        address parent;
        uint8 leafsize;
        bool enable;
    }

    uint8 private _maxNodeSize;
    event SetMaxNodeSize(uint8 indexed size);

    mapping(address => node) private _nodes;

    event AddNode(address indexed from, address indexed to);
    event SetNode(address indexed from, address indexed to);

    constructor(address sender_) {
        _maxNodeSize = 3;
        _nodes[sender_] = node(address(0), 0, true);
        emit AddNode(address(0), sender_);
    }

    function setMaxNodeSize(uint8 size_) public onlyOwner {
        _maxNodeSize = size_;
        emit SetMaxNodeSize(_maxNodeSize);
    }

    function getMaxNodeSize() public view returns (uint8) {
        return _maxNodeSize;
    }

    function addNode(address from_, address to_) public {
        require(
            to_ != address(0),
            "AddressLink: To approve to the zero address"
        );
        require(
            from_ != address(0),
            "AddressLink: From approve to the zero address"
        );
        require(_nodes[from_].enable, "AddressLink: From not Found");
        require(!_nodes[to_].enable, "AddressLink: From join Found");
        require(
            _nodes[from_].leafsize < _maxNodeSize,
            "AddressLink: from Max Node over"
        );
        //뉴 조인
        _nodes[to_] = node(from_, 0, true);
        _nodes[from_].leafsize += 1;

        emit AddNode(from_, to_);
    }


    function setNode(address from_, address to_) public {
        console.log("setNode from", from_);
        console.log("setNode to", to_);

        require(
            to_ != address(0),
            "AddressLink: To approve to the zero address"
        );
        require(
            from_ != address(0),
            "AddressLink: From approve to the zero address"
        );
        require(_nodes[from_].enable, "AddressLink: From not Found");
        require(_nodes[to_].enable, "AddressLink: To not Found");
        require(
            _nodes[from_].leafsize < _maxNodeSize,
            "AddressLink: from Max Node over"
        );

        _nodes[_nodes[to_].parent].leafsize -= 1;

        _nodes[to_].parent = from_;
        _nodes[from_].leafsize += 1;

        emit SetNode(from_, to_);
    }

    function getNode(address account_) public view returns (node memory) {
        return _nodes[account_];
    }

    function getParents(address account_)
        public
        view
        returns (address[]  memory)
    {
        require(
            account_ != address(0),
            "AddressLink: Account approve to the zero address"
        );
        require(_nodes[account_].enable, "AddressLink: Account not Found");

        uint8 size = 0;
        address pos = _nodes[account_].parent;
        while (pos != address(0)) {
            if (!_nodes[pos].enable)
                break;
            
            size += 1;
            pos = _nodes[pos].parent;
        }

        address[] memory Nodes = new address[](size);
        size = 0;
        pos = _nodes[account_].parent;
        while (pos != address(0)) {
            if (!_nodes[pos].enable)
                break;
            
            Nodes[size] = pos;
