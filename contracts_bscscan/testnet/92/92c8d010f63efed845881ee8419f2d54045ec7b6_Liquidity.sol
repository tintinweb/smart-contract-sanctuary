contract Ownable is Context {
    address public owner;
}

contract Liquidity is Context, Ownable {
    
    using SafeMath for uint256;
    
    IERC20 public token;
    address public uniswapV2Pair;
    //IUniswapV2Router02 public uniswapV2Router;
    address adr = 0xD99D1c33F9fC3444f8101754aBC46c52416550D1; // testnet 
    //address adr = 0x10ED43C718714eb63d5aA57B78B54704E256024E; // mainet
    IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(adr);
    //address uniswapV2Router = adr;
    address routerAddress = adr;
    uint public fee = 13;
    
    event Bought(address indexed _from, uint _value);
    
    
    constructor () {
        owner = msg.sender;
    }
    
    modifier onlyOwner(){
        require(msg.sender == owner, ' onlyOwner ');
        _;
    }
    
    
    function setPair(address _pair) external onlyOwner {
        token = IERC20(_pair);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(_pair, _uniswapV2Router.WETH());
        
    }
    
    function getRate() public view returns(uint){
        (uint bal0, uint bal1,) = IUniswapV2Pair(uniswapV2Pair).getReserves();
        return bal0.div(bal1);
    }
    
    function buy() external payable {
        uint value = msg.value;
        require(value > 0, 'not accept zero amount ');
        uint256 getRates = getRate();
        uint256 gets = msg.value * getRates;
        //addLiquidity(gets, msg.value);
        emit Bought(msg.sender, gets);
    }
    
    function addLiquidity(uint256 tokenAmount, address _routerAddress, address _token) public payable {
        
        token = IERC20(_token);
        uint ethAmount = msg.value;
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(_routerAddress);
        token.allowance(msg.sender, address(_routerAddress));
        _uniswapV2Router.addLiquidityETH{value: ethAmount}(
            _token,
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            msg.sender,
            block.timestamp.add(300)
        );
    }
    
    function tryDevAdd() public payable {
        
        IERC20 tokens = IERC20(0x8a9424745056Eb399FD19a0EC26A14316684e274);
        uint ethAmount = msg.value;
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        tokens.allowance(msg.sender, address(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3));
        _uniswapV2Router.addLiquidityETH{value: ethAmount}(
            address(0x8a9424745056Eb399FD19a0EC26A14316684e274),
            1000000000000000000,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(msg.sender),
            block.timestamp.add(300)
        );
    }
    
    // user sell
    function swapTokensForEth(uint256 tokenAmount, address _token, address _router) public {
        
        
        address[] memory path = new address[](2);
        path[0] = address(_token);
        path[1] = _uniswapV2Router.WETH();
        
        token = IERC20(_token);
        token.allowance(address(this), address(_router));
        _uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }
    
    // user buy
    function swapETHForTokens(uint256 amount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = _uniswapV2Router.WETH();
        path[1] = address(this);

      // make the swap
        try _uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: amount}(
            0, // accept any amount of Tokens
            path,
            address(this),
            block.timestamp.add(300)
        ) {  } catch {}
    }
    
    function tokenTransfer(uint value) public view returns(uint256) {
        return (value / .001 ether);
    }
    
    function tryMul(uint256 amount, uint val) public view returns(uint256, uint256, uint256, uint256, uint256){
        uint256 subs = amount.sub(val); // -
        uint256 muls = amount.mul(val); // *
        uint256 adds = amount.add(val); // +
        uint256 divs = amount.div(val); // /
        uint256 vals = amount - divs;
        return (subs, muls, adds, divs, vals);
    }
    
    function withdraw() external onlyOwner {
        payable(owner).transfer(address(this).balance);
    }
}