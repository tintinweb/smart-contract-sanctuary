pragma solidity ^0.6.12;
pragma experimental ABIEncoderV2;

import "./MetaverseNFT.sol";
import "./openzeppelin-solidity/contracts/access/Ownable.sol";


contract MetaverseNFTData is Ownable{
    Metaverse[] public metaverses;
    mapping(address => mapping(uint => uint)) public metaverseIndexes;
    mapping (address => bool) public canStake;

    struct Metaverse {
        uint256 metaverseId;
        MetaverseNFT metaverseNFT;
        uint256 tokenId;
        address ownerAddress;
        address authorAddress;
        uint256 metaversePrice;
        address tokenAddress;
        string ipfsHashOfMetaverse;
        string status;
        string extra;
        //string metaverseNftDescription;
        //string fileformat;
        //string tags;  //"split by #"
        //string cover; //cover of nft
    }

    /**
     * @notice - Save metadata of a metaverseNFT
     */
    function saveMetadataOfMetaverseNFT(
        MetaverseNFT _metaverseNFT,
        uint256 _tokenId,
        address _ownerAddress,
        address _authorAddress,
        uint _metaversePrice,
        address _tokenAddress,
        string memory _ipfsHash,
        string memory _extra
    ) public {
        uint id = metaverses.length;
        Metaverse memory metaverse = Metaverse({
            metaverseId : id,
            metaverseNFT: _metaverseNFT,
            tokenId: _tokenId,
            ownerAddress: _ownerAddress,
            authorAddress: _authorAddress,
            metaversePrice: _metaversePrice,
            tokenAddress: _tokenAddress,
            ipfsHashOfMetaverse: _ipfsHash,
            status: "Cancelled",
            extra: _extra
        });
        metaverses.push(metaverse);
        metaverseIndexes[address(_metaverseNFT)][_tokenId] = id;
    }

    /**
     * @notice - Update owner address of a metaverseNFT by transferring ownership
     * Update status "Cancelled"
     */
    function updateOwnerOfMetaverse(MetaverseNFT _metaverseNFT, uint tokenId, address _newOwner) public {
        require (_newOwner != address(0), "A new owner address should be not empty");
        require(
            _newOwner == _metaverseNFT.ownerOf(tokenId),
            "metaverse newOwner address must be equal to NFT address"
        );
        uint index = getMetaverseIndex(_metaverseNFT, tokenId);
        Metaverse storage metaverse = metaverses[index];
        metaverse.ownerAddress = _newOwner;
    }

    /**
     * @notice - Update status ("Open" or "Cancelled")
     */
    function updateStatus(MetaverseNFT _metaverseNFT, uint tokenId, string memory _newStatus) public {
        require(msg.sender == _metaverseNFT.ownerOf(tokenId), "Metaverse status can be update only by owner");
        uint index = getMetaverseIndex(_metaverseNFT, tokenId);
        Metaverse storage metaverse = metaverses[index];
        metaverse.status = _newStatus;
    }

    function updatePrice(MetaverseNFT _metaverseNFT, uint tokenId, uint _price, address _tokenAddress) public {
        require(msg.sender == _metaverseNFT.ownerOf(tokenId), "Metaverse price can be update only by owner");
        uint index = getMetaverseIndex(_metaverseNFT, tokenId);
        Metaverse storage metaverse = metaverses[index];
        metaverse.metaversePrice = _price;
        metaverse.tokenAddress = _tokenAddress;
    }

    ///-----------------
    /// Getter methods
    ///-----------------
    function getMetaverse(uint index) public view returns (Metaverse memory _metaverse) {
        Metaverse memory metaverse = metaverses[index];
        return metaverse;
    }

    function getMetaverseIndex(MetaverseNFT metaverseNFT, uint256 tokenId) public view returns (uint) {
        uint index = metaverseIndexes[address(metaverseNFT)][tokenId];
        return index;
    }

    function getMetaverseByNFTAddress(MetaverseNFT metaverseNFT, uint256 tokenId) public view returns (Metaverse memory _metaverse) {
        uint index = metaverseIndexes[address(metaverseNFT)][tokenId];

        Metaverse memory metaverse = metaverses[index];
        return metaverse;
    }

    function getMetaversesByRange(uint startIndex, uint blockSize) public view returns (Metaverse[] memory _metaverses) {
        if(getMetaversesCount() == 0) {
            return new Metaverse[](0);
        }
        if (startIndex + blockSize > metaverses.length) {
            blockSize = metaverses.length - startIndex;
        }
        Metaverse[] memory metaversesRange = new Metaverse[](blockSize);
        for (uint i=startIndex; i <= startIndex+blockSize - 1; i++) {
            metaversesRange[i-startIndex] = metaverses[i];
        }
        return metaversesRange;
    }

    function getMetaversesCount() public view returns (uint _metaversesCount) {
        return metaverses.length;
    }

    function getAllMetaverses() public view returns (Metaverse[] memory _metaverses) {
        return metaverses;
    }

    function setCanStake(address _authorAddress, bool _canStake) public onlyOwner {
        canStake[_authorAddress] = _canStake;
    }

    function nftCanStake(address _metaverseNFT, uint256 tokenId) external view returns (bool) {
        uint index = metaverseIndexes[_metaverseNFT][tokenId];
        Metaverse memory metaverse = metaverses[index];
        return canStake[metaverse.authorAddress];
    }

    function stakeWeight(address _metaverseNFT, uint256 tokenId) external view returns (uint weitht) {
        uint index = metaverseIndexes[_metaverseNFT][tokenId];
        Metaverse memory metaverse = metaverses[index];
        return metaverse.metaversePrice;
    }


    /*function getMyMetaverses() public view returns (Metaverse[] memory_metaverses) {
        for (uint i=0; i < metaverses.length; i++) {
            if (metaverses[i].ownerAddress == msg.sender) {
                
            }
        }
    }*/

}

