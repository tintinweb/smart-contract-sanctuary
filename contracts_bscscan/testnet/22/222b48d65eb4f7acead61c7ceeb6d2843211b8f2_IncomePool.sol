        uint256 oldPoolSellFee=sellFeeTotal[_poolOwner];
        uint256 oldCongressSellFee=sellFeeTotal[_congressOwner];
        uint256 oldPartnerSellFee=sellFeeTotal[_partnerOwner];
        uint256 oldPoolMineFee=mineFeeTotal[_poolOwner];
        uint256 oldCongressMineFee=mineFeeTotal[_congressOwner];
        uint256 oldPartnerMineFee=mineFeeTotal[_partnerOwner];
        _poolOwner=poolAddr;
        _congressOwner=feeAddr;
        _partnerOwner=partnerAddr;
        sellFeeTotal[_poolOwner]=oldPoolSellFee;
        sellFeeTotal[_congressOwner]=oldCongressSellFee;
        sellFeeTotal[_partnerOwner]=oldPartnerSellFee;
        mineFeeTotal[_poolOwner]=oldPoolMineFee;
        mineFeeTotal[_congressOwner]=oldCongressMineFee;
        mineFeeTotal[_partnerOwner]=oldPartnerMineFee;
    }

    //获取手续费地址
    function getFeeOwners() view public returns(address,address,address)
    {
        return (_poolOwner,_congressOwner,_partnerOwner);
    }

    //排除扣费的账号
    function setSetter(address setter,bool flag) public
    {
        require(msg.sender==_owner,"Only the owner can perform the operation");
        _allowSetter[setter] =flag;
    }

    //是否有设置权限
    function isSetter(address account) public view returns (bool) {
        return _allowSetter[account];
    }

    //增加拥有者卖出手续费
    function addSellOwnerFee(uint256 poolFee,uint256 congressFee,uint256 partnerFee) public {
        require(_allowSetter[msg.sender],"No operation permission");
        if(poolFee>0){
            sellFeeTotal[_poolOwner]+=poolFee;
        }
        if(congressFee>0){
            sellFeeTotal[_congressOwner]+=congressFee;
        }
        if(partnerFee>0){
            sellFeeTotal[_partnerOwner]+=partnerFee;
        }
     }

    //增加卖出手续费
    function addSellFee(uint256 fee) public {
        require(_allowSetter[msg.sender],"No operation permission");
        uint date=tsToDate(8);
        _dateSellFee[date]+=fee;
        _totalSellFee+=fee;
    }

    //获取累计卖出手续费
    function getTotalSellFee() public view returns (uint256){
        return _totalSellFee;
    }

    //获取日期卖出手续费
    function getDateSellFee(uint date) public view returns (uint256){
        return _dateSellFee[date];
    }

    //增加拥有者挖矿手续费
    function addMineOwnerFee(uint256 poolFee,uint256 congressFee,uint256 partnerFee) public {
        require(_allowSetter[msg.sender],"No operation permission");
        if(poolFee>0){
            mineFeeTotal[_poolOwner]+=poolFee;
        }
        if(congressFee>0){
            mineFeeTotal[_congressOwner]+=congressFee;
        }
        if(partnerFee>0){
            mineFeeTotal[_partnerOwner]+=partnerFee;
        }
     }

    //增加挖矿手续费
    function addMineFee(uint256 fee) public {
        require(_allowSetter[msg.sender],"No operation permission");
        uint date=tsToDate(8);
        _dateMineFee[date]+=fee;
        _totalMineFee+=fee;
    }

    //获取累计挖矿手续费
    function getTotalMineFee() public view returns (uint256){
        return _totalMineFee;
    }

    //获取日期挖矿手续费
    function getDateMineFee(uint date) public view returns (uint256){
        return _dateMineFee[date];
    }
}