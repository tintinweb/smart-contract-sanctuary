    mapping(uint=>uint256) _dateSellFee; //每日保存卖出汇总
    uint256  _totalSellFee;//总卖出手续费汇总
    mapping(address=>uint256) public sellFeeTotal; //卖出手续费累计记录
    mapping(uint=>uint256) _dateMineFee; //每日保存挖矿汇总
    uint256  _totalMineFee;//总挖矿手续费汇总
    mapping(address=>uint256) public mineFeeTotal; //挖矿手续费累计记录

