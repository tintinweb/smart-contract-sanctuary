pragma solidity >=0.6.0 <0.8.0;
pragma abicoder v2;

import "../Library/Context.sol";
import "../Library/SafeMath.sol";
import "../Library/Strings.sol";
import "../Library/IERC20.sol";
import "./MowaItemsCore.sol";
import "./ERC1155.sol";

contract MoniwarItemsNFT is MowaItemsCore, ERC1155 {
    using SafeMath for uint256;
    using Strings for uint256;

    mapping(uint256 => Mowa) public mowaFactory;

    // mapping mowa character to tokenId
    mapping(uint256 => uint256) public mowaCharacterTokenId;

    event AddMinoFactory(uint256 indexed tokenId);
    event AddAmount(uint256 indexed tokenId, address userAddress, uint256 amount);
    event RemoveAmount(uint256 indexed tokenId, address userAddress, uint256 amount);

    modifier onlySafeNFT() {
        require(manager.safeNFT(msg.sender), "require Safe Address.");
        _;
    }

    constructor(
        string memory baseURI,
        address _manager
    ) ERC1155(baseURI, _manager) {}

    /**
     * @dev Withdraw bnb from this contract (Callable by owner only)
     */
    function handleForfeitedBalance(address coinAddress, uint256 value, address payable to) public onlyOwner {
        if (coinAddress == address(0)) {
            return to.transfer(value);
        }
        IERC20(coinAddress).transfer(to, value);
    }

    function safeMintNFT(address _addr, uint256 tokenId, uint256 amount) external override onlySafeNFT {
        _mint(_addr, tokenId, amount, "0x0");
    }

    function safeBatchMintNFT(address _addr, uint256[] memory tokenId, uint256[] memory amount) external override onlySafeNFT {
        _mintBatch(_addr, tokenId, amount, "0x0");
    }

    function burnNFT(address _addr, uint256 tokenId, uint256 amount) external override onlySafeNFT {
        _burn(_addr, tokenId, amount);
    }

    function burnBatchNFT(address _addr, uint256[] memory ids, uint256[] memory amounts) external override onlySafeNFT {
        _burnBatch(_addr, ids, amounts);
    }

    /**
    * @dev Changes the base URI if we want to move things in the future (Callable by owner only)
    */
    function changeBaseURI(string memory baseURI) public onlyOwner {
        _setURI(baseURI);
    }

    function getNFT(uint256 _tokenId) external view override returns (Mowa memory)
    {
        return mowaFactory[_tokenId];
    }

    function getTokenWithCharacter(uint256 _character) external view override returns (uint256)
    {
        return mowaCharacterTokenId[_character];
    }

    function setNFTFactory(Mowa memory _mowa, uint256 _tokenId) external override onlySafeNFT {
        mowaFactory[_tokenId] = _mowa;
        mowaCharacterTokenId[_mowa.character] = _tokenId;
        emit AddMinoFactory(_tokenId);
    }

    function getAllNFT(uint256 _fromTokenId, uint256 _toTokenId) external view override returns (Mowa[] memory)
    {
        Mowa[] memory allMino = new Mowa[](totalSupply());
        uint256 count = 0;
        for (uint256 index = _fromTokenId; index <= _toTokenId; index++) {
            allMino[count] = mowaFactory[index];
            ++count;
        }
        return allMino;
    }

    function getNextNFTId() external view override returns (uint256){
        return totalSupply().add(1);
    }
}

