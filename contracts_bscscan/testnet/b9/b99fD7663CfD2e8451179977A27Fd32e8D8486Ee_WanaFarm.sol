pragma solidity 0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";

import "./interfaces/ITokenVault.sol";
import "./interfaces/ITokenFarm.sol";

import "../BaseFarm.sol";
import "hardhat/console.sol";

contract WanaFarm is BaseFarm, ReentrancyGuard, Ownable, Initializable, ITokenFarm {
    using SafeERC20 for IERC20;

    address public stakeToken;
    address public tokenVault;
    uint256 public tokenPerBlock;
    uint256 public accWanaPerShare;
    uint256 public lockRewardDuration;

    // Info of each user that stakes Wana tokens.
    mapping(address => UserInfo) public userInfo;

    mapping(address => LockRewardInfo[]) public userToLockRewardList;
    mapping(address => uint256) private _totalRewardAmount;
    mapping(address => uint256) private _totalClaimedAmount;

    event Deposit(address indexed user, uint256 amount);
    event Withdraw(address indexed user, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 amount);

    modifier mustActive() {
        require(isActive == true, "Farm: Not active");
        _;
    }

    constructor() {}

    function initialize(
        address _stakeToken,
        address _tokenVault,
        uint256 _tokenPerBlock,
        uint256 _startBlock,
        uint256 _lockDuration,
        uint256[4] memory _rateParameters // 0: firstCycleRate , 1: initRate, 2: reducingRate, 3: reducingCycle
    ) external initializer {
        require(_tokenVault != address(0), "Farm: Invalid vault token");
        require(_tokenPerBlock > 1000, "Farm: Invalid block reward"); // minimum 1000 divisibility per block reward
        require(_startBlock > block.number, "Farm: Invalid start block"); // ideally at least 24 hours more to give farmers time
        require(_rateParameters[0] > 0, "Farm: Invalid first cycle rate");
        require(_rateParameters[1] > 0, "Farm: Invalid initial rate");
        require(_rateParameters[2] > 0 && _rateParameters[2] < 100, "Farm: Invalid reducing rate");
        require(_rateParameters[3] > 0, "Farm: Invalid reducing cycle");

        tokenVault = _tokenVault;
        stakeToken = _stakeToken;
        tokenPerBlock = _tokenPerBlock;
        startBlock = _startBlock;
        lockRewardDuration = _lockDuration;

        rateConfig.firstCycleRate = _rateParameters[0];
        rateConfig.initRate = _rateParameters[1];
        rateConfig.reducingRate = _rateParameters[2];
        rateConfig.reducingCycle = _rateParameters[3];
        isActive = true;

        uint256 _lastRewardBlock = block.number > _startBlock ? block.number : _startBlock;
        lastRewardBlock = _lastRewardBlock;
        accWanaPerShare = 0;
    }

    function pendingToken(address _user) external view override returns (uint256 amount) {
        UserInfo storage user = userInfo[_user];
        uint256 _accWanaPerShare = accWanaPerShare;
        uint256 _tokenSupply = IERC20(stakeToken).balanceOf(address(this));
        if (block.number > lastRewardBlock && _tokenSupply != 0 && isActive == true) {
            uint256 _multiplier = getMultiplier(lastRewardBlock, block.number);
            uint256 _tokenReward = _multiplier * tokenPerBlock;
            _accWanaPerShare = _accWanaPerShare + (_tokenReward / _tokenSupply);
        }
        amount = ((user.amount * _accWanaPerShare) / MAGIC_NUMBER) - user.rewardDebt;
    }

    function updatePool() public mustActive {
        if (block.number <= lastRewardBlock) {
            return;
        }
        uint256 _tokenSupply = IERC20(stakeToken).balanceOf(address(this));
        if (_tokenSupply == 0) {
            lastRewardBlock = block.number;
            return;
        }
        uint256 _multiplier = getMultiplier(lastRewardBlock, block.number);
        uint256 _tokenReward = _multiplier * tokenPerBlock;
        accWanaPerShare = accWanaPerShare + (_tokenReward / _tokenSupply);
        lastRewardBlock = block.number;
    }

    function getTokenPerBlock() external view override returns (uint256) {
        return tokenPerBlock;
    }

    function getUserInfo(address _account) external view override returns (uint256 amount, uint256 rewardDebt) {
        return (userInfo[_account].amount, userInfo[_account].rewardDebt);
    }

    function getToken() external view override returns (address) {
        return stakeToken;
    }

    function deposit(uint256 _amount) external override mustActive {
        UserInfo storage user = userInfo[msg.sender];
        updatePool();
        if (user.amount > 0) {
            uint256 pending = ((user.amount * accWanaPerShare) / MAGIC_NUMBER) - user.rewardDebt;
            if (pending > 0) {
                _calcTransferOfLock(msg.sender, pending);
            }
        }
        if (_amount > 0) {
            user.amount = user.amount + _amount;
        }
        user.rewardDebt = (user.amount * accWanaPerShare) / MAGIC_NUMBER;
        if (_amount > 0) {
            IERC20(stakeToken).safeTransferFrom(msg.sender, address(this), _amount);
        }
        emit Deposit(msg.sender, _amount);
    }

    function withdraw(uint256 _amount) external override {
        UserInfo storage user = userInfo[msg.sender];
        require(user.amount >= _amount, "withdraw: amount is too high");

        if (isActive == true) {
            updatePool();
        }
        uint256 pending = ((user.amount * accWanaPerShare) / MAGIC_NUMBER) - user.rewardDebt;
        if (pending > 0) {
            _calcTransferOfLock(msg.sender, pending);
        }
        if (_amount > 0) {
            user.amount = user.amount - _amount;
        }
        user.rewardDebt = (user.amount * accWanaPerShare) / MAGIC_NUMBER;
        if (_amount > 0) {
            IERC20(stakeToken).safeTransfer(msg.sender, _amount);
        }
        emit Withdraw(msg.sender, _amount);
    }

    function claimLockReward(uint256 _index) external nonReentrant {
        _claimLockReward(_index);
    }

    function claimTotalReward(uint256 _start, uint256 _end) external nonReentrant {
        address _user = msg.sender;
        if (userToLockRewardList[_user].length == 0) {
            return;
        }
        if (_end >= userToLockRewardList[_user].length) {
            _end = userToLockRewardList[_user].length - 1;
        }
        if (_start > _end) {
            return;
        }
        for (uint256 _index = _start; _index <= _end; _index++) {
            _claimLockReward(_index);
        }
    }

    function getTotalClaimableAmount(address _user, uint256 _start, uint256 _end)
    external
    view
    returns (uint256 totalClaimableAmount)
    {
        if (userToLockRewardList[_user].length == 0) {
            return 0;
        }
        if (_end >= userToLockRewardList[_user].length) {
            _end = userToLockRewardList[_user].length - 1;
        }
        if (_start > _end) {
            return 0;
        }
        for (uint256 _index = _start; _index <= _end; _index++) {
            totalClaimableAmount = totalClaimableAmount + _getClaimableAmount(_user, _index);
        }
    }

    function getClaimableAmount(address _user, uint256 _index)
    external
    view
    returns (uint256)
    {
        if (userToLockRewardList[_user].length <= _index) {
            return 0;
        }
        return _getClaimableAmount(_user, _index);
    }

    function getLockRewardCountByUser(address _user) external view returns (uint256 count) {
        count = userToLockRewardList[_user].length;
    }

    function getLockRewardInfo(address _user, uint256 _index)
    external
    view
    returns (LockRewardInfo memory info)
    {
        require(_index < userToLockRewardList[_user].length, "Farm: Invalid index");
        info = userToLockRewardList[_user][_index];
    }

    function getTotalAmountLockedByUser(address _user) external view returns (uint256) {
        return _totalRewardAmount[_user] - _totalClaimedAmount[_user];
    }

    function emergencyWithdraw() external override {
        UserInfo storage user = userInfo[msg.sender];
        uint256 _amount = user.amount;
        user.amount = 0;
        user.rewardDebt = 0;
        IERC20(stakeToken).safeTransfer(msg.sender, _amount);
        emit EmergencyWithdraw(msg.sender, _amount);
    }

    function updateReducingRate(uint256 _reducingRate) external onlyOwner mustActive {
        require(_reducingRate > 0 && _reducingRate <= 100, "Farm: Invalid reducing rate");
        rateConfig.reducingRate = _reducingRate;
    }

    function updateLockDuration(uint256 _lockRewardDuration) external onlyOwner mustActive {
        lockRewardDuration = _lockRewardDuration;
    }

    function forceEnd() external onlyOwner mustActive {
        updatePool();
        isActive = false;
    }
    
    function _claimLockReward(uint256 _index) internal {
        address _user = msg.sender;
        require(_index < userToLockRewardList[_user].length, "Farm: Invalid index");
        uint256 claimableAmount = _getClaimableAmount(_user, _index);
        require(claimableAmount > 0, "Farm: Nothing to claim");
        userToLockRewardList[_user][_index].claimedAmount += claimableAmount;
        if (userToLockRewardList[_user][_index].amount == userToLockRewardList[_user][_index].claimedAmount) {
            userToLockRewardList[_user][_index].isActive = false;
        }
        _totalClaimedAmount[_user] += claimableAmount;
        _safeTokenTransfer(_user, claimableAmount);
    }

    function _getClaimableAmount(address _user, uint256 _index)
        internal
        view
        returns (uint256)
    {
        LockRewardInfo memory info = userToLockRewardList[_user][_index];
        if (!info.isActive) return 0;
        if (block.number <=  info.startBlock + info.lockDuration) return 0;
        return info.amount;
    }

    function _safeTokenTransfer(address _to, uint256 _amount) internal {
        ITokenVault(tokenVault).safeTokenTransfer(_to, _amount);
    }

    function _calcTransferOfLock(address _user, uint256 _amount) internal {
        if (lockRewardDuration == 0) {
            _safeTokenTransfer(_user, _amount);
            return;
        }
        LockRewardInfo memory info = LockRewardInfo(_amount, block.number, lockRewardDuration, 0, true);
        userToLockRewardList[_user].push(info);
        _totalRewardAmount[_user] += _amount;
    }
}

