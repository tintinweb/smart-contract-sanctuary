pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "hardhat/console.sol";
import { Base64 } from "./libraries/Base64.sol";
import  "./libraries/svg.sol";
import "./libraries/Seven.sol";

contract Oddblox is ERC721URIStorage, svg, Seven {
   
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    
    constructor() ERC721 ("Oddblox", "OB") {
        console.log("Oddblox, bringing art to the third world.");
    }

    function pickRandomColorPalette(uint256 tokenId) public view returns (string memory) {

        uint256 rand = random(string(abi.encodePacked("<defs><style>.e{ fill:none; }.A { fill: #D9D023; } .B { fill: #2A3480; } .C { fill: #EE3824; } .D { fill: #1FA378; }</style></defs>", Strings.toString(tokenId))));
        rand = rand % svg.palette.length;
        return svg.palette[rand];
    }

    function pickRandomLayerOne(uint256 tokenId) public view returns (string memory) {

        uint256 rand = random(string(abi.encodePacked("<rect class='A' x='100' y='100' width='500' height='500'/><rect class='B' x='600' y='100' width='500' height='500'/><rect class='C' x='100' y='600' width='500' height='500'/><rect class='D' x='600' y='600' width='500' height='500'/>", Strings.toString(tokenId))));
        rand = rand % svg.layerOne.length;
        return svg.layerOne[rand];
    }

    function pickRandomLayerTwo(uint256 tokenId) public view returns (string memory) {

        uint256 rand = random(string(abi.encodePacked("<rect class='A' x='246.45' y='246.45' width='707.11' height='707.11' transform='translate(-248.53 600) rotate(-45)'/>", Strings.toString(tokenId))));
        rand = rand % svg.layerTwo.length;
        return svg.layerTwo[rand];
    }

    function pickRandomLayerThree(uint256 tokenId) public view returns (string memory) {

        uint256 rand = random(string(abi.encodePacked("<rect class='A' x='350' y='350' width='500' height='500'/>", Strings.toString(tokenId))));
        rand = rand % svg.layerThree.length;
        return svg.layerThree[rand];
    }

    function pickRandomLayerFour(uint256 tokenId) public view returns (string memory) {

        uint256 rand = random(string(abi.encodePacked("<polygon class='A' points='1100 100 100 100 100 1100 1100 100'/>", Strings.toString(tokenId))));
        rand = rand % svg.layerFour.length;
        return svg.layerFour[rand];
    }

    function pickRandomLayerFive(uint256 tokenId) public view returns (string memory) {

        uint256 rand = random(string(abi.encodePacked("<polygon class='A' points='100 100 100 600 600 100 100 100'/>", Strings.toString(tokenId))));
        rand = rand % svg.layerFive.length;
        return svg.layerFive[rand];
    }

    function pickRandomLayerSix(uint256 tokenId) public view returns (string memory) {

        uint256 rand = random(string(abi.encodePacked( "<rect class='A' x='200' y='200' width='300' height='300'/>", Strings.toString(tokenId))));
        rand = rand % svg.layerSix.length;
        return svg.layerSix[rand];
    }

    function pickRandomLayerSeven(uint256 tokenId) public view returns (string memory) {

        uint256 rand = random(string(abi.encodePacked("<polygon class='A' points='200 500 500 500 500 200 200 500'/>", Strings.toString(tokenId))));
        rand = rand % Seven.layerSeven.length;
        return Seven.layerSeven[rand];
    }

    function random(string memory input) internal pure returns (uint256) {
      return uint256(keccak256(abi.encodePacked(input)));
    }

    //Function to make Oddblock
    function makeAnOddblock() public {
        //Get current tokenId
        uint256 newItemId = _tokenIds.current();

        // Random Generation
        string memory color = pickRandomColorPalette(newItemId);
        string memory first = pickRandomLayerOne(newItemId);
        string memory second = pickRandomLayerTwo(newItemId);
        string memory third = pickRandomLayerThree(newItemId);
        string memory fourth = pickRandomLayerFour(newItemId);
        string memory fifth = pickRandomLayerFive(newItemId);
        string memory sixth = pickRandomLayerSix(newItemId);
        string memory seventh = pickRandomLayerSeven(newItemId);
        string memory combinedLayers = string(abi.encodePacked(color, first, second, third, fourth, fifth, sixth, seventh));

        // Putting layers together
        string memory finalSvg = string(abi.encodePacked(svg.baseSvg, combinedLayers, "</svg>"));

        string memory json = Base64.encode(
            bytes(
                string(
                    abi.encodePacked(
                        '{"name": "',
                    // We set the title of our NFT as the generated word.
                        "Oddblox",
                        '", "description": "A highly acclaimed collection of squares.", "image": "data:image/svg+xml;base64,',
                    // We add data:image/svg+xml;base64 and then append our base64 encode our svg.
                        Base64.encode(bytes(finalSvg)),
                        '"}'
                    )
                )
            )
        );

        string memory finalTokenUri = string(
            abi.encodePacked("data:application/json;base64,", json)
        );


        console.log("\n--------------------");
        console.log(finalTokenUri);
        console.log("--------------------\n");



        //MINT
        _safeMint(msg.sender, newItemId);

        //Set NFT data
        _setTokenURI(newItemId, finalTokenUri);

        console.log("An NFT w/ ID %s has been minted to %s", newItemId, msg.sender);

        //Counter for when next NFT is minted
        _tokenIds.increment();
    }
}

