pragma solidity 0.8.2;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";

import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";

import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";


contract ComboCard is
    Initializable,
    ReentrancyGuardUpgradeable,
    OwnableUpgradeable,
    ERC721EnumerableUpgradeable
{
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    using SafeMathUpgradeable for uint256;
    using CountersUpgradeable for CountersUpgradeable.Counter;

    uint256 public cap;
    string public baseTokenUri;
    mapping(address => bool) public minterList;
    
    // cardType
    uint256 public constant Treasure_Box = 1;
    uint256 public constant Magic_Hat = 2;
    uint256 public constant Spade = 3;
    uint256 public constant Ferttilizer = 4;
    uint256 public constant Computer = 5;
    uint256 public constant Noteboard = 6;
    uint256 public constant Shield = 7;
    uint256 public constant Sword = 8;
    uint256 public constant UFO = 9;
    uint256 public constant Spaceship = 10;

    // tokenId => cardType 
    mapping(uint256 => uint256) public tokenIdTypes;

    // cardType => num
    mapping(uint256 => uint256) public cardNums;


    function initialize(
        string memory _name,
        string memory _symbol,
        uint256 _cap,
        string memory _baseTokenUri,
        address _minter
    ) external initializer {
        __ReentrancyGuard_init();
        __Ownable_init();
        __ERC721Enumerable_init();
        __ERC721_init_unchained(_name, _symbol);

        cap = _cap;
        baseTokenUri = _baseTokenUri;
        minterList[_minter] = true;
    }

    modifier onlyMinter() {
        require(
            minterList[msg.sender],
            "ComboCard: msg.sender is not in minterList"
        );
        _;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721EnumerableUpgradeable) {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function setBaseUri(string memory _baseTokenUri) external onlyOwner {
        baseTokenUri = _baseTokenUri;
    }

    function mint(address _to, uint256 cardType) public onlyMinter {
        require(_to != address(0), "ComboCard:invalid address");
        require(cardType >= Treasure_Box && cardType <= Spaceship, "ComboCard:invalid type value");

        uint256 tokenId = totalSupply();
        require(tokenId < cap, "ComboCard: exceed max supply");

        tokenIdTypes[tokenId] = cardType;
        cardNums[cardType] = cardNums[cardType].add(1);

        _safeMint(_to, tokenId);
    }

    function batchMint(address to, uint256 n, uint256 cardType) public onlyMinter {
        require(n > 0, "0");
        for (uint256 i = 0; i < n; i++) {
            mint(to, cardType);
        }
    }

    /**
     * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each
     * token will be the concatenation of the `baseURI` and the `tokenId`. Empty
     * by default, can be overriden in child contracts.
     */
    function _baseURI() internal view override returns (string memory) {
        return baseTokenUri;
    }

    function setMinterList(address account, bool flag) external onlyOwner() {
        minterList[account] = flag;
    }

    function setCap(uint256 _newCap) external onlyOwner() {
        require(_newCap > cap, "invalid cap");
        cap = _newCap;
    }
}

