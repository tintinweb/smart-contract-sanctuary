import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "./libs/interfaces/IPancakeRouter02.sol";

contract DirectSwap is Ownable  {
    using SafeERC20 for IERC20;
    using Address for address;
    using SafeMath for uint;

    IPancakeRouter02 private constant ROUTER = IPancakeRouter02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
    address private constant BUSD = 0x78867BbEeF44f2326bF8DDd1941a4439382EF2A7;
    address private constant USDT = 0x7ef95a0FEE0Dd31b22626fA2e10Ee6A223F8a684;

    mapping(address => bool) private hasApprovedRouter;

    mapping(address => bool) public approvedUsers;
    address[] public supportTokens;

    receive() external payable {} // allow receiving BNB

    // === manage functions ===
    function approveForRouter(address token) public onlyOwner {
        if(hasApprovedRouter[token]) return;
        IERC20(token).approve(address(ROUTER), uint(-1));
        hasApprovedRouter[token] = true;
        supportTokens.push(token);
    }

    function approveUser(address _user) public onlyOwner {
        if(approvedUsers[_user]) return;
        approvedUsers[_user] = true;
    }

    constructor () {
        approveForRouter(BUSD);
        approveForRouter(USDT);
        approveUser(msg.sender);
    }

    event Swapped(address indexed, address indexed, uint256, address indexed, uint256);

    // function isApprovedUser(address _user) public returns (bool) {
    //     return approvedUsers[_user];
    // }

    modifier onlyApprovedUser() {
        require(approvedUsers[msg.sender] == true, "Ownable: caller is not the owner");
        _;
    }

    function swapDirect(address _from,address _to, uint256 _amount, uint256 _blockNumber) public onlyApprovedUser {
        uint256 balance = IERC20(_from).balanceOf(address(this));
        require(_amount <= balance, "Exceed _amount");
        require(_blockNumber == block.number, "Different block number");

        // IERC20(_from).safeTransferFrom(msg.sender, address(this), _amount);
        
        uint256 swapAmount = _swapDirect(_from, _amount, _to);

        uint256 allowance = IERC20(_to).allowance(address(this), msg.sender);
        
        if (allowance < _amount) {
            IERC20(_to).approve(msg.sender, uint(-1));
        }

        IERC20(_to).safeTransfer(msg.sender, swapAmount);

        emit Swapped(msg.sender, _from, _amount, _to, swapAmount);
    }

// ------- internal functions ------- //

    function _swapDirect(address _from, uint amount, address _to) internal returns (uint){
        if(_from == _to) return amount; // no need to swap
        address[] memory path;
        path = new address[](2);
        path[0] = _from;
        path[1] = _to;

        uint[] memory amounts = ROUTER.swapExactTokensForTokens(amount, 0, path, address(this), block.timestamp);

        return amounts[amounts.length - 1];
    }
}

