import "../asset/CRS.sol";

abstract contract Staking is IStaking {

    ASC public coinASC;
    address public ascAddress;

    CRS public coinCRS;
    address public crsAddress;

    uint256 public override totalShare;

    mapping(address => uint256) public userShareBalance;

    uint256 public constant CERES_PRECISION = 1e6;
    uint256 public constant SHARE_PRECISION = 1e8;

    mapping(address => bool) public poolAddress;

    address public owner;

    uint256 public lastInterval;

    uint256 public yieldPercent = 1000; // 0.1%

    uint256 public yieldInterval = 86400; // per day

    //****************
    // MODIFIES
    //****************
    modifier onlyPools() {
        require(coinASC.pools(msg.sender) == true, "Only pools!");
        _;
    }

    modifier onlyGovernance() {
        require(msg.sender == owner, "Only Governance!");
        _;
    }

    // ------------------------------------------------------------------------
    // Query user share balance
    // ------------------------------------------------------------------------
    function shareBalanceOf(address account) public override view returns (uint256){
        return userShareBalance[account];
    }

    // ------------------------------------------------------------------------
    // Set yieldInterval
    // ------------------------------------------------------------------------
    function setYieldInterval(uint256 newInterval) public override onlyGovernance {
        yieldInterval = newInterval;
    }

    // ------------------------------------------------------------------------
    // Set yieldPercent
    // ------------------------------------------------------------------------
    function setYieldPercent(uint256 newPercent) public override onlyGovernance {
        yieldPercent = newPercent;
    }

}

