pragma solidity ^0.8.4;

import "./Staking.sol";
import "../library/SafeERC20.sol";

contract StakingCRS is Staking {

    constructor(address asc, address crs){
        owner = msg.sender;
        lastInterval = block.timestamp;
        
        ascAddress = asc;
        coinASC = ASC(asc);
        crsAddress = crs;
        coinCRS = CRS(crs);
    }

    // ------------------------------------------------------------------------
    // Get staking token address
    // ------------------------------------------------------------------------
    function stakingToken() public override view returns (address){
        return crsAddress;
    }

    // ------------------------------------------------------------------------
    // Get staking balance of account 
    // ------------------------------------------------------------------------
    function stakingBalanceOf(address account) public override view returns (uint256){

        uint256 shareBalance = userShareBalance[account];
        // denominator
        uint256 denominator = shareBalance * SHARE_PRECISION / totalShare;

        uint256 crsBalance = coinCRS.balanceOf(address(this));
        if (crsBalance > 0)
            return denominator * crsBalance / SHARE_PRECISION;
        else
            return 0;
    }

    // ------------------------------------------------------------------------
    // User staking entrance - CRS
    // ------------------------------------------------------------------------
    function stake(uint256 amount) public override {
        require(amount > 0, "amount can't be 0!");
        require(coinCRS.allowance(msg.sender, address(this)) >= amount, "allowance is not enough!");

        // transfer token from sender to this stake
        SafeERC20.safeTransferFrom(coinCRS, msg.sender, address(this), amount);
        totalShare += amount;
        userShareBalance[msg.sender] += amount;

        emit StakeCRS(msg.sender, amount);
    }

    // ------------------------------------------------------------------------
    // User withdraw by shareAmount. Probably coins: ASC / CRS
    // ------------------------------------------------------------------------
    function withdraw(uint256 shareAmount) public override {
        uint256 shareBalance = userShareBalance[msg.sender];
        require(shareAmount <= shareBalance, "your share balance is not enough!");

        // decrease user share balance
        userShareBalance[msg.sender] -= shareAmount;

        // denominator
        uint256 denominator = shareBalance * SHARE_PRECISION / totalShare;

        // check tokens balance in this stake, and transfer
        // ASC
        uint256 ascBalance = coinASC.balanceOf(address(this));
        if (ascBalance > 0)
            coinASC.transfer(msg.sender, denominator * ascBalance / SHARE_PRECISION);

        // CRS
        uint256 crsBalance = coinCRS.balanceOf(address(this));
        if (crsBalance > 0)
            coinCRS.transfer(msg.sender, denominator * crsBalance / SHARE_PRECISION);

        emit WithdrawFromCRS(msg.sender, shareAmount);
    }


    // ------------------------------------------------------------------------
    // Approve to a collateral pool
    // ------------------------------------------------------------------------
    function approvePool(uint256 amount) public onlyPools override {
        require(coinCRS.balanceOf(address(this)) > amount, "CRS staking balance is not enough!");
        coinCRS.approve(msg.sender, amount);
    }


    // ------------------------------------------------------------------------
    // Staking yield
    // ------------------------------------------------------------------------
    function stakingYield() public onlyGovernance override {

        uint256 intervalCount = (block.timestamp - lastInterval) / yieldInterval;
        require(intervalCount > 0, "not a yield interval");

        uint256 yieldAmount = coinCRS.balanceOf(address(this)) * yieldPercent / CERES_PRECISION;
        coinCRS.stakingMint(address(this), yieldAmount);

        lastInterval += yieldInterval * intervalCount;
        emit YieldFromCRS(yieldAmount);
    }

    //****************
    // EVENTS
    //****************
    event StakeCRS(address indexed from, uint256 amount);
    event WithdrawFromCRS(address indexed to, uint256 shareAmount);
    event YieldFromCRS(uint256 yieldAmount);
}

