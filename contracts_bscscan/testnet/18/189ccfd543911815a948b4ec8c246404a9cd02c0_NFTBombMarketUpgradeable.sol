pragma solidity ^0.8.0;
////import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
////import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
////import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
////import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
////import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
////import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
////import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
////import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
////import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
////import "@openzeppelin/contracts-upgradeable/token/ERC1155/utils/ERC1155HolderUpgradeable.sol";
////import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
////import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
////import "@openzeppelin/contracts/utils/math/SafeMath.sol";



interface IaddressController {
    function isManager(address _mAddr) external view returns(bool);
    function getAddr(string calldata _name) external view returns(address);
}

contract NFTBombMarketUpgradeable is Initializable, ERC721HolderUpgradeable, ERC1155HolderUpgradeable, ReentrancyGuardUpgradeable, ContextUpgradeable, OwnableUpgradeable, UUPSUpgradeable{
    using SafeMath for uint256;

    bytes32 public TYPE_NFT721 ;
    bytes32 public TYPE_NFT1155;
    bytes32 public TYPE_ERC20  ;

    IaddressController public addrc;

    // ==== fee ====
    uint256 public feeRate;     // 300
    uint256 public denominator; // 10000

    function setFeeRate(uint256 feeRate_) external virtual onlyManager{
        require(feeRate_ <= 10000, "Market: Invalid feeRate");
        feeRate = feeRate_;
        emit SetFeeRate(address(this), feeRate_, msg.sender);
    }
    event SetFeeRate(address market, uint256 feeRate, address manager);

    // ==== orders ====
    mapping(uint256 => Order) public orderList;
    uint256 public currentOrderId;

    function initialize( IaddressController addrc_, uint256 feeRate_,uint256 donominator_) virtual public initializer{
        TYPE_NFT721 = bytes32("NFT721");
        TYPE_NFT1155 = bytes32("NFT1155");
        TYPE_ERC20 = bytes32("ERC20");

        addrc = addrc_;
        feeRate = feeRate_;
        denominator = donominator_;
        __Ownable_init();
        __ERC721Holder_init();
        __ERC1155Holder_init();
        __UUPSUpgradeable_init();
    }

    // ==== ==== Struct ==== ==== //
    enum Status {
        uncreate, onlist, sold
    }

    struct Order {
        uint256 orderId;
        address nftAddr;
        uint256 tokenId;
        uint256 tokenAmount;
        bytes32 assetType;
        address seller;
        uint256 price;
        address paymentToken;
        Status status;
        uint256 exTime;
    }

    function createOrder(
        address assetAddress_,
        uint256 tokenId_,
        uint256 tokenAmount_,
        bytes32 assetType_,
        uint256 price_,
        address paymentToken_,
        uint256 exTime_
    ) external {
        require(
            assetAddress_ != address(0),
            "Market: Invalid asset address");

        require(
            assetType_ == TYPE_NFT1155 ||
            assetType_ == TYPE_NFT721 ||
            assetType_ == TYPE_ERC20,
            "Market: AssetType Error");

        require(
            exTime_ > block.timestamp,
            "Market: Invalid exTime");

        // ==== deposit the asset
        _transferAsset(msg.sender, address(this), assetAddress_, tokenId_, tokenAmount_, assetType_);

        currentOrderId ++;

        // ==== populate info
        Order storage order = orderList[currentOrderId];
        order.orderId = currentOrderId;
        order.nftAddr = assetAddress_;
        order.tokenId = tokenId_;
        order.tokenAmount = tokenAmount_;
        order.assetType = assetType_;
        order.seller = msg.sender;
        order.price = price_;
        order.paymentToken = paymentToken_;
        order.exTime = exTime_;
        order.status = Status.onlist;

        emit CreateOrder(
            address(this),
            currentOrderId,
            assetAddress_,
            tokenId_,
            tokenAmount_,
            assetType_,
            msg.sender,
            price_,
            paymentToken_,
            exTime_,
            order.status);
    }

    event CreateOrder(
        address indexed market,
        uint256 orderId,
        address nftAddr,
        uint256 tokenId,
        uint256 tokenAmount,
        bytes32 assetType,
        address seller,
        uint256 price,
        address paymentToken,
        uint256 exTime,
        Status  status
    );

    function cancelOrder(uint256 orderId_) external virtual onlyManagerOrSeller(orderId_) nonReentrant {
        // 1. return the asset
        Order storage order = orderList[orderId_];
        require(order.status == Status.onlist, "Market: invalid orderId");

        _transferAsset(
            address(this),
            order.seller,
            order.nftAddr,
            order.tokenId,
            order.tokenAmount,
            order.assetType
        );
        // 2. set Order status to 2
        order.status = Status.sold;
        emit CancelOrder(address(this), orderId_, msg.sender);
    }
    event CancelOrder(address market, uint256 orderId, address operator);

    function _transferAsset(
        address from_,
        address to_,
        address assetAddress_,
        uint256 tokenId_,
        uint256 tokenAmount_,
        bytes32 assetType_
    ) internal virtual {
        if( assetType_ == TYPE_NFT721){
            IERC721(assetAddress_).safeTransferFrom(from_, to_, tokenId_);
            return;
        }

        if(assetType_  == TYPE_NFT1155){
            IERC1155(assetAddress_).safeTransferFrom(from_, to_, tokenId_, tokenAmount_, "0x00");
            return;
        }

        if(assetType_  == TYPE_ERC20){
            if(from_ == address(this)){
                IERC20(assetAddress_).transfer(to_, tokenAmount_);
                return;
            }
            IERC20(assetAddress_).transferFrom(from_, to_, tokenAmount_);
        }
    }


    function buyOne(uint256 orderId) external virtual payable nonReentrant{
        require(orderId > 0,"Market: invalid orderId");
        require(orderId <= currentOrderId,"Market: invalid orderId");
        require(orderList[orderId].exTime > block.timestamp, "Market: time expired");
        Order storage order = orderList[orderId];
        require(order.status == Status.onlist, "Market: not onlist");

        _payThePrice(order);
        order.status = Status.sold;
        _transferAsset(address(this), msg.sender, order.nftAddr, order.tokenId, order.tokenAmount, order.assetType);
        emit BuyOne(address(this), msg.sender, order.nftAddr, order.tokenId, order.tokenAmount, order.assetType);
    }

    event BuyOne(address marketAddress, address Buyer, address nftAddress, uint256 tokenId, uint256 tokenAmount, bytes32 assetType);

    function _payThePrice(Order storage order) virtual internal{
        uint256 price = order.price;
        address paymentToken = order.paymentToken;
        address seller = order.seller;

        address feeTo = addrc.getAddr("FEETO");
        uint256 feeAmount = price.mul(feeRate).div(denominator);
        uint256 remaining = price.sub(feeAmount);

        if(paymentToken == address(1)){
            require(msg.value == price, "Market: invalid amount ether to buy");
            payable(feeTo).transfer(feeAmount);
            payable(seller).transfer(remaining);
            return;
        }

        IERC20(paymentToken).transferFrom(msg.sender, feeTo, feeAmount);
        IERC20(paymentToken).transferFrom(msg.sender, seller, remaining);
    }

    modifier onlyManager(){
        require(addrc.isManager(msg.sender), "Market: onlyManager");
        _;
    }

    modifier onlyManagerOrSeller(uint256 orderId_){
        Order memory order = orderList[orderId_];
        require(addrc.isManager(msg.sender) || order.seller == msg.sender, "Market: onlyManager or Creator");
        _;
    }

    function _authorizeUpgrade(address newImplementation)
    internal
    onlyOwner
    override
    {}
}