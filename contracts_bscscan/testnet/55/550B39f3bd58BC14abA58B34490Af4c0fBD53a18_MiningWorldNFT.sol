pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";

contract MiningWorldNFT is Initializable, ERC721Upgradeable, ERC721EnumerableUpgradeable, ERC721URIStorageUpgradeable, PausableUpgradeable, OwnableUpgradeable, ERC721BurnableUpgradeable, UUPSUpgradeable, ReentrancyGuardUpgradeable {
    using SafeMathUpgradeable for uint256;
    using StringsUpgradeable for uint256;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    CountersUpgradeable.Counter internal _tokenIdCounter;

    struct NFT {
        uint256 id;
        uint256 tokenId;
        string name;
        string series;
        uint256 cat;
    }

    struct MiningWorldPrototype {
        uint256 id;
        string name;
        string series;
        uint256 cat;
        uint256 maxMint;
        uint256 minted;
        uint price;
    }

    NFT[] internal nftList;
    MiningWorldPrototype[] internal prototypes;
    address internal marketAddress;
    address internal tokenAddress;
    string internal _baseTokenURI;

    modifier hasPrototypes() {
        require(prototypes.length > 0, "Not prototypes registered yet");
        _;
    }

    event Minted(uint256 _tokenId, address _owner);
    event PrototypeAdded(uint256 _id, string name);

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize() initializer public {
        __ERC721_init("MW", "MINNWW");
        __ERC721Enumerable_init();
        __ERC721URIStorage_init();
        __Pausable_init();
        __Ownable_init();
        __ERC721Burnable_init();
        __UUPSUpgradeable_init();
        __ReentrancyGuard_init();
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }

    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyOwner
        override
    {}

    // The following functions are overrides required by Solidity.

    function _burn(uint256 tokenId)
        internal
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
    {
        super._burn(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _baseURI() virtual internal pure override returns (string memory) {
        return "https://miningworldnft.com/api/token/";
    }

    function mintNFT(uint256 _id) public virtual payable nonReentrant hasPrototypes {
        require(msg.value > 0, "Not sufficient funds");

        MiningWorldPrototype memory hp = getPrototypeById(_id);

        require(hp.id == _id, "No prototype matches the id");
        require(hp.minted < hp.maxMint, "Max nft minted");
        require(msg.value >= hp.price, "Not sufficient funds");
        _safeMintNFT(_msgSender(), hp);
    }

    function mint(uint256 _id) public virtual onlyOwner hasPrototypes {
        MiningWorldPrototype memory hp = getPrototypeById(_id);
        _safeMintNFT(_msgSender(), hp);
    }

    function _safeMintNFT(address to, MiningWorldPrototype memory proto) internal virtual {
        uint256 tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();

        NFT memory nft = NFT({
            id: proto.id,
            tokenId: tokenId,
            name: proto.name,
            series: proto.series,
            cat: proto.cat
        });

        string memory tknURI = StringsUpgradeable.toString(proto.id);
        nftList.push(nft);
        _safeMint(to, tokenId);
        _setTokenURI(tokenId, tknURI);
        _updateMintedNFT(proto.id);
    }

    function _updateMintedNFT(uint protoId) internal virtual {
        for (uint i = 0 ; i < prototypes.length ; i++) {
            MiningWorldPrototype memory current = prototypes[i];
            if (current.id == protoId) {
                prototypes[i].minted = SafeMathUpgradeable.add(prototypes[i].minted, 1);
                break;
            }
        }
    }

    function _tokensByOwner(address _owner) internal virtual view returns(uint256[] memory) {
        uint256 tokenCount = balanceOf(_owner);
        uint256[] memory tokens = new uint256[](tokenCount);

        for (uint256 i = 0; i < tokenCount; i++) {
            tokens[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokens;
    }

    function addPrototype(
        uint _id,
        string memory _proName,
        string memory _series,
        uint _cat,
        uint _maxMint,
        uint _price
    ) public virtual onlyOwner {
        bool added = false;
        for (uint i = 0 ; i < prototypes.length ; i++) {
            if (prototypes[i].id == _id) {
                added = true;
            }
        }
        require(added == false, "already added into the list");
        MiningWorldPrototype memory proto = MiningWorldPrototype({
            id: _id,
            name: _proName,
            series: _series,
            cat: _cat,
            maxMint: _maxMint,
            price: _price,
            minted: 0
            });
        prototypes.push(proto);
        emit PrototypeAdded(_id, _proName);
    }

    function removePrototypeByIndex(uint _index) public virtual onlyOwner hasPrototypes {
        for (uint i = _index; i < prototypes.length - 1; i++) {
            prototypes[i] = prototypes[i + 1];
        }
        prototypes.pop();
    }

    /**
     * @dev Backup way to change Base URI in case of losing the domain
     */
    function updateBaseURI(string memory _newBaseURI) public onlyOwner {
        _baseTokenURI = _newBaseURI;
    }

    function setMarketAddress(address _address) public onlyOwner {
        marketAddress = _address;
    }

    function setTokenAddress(address _address) public onlyOwner {
        tokenAddress = _address;
    }

    function getPrototypeById(uint256 _id) public virtual view hasPrototypes returns (MiningWorldPrototype memory) {
        MiningWorldPrototype memory hp;
        for (uint256 i = 0 ; i < prototypes.length ; i++) {
            if (prototypes[i].id == _id) {
                hp = prototypes[i];
                break;
            }
        }
        require(hp.id == _id, "No prototype matches the id");
        return hp;
    }

    function getPricePrototype(uint256 _id) public view hasPrototypes returns (uint256) {
        MiningWorldPrototype memory hp;
        for (uint256 i = 0 ; i < prototypes.length ; i++) {
            if (prototypes[i].id == _id) {
                hp = prototypes[i];
                break;
            }
        }
        require(hp.id == _id, "No prototype matches the id");
        return hp.price;
    }

    function getAllTokensByOwner(address _owner) public view returns (NFT[] memory nft) {
        require(_owner != address(0), "Param should not be address 0x0");

        uint256[] memory tokenIds = _tokensByOwner(_owner);
        NFT[] memory tokens = new NFT[](tokenIds.length);
        
        if (tokenIds.length == 0) {
            return tokens;
        }

        for (uint256 i = 0 ; i < tokenIds.length; i++) {
            tokens[i] = nftList[tokenIds[i]];
        }

        return tokens;
    }

    function getAllPrototypes() public view returns (MiningWorldPrototype[] memory) {
        return prototypes;
    }

    function getMarketAddress() public view returns(address) {
        return marketAddress;
    }

    function getTokenAddress() public view returns(address) {
        return tokenAddress;
    }

    function getFunding() public view onlyOwner returns(uint) {
        return address(this).balance;
    }

    function funding() public onlyOwner {
        payable(_msgSender()).transfer(getFunding());
    }

    function fundingValue(uint amount) public onlyOwner {
        require(amount > 0, "Amount should be greater than 0");
        address payable to = payable(_msgSender());
        to.transfer(amount);
    }

    function version() public virtual pure returns (string memory) {
        return "v1.0.0";
    }
}

