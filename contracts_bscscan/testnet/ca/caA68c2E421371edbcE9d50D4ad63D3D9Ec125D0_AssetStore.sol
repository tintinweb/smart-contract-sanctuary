pragma solidity 0.8.0;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/Context.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

import "hardhat/console.sol";

import "./commons/PermissionRight.sol";
import "./commons/Withdrawable.sol";
import "./interfaces/IAssetStore.sol";
import "./interfaces/IOrderContract.sol";
import "./interfaces/IERC721.sol";
import "./interfaces/IWaiToken.sol";

contract AssetStore is IAssetStore, PermissionRight,
    ReentrancyGuard, Withdrawable, Pausable, IERC721Receiver
{
    using Address for address;
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.UintSet;

    uint public withdrawItemsMax = 50;
    uint public totalLands;
    uint256 public totalWanas;

    address public wLandContractAddress;
    address public wanaContractAddress;
    address public waiContractAddress;
    address public orderContractAddress;

    // tenantry address => EnumerableSet.UintSet
    mapping(address => EnumerableSet.UintSet) landStorage;

    /**
      * @dev Initialize this contract. Acts as a constructor
      * @param _owner - owner of this contract
    */
    constructor (
        address _owner
    )
    public
    {
        _addAdminUser(_owner);
        _addOperatorUser(_owner);
        transferOwnership(_owner);
    }

    modifier notInOrder(uint256 itemId) {
        require(orderContractAddress != address(0), "wanaka: Order contract is in-valid");
        IOrderContract orderContract = IOrderContract(orderContractAddress);
        require(!orderContract.orderExist(wLandContractAddress, itemId), "Wanaka: Item still in listing");
        _;
    }

    modifier notInOrders(uint256[] memory itemIds) {
        require(orderContractAddress != address(0), "wanaka: Order contract is in-valid");
        IOrderContract orderContract = IOrderContract(orderContractAddress);
        for (uint256 i = 0; i < itemIds.length; i++) {
            require(!orderContract.orderExist(wLandContractAddress, itemIds[i]), "Wanaka: Item still in listing");
        }
        _;
    }

    function setLandContract(address contractAddress) external onlyAdmin {
        require(_validateContract(contractAddress), "Wanaka: Contract address is in-valid");
        wLandContractAddress = contractAddress;
        emit LandContractAdded(contractAddress);
    }

    function setOrderContract(address contractAddress) external onlyAdmin {
        require(_validateContract(contractAddress), "Wanaka: Contract address is in-valid");
        orderContractAddress = contractAddress;
        emit OrderContractAdded(contractAddress);
    }

    function setWanaContract(address contractAddress) external onlyAdmin {
        require(_validateContract(contractAddress), "Wanaka: Contract address is in-valid");
        wanaContractAddress = contractAddress;
        emit WanaContractAdded(contractAddress);
    }

    function setWaiContract(address contractAddress) external onlyAdmin {
        require(_validateContract(contractAddress), "Wanaka: Contract address is in-valid");
        waiContractAddress = contractAddress;
        emit WaiContractAdded(contractAddress);
    }

    function listLand() external view returns (uint256[] memory _lands) {
        address user = _msgSender();
        uint256 countItem = landStorage[user].length();
        _lands = new uint256[](countItem);
        for (uint256 i = 0; i < countItem; i++) {
            _lands[i] = landStorage[user].at(i);
        }
        return _lands;
    }

    function depositLand(uint256 itemId) external override notInOrder(itemId) whenNotPaused nonReentrant {
        address user = _msgSender();
        IERC721 nftContract = IERC721(wLandContractAddress);
        require(nftContract.ownerOf(itemId) == user, "Wabaka: unauthorized access");
        landStorage[user].add(itemId);
        totalLands += 1;
        nftContract.safeTransferFrom(user, address(this), itemId);

        emit LandDeposit(user, itemId);
    }

    function withdrawLand(address user, uint256 itemId) external override whenNotPaused onlyOperator {
        if (landStorage[user].contains(itemId)) {
            IERC721 nftContract = IERC721(wLandContractAddress);
            landStorage[user].remove(itemId);
            totalLands -= 1;
            nftContract.safeTransferFrom(address(this), user, itemId);

            emit LandWithdraw(user, itemId);
        }
    }

    function withdrawLands(address user) external override whenNotPaused onlyOperator returns (bool) {
        uint itemCount = landStorage[user].length();
        if (itemCount == 0) {
            return false;
        }
        if (itemCount > withdrawItemsMax) {
            itemCount = withdrawItemsMax;
        }
        IERC721 nftContract = IERC721(wLandContractAddress);
        while (itemCount > 0) {
            uint256 itemId = landStorage[user].at(0);
            landStorage[user].remove(itemId);
            totalLands = totalLands - 1;
            itemCount = itemCount - 1;
            nftContract.safeTransferFrom(address(this), user, itemId);

            emit LandWithdraw(user, itemId);
        }
        return true;
    }

    function depositWana(uint256 amount) external override whenNotPaused nonReentrant {
        address user = _msgSender();
        IERC20 wanaContract = IERC20(wanaContractAddress);
        totalWanas += amount;
        wanaContract.safeTransferFrom(user, address(this), amount);
        emit WanaDeposit(user, amount);
    }

    function withdrawWana(address user, uint256 amount) external override onlyOperator {
        require(totalWanas >= amount, "Wanaka: amount is exceeded");
        IERC20 wanaContract = IERC20(wanaContractAddress);
        totalWanas -= amount;
        wanaContract.safeTransfer(user, amount);
        emit WanaWithdraw(user, amount);
    }

    function depositWai(uint256 amount) external override nonReentrant {
        address user = _msgSender();
        IWaiToken waiContract = IWaiToken(waiContractAddress);
        waiContract.burnFrom(user, amount);
        emit WaiDeposit(user, amount);
    }

    function withdrawWai(address user, uint256 amount) external override onlyOperator {
        IWaiToken waiContract = IWaiToken(waiContractAddress);
        waiContract.mint(user, amount);
        emit WaiWithdraw(user, amount);
    }

    function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    function _validateContract(address contractAddress) private view returns (bool) {
        if (contractAddress == address(0)) return false;
        if (!contractAddress.isContract()) return false;
        return true;
    }
}

