pragma solidity ^0.8.6;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/proxy/Clones.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./IDex.sol";
import "./IStruct.sol";

interface ILocker is IStruct{
    function initialize (address _lpAddressToLock, uint256 _amount, uint256 _unlockDate, address _lpAddresssOwner, TokenVesting memory _tokenVesting, uint256 extendLockerFee) external returns(bool);
}

interface IPresaleFactory{
    function isValidPresale(address _presaleAddress) external returns(bool);
}

contract LPLockerFactory is Ownable, IStruct{
    using EnumerableSet for EnumerableSet.AddressSet;
    using Address for address payable;

    mapping(address => bool) private isWhitelisted;
    mapping(address => mapping(address => address)) private userLockers;   // track Locker address based on user and token address
    mapping(address => address[]) private allUsersLockers;
    EnumerableSet.AddressSet private allLockers;

    IFactory public factory;
    IPresaleFactory public presaleFactory;

    address public implementation;
    address public feesRecipient;

    uint256 public createLockerFee = 0.01 ether;
    uint256 public extendLockerFee = 0.01 ether;

    constructor(address _implementation, address _feesRecipient, address _factory, address _presaleFactory){
        implementation = _implementation;
        feesRecipient = _feesRecipient;
        factory = IFactory(_factory);
        presaleFactory = IPresaleFactory(_presaleFactory);
    }

    event LockerCreated(address newLocker, uint256 timestamp, address tokensOwner);
    event ImplementationUpdated(address newImplementation);
    
    receive() external payable {}

    function lockLP(address _lpAddress, uint256 _amount, uint256 _unlockDate, address _lpOwner, TokenVesting memory _tokenVesting) external payable{
        require(_amount > 0, "Insufficient amount");
        require(_unlockDate > block.timestamp, "Unlock date can't be now");
        if(_tokenVesting.enabled){
            require(_tokenVesting.firstPercentage < 100 &&
                _tokenVesting.tokensPerCyclePercentage < 100 &&
                _tokenVesting.tokensPerCyclePercentage + _tokenVesting.firstPercentage <= 100,
                "First release for presale and Percent token release each cycle must <= 100%"
            );
            require( _tokenVesting.cycle > 1,"Vesting period each cycle must be 1 or more");
            require(_tokenVesting.firstReleaseDate > block.timestamp + 5 minutes, "First release date must be >= 5 minutes from now");
        }
        // Check if _lpAddress is a valid pair
        IPair pair = IPair(_lpAddress);
        require(_lpAddress == factory.getPair(pair.token0(), pair.token1()), "Incorrect pair address");
        // If msg.sender is a presale created with the launchpad, no tax applied
        if(presaleFactory.isValidPresale(msg.sender) == false){
            require(msg.value >= createLockerFee, "Tax not met");
            payable(owner()).transfer(createLockerFee);
            if(msg.value - createLockerFee > 0) payable(msg.sender).sendValue(msg.value - createLockerFee);
        }

        address lockerCreated = Clones.clone(implementation);
        require(IERC20(_lpAddress).transferFrom(msg.sender, lockerCreated, _amount ), "Not enough allowance");
        bool success = ILocker(lockerCreated).initialize(_lpAddress, _amount ,_unlockDate, _lpOwner, _tokenVesting, extendLockerFee);
        if(success) {
            allLockers.add(lockerCreated);
            userLockers[_lpOwner][_lpAddress] = lockerCreated;
            allUsersLockers[_lpOwner].push(lockerCreated);
            emit LockerCreated(lockerCreated, block.timestamp, msg.sender);
        }
    }

    function updateCreateLockerTax(uint256 _amount) external onlyOwner{
        createLockerFee = _amount;
    }

    function updateExtendLockerTax(uint256 _amount) external onlyOwner{
        extendLockerFee = _amount;
    }

    function withdrawBNBToFeeRecipient() external{
        require(address(this).balance > 0, "Insufficient BNB");
        payable(feesRecipient).sendValue(address(this).balance);
    }

    function withdrawBEP20ToFeeRecipient(address tokenAddress) external{
        require(IERC20(tokenAddress).balanceOf(address(this)) > 0, "Insufficient BEP20 balance");
        IERC20(tokenAddress).transfer(feesRecipient, IERC20(tokenAddress).balanceOf(address(this)));
    }

    function updateImplementation(address newImplementation) external onlyOwner{
        require(implementation != newImplementation, "implementation already set");
        implementation = newImplementation;
        emit ImplementationUpdated(newImplementation);
    }

    function updateFeesRecipient(address newAddress) external onlyOwner{
        require(feesRecipient != newAddress, "Address already set");
        feesRecipient = newAddress;
    }

     function getLockerAtIndex(uint256 _index) external view returns(address){
        return allLockers.at(_index);
    }

    function getLockersLenght() external view returns(uint256) {
        return allLockers.length();
    }

    function getLockerFromUserAndTokenAddress(address _user, address _lpAddress) external view returns(address){
        return userLockers[_user][_lpAddress];
    }

    function getAllUserLockers(address _user) external view returns(address[] memory){
        return allUsersLockers[_user];
    }

}

