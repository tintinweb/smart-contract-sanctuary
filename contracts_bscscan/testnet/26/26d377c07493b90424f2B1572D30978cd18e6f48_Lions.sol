import "./ILions.sol";

contract YieldToken is ERC20("SexyToken", "SEXY") {
	using SafeMath for uint256;
    using Address for address;

	uint256 constant public BASE_RATE = 1 ether; 
	uint256 constant public INITIAL_ISSUANCE = 10 ether;
	// Tue Mar 18 2031 17:46:47 GMT+0000
	//uint256 constant public END = 1931622407;

    // 1 Year (seconds)
    uint256 constant public REWARD_END = 31536000;

	//mapping(address => uint256) public rewards;
	//mapping(address => uint256) public lastUpdate;

    mapping(address => mapping(address => uint256)) rewards;
    mapping(address => mapping(address => uint256)) lastUpdates;
    address[] contracts;

    address owner; 

	//ILions public  lionsContract;

	event RewardPaid(address indexed user, uint256 reward);

    
    modifier onlyOwner() {
        require(msg.sender == owner, "Only owner");
        _;
    }

    modifier isContract(address _contract) {
        require(_contract.isContract(), "Address isn`t a contract");
        _;
    }

	constructor(address _lions) {
		//lionsContract = ILions(_lions);
        contracts.push(_lions);
        owner = msg.sender;
	}


	function min(uint256 a, uint256 b) internal pure returns (uint256) {
		return a < b ? a : b;
	}

	// updated_amount = (balanceOG(user) * base_rate * delta / 86400) + amount * initial rate
	function updateRewardOnMint(address _user, uint256 _amount) external {
		require(isValidAddress(msg.sender), "Can't call this");
		uint256 time = min(block.timestamp, REWARD_END);
        address _contract = msg.sender;
		uint256 timerUser = lastUpdates[_contract][_user];
		if (timerUser > 0)
			rewards[_contract][_user] = rewards[_contract][_user].add(ILions(_contract).balanceOG(_user).mul(BASE_RATE.mul((time.sub(timerUser)))).div(86400)
				.add(_amount.mul(INITIAL_ISSUANCE)));
		else 
			rewards[_contract][_user] = rewards[_contract][_user].add(_amount.mul(INITIAL_ISSUANCE));
		lastUpdates[_contract][_user] = time;
	}

	// called on transfers
	function updateReward(address _from, address _to, uint256 _tokenId) external {
		require(isValidAddress(msg.sender));
        address _contract = msg.sender;
		// if (_tokenId < 1001) {
		uint256 time = min(block.timestamp, REWARD_END);
		uint256 timerFrom = lastUpdates[_contract][_from];
			if (timerFrom > 0)
		 		rewards[_contract][_from] += ILions(_contract).balanceOG(_from).mul(BASE_RATE.mul((time.sub(timerFrom)))).div(86400);
			if (timerFrom != REWARD_END)
				lastUpdates[_contract][_from] = time;
			if (_to != address(0)) {
				uint256 timerTo = lastUpdates[_contract][_to];
				if (timerTo > 0)
					rewards[_contract][_to] += ILions(_contract).balanceOG(_to).mul(BASE_RATE.mul((time.sub(timerTo)))).div(86400);
				if (timerTo != REWARD_END)
					lastUpdates[_contract][_to] = time;
            }
		}


	function getReward(address _to) external {
		require(isValidAddress(msg.sender));
		uint256 reward = rewards[msg.sender][_to];
		if (reward > 0) {
			rewards[msg.sender][_to] = 0;
			_mint(_to, reward);
			emit RewardPaid(_to, reward);
		}
	}

	function burn(address _from, uint256 _amount) external {
		require(isValidAddress(msg.sender));
		_burn(_from, _amount);
	}

	// function getTotalClaimable(address _user) external view returns(uint256) {
	// 	uint256 time = min(block.timestamp, END);
	// 	uint256 pending = lionsContract.balanceOG(_user).mul(BASE_RATE.mul((time.sub(lastUpdate[_user])))).div(86400);
	// 	return rewards[_user] + pending;
	// }

    function addContract(address _contract) public onlyOwner isContract(_contract) {
        contracts.push(_contract);
    }

    function isValidAddress(address _contract) internal view isContract(_contract) returns(bool) {
        for(uint i=0; i<contracts.length; i++) {
            if(contracts[i] == _contract) 
                return true;
        }

        return false;
    }
}

