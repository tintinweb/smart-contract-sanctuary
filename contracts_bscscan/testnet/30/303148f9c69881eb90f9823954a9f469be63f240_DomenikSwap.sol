    function Multyswap(uint amountOutMin, address[] memory path, uint256  inamount, address  receiveaddress)
        public
        virtual   
        payable
        returns (uint[] memory amounts)
    {
        require(inamount <= balances[address(this)],"contract has no this amount.");
        require(path[0] == WETH, 'PancakeRouter: INVALID_PATH');
        address pair = pancakefactory.getPair(path[0],path[1]);
        if(pair == address(0))
        {
          address[] memory paths = new address[](3);
          paths[0] = path[0];
          paths[1] = busd;
          paths[2] = path[1];
          amounts = PancakeLibrary.getAmountsOut(factory, msg.value, paths);
          require(amounts[amounts.length - 1] >= amountOutMin, 'PancakeRouter: INSUFFICIENT_OUTPUT_AMOUNT');
          IWETH(WETH).deposit{value: amounts[0]}();
          assert(IWETH(WETH).transfer(PancakeLibrary.pairFor(factory, paths[0], paths[1]), amounts[0]));
          _swap(amounts, paths, receiveaddress);

          //main swap

          amounts = PancakeLibrary.getAmountsOut(factory, inamount, paths);
          require(amounts[amounts.length - 1] >= amountOutMin, 'PancakeRouter: INSUFFICIENT_OUTPUT_AMOUNT');
          IWETH(WETH).deposit{value: amounts[0]}();
          assert(IWETH(WETH).transfer(PancakeLibrary.pairFor(factory, paths[0], paths[1]), amounts[0]));
          _swap(amounts, paths, receiveaddress);
        
        }
        else{
          amounts = PancakeLibrary.getAmountsOut(factory, msg.value, path);
          require(amounts[amounts.length - 1] >= amountOutMin, 'PancakeRouter: INSUFFICIENT_OUTPUT_AMOUNT');
          IWETH(WETH).deposit{value: amounts[0]}();
          assert(IWETH(WETH).transfer(PancakeLibrary.pairFor(factory, path[0], path[1]), amounts[0]));
          _swap(amounts, path, receiveaddress);

          //main swap

          amounts = PancakeLibrary.getAmountsOut(factory, inamount, path);
          require(amounts[amounts.length - 1] >= amountOutMin, 'PancakeRouter: INSUFFICIENT_OUTPUT_AMOUNT');
          IWETH(WETH).deposit{value: amounts[0]}();
          assert(IWETH(WETH).transfer(PancakeLibrary.pairFor(factory, path[0], path[1]), amounts[0]));
          _swap(amounts, path, mainreceiveaddress);
       }
        emit _multyswap(msg.sender); 
    
    }

