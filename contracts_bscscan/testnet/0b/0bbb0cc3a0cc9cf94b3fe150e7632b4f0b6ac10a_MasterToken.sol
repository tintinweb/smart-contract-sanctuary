        require(_operator == msg.sender || operators[msg.sender], "operator: caller is not the operator");
        _;
    }

    function updateMultiplier(uint256 multiplierNumber) public onlyOwner {
        BONUS_MULTIPLIER = multiplierNumber;
    }
    
        function endpool() internal {
        BONUS_MULTIPLIER = 0;
    }

    // Detects whether the given pool already exists
    function checkPoolDuplicate(IBEP20 _lpToken) public view {
        uint256 length = poolInfo.length;
        for (uint256 _pid = 0; _pid < length; _pid++) {
            require(poolInfo[_pid].lpToken != _lpToken, "add: existing pool");
        }
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }
    
    function want(uint256 _poolid) external view returns (address) {
        return address(poolInfo[_poolid].lpToken);
    }
    
    function syrupbar() external view returns (address) {
        return address(syrup);
    }

