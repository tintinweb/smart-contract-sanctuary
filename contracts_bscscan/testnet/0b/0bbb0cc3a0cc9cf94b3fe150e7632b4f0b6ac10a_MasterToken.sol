        require(startTime>block.timestamp || poolInfo.length==0,"start block passed");
        startTime=_startTime; 
        token = _dcvc;
        devaddr = _devaddr;
        feeAddress = _feeAddress;
        singPerSec = _singPerSec;
        for(uint i=0;i<poolInfo.length;i++){
            poolInfo[i].lastRewardTime=startTime;
        }
    }

    // Only tokens in the whitelist can be mined DCVC
    function addWhitelist(address _addToken) public onlyOwner returns (bool) {
        require(_addToken != address(0), "SwapMining: token is the zero address");
        return EnumerableSet.add(_whitelist, _addToken);
    }

    function delWhitelist(address _delToken) public onlyOwner returns (bool) {
        require(_delToken != address(0), "SwapMining: token is the zero address");
        return EnumerableSet.remove(_whitelist, _delToken);
    }

    function getWhitelistLength() public view returns (uint256) {
        return EnumerableSet.length(_whitelist);
    }

    function isWhitelist(address _token) public view returns (bool) {
        return EnumerableSet.contains(_whitelist, _token);
    }

    function getWhitelist(uint256 _index) public view returns (address){
        require(_index <= getWhitelistLength() - 1, "SwapMining: index out of bounds");
        return EnumerableSet.at(_whitelist, _index);
    }

    function setOracle(IOracle _oracle) public onlyOwner {
        require(address(_oracle) != address(0), "SwapMining: new oracle is the zero address");
        oracle = _oracle;
    }
    
       function setreferralvault(address _address) public onlyOperator {
        require(address(_address) != address(0), "SwapMining: new referal vault is the zero address");
        referralvault = IVaultReferral(_address);
    }
    
    function setreferralreward(address _address) public onlyOperator {
        require(address(_address) != address(0), "SwapMining: new referal reward pool is the zero address");
        referralrewardpool = IReferralPool(_address);
    }
     function settechnical(address _address) public onlyOperator {
        technical = address(_address);
    }
     
    
      function setreferralreward(uint256 _rate) public onlyOperator {
          require(_rate>0, 'prevent zero rate error happen');
        ratereferer = (_rate);
    }
    
    function addpool(uint256 _allocPoint, IBEP20 _lpToken, uint16 _depositFeeBP, bool _isStrat,uint _stratId,uint _stratFee, bool _withUpdate,address _cheft,IBEP20 _earn,bool _isMdex) public onlyOperator {
        if (_withUpdate) {
            massUpdatePools();
        }
        checkPoolDuplicate(_lpToken);
        uint256 lastRewardTime = block.timestamp > startTime ? block.timestamp : startTime;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolInfo.push(PoolInfo({
            lpToken: _lpToken,
            allocPoint: _allocPoint,
            lastRewardTime: lastRewardTime,
            accTokenPerShare: 0,
            depositFeeBP: _depositFeeBP,
            totalcap:0,
            isStrat:_isStrat,
            stratId : _stratId,
            stratFee:_stratFee,
            earned:0,
            accEarnPerShare:0,
            WL_master:IChef(_cheft),
            WL_earn:_earn,
            isMdex:_isMdex
       
        }));
          updateStakingPool();
    }

    function set(uint256 _pid, uint256 _allocPoint, uint16 _depositFeeBP, bool _withUpdate, uint256 _stratId, bool _isStrat,bool _isMdex) public onlyOperator {
        require(_depositFeeBP <= 400, "max 4%");
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 prevAllocPoint = poolInfo[_pid].allocPoint;
        poolInfo[_pid].depositFeeBP = _depositFeeBP;
        poolInfo[_pid].allocPoint = _allocPoint;
        poolInfo[_pid].stratId=_stratId;
        poolInfo[_pid].isStrat=_isStrat;
        poolInfo[_pid].isMdex=_isMdex;
      
        if (prevAllocPoint != _allocPoint) {
            totalAllocPoint = totalAllocPoint.sub(prevAllocPoint).add(_allocPoint);
             updateStakingPool();
        }
    }
    
    function pausepool(uint256 _pid, bool _status) public onlyOperator{
        poolStatus[_pid]=_status;
    }

    function updateStakingPool() internal {
        uint256 length = poolInfo.length;
        uint256 points = 0;
        for (uint256 pid = 1; pid < length; ++pid) {
            points = points.add(poolInfo[pid].allocPoint);
        }
        if (points != 0) {
            points = points.div(3);
            totalAllocPoint = totalAllocPoint.sub(poolInfo[0].allocPoint).add(points);
            poolInfo[0].allocPoint = points;
        }
    }
    
    // Return reward multiplier over the given _from to _to block.
    function getMultiplier(uint256 _from, uint256 _to) public view returns (uint256) {
        return _to.sub(_from).mul(BONUS_MULTIPLIER);
    }

    function pendingToken(uint256 _pid, address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accTokenPerShare = pool.accTokenPerShare;
        if (block.timestamp > pool.lastRewardTime && pool.totalcap != 0) {
            uint256 multiplier = getMultiplier(pool.lastRewardTime,block.timestamp);
            uint256 singReward = multiplier.mul(singPerSec).mul(pool.allocPoint).div(totalAllocPoint);
            accTokenPerShare = accTokenPerShare.add(singReward.mul(ACC_TOKEN_PRECISION).div(pool.totalcap));
        }
        uint256 pending= user.amount.mul(accTokenPerShare).div(ACC_TOKEN_PRECISION).sub(user.rewardDebt);
         uint256 commision=pending.mul(ratereferer).div(1000);
        pending=pending.sub(commision);
        return pending;
    }
    function pendingEarned(uint _pid,address _user) external view returns (uint256) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        
        uint256 accEarnPerShare=pool.accEarnPerShare;
        if(pool.totalcap!=0){
            uint256 earn=pool.WL_master.pendingCake(pool.stratId, address(this)).mul(100-rewardfee).div(100);
            // adjust earn to control user rewards
            accEarnPerShare=accEarnPerShare.add(earn.mul(ACC_TOKEN_PRECISION).div(pool.totalcap));
        }
        return user.amount.mul(accEarnPerShare).div(ACC_TOKEN_PRECISION).sub(user.earnedDebt);
    }
    function migratetoken(DCVCToken _token)public onlyOwner{
        token=_token;
    }

    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        }
    }

    function harvestAll(address _address) public {
        for (uint256 pid = 0; pid < poolInfo.length; ++pid) {
            if(userInfo[pid][address(msg.sender)].amount>0){
                deposit(pid, 0,_address);
            }
        }
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) public whenNotPaused validatePool(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        if (block.timestamp <= pool.lastRewardTime) {
            return;
        }
        if (pool.totalcap == 0 || pool.allocPoint == 0) {
            pool.lastRewardTime = block.timestamp;
            return;
        }
        uint256 multiplier = getMultiplier(pool.lastRewardTime,block.timestamp);
        uint256 singReward = multiplier.mul(singPerSec).mul(pool.allocPoint).div(totalAllocPoint);
        // token.mint(devaddr, singReward.div(10));
        // token.mint(address(this), singReward);
        pool.accTokenPerShare = pool.accTokenPerShare.add(singReward.mul(ACC_TOKEN_PRECISION).div(pool.totalcap));
        pool.lastRewardTime = block.timestamp;
    }
    function updateReward(uint _pid,uint _added) internal {
        PoolInfo storage pool = poolInfo[_pid];
        if(_added>0 && pool.totalcap>0){
            safeEarnTransfer(devaddr, _added.mul(rewardfee).div(100),_pid);
            uint added=_added.sub(_added.mul(rewardfee).div(100));
            pool.accEarnPerShare=pool.accEarnPerShare.add(added.mul(ACC_TOKEN_PRECISION).div(pool.totalcap));
        }
    }
  
    // To be tested in mainnet
    function buyToken(address token0,address token1,uint256 amount) public {
        address[] memory path = new address[](2);
        path[0] = address(token0);//WBNB
        path[1] = address(token1);
        uint wMaticbal=IBEP20(path[0]).balanceOf(address(this));
        if(wMaticbal>0){
            IBEP20(path[0]).approve(address(PancakeRouter), amount);
            PancakeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                wMaticbal,
                0,
                path,
                address(this),
                block.timestamp
            );
        }
    }
    

  function deposit(uint256 _pid, uint256 _amount,address _address) public payable whenNotPaused  validatePool(_pid) {
      require(poolStatus[_pid],'Pool havent started');
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(msg.value>=fee);
        address(uint160(technical)).transfer( msg.value);
        updatePool(_pid);
 
        if (user.amount > 0) {
            uint256 pending = user.amount.mul(pool.accTokenPerShare).div(ACC_TOKEN_PRECISION).sub(user.rewardDebt);
            if(pending > 0) {
                safeTokenTransfer(msg.sender, pending);
            }
        }
        if(_amount > 0) {
            pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount);
            if(pool.depositFeeBP > 0 || pool.stratFee>0){
                uint256 depositFee = _amount.mul(pool.depositFeeBP).div(10000);
                pool.lpToken.safeTransfer(feeAddress, depositFee);
                uint256 finalamt=_amount.sub(depositFee);
                uint256 stratFee=finalamt.mul(pool.stratFee).div(10000);//0 in spooky,PCS,ape,quick
                stratDeposit(_pid, finalamt);
                user.amount = user.amount.add(finalamt).sub(stratFee);
                pool.totalcap=pool.totalcap.add(finalamt).sub(stratFee);
            }else{
                stratDeposit(_pid, _amount);
                user.amount = user.amount.add(_amount);
                pool.totalcap=pool.totalcap.add(_amount);
            }
        }
        user.rewardDebt = user.amount.mul(pool.accTokenPerShare).div(ACC_TOKEN_PRECISION);
        user.earnedDebt=user.amount.mul(pool.accEarnPerShare).div(ACC_TOKEN_PRECISION);
        user.blockNumber = block.number;
        // syrup.mint(msg.sender, _amount);
        referralvault.recordReferral(msg.sender,_address);
        emit Deposit(msg.sender, _pid, _amount);
    }

  function depositzap(uint256 _pid, uint256 _amount,address _address,address _owner) external onlyOperator  whenNotPaused  validatePool(_pid) {
      require(poolStatus[_pid],'Pool havent started');
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_owner];
        updatePool(_pid);
        if (user.amount > 0) {
            uint256 pending = user.amount.mul(pool.accTokenPerShare).div(ACC_TOKEN_PRECISION).sub(user.rewardDebt);
            if(pending > 0) {
                safeTokenTransfer(_owner, pending);
            }
        }
        if(_amount > 0) {
            pool.lpToken.safeTransferFrom(address(msg.sender), address(this), _amount);
            if(pool.depositFeeBP > 0 || pool.stratFee>0){
                uint256 depositFee = _amount.mul(pool.depositFeeBP).div(10000);
                pool.lpToken.safeTransfer(feeAddress, depositFee);
                uint256 finalamt=_amount.sub(depositFee);
                uint256 stratFee=finalamt.mul(pool.stratFee).div(10000);//0 in spooky,PCS,ape,quick
                stratDeposit(_pid, finalamt);
                user.amount = user.amount.add(finalamt).sub(stratFee);
                pool.totalcap=pool.totalcap.add(finalamt).sub(stratFee);
            }else{
                stratDepositowner(_pid, _amount,_owner);
                user.amount = user.amount.add(_amount);
                pool.totalcap=pool.totalcap.add(_amount);
            }
        }
        user.rewardDebt = user.amount.mul(pool.accTokenPerShare).div(ACC_TOKEN_PRECISION);
        user.earnedDebt=user.amount.mul(pool.accEarnPerShare).div(ACC_TOKEN_PRECISION);
        user.blockNumber = block.number;
        // syrup.mint(msg.sender, _amount);
        referralvault.recordReferral(_owner,_address);
        emit Deposit(_owner, _pid, _amount);
    }

  function stratDepositowner(uint256 _pid, uint256 _amount,address _owner) internal {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_owner];
        if (pool.isStrat) {
            if (pool.lpToken.allowance(address(this), address(pool.WL_master)) < uint(-1)) {
                pool.lpToken.approve(address(pool.WL_master), uint(-1));
            }
            uint preharvest=pool.WL_earn.balanceOf(address(this));
            pool.WL_master.deposit(pool.stratId, _amount);
                      //  buyBanana();
            updateReward(_pid, pool.WL_earn.balanceOf(address(this)).sub(preharvest));
            if(user.amount>0){
                uint256 pending=user.amount.mul(pool.accEarnPerShare).div(ACC_TOKEN_PRECISION).sub(user.earnedDebt);
                if(pending>0){
                    uint256 commision=pending.mul(ratereferer).div(1000);
                    pending=pending.sub(commision);
                    address refer= referralvault.getReferrer(_owner);
                     referralrewardpool.recordReferralCommission(refer,address(pool.WL_earn),commision);
                    safeEarnTransfer(address(referralrewardpool), commision,_pid);
                    safeEarnTransfer(_owner, pending,_pid);
                }
            }
        }
    }
  
  function withdrawzap(uint256 _pid, uint256 _amount,address _owner) external onlyOperator nonReentrant validatePool(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_owner];
        require(user.amount >= _amount, "withdraw: not good");
        stratWithdrawowner(_pid, _amount,_owner);
        updatePool(_pid);
        uint256 pending = user.amount.mul(pool.accTokenPerShare).div(ACC_TOKEN_PRECISION).sub(user.rewardDebt);
        if(pending > 0) {
            safeTokenTransfer(_owner, pending);
        }
        if(_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.totalcap=pool.totalcap.sub(_amount);
            pool.lpToken.safeTransfer(address(msg.sender), _amount);
        }
        user.rewardDebt = user.amount.mul(pool.accTokenPerShare).div(ACC_TOKEN_PRECISION);
        user.earnedDebt=user.amount.mul(pool.accEarnPerShare).div(ACC_TOKEN_PRECISION);
        user.blockNumber = block.number;
        emit Withdraw(_owner, _pid, _amount);
    }
      
  function stratWithdrawowner(uint _pid,uint _amount,address _owner) internal onlyOperator {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_owner];
        if (pool.isStrat) {
            uint preharvest=pool.WL_earn.balanceOf(address(this));
            pool.WL_master.withdraw(pool.stratId, _amount);
          //  buyBanana();
            updateReward(_pid, pool.WL_earn.balanceOf(address(this)).sub(preharvest));
            if(user.amount>0){
                uint256 pending=user.amount.mul(pool.accEarnPerShare).div(ACC_TOKEN_PRECISION).sub(user.earnedDebt);
                if(pending>0){
                   uint256 commision=pending.mul(ratereferer).div(1000);
                    pending=pending.sub(commision);
                    address refer= referralvault.getReferrer(_owner);
                    referralrewardpool.recordReferralCommission(refer,address(pool.WL_earn),commision);
                    safeEarnTransfer(address(referralrewardpool), commision,_pid);
                    safeEarnTransfer(_owner, pending,_pid);
                }
            }
        }
    }
  

    function withdraw(uint256 _pid, uint256 _amount) public payable nonReentrant validatePool(_pid) {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(user.amount >= _amount, "withdraw: not good");
        require(msg.value>=fee);
        address(uint160(technical)).transfer( msg.value);
        stratWithdraw(_pid, _amount);
        updatePool(_pid);
        uint256 pending = user.amount.mul(pool.accTokenPerShare).div(ACC_TOKEN_PRECISION).sub(user.rewardDebt);
        if(pending > 0) {
            safeTokenTransfer(msg.sender, pending);
        }
        if(_amount > 0) {
            user.amount = user.amount.sub(_amount);
            pool.totalcap=pool.totalcap.sub(_amount);
            pool.lpToken.safeTransfer(address(msg.sender), _amount);
        }
        user.rewardDebt = user.amount.mul(pool.accTokenPerShare).div(ACC_TOKEN_PRECISION);
        user.earnedDebt=user.amount.mul(pool.accEarnPerShare).div(ACC_TOKEN_PRECISION);
        user.blockNumber = block.number;
        emit Withdraw(msg.sender, _pid, _amount);
    }
    function stratDeposit(uint256 _pid, uint256 _amount) internal {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        if (pool.isStrat) {
            if (pool.lpToken.allowance(address(this), address(pool.WL_master)) < uint(-1)) {
                pool.lpToken.approve(address(pool.WL_master), uint(-1));
            }
            uint preharvest=pool.WL_earn.balanceOf(address(this));
            pool.WL_master.deposit(pool.stratId, _amount);
                      //  buyBanana();
            updateReward(_pid, pool.WL_earn.balanceOf(address(this)).sub(preharvest));
            if(user.amount>0){
                uint256 pending=user.amount.mul(pool.accEarnPerShare).div(ACC_TOKEN_PRECISION).sub(user.earnedDebt);
                if(pending>0){
                    uint256 commision=pending.mul(ratereferer).div(1000);
                    pending=pending.sub(commision);
                    address refer= referralvault.getReferrer(msg.sender);
                     referralrewardpool.recordReferralCommission(refer,address(pool.WL_earn),commision);
                    safeEarnTransfer(address(referralrewardpool), commision,_pid);
                    safeEarnTransfer(msg.sender, pending,_pid);
                }
            }
        }
    }
    function stratWithdraw(uint _pid,uint _amount) internal {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        if (pool.isStrat) {
            uint preharvest=pool.WL_earn.balanceOf(address(this));
            pool.WL_master.withdraw(pool.stratId, _amount);
          //  buyBanana();
            updateReward(_pid, pool.WL_earn.balanceOf(address(this)).sub(preharvest));
            if(user.amount>0){
                uint256 pending=user.amount.mul(pool.accEarnPerShare).div(ACC_TOKEN_PRECISION).sub(user.earnedDebt);
                if(pending>0){
                   uint256 commision=pending.mul(ratereferer).div(1000);
                    pending=pending.sub(commision);
                    address refer= referralvault.getReferrer(msg.sender);
                    referralrewardpool.recordReferralCommission(refer,address(pool.WL_earn),commision);
                    safeEarnTransfer(address(referralrewardpool), commision,_pid);
                    safeEarnTransfer(msg.sender, pending,_pid);
                }
            }
        }
    }
    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) public nonReentrant validatePool(_pid){
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        uint256 amount = user.amount;
        if (pool.isStrat) {
            pool.WL_master.withdraw(pool.stratId, amount);
            //not harvested when withdrawing in apeswap.            
        }
        pool.totalcap=pool.totalcap.sub(amount);
        user.amount = 0;
        user.rewardDebt = 0;
        pool.lpToken.safeTransfer(address(msg.sender), amount);

        emit EmergencyWithdraw(msg.sender, _pid, amount);
    }

    function safeTokenTransfer(address _to, uint256 _amount) internal {
        require(_to != address(0),'Empty Address!');
        uint256 DCVCBal = token.balanceOf(address(syrup));
        if (_amount > DCVCBal) {
         //allowed for last withdraw 
         // temporary stop masterchef until furture action
          syrup.safeCakeTransfer(_to, DCVCBal); 
          endpool();
        } else {
           syrup.safeCakeTransfer(_to, _amount);
        }
    } 
    
     
    function safeEarnTransfer(address _to,uint256 _amount,uint256 _poolid) internal {
        require(_to != address(0),'Empty address!');
        uint256 earnBal=poolInfo[_poolid].WL_earn.balanceOf(address(this));
        if(_amount>earnBal){
            poolInfo[_poolid].WL_earn.transfer(_to, earnBal);
        }else{
            poolInfo[_poolid].WL_earn.transfer(_to, _amount);
        }
    }

