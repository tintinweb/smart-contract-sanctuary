pragma solidity 0.6.12;

contract Test {

    IUniswapV2Router02 router;
    IWBNB weth;

    constructor() public {
        router = IUniswapV2Router02(0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3);
        weth = IWBNB(router.WETH());
    }

    function swapTokensForEth(IBEP20 _token) external {
        address[] memory path = new address[](2);
        path[0] = address(_token);
        path[1] = router.WETH();

        uint256 balance = _token.balanceOf(address(this));

        _token.approve(address(router), balance);

        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            balance,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function removeLiquidity(IUniswapV2Pair _pair) external {
        uint256 balance = _pair.balanceOf(address(this));
        require(balance > 0, "balance error");

        _pair.approve(address(router), balance); 

        router.removeLiquidity(
            _pair.token0(),
            _pair.token1(),
            balance,
            0,
            0,
            address(this),
            block.timestamp
        );
    }

    function swapWethForEth() external {
        weth.withdraw(weth.balanceOf(address(this)));
    }

}