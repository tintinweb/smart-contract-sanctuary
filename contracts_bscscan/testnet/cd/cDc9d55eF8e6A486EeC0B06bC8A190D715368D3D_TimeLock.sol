pragma solidity ^0.8.4;

import "../Utils/Interfaces/IERC20.sol";
import "../Utils/Ownable.sol";
import "../Utils/SafeMath.sol";
import "hardhat/console.sol";

contract TimeLock is Ownable {
    using SafeMath for uint256;

    IERC20 token;

    uint256 public totalTokenLock;
    uint256 public lockContractTotal;
    uint256 public totalTokenClaim;

    struct LockContract {
        uint256 totalQty;
        string contractType;
        bool isVestingStart;
    }

    struct VestingScheduleStats {
        uint256 total;
        uint32 count;
    }

    struct VestingSchedule {
        uint256 qty;
        uint32 release_date;
        uint32 claim_date;
    }

    event ContracVestingStarted(address contractAdress);

    event LockContractAdded(
        address contractAdress,
        uint256 totalQty,
        string contractType
    );

    event LockContractDeleted(address contractAdress);

    event VestingScheduleAdded(
        address contracAddress,
        uint256 qty,
        uint32 release_date
    );

    event VestingScheduleDeleted(address contracAddress);

    event TokenClaim(address contracAddress, uint256 qty, uint32 claim_date);

    mapping(address => LockContract) public lockContract;
    mapping(uint256 => address ) public lockContractList;
    mapping(address => VestingSchedule[]) public vestingSchedule;
    mapping(address => VestingScheduleStats) public vestingScheduleStats;

    function setToken(IERC20 _token) public {
        token = _token;
    }

    function lockAddress(
        address _contract,
        uint256 _totalQty,
        string memory _contractType
    ) public onlyOwner {
        require(_totalQty > 0, "Must greater than zero");
        require(_contract != address(0), "Address zero detected");
        require(totalTokenLockBalance() > 0 , "Not enough token");
        require(lockContract[_contract].totalQty <= 0, "Duplicate contract not allowed" );
        
        totalTokenLock += _totalQty;
        lockContractTotal++;
        
        lockContractList[lockContractTotal] = _contract;

        lockContract[_contract] = LockContract(_totalQty, _contractType, false);

        emit LockContractAdded(_contract, _totalQty, _contractType);
    }

    function isOverQty(address _contract, uint256 _addedQty)
        public
        view
        returns (bool)
    {
        uint256 vestingTotal = vestingScheduleStats[_contract].total;
        uint256 lockContractTotalQty = lockContract[_contract].totalQty;
        return lockContractTotalQty < (vestingTotal + _addedQty);
    }

    function addVestingSchedule(
        address _contract,
        uint256 _qty,
        uint32 _releaseDate
    ) public validateLockContract(_contract) onlyOwner {
        require(_qty > 0, "Must greater than zero");
        require(!isOverQty(_contract, _qty), "Over qty.");

        vestingSchedule[_contract].push(VestingSchedule(_qty, _releaseDate, 0));

        vestingScheduleStats[_contract].total += _qty;
        vestingScheduleStats[_contract].count++;

        emit VestingScheduleAdded(_contract, _qty, _releaseDate);
    }

    function totalSupply() public view returns (uint256) {
        return token.balanceOf(address(this)).add(totalTokenClaim);
    }

    function totalSupplyBalance() public view returns (uint256) {
        return token.balanceOf(address(this));
    }

    function totalTokenLockBalance() public view returns (uint256) {
        return totalSupply().sub(totalTokenLock);
    }

    function deleteLockAddress(address _contract, uint256 index)
        public
        validateLockContract(_contract)
        onlyOwner
    {
     
        require(lockContractList[index] == _contract, "Record not match" );
        
        totalTokenLock -= lockContract[_contract].totalQty;
        lockContractTotal--;
        
        delete lockContractList[index];
        delete lockContract[_contract];
        delete vestingSchedule[_contract];

        emit VestingScheduleDeleted(_contract);
    }

    modifier validateLockContract(address _contract) {
        require(lockContract[_contract].totalQty > 0, "Contract not in locked");
        require(
            !lockContract[_contract].isVestingStart,
            "Action not permitted. Vesting already started."
        );
        require(_contract != address(0), "Address zero detected");
        _;
    }

    function deleteVestingSchedule(address _contract, uint32 index)
        public
        validateLockContract(_contract)
        onlyOwner
    {
        require(
            vestingSchedule[_contract][index].qty > 0,
            "Schedule not found"
        );
        require(index >= 0, "Must greater than zero");

        uint256 vestedQty = vestingSchedule[_contract][index].qty;

        vestingScheduleStats[_contract].total -= vestedQty;
        vestingScheduleStats[_contract].count--;

        delete vestingSchedule[_contract][index];

        emit VestingScheduleDeleted(_contract);
    }

    function claim(address _contract, uint32 index) public {
        require(lockContract[_contract].totalQty > 0, "Contract not in locked");
        require(lockContract[_contract].isVestingStart,"Vesting is not yet started");
        require(_contract != address(0), "Address zero detected");

        VestingSchedule storage vestingSched = vestingSchedule[_contract][
            index
        ];

        require(vestingSched.qty > 0, "Schedule not found");
        require(
            block.timestamp >= vestingSched.release_date,
            "Not yet claimable"
        );

        uint256 claim_qty = vestingSched.qty;
        uint32 current_date = uint32(block.timestamp);

        totalTokenClaim += claim_qty;

        vestingSched.claim_date = current_date;

        token.transfer(_contract, claim_qty);

        emit TokenClaim(_contract, claim_qty, current_date);
    }

    function startVesting(address _contract) public onlyOwner {
        LockContract storage lock = lockContract[_contract];

        require(lock.totalQty > 0, "Contract not in locked");

        lock.isVestingStart = true;

        emit ContracVestingStarted(_contract);
    }
}

