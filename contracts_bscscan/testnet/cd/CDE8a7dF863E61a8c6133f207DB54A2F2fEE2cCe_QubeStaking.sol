pragma solidity ^0.8.0;

contract QubeStaking is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.AddressSet;
    using SignatureChecker for address;

    // Info of each user.
    struct UserInfo {
        uint256 amount;  
        uint256 totalReward;
    }
    
    bytes32 public constant SIGNATURE_PERMIT_TYPEHASH = keccak256("withdrawVars(address user,address depositToken,address rewardToken,uint256 withdrawAmount,uint256 rewardAmount,uint256 deadline,)");

    EnumerableSet.AddressSet private poolInfo;
    // Info of each user that stakes LP tokens.
    mapping (address => mapping (address => UserInfo)) public userInfo;
    
    address private signerAddress;

    event Deposit(address indexed user, address indexed pool, uint256 amount);
    event Withdraw(address indexed user, address indexed pool, uint256 amount);
    event EmergencyWithdraw(address indexed user, address indexed pool, uint256 amount);

    constructor(address _signerAddress){
        signerAddress = _signerAddress;
    }

    function poolLength() external view returns (uint256) {
        return poolInfo.length();
    }

    function addLpToken(address lpToken) public onlyOwner {
        poolInfo.insert(lpToken);
    }
    
    function removeLpToken(address lpToken) public onlyOwner {
        poolInfo.remove(lpToken);
    }
    
    function contains(address lpToken) public view returns (bool) {
        return poolInfo.contains(lpToken);
    }
    
    function lpTokens() public view returns (address[] memory) {
        return poolInfo.values();
    }

    // Deposit LP tokens to MasterChef for Qube allocation.
    function deposit(address depositToken, uint256 _amount) public {
        require(poolInfo.contains(depositToken), "Invalid deposit token");
        UserInfo storage user = userInfo[depositToken][msg.sender];
        IERC20(depositToken).safeTransferFrom(address(msg.sender), address(this), _amount);
        user.amount = user.amount.add(_amount);
        emit Deposit(msg.sender, depositToken, _amount);
    }
    
    struct withdrawVars{
        address user;
        address depositToken;
        address rewardToken;
        uint256 withdrawAmount;
        uint256 rewardAmount;
        uint256 deadline;
        bytes signature;
    }

    // Withdraw LP tokens from MasterChef.
    function withdraw(withdrawVars memory store) public {
        require(store.user == _msgSender(), "Msg.sender is Invalid");
        require(poolInfo.contains(store.depositToken), "Invalid deposit token");
        require(block.timestamp < store.deadline, "Time Expired");
        require(signCheck(store), "Sign Error");
        UserInfo storage user = userInfo[store.depositToken][store.user];
        require(user.amount >= store.withdrawAmount, "withdraw: not good");
        user.amount = user.amount.sub(store.withdrawAmount);
        IERC20(store.depositToken).safeTransfer(store.user, store.withdrawAmount);
        user.totalReward = user.totalReward.add(store.rewardAmount);
        safeTokenTransfer(store.rewardToken,store.user,store.rewardAmount);
        emit Withdraw(store.user, store.depositToken, store.withdrawAmount);
    }
    
    function recover(bytes32 hash,uint8 v,bytes32 r,bytes32 s) internal pure returns (address) {
        return ecrecover(hash, v, r, s);
    }
      
    function toSignedMessageHash(bytes32 hash) internal pure returns (bytes32) {
        return keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", hash));
    }
    
    function signCheck(withdrawVars memory store) public view returns (bool) {
        bytes32 hash = keccak256(abi.encodePacked(SIGNATURE_PERMIT_TYPEHASH,
            store.user,
            store.depositToken,
            store.rewardToken,
            store.withdrawAmount,
            store.rewardAmount,
            store.deadline
        ));
        return signerAddress.isValidSignatureNow(toSignedMessageHash(hash),store.signature);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(address depositToken) public {
        UserInfo storage user = userInfo[depositToken][msg.sender];
        IERC20(depositToken).safeTransfer(address(msg.sender), user.amount);
        emit EmergencyWithdraw(msg.sender, depositToken, user.amount);
        user.amount = 0;
    }

    // Safe qube transfer function, just in case if rounding error causes pool to not have enough Qubes.
    function safeTokenTransfer(address _token,address _to, uint256 _amount) internal {
        uint256 Bal = IERC20(_token).balanceOf(address(this));
        if (_amount > Bal) {
            IERC20(_token).transfer(_to, Bal);
        } else {
            IERC20(_token).transfer(_to, _amount);
        }
    }

    function adminEmergency(address addr,address account,uint256 amount) public onlyOwner {
        if(addr == address(0)){
            payable(account).transfer(amount);
        }else {
            IERC20(addr).transfer(account,amount);
        }
    }
}