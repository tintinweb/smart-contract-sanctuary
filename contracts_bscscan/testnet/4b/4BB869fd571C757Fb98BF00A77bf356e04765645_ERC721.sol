    function stake(uint256 tokenId, address _stakingAddress) public {
        require(_isApprovedOrOwner(_msgSender(), tokenId), "ERC721: the only owner can stake the token.");
        require(_tokenStakings[tokenId] == address(0), "ERC721: the token has been already staked.");
        
        _tokenStakings[tokenId] = _stakingAddress;
        _tokenStakingBlockNumber[tokenId] = block.number;
    }
    
    function clearStake(uint256 tokenId) public {
        require(_tokenStakings[tokenId] == _msgSender(), "ERC721: the only staking owner can release the stake.");
        
        _tokenStakings[tokenId] = address(0);
        _tokenStakingBlockNumber[tokenId] = 0;
    }
    
    function getStakingBlockNumber(uint256 tokenId) public returns(uint256) {
        return _tokenStakingBlockNumber[tokenId];
    }
