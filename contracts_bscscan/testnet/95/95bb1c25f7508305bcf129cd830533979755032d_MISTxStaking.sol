    /**
     * @notice Mint NFT token
     * @param amountToStake amount of total tokens user staking and get NFT basis on that 
     */
    
    function mintNFTToken(uint amountToStake) internal {
        uint fee = amountToStake.mul(stakingFeeRate).div(1e4);
        uint amountAfterFee = amountToStake.sub(fee);
        totalStakedToken = totalStakedToken.add(amountAfterFee);
        uint userBalance = depositedTokens[msg.sender].add(amountAfterFee);
        if(userBalance > minSSGTForGov){
            uint amountOfToken = userBalance.div(minSSGTForGov);
            for(uint i = 0; i < amountOfToken; i++){
                nftToken.mintToken(msg.sender);
            }
            depositedTokens[msg.sender] = userBalance.sub(amountOfToken.mul(minSSGTForGov));
            userSSGTPeggedByNFT[msg.sender] = userSSGTPeggedByNFT[msg.sender].add(amountOfToken.mul(minSSGTForGov));
        }else{
            depositedTokens[msg.sender] = depositedTokens[msg.sender].add(amountAfterFee);
        }
    }

    /**
     * @notice Deposit
     * @notice A transfer is used to bring tokens into the staking contract so pre-approval is required
     * @param amountToStake amount of total tokens user staking and get NFT basis on that 
     */
    function deposit(uint amountToStake) public {
        require(amountToStake > 0, "Cannot deposit 0 Tokens");
        require(amountToStake <= MAX_SSGT_STAKE, "NFTContract:Hard stop to mint 197 NFTs"); //Because the NFT contract can only mint 197 NFTs in one transection, user can only stake MAX_SSGTSTAKE in one transection
        require(IERC20(mistTokenAddress).transferFrom(msg.sender, address(this), amountToStake), "Insufficient Token Allowance");

        // updateAccount(msg.sender);
        
        mintNFTToken(amountToStake);

        uint fee = amountToStake.mul(stakingFeeRate).div(1e4);
        require(IERC20(mistTokenAddress).transfer(owner, fee), "Could not transfer deposit fee.");

        if(!onlyHolder()){
            holders.push(msg.sender);
            stakingTime[msg.sender] = block.timestamp;
        }
    }
    
    /**
     * @notice withdraw
     * @dev Must be called directly by the user 
     * @param amountToWithdraw amount of total tokens user user wants to unstake
     */
    function withdraw(uint amountToWithdraw) public {
        require(depositedTokens[msg.sender] >= amountToWithdraw, "Invalid amount to withdraw");

        require(block.timestamp.sub(stakingTime[msg.sender]) > cliffTime, "You recently staked, please wait before withdrawing.");

        // updateAccount(msg.sender);

        uint fee = amountToWithdraw.mul(unstakingFeeRate).div(1e4);
        uint amountAfterFee = amountToWithdraw.sub(fee);

        require(IERC20(mistTokenAddress).transfer(owner, fee), "Could not transfer withdraw fee.");
        require(IERC20(mistTokenAddress).transfer(msg.sender, amountAfterFee), "Could not transfer tokens.");

        depositedTokens[msg.sender] = depositedTokens[msg.sender].sub(amountToWithdraw);

        if(onlyHolder() && depositedTokens[msg.sender] == 0){
            for(uint i= 0; i< holders.length; i++){
            if(holders[i] == msg.sender){
                delete holders[i];
            }
        }
        }
    }
    
    /**
     * @notice Withdraw with NFT
     * @dev Must be called directly by the user 
     * @param _tokenId token id of NFT which user holds to withdraw amount
     */
    function withdrawWithNFT(uint _tokenId) public {
        require(block.timestamp.sub(stakingTime[msg.sender]) > cliffTime, "You recently staked, please wait before withdrawing.");
        
        nftToken.removeTokenIdFromUser(_tokenId);
        nftToken.safeTransferFrom(msg.sender, address(this), _tokenId);
        nftToken.burnNFT(_tokenId);

        // updateAccount(msg.sender);

        uint fee = minSSGTForGov.mul(unstakingFeeRate).div(1e4);
        uint amountAfterFee = minSSGTForGov.sub(fee);

        require(IERC20(mistTokenAddress).transfer(owner, fee), "Could not transfer withdraw fee.");
        require(IERC20(mistTokenAddress).transfer(msg.sender, amountAfterFee), "Could not transfer tokens.");
        userSSGTPeggedByNFT[msg.sender] = userSSGTPeggedByNFT[msg.sender].sub(minSSGTForGov);
    }

    /**
     * @notice unstake invested amount
     * @dev Must be called directly by the user 
     * @param _tokenId token id of NFT which user holds to withdraw amount,
     * @param _amount amount of deposited token
     * @param _type it indicates either only amount or only NFT or both
     */
    function unstake(uint _amount, uint[] memory _tokenId, uint _type) public{
        require(_type < 3, "Invalid type");
        if(_type == 0){
            withdraw(_amount);
        }else if(_type == 1){
            for(uint i = 0; i < _tokenId.length; i++){
                withdrawWithNFT(_tokenId[i]);
            }
        }else if(_type == 2){
            withdraw(_amount);
            for(uint i = 0; i < _tokenId.length; i++){
                withdrawWithNFT(_tokenId[i]);
            }
        }
    }
    
