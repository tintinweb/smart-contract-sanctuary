contract MistGovernance is ERC721Enumerable, Ownable{

    uint public tokenId;
    
    using Strings for uint256;
    
    string public baseURI;
    
    struct User{
        address userAddress;
        uint[] tokenId;
    }
    
    mapping(address => User) public userToken;
    
    constructor() ERC721("SSGTx Governance", "SSGTx Governance") {
    }    
    
    address private minter;
    
    /**
     * @notice Set minter role to mint and burn NFT token call by only owner
     * @param _minter account address of the minter
     */
    function setMinterRole(address _minter) public onlyOwner{
        minter = _minter;
    }
    
    /**
     * @notice To get minter address
     * @return minter account address
     */
    function getMinterAddress() public view returns(address){
        return minter;
    }
    
    function setBaseURI(string memory _newBaseURI) public onlyOwner{
        baseURI = _newBaseURI;
    }

    
     /**
     * @notice Mint Token - only minted by minter
     * @param _toAddress account address of the user 
     */
    function mintToken(address _toAddress) public {
        require(msg.sender == minter, "Sender does not have minter role");
        _mint(_toAddress, tokenId);
        userToken[_toAddress].userAddress = _toAddress;
        userToken[_toAddress].tokenId.push(tokenId);
        tokenId++;
    }
    
    /**
     * @notice Get token list owned by user
     * @param _userAddress account address of the user 
     * @return array of token
     */
    function getTokenList(address _userAddress) public view returns(uint[] memory){
        return userToken[_userAddress].tokenId;
    }
    
    /**
     * @notice Burn NFT token call by only token owner
     * @param _tokenId of the user 
     */
    function burnNFT(uint256 _tokenId) public {
        require(ownerOf(_tokenId) == msg.sender);
        _burn(_tokenId);
    }
    
     /**
     * @notice remove tokenId from user address
     * @param _tokenId of the user 
     */
    function removeTokenIdFromUser(uint256 _tokenId) public {
        require(msg.sender == minter, "Sender does not have minter role");
        for(uint i = 0; i < userToken[ownerOf(_tokenId)].tokenId.length; i++){
            if(_tokenId == userToken[ownerOf(_tokenId)].tokenId[i]){
                for (uint j = i; j < userToken[ownerOf(_tokenId)].tokenId.length - 1; j++) {
                    userToken[ownerOf(_tokenId)].tokenId[j] = userToken[ownerOf(_tokenId)].tokenId[j + 1];
                }
                userToken[ownerOf(_tokenId)].tokenId.pop();
            }
        }
    }
    
     /**
     * @notice Bulk Burn NFT token call by only token owner 
     * @param _tokenId array of tokenIds of the user 
     */
    function burnInBulk(uint256[] memory _tokenId) public{
        for(uint i = 0; i < _tokenId.length; i++){
            require(ownerOf(_tokenId[i]) == msg.sender);
            _burn(_tokenId[i]);
        }
    }
    
    
    function tokenOwnedByUser(address _owner) public view returns (uint256[] memory){
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i = 0; i < ownerTokenCount; i++) {
          tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokenIds;
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
          _exists(_tokenId),
          "ERC721Metadata: URI query for nonexistent token"
        );
    
        return bytes(baseURI).length > 0
            ? string(abi.encodePacked(baseURI, _tokenId.toString()))
            : "";
    }

}


