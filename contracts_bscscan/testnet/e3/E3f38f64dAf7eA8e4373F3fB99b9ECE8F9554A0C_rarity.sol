        require(1 <= _class && _class <= 11);
        require(summonPrice <= msg.value, "Please pay mint fee");
        uint256 _next_summoner = next_summoner;
        class[_next_summoner] = _class;
        level[_next_summoner] = 1;
        _safeMint(msg.sender, _next_summoner);
