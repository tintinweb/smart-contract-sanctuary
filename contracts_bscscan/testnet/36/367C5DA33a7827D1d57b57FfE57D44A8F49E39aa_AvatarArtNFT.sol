pragma solidity 0.8.9;

import "./ERC721.sol";

contract AvatarArtNFT is ERC721{
    modifier onlyAdmin{
        require(_admins[_msgSender()], "Forbidden");
        _;
    }

    mapping(address => bool) public _admins;

    constructor() ERC721("AvatarArtNFT", "AvatarArtNFT"){}
    
    function mint(address to) public onlyAdmin returns(uint256){
        return _safeMint(to);
    }

    function setAdmin(address addressAddress, bool value) public onlyOwner{
        _admins[addressAddress] = value;
        emit AdminSetted(addressAddress, value);
    }

    function _baseURI() internal override view virtual returns (string memory) {
        return "https://statics.avatarart.io/nft/avatar-art/";
    }

    event AdminSetted(address addressAddress, bool value);
}

