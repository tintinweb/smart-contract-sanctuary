     * @notice Gets the token ids and names
     * @param _owner Address to retrieve tokens for
     * @return Array of token ids belonging to this address
     * @return Array of token names belonging to this address
     */
    function getWalletOfOwner(address _owner) public view returns (uint256[] memory, string[] memory) {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        string[] memory names = new string[](ownerTokenCount);
        for (uint256 i; i < ownerTokenCount; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
            names[i] = getTokenNameByIndex(tokenIds[i]);
        }
        return (tokenIds, names);
    }
    
    /**
     * @dev Gets a tokens name
     * 
     * @param _index Index of token to retrieve name
     * @return Name of the Nibbler
     */
    function getTokenNameByIndex(uint256 _index) public view returns (string memory) {
        return tokenName[_index];
    }

    /**
     * @dev Sets the max amount of tokens that an address can hold 
     * 
     * @param _limit The limit to set
     */
    function setNftPerAddressLimit(uint256 _limit) public onlyOwner {
        nftPerAddressLimit = _limit;
    }

    /**
     * @dev Sets the max amount of tokens that can be minted in a single call
     * 
     * @param _newmaxMintAmount The limit to set
     */
    function setmaxMintAmount(uint256 _newmaxMintAmount) public onlyOwner {
        maxMintAmount = _newmaxMintAmount;
    }

    /**
     * @dev Updates the base url containing Nibbler images and metadata
     * 
     * @param _newBaseURI The new base url to set
     */
    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
    }
  
    /**
     * @dev Set the address of the community wallet
     * 
     * @param _newAddress The new address to set
     */
    function setCommunityAddress(address _newAddress) public onlyOwner {
        communityAddress = _newAddress;
    }
  
    /**
     * @dev Set the base extension for files containing token metadata
     * 
     * @param _newBaseExtension The new extension to set
     */
    function setBaseExtension(string memory _newBaseExtension) public onlyOwner {
        baseExtension = _newBaseExtension;
    }
  
    /**
     * @dev Set the cost for changing the name of a Nibbler
     * 
     * @param _newPrice The new price to set
     */
    function setNameChangePrice(uint256 _newPrice) public onlyOwner {
        nameChangePrice = _newPrice;
    }

    /**
     * @dev Sets the paused status of the smart contract
     * 
     * @notice Set to true to disble minting
     * @param _state Whether to pause or not
     */
    function setPauseStatus(bool _state) public onlyOwner {
        paused = _state;
    }
