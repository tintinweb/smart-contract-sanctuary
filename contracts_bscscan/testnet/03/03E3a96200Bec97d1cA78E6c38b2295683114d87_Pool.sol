contract Pool is Ownable, Constants, ReentrancyGuard, Initializable, IPool {
    using SafeERC20 for IERC20;
    using Set for Set.AddressSet;

    /* ========== ADDRESSES ================ */
     address public oracleBusdParissqm; // return how much ParisSQM you can buy for 1 BUSD (ParrisSQM/BUSD)

    address public collateral;
    address public dollar;
    address public treasury;
    IERC20 public share;

    /* ========== STATE VARIABLES ========== */

    mapping(address => uint256) public redeem_share_balances;
    mapping(address => uint256) public redeem_collateral_balances;

    uint256 public override unclaimed_pool_collateral;
    uint256 public override unclaimed_pool_share;

    mapping(address => uint256) public last_redeemed;

    // Constants for various precisions
    uint256 private constant COLLATERAL_RATIO_PRECISION = 1e6;
    uint256 private constant COLLATERAL_RATIO_MAX = 1e6;

    /**
     * @dev Maximum percent of share in BUSD that user can give while minting.
     *
     * For more details see `setMaxShareValuePercentOnMint`.
     */
    uint256 private share_value_max = 300000;

    /**
     * @dev Number of decimals needed to get to 18
     */
    uint256 private missing_decimals;

    /**
     * @dev Number of blocks to wait before being able to `collectRedemption()`.
     */
    uint256 public redemption_delay = 1;

    // AccessControl state variables
    bool public mint_paused = false;
    bool public redeem_paused = false;

    // Excluded from dex fee
    Set.AddressSet private _excludedFromRedeemFee;

    /* ========== MODIFIERS ========== */

    /**
     * @dev Reverts if `msg.sender` is not pre-specified Treasury smart contract.
     */
    modifier onlyTreasury() {
        require(msg.sender == treasury, "!treasury");
        _;
    }

    /* ========== CONSTRUCTOR ========== */

    /**
     * @dev Initializes Dollar, Share, Collateral, Treasury addresses.
     * @param _dollar Address of REI token.
     * @param _share Address of LTY.
     * @param _collateral Address of BUSD.
     * @param _treasury Address of Treasury contract.
     */
    function initialize(
        address _dollar,
        IERC20 _share,
        address _collateral,
        address _treasury
    ) external initializer onlyOwner {
        dollar = _dollar;
        share = _share;
        collateral = _collateral;
        treasury = _treasury;
        missing_decimals = 18 - ERC20(_collateral).decimals();
    }

    /* ========== VIEWS ========== */

    /**
     * @dev Returns unclaimed amount of collateral, unclaimed amount of share, collateral price and two bools: is mint paused, is redeem paused.
     */
    function info()
        external
        view
        returns (
            uint256,
            uint256,
            bool,
            bool
        )
    {
        return (
            unclaimed_pool_collateral, // unclaimed amount of COLLATERAL
            unclaimed_pool_share, // unclaimed amount of SHARE
            mint_paused,
            redeem_paused
        );
    }

    /**
     * @dev Returns address of CollateralReserve smart contract.
     */
    function collateralReserve() public view returns (address) {
        return ITreasury(treasury).collateralReserve();
    }

    /**
     * Returns collateral price obtained by oracle.
     */
    function getPrice(address _oracle) public view override returns (uint256) {
        return IOracle(_oracle).consult();
    }

    /* ========== PUBLIC FUNCTIONS ========== */

    /**
     * @dev Creates the number of tokens depending on the incoming tokens.
     * It takes the _minting_fee commission.
     * @param _collateral_amount Number of transferred BUSD tokens.
     * @param _share_amount Number of transferred LTY tokens.
     * @param _dollar_out_min Minimum number of tokens created.
     */
    function mint(
        uint256 _collateral_amount,
        uint256 _share_amount,
        uint256 _dollar_out_min
    ) external {
        require(mint_paused == false, "Minting is paused");
        (uint256 _price_share, uint256 _tcr, uint256 _minting_fee) = ITreasury(treasury).infoForMinting(); // _price_share - LTY/BUSD
        (uint256 excessShare, uint256 excessCollateral, uint256 _total_dollar_value) = calculateProportion(_collateral_amount, _share_amount, _price_share, _tcr);

        uint256 _actual_dollar_amount = _total_dollar_value - ((_total_dollar_value * _minting_fee) / RATIO_PRECISION);
        require(_dollar_out_min <= _actual_dollar_amount, "slippage");

        if (_share_amount > 0) {
            _transferShareToReserve(msg.sender, _share_amount, excessShare);
        }
        if (_collateral_amount > 0) {
            _transferCollateralToReserve(msg.sender, _collateral_amount, excessCollateral);
        }
        IDollar(dollar).poolMint(msg.sender, _actual_dollar_amount);
    }

    function calculateProportion(uint256 _collateral_amount, uint256 _share_amount, uint256 _price_share, uint256 _tcr) view internal returns(uint256, uint256, uint256) {
        require(_price_share > 0, "Invalid share price");
        uint256 _price_ParisSQM_BUSD = getPrice(oracleBusdParissqm);

        uint256 collateral_value = (_collateral_amount * (10**missing_decimals)); // BUSD
        uint256 share_value = (_share_amount * _price_share) / PRICE_PRECISION;  // LTY in BUSD
        uint256 inputValue = collateral_value + share_value;
        uint256 expectedShareValue = (inputValue * _tcr) / RATIO_PRECISION;  // expected Share Value

        require(share_value * RATIO_PRECISION / inputValue <= share_value_max, "Pool: share value exceeds its maximum");

        uint256 excessShare;
        uint256 excessCollateral;
        if(expectedShareValue != share_value) {
            if(expectedShareValue < share_value) {
                uint256 excessValue = share_value - expectedShareValue;
                excessShare = ((excessValue * PRICE_PRECISION) / _price_share); // Amount of excess LTY
            } else if(expectedShareValue > share_value) {
                uint256 expectedCollateralValue = inputValue - expectedShareValue;
                excessCollateral = (collateral_value - expectedCollateralValue)/(10**missing_decimals);
            }
        }
        uint256 _total_dollar_value = (inputValue * _price_ParisSQM_BUSD) / PRICE_PRECISION;
        return (excessShare, excessCollateral, _total_dollar_value);
    }

    /**
     * @dev Burns REI tokens and reserves LTY and BUSD tokens for redeem from the contract.
     * @param _dollar_amount Number of transmitted REI tokens.
     * @param _share_out_min The minimum number of LTY tokens reserved for withdrawal.
     * @param _collateral_out_min The minimum number of BUSD tokens reserved for withdrawal.
     */
    function redeem(
        uint256 _dollar_amount,
        uint256 _share_out_min,
        uint256 _collateral_out_min
    ) external {
        require(redeem_paused == false, "Redeeming is paused");
        (uint256 _share_price, uint256 _ecr, uint256 _redemption_fee) = ITreasury(treasury).infoForRedeeming();
        require(_share_price > 0, "Invalid share price");
        uint256 _dollar_amount_post_fee = _dollar_amount - ((_dollar_amount * _redemption_fee) / RATIO_PRECISION);
        uint256 _collateral_output_amount = 0;
        uint256 _share_output_amount = 0;

        uint256 _price_ParisSQM_BUSD = getPrice(oracleBusdParissqm);

        // priceBusdUsd                        // BUSD/USD price
        //     * PRICE_PRECISION / priceEurUsd        // BUSD/EUR price
        //     * PRICE_PRECISION / priceEurParissqm;  // BUSD/PARISSQM price

        if (_ecr < COLLATERAL_RATIO_MAX) {
            uint256 _share_output_value_in_dollar = _dollar_amount_post_fee - ((_dollar_amount_post_fee * _ecr) / RATIO_PRECISION);
            _share_output_amount = _share_output_value_in_dollar // REI
                * PRICE_PRECISION / _price_ParisSQM_BUSD // BUSD
                * PRICE_PRECISION / _share_price; // LTY
        }

        uint256 _collateral_output_value = ((_dollar_amount_post_fee * _ecr) / RATIO_PRECISION) / (10**missing_decimals);
        _collateral_output_amount = (_collateral_output_value * PRICE_PRECISION) / _price_ParisSQM_BUSD;

        // Check if collateral balance meets and meet output expectation
        uint256 _totalCollateralBalance = ITreasury(treasury).globalCollateralBalance();
        if (!isExcludedFromRedeemFee(msg.sender)) {
            _collateral_output_amount = getTax(_collateral_output_amount);
            _share_output_amount = getTax(_share_output_amount);
        }

        require(_collateral_output_amount <= _totalCollateralBalance, "<collateralBalance");
        require(_collateral_out_min <= _collateral_output_amount && _share_out_min <= _share_output_amount, ">slippage");

        if (_collateral_output_amount > 0) {
            redeem_collateral_balances[msg.sender] = redeem_collateral_balances[msg.sender] + _collateral_output_amount;
            unclaimed_pool_collateral = unclaimed_pool_collateral + _collateral_output_amount;
        }

        if (_share_output_amount > 0) {
            redeem_share_balances[msg.sender] = redeem_share_balances[msg.sender] + _share_output_amount;
            unclaimed_pool_share = unclaimed_pool_share + _share_output_amount;
        }

        last_redeemed[msg.sender] = block.number;

        // Move all external functions to the end
        IDollar(dollar).poolBurnFrom(msg.sender, _dollar_amount);
    }

    /**
     * @dev Withdrawal of tokens reserved for redeem.
     */
    function collectRedemption() external {
        require((last_redeemed[msg.sender] + redemption_delay) <= block.number, "<redemption_delay");

        if (redeem_share_balances[msg.sender] > 0) {
            uint256 _share_amount = redeem_share_balances[msg.sender];
            redeem_share_balances[msg.sender] = 0;
            unclaimed_pool_share = unclaimed_pool_share - _share_amount;
            _requestTransferShare(msg.sender, _share_amount);
        }

        if (redeem_collateral_balances[msg.sender] > 0) {
            uint256 _collateral_amount = redeem_collateral_balances[msg.sender];
            redeem_collateral_balances[msg.sender] = 0;
            unclaimed_pool_collateral = unclaimed_pool_collateral - _collateral_amount;
            _requestTransferCollateral(msg.sender, _collateral_amount);
        }
    }

    /* ========== INTERNAL FUNCTIONS ========== */

    /**
     * @dev Transfers collateral to reserve.
     * @param _sender Address of sender.
     * @param _amount Amount of collateral to transfer.
     */
    function _transferCollateralToReserve(address _sender, uint256 _amount, uint256 _amountOfExcess) internal { // TODO: chenge Add method to RESERVE swapBeforeMint(token, amount)...
        address _reserve = collateralReserve();
        require(_reserve != address(0), "Invalid reserve address");
        IERC20(collateral).safeTransferFrom(_sender, _reserve, _amount);
        if(_amountOfExcess > 0) {
        ITreasury(treasury).requestSwapBeforeMint(collateral, address(share), _amountOfExcess); // Swap in reserve excess tokens
        }
    }

    /**
     * @dev Transfers share to reserve.
     * @param _sender Address of sender.
     * @param _amount Amount of share to transfer.
     */
    function _transferShareToReserve(address _sender, uint256 _amount, uint256 _amountOfExcess) internal { // TODO: chenge Add method to RESERVE swapBeforeMint(token, amount)...
        address _reserve = collateralReserve();
        require(_reserve != address(0), "Invalid reserve address");
        share.safeTransferFrom(_sender, _reserve, _amount);
        if(_amountOfExcess > 0) {
            ITreasury(treasury).requestSwapBeforeMint(address(share), collateral, _amountOfExcess); // Swap in reserve excess tokens
        }
    }

    /**
     * @dev Transfers `_amount` collateral to `_receiver`.
     * @param _receiver Address of receiver account.
     * @param _amount Amount of collateral to transfer.
     */
    function _requestTransferCollateral(address _receiver, uint256 _amount) internal {
        ITreasury(treasury).requestTransfer(collateral, _receiver, _amount);
    }

    /**
     * @dev Transfers `_amount` shares to `_receiver`.
     * @param _receiver Address of receiver account.
     * @param _amount Amount of shares to transfer.
     */
    function _requestTransferShare(address _receiver, uint256 _amount) internal {
        ITreasury(treasury).requestTransfer(address(share), _receiver, _amount);
    }

    /**
     * @dev Returns amount of tokens which will be transferred after the removal of the commissions.
     * @param _tokenAmount Token amount.
     * @return Token amount after the removal of the commissions.
     */
    function getTax(uint256 _tokenAmount) internal view returns(uint256) {
        uint256 _globalReserveValue = ITreasury(treasury).globalCollateralValue() + ITreasury(treasury).globalShareValue();
        uint256 _globalDollarValue = ITreasury(treasury).globalDollarValue();
        uint256 gap = _globalReserveValue * 100 / _globalDollarValue;
        console.log('GAP -',gap);
        uint256 tax = 0;
        if(gap <= 80) {
            tax = 25;
        } else if(gap <= 90) {
            tax = 20;
        } else if(gap <= 95) {
            tax = 10;
        }
        if(_tokenAmount > 0 && tax > 0)
            _tokenAmount = _tokenAmount - ((_tokenAmount * tax)/100);
        return(_tokenAmount);
    }

    /* ========== RESTRICTED FUNCTIONS ========== */

    /**
     * @dev Pauses/unpauses minting. Can be called only by the owner.
     */
    function toggleMinting() external onlyOwner {
        mint_paused = !mint_paused;
    }

    /**
     * @dev Pauses/unpauses redeeming. Can be called only by the owner.
     */
    function toggleRedeeming() external onlyOwner {
        redeem_paused = !redeem_paused;
    }

    /**
     * @dev Sets Oracle smart contract address. Can be called only by the owner.
     * @param _oracle Address of Oracle smart contract.
     *
     * Reverts if `_oracle` is zero address.
     */

    /**
     * @dev Sets maximum share value percent.
     * Mint will be reverting if share in BUSD is more than `share_value_percent`%.
     *
     * Let's say `share_value_max` = 300000 (30%).
     * And user tries to mint REI by giving 700 BUSD and 300 LTY. Then transaction will succeed.
     * But, if the user will try to give 700 BUSD and 301 LTY then the transaction will be reverted.
     *
     * @param _share_value_max Maximum percent of share in BUSD that user can give while minting.  (6 decimals)
     */
    function setMaxShareValuePercentOnMint(uint256 _share_value_max) external onlyOwner {
        require(_share_value_max <= 1e6, "Pool: maximum share value percent exceeds 100%");
        share_value_max = _share_value_max;
    }

    /**
     * @dev Sets Oracle smart contract address. Can be called only by the owner.
     * @param _oracle Address of Oracle smart contract.
     *
     * Reverts if `_oracle` is zero address.
     */
    function setBusdParissqmOracle(address _oracle) external onlyOwner {
        require(_oracle != address(0), "Invalid address");
        oracleBusdParissqm = _oracle;
    }

    /**
     * @dev Sets new redemption delay value. Can be called only by the owner.
     * @param _redemption_delay New redemption delay value.
     */
    function setRedemptionDelay(uint256 _redemption_delay) external onlyOwner {
        redemption_delay = _redemption_delay;
    }

    /**
     * @dev Sets Treasury smart contract's address. Can be called only by the owner.
     * @param _treasury Address of Treasury smart contract.
     *
     * Reverts if `_treasury` is zero address.
     * Emits `TreasuryChanged` event.
     */
    function setTreasury(address _treasury) external onlyOwner {
        require(_treasury != address(0), "Invalid address");
        treasury = _treasury;
        emit TreasuryChanged(_treasury);
    }

    /**
     * @dev Excludes/not excludes `account` from redeem fee depending on boolean `isExcluded` value. Can be called only by the owner.
     * @param account Account address.
     * @param isExcluded `true` to exclude from redeem fee, `false` to not exclude from redeem fee.
     */
    function setIsExcludedFromRedeemFee(address account, bool isExcluded) public onlyOwner {
        if (isExcluded) {
            _excludedFromRedeemFee.add(account);
        } else {
            _excludedFromRedeemFee.remove(account);
        }
    }

    /**
     * @dev Returns `true` if `account` is excluded from redeem fee, otherwise returns `false`.
     */
    function isExcludedFromRedeemFee(address account) public view returns (bool) {
        return _excludedFromRedeemFee.has(account);
    }

    // EVENTS

    /**
     * @dev Triggered when new Treasury smart contract address has been set up.
     * @param newTreasury New address of Treasury smart contract.
     */
    event TreasuryChanged(address indexed newTreasury);
}

