contract ELPDistribute is Ownable, Pausable, ReentrancyGuard {
  using SafeBEP20 for IBEP20;
  using SafeMath for uint256;

    //tNature
    IBEP20 inTokenId;

    //$Nature
    IBEP20 outTokenId;

    uint32 startTime = safe32(now);
     // Mapping from user account to user deposits
    mapping(address => Deposit) public deposits;

    mapping(address => bool) public hasDeposit;

    uint256 totalTNatureDeposits = 0;
    uint256 totalNatureDeposits = 0;


    struct Deposit {
        uint256 totalDeposit;
        uint256 amountDue;      // Amount due, in "repay" token units
        uint32 lastWithdrawTime;// Time of the most recent interim withdrawal
    }

    constructor(address _inTokenId, address _outTokenId) public{
        inTokenId = IBEP20(_inTokenId);
        outTokenId = IBEP20(_outTokenId);
    }

    function deposit() public nonReentrant {

        Deposit storage userDeposit = deposits[msg.sender];
        uint256 userBal = inTokenId.balanceOf(msg.sender);
        require(totalNatureDeposits >= totalTNatureDeposits.add(userBal), "ELPDistribute: Please deposit more Nature");
        require(userBal > 0 , "ELPDistribute: User has no tokens");
        uint256 amountDue = userBal;
        inTokenId.safeTransferFrom(msg.sender, address(this), userBal);
        userDeposit.amountDue = userDeposit.amountDue.add(amountDue);
        userDeposit.totalDeposit = userDeposit.totalDeposit.add(amountDue);
        totalTNatureDeposits = totalTNatureDeposits.add(userBal);
        if(hasDeposit[msg.sender] == false){
            userDeposit.lastWithdrawTime = startTime;
            hasDeposit[msg.sender] = true;
        }

    }

    function depositNature(uint256 _amount) public nonReentrant onlyOwner{
        uint256 userBal = outTokenId.balanceOf(msg.sender);
        require(userBal > 0 , "ELPDistribute: User has no tokens");
        require(_amount <= userBal, "ELPDistribute: User has not enough tokens");
        outTokenId.safeTransferFrom(msg.sender, address(this), _amount);
        totalNatureDeposits = totalNatureDeposits.add(_amount);
    }



    //withdraw entitled tokens during Locking Period
    function withdraw() public nonReentrant{
        require(hasDeposit[msg.sender] == true, "ELPDistribute: User has no deposit");
        Deposit storage userDeposit = deposits[msg.sender];
        uint256 amountToWithdraw = 0;
        uint256 amountBeforePenalty = 0;
        (amountToWithdraw,amountBeforePenalty) = checkEntitledNature(msg.sender);
        
        if(userDeposit.amountDue >= amountBeforePenalty){
            userDeposit.amountDue = userDeposit.amountDue.sub(amountBeforePenalty);
        }else{
            amountToWithdraw = userDeposit.amountDue;
            userDeposit.amountDue = 0;
            hasDeposit[msg.sender] = false;
        }
        userDeposit.lastWithdrawTime = safe32(now);
        outTokenId.transfer(msg.sender, amountToWithdraw);
    }

    function withdrawExcessNature() public nonReentrant onlyOwner{
        uint256 contractBalance = outTokenId.balanceOf(address(this));
        uint256 actualExcess = contractBalance.sub(totalNatureDeposits);
        outTokenId.transfer(msg.sender, actualExcess);
    }


    function checkEntitledNature(address _address) public view  returns(uint256,uint256)  {
        require(hasDeposit[_address] == true, "ELPDistribute: User has no deposit");
        Deposit storage userDeposit = deposits[_address];
        uint256 amountToWithdraw = 0;

        //30.5 days = 2635200
        //4hours = 14400 Testing
        uint32 currentMonth =  safe32((now.sub(startTime)).div(14400));

         // check the no of month of between current time and last withdrawal
        uint32 lastWithdrawMonthFromCurrentMonth = safe32((now.sub(userDeposit.lastWithdrawTime)).div(14400));

        // check the no of month of between current time and last withdrawal
        uint32 lastWithdrawMonthFromStartTime = safe32(uint256(userDeposit.lastWithdrawTime).sub(startTime).div(14400));

        // check if no withdrawal done since startTime;
        if(userDeposit.lastWithdrawTime == startTime){
            amountToWithdraw = amountToWithdraw.add(userDeposit.totalDeposit.div(50));
        }

       
        //check if the lastWithdraw month 
        if(currentMonth >= 1  && lastWithdrawMonthFromStartTime <1){
            amountToWithdraw = amountToWithdraw.add((userDeposit.totalDeposit.div(50)));
        }
            uint32 month = 0;

        if(currentMonth >= 2 && lastWithdrawMonthFromStartTime <8){
            
            if(currentMonth >= uint32(8)){
                month = uint32(uint256(8).sub(uint256(lastWithdrawMonthFromStartTime)));
            }else{
                month = lastWithdrawMonthFromCurrentMonth;
            }

            if(lastWithdrawMonthFromStartTime < 1){
                month = month - 1;
            }

            amountToWithdraw = amountToWithdraw.add((userDeposit.totalDeposit.div(25)).mul(month));
        }

        if(currentMonth >= 9 && lastWithdrawMonthFromStartTime <14){
            if(currentMonth > uint32(14)){
                month = uint32(uint256(14).sub(uint256(lastWithdrawMonthFromStartTime)));
            }else{
                month = lastWithdrawMonthFromCurrentMonth;
            }

            if(lastWithdrawMonthFromStartTime < 8){
                month = month - (8 - lastWithdrawMonthFromStartTime);
            }
            amountToWithdraw = amountToWithdraw.add((userDeposit.totalDeposit.mul(3).div(50)).mul(month));
        }

        if(currentMonth >= 15 && lastWithdrawMonthFromStartTime <18){
            if(currentMonth > uint32(18)){
                month = uint32(uint256(18).sub(uint256(lastWithdrawMonthFromStartTime)));
            }else{
                month = lastWithdrawMonthFromCurrentMonth;
            }

            if(lastWithdrawMonthFromStartTime < 14){        
                month = month - (14 - lastWithdrawMonthFromStartTime);
            }
            amountToWithdraw = amountToWithdraw.add((userDeposit.totalDeposit.mul(2).div(25)).mul(month));
        }

        uint256 amountBeforePenalty = amountToWithdraw;

        //Penalty
        if(currentMonth <= 6){
            amountToWithdraw = amountToWithdraw.div(2);
            amountToWithdraw = amountToWithdraw.div(2);
        } else if(currentMonth <= 12 && currentMonth > 6 ){
            amountToWithdraw = amountToWithdraw.mul(3).div(4);
            amountToWithdraw = amountToWithdraw.mul(3).div(4);
        }
        return (amountToWithdraw,amountBeforePenalty);
    }
    


    function safe32(uint256 n) private pure returns (uint32) {
        require(n < 2**32, "ELPDistribute:UNSAFE_UINT32");
        return uint32(n);
    }






