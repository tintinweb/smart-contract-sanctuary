    event SetPaused(bool _paused);
    event Stake(address _token, uint256 _pid, uint256 _stake_amount, uint256 _balance);
    event Exit(address _token, uint256 _pid, uint256 _nftID, uint256 _stake_amount, uint256 _balance);

    constructor() public {
    }

    function initialize(IERC721 _nftToken) external onlyFactoryOrOwner{
        NFTToken = _nftToken;
        emit Initialize(_nftToken);
    }

    function pool_length(address _token) external view returns (uint256) {
        return pools[_token].length;
    }

    function get_pool_configure(address _token, uint256 _pid) public view returns 
        (uint256 start_time, uint256 end_time, uint256 lock_duration, uint256 stake_amount, 
        uint256 max_user_count, uint256 attend_user_count, uint256 claimed_user_count) 
    {
        start_time = pools[_token][_pid].start_time;
        end_time = pools[_token][_pid].end_time;
        lock_duration = pools[_token][_pid].lock_duration;
        stake_amount = pools[_token][_pid].stake_amount;
        max_user_count = pools[_token][_pid].max_user_count;
        attend_user_count = pools[_token][_pid].attend_user_count;
        claimed_user_count = pools[_token][_pid].claimed_user_count;
    }

    function get_pool_user_info(address _token, uint256 _pid, address _account) public view returns 
        (uint256 start_ts, uint256 unlock_ts, uint256 stake_amount, bool claimed)
    {
        start_ts = pools[_token][_pid].start_ts[_account];
        unlock_ts = pools[_token][_pid].unlock_ts[_account];
        stake_amount = pools[_token][_pid].balances[_account];
        claimed = pools[_token][_pid].claimed[_account];
    }

    function stake(address _token, uint256 _pid) public checkNotContract(msg.sender) checkPaused checkStart(_token, _pid) checkNotEnd(_token, _pid){
        require(pools[_token][_pid].balances[msg.sender] == 0, "SupeNFTStakingPool: already staked");
        require(pools[_token][_pid].max_user_count > pools[_token][_pid].attend_user_count, "SupeNFTStakingPool: not enough seats");
        uint256 before_token_amount = IERC20(_token).balanceOf(address(this));
        IERC20(_token).safeTransferFrom(msg.sender, address(this), pools[_token][_pid].stake_amount);
        uint256 after_token_amount = IERC20(_token).balanceOf(address(this));
        pools[_token][_pid].balances[msg.sender] = after_token_amount.sub(before_token_amount);//pools[_token][_pid].stake_amount;
        pools[_token][_pid].start_ts[msg.sender] = block.timestamp;
        pools[_token][_pid].unlock_ts[msg.sender] = pools[_token][_pid].start_ts[msg.sender].add(pools[_token][_pid].lock_duration);
        pools[_token][_pid].attend_user_count = pools[_token][_pid].attend_user_count.add(1);
        emit Stake(_token, _pid, pools[_token][_pid].stake_amount, pools[_token][_pid].balances[msg.sender]);
    }

    function exit(address _token, uint256 _pid) public checkNotContract(msg.sender) checkPaused{
        require(pools[_token][_pid].balances[msg.sender] > 0, "SupeNFTStakingPool: not staked");
        require(!pools[_token][_pid].claimed[msg.sender], "SupeNFTStakingPool: already claimed");
        require(block.timestamp > pools[_token][_pid].unlock_ts[msg.sender], "SupeNFTStakingPool: not match unlock");
        uint256 nftID;
        uint256 balance = pools[_token][_pid].balances[msg.sender];
        IERC20(_token).safeTransfer(msg.sender, balance);
        nftID = pools[_token][_pid].reward_nft_preid.add(start_id);
        pools[_token][_pid].claimed[msg.sender] = true;
        pools[_token][_pid].claimed_user_count = pools[_token][_pid].claimed_user_count.add(1);
        NFTToken.mintNFT(nftID, msg.sender);
        start_id = start_id.add(1);
        emit Exit(_token, _pid, nftID, pools[_token][_pid].stake_amount, pools[_token][_pid].balances[msg.sender]);
    }

    function pool_add(address _token, uint256 start_time, uint256 end_time, uint256 lock_duration, uint256 stake_amount, uint256 max_user_count, uint256 reward_nft_preid) external onlyFactoryOrOwner{
        pools[_token].push(PoolInfo({
            start_time:         start_time,
            end_time:           end_time,
            lock_duration:      lock_duration,
            stake_amount:       stake_amount,
            max_user_count:     max_user_count,
            reward_nft_preid:   reward_nft_preid,
            attend_user_count:  0,
            claimed_user_count: 0
        }));
    }

    function pool_update(address _token, uint256 _pid, uint256 start_time, uint256 end_time, uint256 lock_duration, uint256 stake_amount, uint256 max_user_count, uint256 reward_nft_preid) external onlyFactoryOrOwner{
        pools[_token][_pid] = PoolInfo({
            start_time:         start_time,
            end_time:           end_time,
            lock_duration:      lock_duration,
            stake_amount:       stake_amount,
            max_user_count:     max_user_count,
            reward_nft_preid:   reward_nft_preid,
            attend_user_count:  pools[_token][_pid].attend_user_count,
            claimed_user_count: pools[_token][_pid].claimed_user_count
        });
    }

    function withdrawToken(address token) external onlyOwner {
        IERC20(token).safeTransfer(msg.sender, IERC20(token).balanceOf(address(this)));
    }
    function withdrawETH() external onlyOwner{
        _safeTransferETH(msg.sender, address(this).balance);
    }
    function _safeTransferETH(address to, uint value) internal {
        (bool success,) = to.call.value(value)(new bytes(0));
        require(success, 'SupeNFTStakingPool Transfer: ETH_TRANSFER_FAILED');
    }
    function _isContract(address account) internal view returns (bool) {
        bytes32 codehash;
        bytes32 accountHash = 0xc5d2460186f7233c927e7db2dcc703c0e500b653ca82273b7bfad8045d85a470;
        // solhint-disable-next-line no-inline-assembly
        assembly { codehash := extcodehash(account) }
        return (codehash != 0x0 && codehash != accountHash);
    }

    function set_paused(bool _paused) external onlyOwner {
        paused = _paused;
        emit SetPaused(_paused);
    }

    modifier checkNotContract(address _account) {
        require(!_isContract(_account), "SupeNFTStakingPool: is contract");
        _;
    }

    modifier checkPaused() {
        require(!paused, "SupeNFTStakingPool: Pool is paused");
        _;
    }

    modifier checkStart(address _token, uint256 _pid) {
        require(pools[_token][_pid].start_time != 0 && block.timestamp > pools[_token][_pid].start_time, "SupeNFTStakingPool: Not Started");
        _;
    }

    modifier checkNotEnd(address _token, uint256 _pid) {
        require(pools[_token][_pid].end_time != 0 && block.timestamp <= pools[_token][_pid].end_time, "SupeNFTStakingPool: Ended");
        _;
    }

    modifier checkEnd(address _token, uint256 _pid) {
        require(pools[_token][_pid].end_time != 0 && block.timestamp > pools[_token][_pid].end_time, "SupeNFTStakingPool: Not End");
        _;
    }
