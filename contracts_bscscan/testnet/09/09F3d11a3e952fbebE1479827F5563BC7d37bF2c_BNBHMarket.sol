pragma solidity ^0.8.0;

import "./IBNBHCharacter.sol";
import "./IERC721Receiver.sol";
import "./SafeMath.sol";
import "./EnumerableSet.sol";
import "./IBEP20.sol";
import "./Ownable.sol";
import "./Hero.sol";

contract BNBHMarket is IERC721Receiver, Ownable {
    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;

    IBEP20 public bnbhToken;
    IBNBHCharacter public characters;
    uint256 public taxFee = 10;
    address public taxFeeRecepient = 0xb39169b41A582f3aa609c8D95d13E9BDa14DCB13;
    string public name = "BNBHMarket";
    mapping(address => mapping(uint256 => uint256)) lastPriceChangedTimes;

    event NewListing(address seller, uint256 tokenId, uint256 price);
    event ListingPriceChange(address seller, uint256 tokenId, uint256 price);
    event CancelledListing(address seller, uint256 tokenId);
    event PurchaseListing(address buyer, address seller, uint256 id, uint256 price);
    constructor () {

    }

    struct Listing {
        address seller;
        uint256 price;
        uint256 listingTime;
    }

    mapping(uint256 => Listing) private listings;

    EnumerableSet.UintSet private listedTokenIDs;

    modifier isListed(uint256 id) {
        require(listedTokenIDs.contains(id),
            "Token ID not listed"
        );
        _;
    }

    modifier isNotListed(uint256 id) {
        require(!listedTokenIDs.contains(id),
            "Token ID must not be listed"
        );
        _;
    }

    modifier isSeller(uint256 id) {
        require(
            listings[id].seller == msg.sender,
            "Access denied"
        );
        _;
    }

    
    struct MarketHero {
        uint name;
        uint heroType;
        uint256 xp;       
        uint256 attack;
        uint256 armor;
        uint256 speed;     
        uint256 hp;
        uint256 tokenId;
        address seller;
        uint256 price;
        uint256 level;
    }

    function setTokenAddress(address token) public onlyOwner {
        bnbhToken = IBEP20(token);
    }
    function setCharacterAddress(address character) public onlyOwner {
        characters = IBNBHCharacter(character);
    }

    function setTaxFee(uint256 fee) public onlyOwner {
        taxFee = fee;
    }
    function setTaxFeeRecepient(address account) public onlyOwner {
        taxFeeRecepient = account;
    }
    // 0: oldest 1: newest 2: price asc 3: price desc
    function sortIDs(uint8 order) public view returns(uint256[] memory) {
        uint256[] memory ids = new uint256[](listedTokenIDs.length());
        for(uint256 i = 0; i < listedTokenIDs.length(); i++) {
            if (order != 1) {
                ids[i] = listedTokenIDs.at(i); 
            } else {
                ids[listedTokenIDs.length() - i - 1] = listedTokenIDs.at(i); 
            }
           
        }
        if (order > 1) {
            for(uint256 i = 0; i < ids.length - 1; i++) {
                for(uint256 j = i + 1; j < ids.length; j++) {
                    if (order == 2) {
                        if (listings[ids[i]].price > listings[ids[j]].price) {
                            uint256 temp = ids[i];
                            ids[i] = ids[j];
                            ids[j] = temp;
                        }
                    } else {
                        if (listings[ids[i]].price < listings[ids[j]].price) {
                            uint256 temp = ids[i];
                            ids[i] = ids[j];
                            ids[j] = temp;
                        } 
                    }
                    
                }
            }
        }
        return ids;
    }

    function getTokenIdsByPage(uint256[] memory sortedIds, uint256 pageEnd, uint256 _limit, uint256 tokensSize, uint256 _minLevel, uint256 _maxLevel, uint256 rarity) internal view returns(uint256[] memory){
        uint256[] memory tokens = new uint256[](tokensSize);
        uint256 counter = 0;
        uint8 tokenIterator = 0;
        for (uint256 i = 0; i < sortedIds.length && counter < pageEnd; i++) {
            uint256 characterLevel = characters.getLevel(sortedIds[i]);
            if(rarity == 0) {
                if (characterLevel >= _minLevel && characterLevel <= _maxLevel){
                    if(counter >= pageEnd - _limit) {
                        tokens[tokenIterator] = sortedIds[i];
                        tokenIterator++;
                    }
                    counter++;
                }
            } else {
                Hero memory hero = characters.getHero(sortedIds[i], false);
                if (hero.heroType == rarity && characterLevel >= _minLevel && characterLevel <= _maxLevel){
                    if(counter >= pageEnd - _limit) {
                        tokens[tokenIterator] = sortedIds[i];
                        tokenIterator++;
                    }
                    counter++;
                }
            }
        }
        return tokens;
    }
    function getCharacterListingIDsPage(uint256 _limit, uint256 _pageNumber, uint256 _minLevel, uint256 _maxLevel, uint256 rarity, uint8 order)
        public
        view
        returns (uint256[] memory)
    {
        uint256 matchingCharactersAmount = getNumberOfCharacterListings(_minLevel, _maxLevel, rarity);
        uint256 pageEnd = _limit * (_pageNumber + 1);
        uint256 tokensSize = matchingCharactersAmount >= pageEnd ? _limit : matchingCharactersAmount.sub(_limit * _pageNumber);  
        
        uint256[] memory sortedIds = sortIDs(order);

        return getTokenIdsByPage(sortedIds, pageEnd, _limit, tokensSize, _minLevel, _maxLevel, rarity);
    }

    function getCharactersForPage(uint256 _limit, uint256 _pageNumber, uint256 _minLevel, uint256 _maxLevel, uint256 rarity, uint8 order) public view returns (MarketHero[] memory) {
        uint256[] memory tokenIds = getCharacterListingIDsPage(_limit, _pageNumber, _minLevel, _maxLevel, rarity, order);
        return getCharacterDataByIds(tokenIds);
    }

    function getCharactersForSeller(address seller) public view returns (MarketHero[] memory) {
        uint256[] memory tokenIds = getListingIDsBySeller(seller);
        return getCharacterDataByIds(tokenIds);
    }
    function getCharacterDataByIds(uint256[] memory tokenIds) public view returns (MarketHero[] memory){
        MarketHero[] memory marketHeroes = new MarketHero[](tokenIds.length);
        for (uint256 index = 0; index < tokenIds.length; index ++) {
            Hero memory hero = characters.getHero(tokenIds[index], false);
            Listing memory listing = listings[tokenIds[index]];
            hero.hp = characters.calcHpPointsInMarket(tokenIds[index], block.timestamp - listing.listingTime);
            marketHeroes[index] = MarketHero(hero.name, hero.heroType, hero.xp, hero.attack, hero.armor, hero.speed, hero.hp, hero.tokenId, listings[tokenIds[index]].seller, listings[tokenIds[index]].price, hero.level);
        }
        return marketHeroes;
    }

    function getSellerOfNftID(uint256 _tokenId) public view returns (address) {
        if(!listedTokenIDs.contains(_tokenId)) {
            return address(0);
        }

        return listings[_tokenId].seller;
    }

    function getNumberOfListingsBySeller(
        address _seller
    ) public view returns (uint256) {

        uint256 amount = 0;
        for (uint256 i = 0; i < listedTokenIDs.length(); i++) {
            if (
                listings[listedTokenIDs.at(i)].seller == _seller
            ) amount++;
        }

        return amount;
    }

    function getListingIDsBySeller(address _seller)
        public
        view
        returns (uint256[] memory tokens)
    {
        uint256 amount = getNumberOfListingsBySeller(_seller);
        tokens = new uint256[](amount);
        uint256 index = 0;
        for (uint256 i = 0; i < listedTokenIDs.length(); i++) {
            uint256 id = listedTokenIDs.at(i);
            if (listings[id].seller == _seller)
                tokens[index++] = id;
        }
    }

    function getNumberOfCharacterListings(uint256 _minLevel, uint256 _maxLevel, uint256 rarity)
        public
        view
        returns (uint256)
    {
        uint256 counter = 0;
        uint256 characterLevel;
        for(uint256 i = 0; i < listedTokenIDs.length(); i++) {
            characterLevel = characters.getLevel(listedTokenIDs.at(i));       
            if(rarity == 0) {
                if (characterLevel >= _minLevel && characterLevel <= _maxLevel){
                    counter++;
                }
            } else {
                Hero memory hero = characters.getHero(listedTokenIDs.at(i), false);
                if (hero.heroType == rarity && characterLevel >= _minLevel && characterLevel <= _maxLevel){
                    counter++;
                }
            }
        }
        return counter;
    }
    function canChangePrice(address account, uint256 _heroId) public view returns(bool) {
        return block.timestamp >= lastPriceChangedTimes[account][_heroId] + 24* 3600;
    }
    function addListing(
        uint256 _id,
        uint256 _price
    )
        public
        isNotListed(_id)
    {
        // require (characters.balanceOf(address(this)) <= 10000, "can not list over 10k heroes");
        Hero memory hero = characters.getHero(_id, true);
        require(hero.arrivalTime <= 0 , "Hero did not arrive yet");
        uint256 balance = bnbhToken.balanceOf(msg.sender);
        require(_price > 0);
        require(balance >= _price.mul(taxFee).div(100), "Balance is not enough");
        characters.safeTransferFrom(msg.sender, address(this), _id);
        
        listings[_id] = Listing(msg.sender, _price, block.timestamp);
        listedTokenIDs.add(_id);
        lastPriceChangedTimes[msg.sender][_id] = block.timestamp;
        bnbhToken.transferFrom(msg.sender, taxFeeRecepient, _price.mul(10).div(100));        
        
        emit NewListing(msg.sender, _id, _price);
    }

    function changeListingPrice(
        uint256 _id,
        uint256 _newPrice
    )
        public
        isListed(_id)
        isSeller(_id)
    {
        require(block.timestamp >= lastPriceChangedTimes[msg.sender][_id] + 24 * 3600, "You can only change price every 24 hours");
        listings[_id].price = _newPrice;
        lastPriceChangedTimes[msg.sender][_id] = block.timestamp;
        emit ListingPriceChange(
            msg.sender,
            _id,
            _newPrice
        );
    }

    function cancelListing(uint256 _id)
        public
        isListed(_id)
        isSeller(_id)
    {
        Listing memory listing = listings[_id];
        characters.resumeStaminaTimeStamp(_id, block.timestamp - listing.listingTime);   
        characters.safeTransferFrom(address(this), msg.sender, _id);        
        delete listings[_id];
        listedTokenIDs.remove(_id);
        emit CancelledListing(msg.sender, _id);
    }

    function purchaseListing(uint256 _id) public isListed(_id) {
        Listing memory listing = listings[_id];
        require (bnbhToken.balanceOf(msg.sender) >= listing.price, "balance is not enough");     
        characters.resumeStaminaTimeStamp(_id, block.timestamp - listing.listingTime);   
        characters.safeTransferFrom(address(this), msg.sender, _id);
        bnbhToken.transferFrom(msg.sender, listing.seller, listing.price);
        delete listings[_id];
        listedTokenIDs.remove(_id);
        emit PurchaseListing(msg.sender, listing.seller, _id, listing.price);
    }

    function onERC721Received(
        address,
        address,
        uint256 tokenId,
        bytes calldata
    ) external override view returns (bytes4) {
        require(listedTokenIDs.contains(tokenId) == false,
            "Token ID should be not listed"
        ); 
        return this.onERC721Received.selector;
    }
}

