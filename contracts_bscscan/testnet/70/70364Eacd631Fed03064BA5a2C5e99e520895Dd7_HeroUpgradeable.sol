pragma solidity ^0.8.0;
import "../upgradeability/BaseUpgradeableNFT.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/ECDSAUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract HeroUpgradeable is BaseUpgradeableNFT{
  using SafeMath for uint256;
  using SafeERC20 for IERC20;
  using CountersUpgradeable for CountersUpgradeable.Counter;

  string private constant SIGNING_DOMAIN = "Hero-Voucher";
  string private constant SIGNATURE_VERSION = "1";

  CountersUpgradeable.Counter private _tokenIds;
  address public erc20Add;

  struct BoxInfo {
    uint256 erc20Price;
    uint256 altPrice;
    string boxURI;
  }

  struct HeroVoucher {
    address redeemer;
    uint256 minPrice;
    string uri;
    string nonce;
    bytes signature;
  }

  mapping(address => uint256[]) public allTokenIds;
  mapping(address => mapping(string => bool)) private _usedNonce;
  mapping(uint => BoxInfo) public boxs;

  event Mint(address indexed from, uint256 tokenId, string tokenURI, uint64 timestamp);
  event Redeem(address indexed from, uint256 tokenId, string nonce, uint64 timestamp);
  event SetNewURI(address indexed user, uint256 tokenId, string newTokenURI, uint64 timestamp);

  constructor() BaseUpgradeableNFT() {

  }

  function initializeNFT(
    address _storage,
    address _olymAdd
  ) public initializer {
    BaseUpgradeableNFT.initialize(
      "The Olympus",
      "OLY",
      SIGNING_DOMAIN,
      SIGNATURE_VERSION,
      _storage,
      _olymAdd,
      1 hours);

    erc20Add = _olymAdd;
  }

   function setBox(uint typeBox, uint256 erc20PriceBox, uint256 altPriceBox, string memory boxUri) external onlyGovernance {
    BoxInfo memory newBox = BoxInfo({erc20Price:erc20PriceBox,altPrice:altPriceBox, boxURI:boxUri});
    boxs[typeBox] = newBox;
  }

  function boxERC20Price(uint typeBox) public view returns (uint256){
    return boxs[typeBox].erc20Price;
  }

  function boxAltPrice(uint typeBox) public view returns (uint256){
    return boxs[typeBox].altPrice;
  }

  function mint(uint typeBox, uint256 amount) payable public {
    require((amount == boxs[typeBox].erc20Price)
            || (msg.value == boxs[typeBox].altPrice)
            , "HR: Not the right box price");
    if(amount > 0 ){
      IERC20(erc20Add).transferFrom(_msgSender(),address(this),amount);
    }

    _tokenIds.increment();
    uint256 newItemId = _tokenIds.current();
    _mint(_msgSender(),newItemId);

    _setTokenURI(newItemId, boxs[typeBox].boxURI);
    string memory tokenUri = tokenURI(newItemId);

    emit Mint(_msgSender(),newItemId,tokenUri, uint64(block.timestamp));
  }

  function redeem(HeroVoucher calldata voucher) public {
    require(!_usedNonce[_msgSender()][voucher.nonce], "HR: Nonce has used");
    _usedNonce[_msgSender()][voucher.nonce] = true;
    // make sure signature is valid and get the address of the signer
    address signer = _verify(voucher);
    // make sure that the signer is authorized to mint NFTs
    require(signer == governance(), "HR: Signature invalid or unauthorized");
    // record payment to signer's withdrawal balance
    IERC20(erc20Add).transferFrom(_msgSender(),address(this),voucher.minPrice);
    // first assign the token to the signer, to establish provenance on-chain
    _tokenIds.increment();
    uint256 tokenId = _tokenIds.current();

    _mint(signer, tokenId);
    _setTokenURI(tokenId, voucher.uri);

    // transfer the token to the redeemer
    _transfer(signer, _msgSender(), tokenId);

    emit Redeem(_msgSender(),tokenId, voucher.nonce, uint64(block.timestamp));
  }

  function getCurrentId() public view returns(uint256){
    return _tokenIds.current();
  }

  function getAllTokenURIs(address user) public view returns(uint256[] memory){
    return allTokenIds[user];
  }

  function setTokenURI(uint256 tokenId, string memory tokenURI,string memory nonce, bytes memory signature) public {
    require(!_usedNonce[_msgSender()][nonce],"HR: Nonce has used");
    _usedNonce[_msgSender()][nonce] = true;
    bytes32 rawMessage = keccak256(abi.encodePacked(_msgSender(),tokenId, tokenURI, nonce, address(this)));
    bytes memory s = abi.encodePacked(rawMessage);
    bytes32 message = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n",StringsUpgradeable.toString(s.length),s));
    require(isValidAccessMessage(message,signature),"HR: Signature invalid");

    _setTokenURI(tokenId,tokenURI);
    emit SetNewURI(_msgSender(),tokenId,tokenURI, uint64(block.timestamp));
  }

  function _beforeTokenTransfer(address from, address to, uint256 tokenId) internal virtual override {
    super._beforeTokenTransfer(from, to, tokenId);
    if(from != address(0) ){
      uint256 balanceUser = balanceOf(from);
      removeToken(from,balanceUser, tokenId);
    }
    allTokenIds[to].push(tokenId);
  }

  function removeToken(address user, uint256 balanceUser, uint256 tokenId) internal  {
    uint256[] storage tokenIdUser = allTokenIds[user];
    for(uint i = 0; i < balanceUser; i++){
      uint256 id = tokenIdUser[i];
      if(id == tokenId){
        tokenIdUser[i] = tokenIdUser[balanceUser - 1];
        tokenIdUser.pop();
        break;
      }
    }
  }

  function splitSignature(bytes memory sig) internal pure returns(uint8 v, bytes32 r, bytes32 s) {
      require(sig.length == 65, "Signature isn't validate");

      assembly {
          // first 32 bytes, after the length prefix.
          r := mload(add(sig, 32))
          // second 32 bytes.
          s := mload(add(sig, 64))
          // final byte (first byte of the next 32 bytes).
          v := byte(0, mload(add(sig, 96)))
      }
      return (v,r,s);
  }

  function isValidAccessMessage(bytes32 message, bytes memory signature) internal view returns (bool){
      (uint8 v, bytes32 r, bytes32 s) = splitSignature(signature);
      return ecrecover(message,v,r,s) == governance();
  }

  function _verify(HeroVoucher calldata voucher) internal view returns (address) {
    bytes32 digest = _hash(voucher);
    return ECDSAUpgradeable.recover(digest, voucher.signature);
  }

  function _hash(HeroVoucher calldata voucher) internal view returns (bytes32) {
    return _hashTypedDataV4(keccak256(abi.encode(
      keccak256("HeroVoucher(address redeemer,string nonce,uint256 minPrice,string uri)"),
      _msgSender(),
      keccak256(bytes(voucher.nonce)),
      voucher.minPrice,
      keccak256(bytes(voucher.uri))
    )));
  }

}

// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "../../proxy/inheritance/GovernableInit.sol";
import "./BaseUpgradeableNFTStorage.sol";
import "hardhat/console.sol";

contract BaseUpgradeableNFT is Initializable, GovernableInit, BaseUpgradeableNFTStorage,EIP712Upgradeable, ERC721URIStorageUpgradeable {
  using SafeMath for uint256;
  using SafeERC20 for IERC20;

  constructor() BaseUpgradeableNFTStorage() {

  }

  function initialize(
    string memory name,
    string memory symbol,
    string memory signingDomain,
    string memory signatureVersion,
    address _storage,
    address _erc20token,
    uint256 _implementationChangeDelay
    ) public onlyInitializing {
    __ERC721_init(name,symbol);
    __ERC721URIStorage_init();
    __EIP712_init(signingDomain,signatureVersion);
    GovernableInit.initialize(_storage);
    _setERC20Token(_erc20token);
    _setNextImplementationDelay(_implementationChangeDelay);
  }

  function scheduleUpgrade(address impl) public onlyGovernance {
    _setNextImplementation(impl);
    _setNextImplementationTimestamp(block.timestamp.add(nextImplementationDelay()));
  }

  function finalizeUpgrade() external onlyGovernance {
    _setNextImplementation(address(0));
    _setNextImplementationTimestamp(0);
  }

  function shouldUpgrade() external view returns (bool, address) {
    return (
      nextImplementationTimestamp() != 0
        && block.timestamp > nextImplementationTimestamp()
        && nextImplementation() != address(0),
      nextImplementation()
    );
  }
}

