pragma solidity ^0.8.2;










// import "./PentasNFT.sol";

contract GuinFarmingBank is
    Initializable,
    ContextUpgradeable,
    UUPSUpgradeable,
    OwnableUpgradeable,
    IERC721ReceiverUpgradeable
{
    using SafeMath for uint256;

    //===============VARIABLES=====================//

    struct UserInfo {
        address userAddress;
        uint256 totalStaked;
        uint256 lastClaimedTime;
        uint256 deployTime;
        uint256[] stakedNfts;
    }

    struct LockInfo {
        uint256 lockStart;
        uint256 lockEnd;
    }

    address public guinToken;
    address public guinInfo;
    address public _pentasNFT;

    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");
    bytes32 public constant OWNER_ROLE = keccak256("OWNER_ROLE");

    // PentasNFT private _pentasNFT;
    uint256 public dayRate;
    uint256 public lockPeriod;
    uint256 public totalNftStaked;

    mapping(address => UserInfo) public userInfo;
    mapping(uint256 => LockInfo) public lockInfo;
    mapping(uint256 => bool) public hasLocked;

    //======================INIT=====================//

    function initialize() public initializer {
        __Ownable_init();
        // _pentasNFT = 0x947b4167FfCA71d5F8cea4ecFE8ed6f7d7D33398;
        // for (uint256 i = 0; i < operators.length; i++) {
        //     _setupRole(OPERATOR_ROLE, operators[i]);
        // }
        // _setupRole(OWNER_ROLE, owner);
        // _setupRole(DEFAULT_ADMIN_ROLE, owner);
    }

    // constructor() initializer {}

    function _authorizeUpgrade(address) internal override onlyOwner {}

    //======================MODIFIERS=====================//

    // modifier onlyOwnerOrOperator() {
    //     require(
    //         hasRole(OPERATOR_ROLE, _msgSender()) ||
    //             hasRole(OWNER_ROLE, _msgSender()),
    //         "Only team is allowed!"
    //     );
    //     _;
    // }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes memory
    ) public virtual override returns (bytes4) {
        return this.onERC721Received.selector;
    }

    //======================ADMIN FUNCTIONS=====================//

    function setPentasNFT(address pentasNFT) public onlyOwner {
        _pentasNFT = pentasNFT;
    }

    function setLockPeriod(uint256 _days) public onlyOwner {
        lockPeriod = _days;
    }

    function setGuinToken(address _guinToken) public onlyOwner {
        guinToken = _guinToken;
    }

    function setGuinInfo(address _guinInfo) public onlyOwner {
        guinInfo = _guinInfo;
    }

    function setDayRate(uint256 _rate) public onlyOwner {
        dayRate = _rate * 10**18;
    }

    //======================PUBLIC FUNCTIONS=====================//

    function stake(uint256 _tokenId) public {
        uint256 rarity = IGuinInfo(guinInfo).getRarityInt(_tokenId);
        require(rarity > 0, "Not a valid GuinGang NFT!");
        require(
            IERC721(_pentasNFT).ownerOf(_tokenId) == _msgSender(),
            "Token not owned by sender!"
        );

        LockInfo storage _lockInfo = lockInfo[_tokenId];
        require(_lockInfo.lockStart == 0, "NFT already staked!");

        UserInfo storage _info = userInfo[_msgSender()];

        if (_info.totalStaked == 0) {
            _info.userAddress = _msgSender();
            _info.deployTime = block.timestamp;
            _info.lastClaimedTime = block.timestamp;
        } else {
            _harvestPendingRewards(_msgSender());
        }
        _info.totalStaked++;
        _info.stakedNfts.push(_tokenId);

        if (hasLocked[_tokenId] == false) {
            _lockInfo.lockStart = block.timestamp;
            _lockInfo.lockEnd = block.timestamp + lockPeriod * 1 days;
        }

        totalNftStaked++;

        IERC721(_pentasNFT).safeTransferFrom(
            IERC721(_pentasNFT).ownerOf(_tokenId),
            address(this),
            _tokenId
        );
    }

    function unstake(uint256 _tokenId) public {
        uint256 rarity = IGuinInfo(guinInfo).getRarityInt(_tokenId);
        require(rarity > 0, "Not a valid GuinGang NFT!");

        LockInfo storage _lockInfo = lockInfo[_tokenId];
        require(_lockInfo.lockStart != 0, "NFT is not staked!");
        require(block.timestamp > _lockInfo.lockEnd, "NFT is locked!");

        if (hasLocked[_tokenId] == false) {
            hasLocked[_tokenId] = true;
        }

        _lockInfo.lockStart = 0;
        _lockInfo.lockEnd = 0;

        UserInfo storage _info = userInfo[_msgSender()];

        uint256 _index = 0;
        while (_info.stakedNfts[_index] != _tokenId) {
            _index++;
            require(_index < _info.stakedNfts.length, "index out of bound");
        }

        for (uint256 i = _index; i < _info.stakedNfts.length - 1; i++) {
            _info.stakedNfts[i] = _info.stakedNfts[i + 1];
        }
        _info.stakedNfts.pop();

        _harvestPendingRewards(_msgSender());

        _info.totalStaked = _info.totalStaked.sub(1);
        totalNftStaked = totalNftStaked.sub(1);

        IERC721(_pentasNFT).safeTransferFrom(
            address(this),
            _info.userAddress,
            _tokenId
        );
    }

    function harvest() public {
        _harvestPendingRewards(_msgSender());
    }

    function pendingRewards(address _user)
        public
        view
        returns (uint256 rewards)
    {
        rewards = _checkPendingRewards(_user);
    }

    //======================INTERNAL FUNCTIONS=====================//

    function _harvestPendingRewards(address _user) internal {
        UserInfo storage _info = userInfo[_user];
        uint256 toTransfer = _checkPendingRewards(_user);

        _info.lastClaimedTime = block.timestamp;

        if (toTransfer > 0) {
            IERC20(guinToken).transfer(_user, toTransfer);
        }
    }

    function _checkPendingRewards(address _user)
        internal
        view
        returns (uint256 accumulatedEarnings)
    {
        UserInfo storage _info = userInfo[_user];

        uint256 timeDiff = block.timestamp.sub(_info.lastClaimedTime) * 10**18;
        uint256 secondsInDay = 86400;
        uint256 timeRate = timeDiff / secondsInDay;

        accumulatedEarnings = (_getMultiplier(_user) * timeRate) / 10**18;

        return accumulatedEarnings;
    }

    function _getMultiplier(address _user)
        internal
        view
        returns (uint256 multiplier)
    {
        UserInfo storage _info = userInfo[_user];

        multiplier = _info.totalStaked.mul(dayRate);

        return multiplier;
    }
}