pragma solidity ^0.8.5;

import "@openzeppelin/contracts/token/ERC1155/IERC1155Receiver.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "../Collection.sol";
import "../lib/UintSet.sol";
import "hardhat/console.sol";

interface IMintableToken {
    function mint(address, uint256) external;
}

/**
 * @title NFT Staking
 * Distribute ERC20 rewards over discrete-time schedules for the staking of NFTs.
 * This contract is designed on a self-service model, where users will stake NFTs, unstake NFTs and claim rewards through their own transactions only.
 */
contract NftStakingV2 is AccessControl, IERC1155Receiver {
    using UintSet for Set;

    event NftStaked(
        address staker,
        address collection,
        uint256 tokenId,
        uint256 seedId,
        uint256 weight
    );
    event NftUnstaked(
        address staker,
        address collection,
        uint256 tokenId,
        uint256 seedId,
        uint256 weight
    );
    event NftsBatchStaked(
        address staker,
        address collection,
        uint256[] tokenIds,
        uint256[] seedIds,
        uint256[] weights
    );
    event NftsBatchUnstaked(
        address staker,
        address collection,
        uint256[] tokenIds,
        uint256[] seedIds,
        uint256[] weights
    );
    event RewardsClaimed(
        address staker,
        uint256 totalAmount,
        uint256[] tokenIds,
        uint256[] claimedRewards
    );
    event Started();

    struct Stakers {
        address owner;
        uint256 seedId;
        uint256 weight;
        uint256 startTimestamp;
        uint256 stageEndTime;
        uint256 exitTimestamp;
        uint256 accumulatedTime;
        uint256 lastClaimed;
        uint256 currentStage;
        bool isFinished;
        bool exist;
    }

    /* seedId => month => rewardAmount  */
    mapping(uint256 => mapping(uint256 => uint256))
        public getTotalMintPerMonths;

    /* tokenId => stage(month) => end time */
    mapping(uint256 => mapping(uint256 => uint256)) internal stageEndTimes;

    /* tokenId => Stakers */
    mapping(uint256 => Stakers) public staker;

    /* List of owned tokens corresponding to user addresses during staking */
    mapping(address => Set) internal stakerTokenId;

    /* The total amount of collected rewards corresponding to each owner address */
    mapping(address => uint256) public totalClaimedRewards;

    /* amount of reward collected corresponding to each token Id */
    mapping(uint256 => uint256) public claimedRewByTokenId;

    /* Contract where the NFTs are minted */
    Collection public collection;

    /* rewarded token must be BOM token */
    IMintableToken public rewardToken;

    /* Contract address that can only send nft to this contract */
    address public whitelistedNftContract;

    /* staking start time */
    uint256 startTimestamp;

    bytes4 internal constant _ERC1155_RECEIVED = 0xf23a6e61;
    bytes4 internal constant _ERC1155_BATCH_RECEIVED = 0xbc197c81;
    bytes4 internal constant _ERC1155_REJECTED = 0xffffffff;

    constructor() public {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    modifier hasStarted() {
        require(startTimestamp != 0, "NftStaking: staking not started");
        _;
    }

    modifier hasNotStarted() {
        require(startTimestamp == 0, "NftStaking: staking has started");
        _;
    }

    function start() public onlyRole(DEFAULT_ADMIN_ROLE) hasNotStarted {
        startTimestamp = block.timestamp;
        emit Started();
    }

    function setRewardToken(address token)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        rewardToken = IMintableToken(token);
    }

    /**
     * @notice this function should be called before staking starts and required values ​​should be assigned.
     * Each seedId has different rewards for each month from a total of 3 months.
     * It is necessary to keep a mapping of the rewards corresponding to each month.
     * These rewards are kept in the 'getTotalMintPerMonths' mapping.
     * @param _seedId seed identifier referring to the NFT category to which the reward will be added.
     * @param _month in which the reward will be added.
     * @param _totalAmount is reward amount.
     */
    function addReward(
        uint256 _seedId,
        uint256 _month,
        uint256 _totalAmount
    ) external hasNotStarted onlyRole(DEFAULT_ADMIN_ROLE) {
        getTotalMintPerMonths[_seedId][_month] = _totalAmount;
    }

    /**
     * Adds whitelists the given `collecAdd` address.
     */
    function addWhitelist(address collecAdd)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        whitelistedNftContract = collecAdd;
    }

    /**
     * Used when tokens are accidentally sent to the contract.
     * @param token address will be recover.
     */
    function withdrawERC20(address token)
        external
        onlyRole(DEFAULT_ADMIN_ROLE)
    {
        uint256 amount = IERC20(token).balanceOf(address(this));
        IERC20(token).transfer(msg.sender, amount);
    }

    /**
     * Each token Id belongs to an NFT category.
     * @param tokenId The token identifier
     * @param collecAdd The address of the contract where the NFTs are minted.
     * @return `seedId` referring the category each tokenId corresponds to.
     */
    function _getSeedIds(uint256 tokenId, address collecAdd)
        internal
        returns (uint256)
    {
        collection = Collection(collecAdd);
        return collection.seedIdByTokenAttribute(tokenId);
    }

    /**
     * Every seedId referring the category has a governance power.
     * @param seedId The category identifier
     * @param collecAdd The address of the contract where the NFTs are minted.
     * @return `weight` reffering the governance power for category.
     */
    function _getWeight(uint256 seedId, address collecAdd)
        internal
        returns (uint256)
    {
        collection = Collection(collecAdd);
        return collection.weightByTokenAttribute(seedId);
    }

    /**
     * Each time the user sends an NFT, it determine an end time according to the stage of the NFT sent.
     * @param staker structure is the nft data to be calculated.
     * @param tokenId The token identifier, referencing the NFT being staked.
     */
    function _stageControl(Stakers storage staker, uint256 tokenId) private {
        if (staker.currentStage == 0) {
            staker.stageEndTime = block.timestamp + staker.accumulatedTime;
            stageEndTimes[tokenId][0] = staker.stageEndTime;
            stageEndTimes[tokenId][1] = staker.stageEndTime + 30 days;
            stageEndTimes[tokenId][2] = staker.stageEndTime + 60 days;
        }
        if (staker.currentStage == 1) {
            staker.stageEndTime = block.timestamp + staker.accumulatedTime;
            stageEndTimes[tokenId][1] = staker.stageEndTime;
            stageEndTimes[tokenId][2] = staker.stageEndTime + 30 days;
        }
        if (staker.currentStage == 2) {
            staker.stageEndTime = block.timestamp + staker.accumulatedTime;
            stageEndTimes[tokenId][2] = staker.stageEndTime + 30 days;
        }
    }

    /**
     * @notice function works if contract is staked for the first time this `tokenId`
     * @param staker The structure that holds the properties of the nft to be registered in the contract for the first time.
     * @param tokenId The token identifier, referencing the NFT being staked.
     * @param collecAdd The address of the contract where the NFTs are minted.
     */
    function _registerStaker(
        Stakers storage staker,
        uint256 tokenId,
        address collecAdd
    ) private {
        staker.seedId = _getSeedIds(tokenId, collecAdd);
        staker.weight = _getWeight(staker.seedId, collecAdd);
        staker.currentStage = 0;
        staker.exist = true;
        staker.stageEndTime = block.timestamp + 30 days;

        stageEndTimes[tokenId][0] = block.timestamp + 30 days;
        stageEndTimes[tokenId][1] = staker.stageEndTime + 30 days;
        stageEndTimes[tokenId][2] = staker.stageEndTime + 60 days;
    }

    /**
     * Calculates the reward of the stage where the nft is.
     * @param staker is the nft data to be calculated.
     */
    function _calculateTotalRew(Stakers memory staker)
        private
        view
        returns (uint256)
    {
        return
            (getTotalMintPerMonths[staker.seedId][1 + staker.currentStage] *
                (1e36)) / (86400 * 30);
    }

    /**
     * Called by the 'onERC1155Received' when nft is sent to the contract with the 'safeTransferFrom' function.
     * @param tokenId The token identifier, referencing the NFT being staked.
     * @param owner the address of the owner who owns the nfts.
     * @param collecAdd The address of the contract where the NFTs are minted.
     * @dev Reverts if the staking not started.
     * @dev Reverts if the `owner` rewards ended.
     */
    function _stakeNft(
        uint256 tokenId,
        address owner,
        address collecAdd
    ) internal hasStarted {
        Stakers storage stakers = staker[tokenId];
        require(stakers.isFinished == false, "Your Rewards Ended");
        stakers.owner = owner;
        stakers.startTimestamp = block.timestamp;
        stakers.lastClaimed = block.timestamp;

        if (stakers.exist == false) {
            _registerStaker(stakers, tokenId, collecAdd);
        } else {
            _stageControl(stakers, tokenId);
        }
        stakerTokenId[owner].add(tokenId);

        emit NftStaked(
            owner,
            collecAdd,
            tokenId,
            stakers.seedId,
            stakers.weight
        );
    }

    /**
     * Called by the 'onERC1155BatchReceived' when batch nft tokens is sent to the contract with the 'safeBatchTransferFrom' function.
     * @param tokenIds The token identifiers, referencing the NFTs being staked.
     * @param owner the address of the owner who owns the nfts
     * @param collecAdd The address of the contract where the NFTs are minted.
     * @dev Reverts if the staking not started.
     * @dev Reverts if the `owner` rewards ended.
     */
    function _batchStakeNfts(
        uint256[] memory tokenIds,
        address owner,
        address collecAdd
    ) internal hasStarted {
        uint256 numTokens = tokenIds.length;
        uint256[] memory seedIds = new uint256[](numTokens);
        uint256[] memory weights = new uint256[](numTokens);
        require(numTokens != 0, "NftStaking: no tokens");
        Stakers storage stakers;
        for (uint256 index = 0; index < numTokens; ++index) {
            uint256 tokenId = tokenIds[index];
            uint256 seedId = _getSeedIds(tokenId, collecAdd);
            uint256 weight = _getWeight(seedId, collecAdd);

            weights[index] = weight;
            stakers = staker[tokenId];
            require(stakers.isFinished == false, "Your Rewards Ended");
            stakers.owner = owner;
            stakers.startTimestamp = block.timestamp;
            stakers.lastClaimed = block.timestamp;

            if (stakers.exist == false) {
                _registerStaker(stakers, tokenId, collecAdd);
            } else {
                _stageControl(stakers, tokenId);
            }
            stakerTokenId[owner].add(tokenId);
        }
        emit NftsBatchStaked(owner, collecAdd, tokenIds, seedIds, weights);
    }

    /**
     * @notice If the user has the reward, contract will deliver it to her, send back the nft token.
     * @param tokenId The token identifiers, referencing the NFTs being unstaked.
     * @param collecAdd The address of the contract where the NFTs are minted
     * @dev Reverts if `tokenId` is empty.
     * @dev Reverts if the user does not owner `tokenId`
     * @dev Reverts if the caller is not the original owner of any of the NFTs.
     * @dev Reverts if the `collecAdd` not true
     */
    function unStakeNft(uint256 tokenId, address collecAdd) external {
        Stakers storage stakers = staker[tokenId];
        uint256[] memory tokenIds = new uint256[](1);
        tokenIds[0] = tokenId;

        require(
            stakers.owner == msg.sender,
            "NftStaking: not staked for owner"
        );
        claimRewards(tokenIds);
        stakers.exitTimestamp = block.timestamp;
        if (stakers.isFinished == false) {
            stakers.accumulatedTime =
                (stakers.stageEndTime - block.timestamp) +
                stakers.accumulatedTime;
        }
        stakers.owner = address(0);

        IERC1155 _whitelistedNftContract = IERC1155(collecAdd);
        _whitelistedNftContract.safeTransferFrom(
            address(this),
            msg.sender,
            tokenId,
            1,
            ""
        );

        uint256 seedId = _getSeedIds(tokenId, collecAdd);
        uint256 weight = _getWeight(seedId, collecAdd);

        stakerTokenId[msg.sender].remove(tokenId);

        emit NftUnstaked(msg.sender, collecAdd, tokenId, seedId, weight);
    }

    /**
     * @notice If the user has the reward, contract will deliver it to her, send back the nft tokens.
     * @param tokenIds IDs of NFT tokens to be unstaked.
     * @param collecAdd The address of the contract where the NFTs are minted
     * @dev Reverts if `tokenIds` is empty.
     * @dev Reverts if the user does not owner `tokenIds`
     * @dev Reverts if the caller is not the original owner of any of the NFTs.
     * @dev Reverts if `collecAdd` is not true.
     */
    function batchUnstakeNfts(uint256[] calldata tokenIds, address collecAdd)
        external
    {
        uint256 numTokens = tokenIds.length;
        require(numTokens != 0, "NftStaking: no tokens");

        uint256[] memory values = new uint256[](numTokens);
        uint256[] memory seedIds = new uint256[](numTokens);
        uint256[] memory weights = new uint256[](numTokens);

        claimRewards(tokenIds);

        for (uint256 index = 0; index < numTokens; ++index) {
            uint256 tokenId = tokenIds[index];

            Stakers storage stakers = staker[tokenId];
            require(
                stakers.owner == msg.sender,
                "NftStaking: not staked for owner"
            );

            stakers.exitTimestamp = block.timestamp;
            if (stakers.isFinished == false) {
                stakers.accumulatedTime =
                    (stakers.stageEndTime - block.timestamp) +
                    stakers.accumulatedTime;
            }
            stakers.owner = address(0);

            values[index] = 1;
            seedIds[index] = stakers.seedId;
            weights[index] = _getWeight(seedIds[index], collecAdd);
            stakerTokenId[msg.sender].remove(tokenId);
        }

        IERC1155 _whitelistedNftContract = IERC1155(collecAdd);
        _whitelistedNftContract.safeBatchTransferFrom(
            address(this),
            msg.sender,
            tokenIds,
            values,
            ""
        );

        emit NftsBatchUnstaked(
            msg.sender,
            collecAdd,
            tokenIds,
            seedIds,
            weights
        );
    }

    /**
     * @param tokenIds nft ids of the user calling the function
     * @dev Reverts if the caller does not staked any NFT.
     */
    function claimRewards(uint256[] memory tokenIds) public {
        uint256 totalAmount;
        uint256[] memory claimedRewards = new uint256[](tokenIds.length);

        for (uint256 i = 0; i < tokenIds.length; i++) {
            Stakers storage stakers = staker[tokenIds[i]];
            uint256 totRew;
            //require(stakers.isFinished != true, "You claimed all Rewards");
            if (stakers.isFinished == true) {
                continue;
            }
            require(
                stakers.owner == msg.sender,
                "You are not currently staking"
            );

            uint256 totalStakedTime = (block.timestamp - stakers.lastClaimed);

            if (block.timestamp < stakers.stageEndTime) {
                totRew += _calculateTotalRew(stakers);

                totalAmount += (totRew * totalStakedTime) / 1e36;
                claimedRewards[i] += (totRew * totalStakedTime) / 1e36;
                claimedRewByTokenId[tokenIds[i]] +=
                    (totRew * totalStakedTime) /
                    1e36;
            } else {
                totalStakedTime = (stakers.stageEndTime - stakers.lastClaimed);
                for (uint256 j = 0; j < 3; j++) {
                    totRew = _calculateTotalRew(stakers);

                    totalAmount += (totRew * totalStakedTime) / 1e36;
                    claimedRewards[i] += (totRew * totalStakedTime) / 1e36;
                    claimedRewByTokenId[tokenIds[i]] +=
                        (totRew * totalStakedTime) /
                        1e36;

                    totalStakedTime = block.timestamp - stakers.stageEndTime;
                    stakers.currentStage++;
                    stakers.stageEndTime = stageEndTimes[tokenIds[i]][
                        stakers.currentStage
                    ];
                    if (totalStakedTime > 30 days) {
                        totalStakedTime = 30 days;
                    }
                    if (stakers.stageEndTime > block.timestamp) {
                        totalStakedTime =
                            block.timestamp -
                            stageEndTimes[tokenIds[i]][
                                stakers.currentStage - 1
                            ];

                        totRew = _calculateTotalRew(stakers);

                        totalAmount += (totRew * totalStakedTime) / 1e36;
                        claimedRewards[i] += (totRew * totalStakedTime) / 1e36;
                        claimedRewByTokenId[tokenIds[i]] +=
                            (totRew * totalStakedTime) /
                            1e36;

                        stakers.lastClaimed = block.timestamp;
                        stakers.accumulatedTime = 0;
                        break;
                    }
                    if (stakers.currentStage == 3) {
                        stakers.isFinished = true;
                        stakers.lastClaimed = block.timestamp;
                        stakers.accumulatedTime = 0;
                        break;
                    }
                }
            }
            stakers.lastClaimed = block.timestamp;
            stakers.accumulatedTime = 0;
        }
        if (totalAmount > 0) {
            totalClaimedRewards[msg.sender] += totalAmount;
            rewardToken.mint(msg.sender, totalAmount);
            emit RewardsClaimed(
                msg.sender,
                totalAmount,
                tokenIds,
                claimedRewards
            );
        }
    }

    /**
     * @return Returns the amount of erc20 rewards the token 'owner' can withdraw.
     * @dev Reverts if the staking not started.
     */
    function getRewards(address owner)
        public
        view
        hasStarted
        returns (uint256)
    {
        Stakers memory stakers;
        uint256 length = stakerTokenId[owner].getArray().length;

        uint256[] memory tokenIds = new uint256[](length);

        tokenIds = stakerTokenId[owner].getArray();
        uint256 totalAmount;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 totRew;
            stakers = staker[tokenIds[i]];
            if (stakers.isFinished == true) {
                continue;
            }
            require(stakers.owner == owner, "You are not currently staking");

            uint256 totalStakedTime = (block.timestamp - stakers.lastClaimed);
            if (block.timestamp < stakers.stageEndTime) {
                totRew += _calculateTotalRew(stakers);

                totalAmount += (totRew * totalStakedTime) / 1e36;
            } else {
                totalStakedTime = (stakers.stageEndTime - stakers.lastClaimed);
                for (uint256 j = 0; j < 3; j++) {
                    totRew = _calculateTotalRew(stakers);

                    totalAmount += (totRew * totalStakedTime) / 1e36;

                    totalStakedTime = block.timestamp - stakers.stageEndTime;
                    stakers.currentStage++;
                    stakers.stageEndTime = stageEndTimes[tokenIds[i]][
                        stakers.currentStage
                    ];
                    if (totalStakedTime > 30 days) {
                        totalStakedTime = 30 days;
                    }

                    if (stakers.stageEndTime > block.timestamp) {
                        totalStakedTime =
                            block.timestamp -
                            stageEndTimes[tokenIds[i]][
                                stakers.currentStage - 1
                            ];

                        totRew = _calculateTotalRew(stakers);

                        totalAmount += (totRew * totalStakedTime) / 1e36;
                        break;
                    }
                }
            }
        }
        return totalAmount;
    }

    /**
     * @return tokenIds is IDs of nft tokens deposited by the user.
     * @return weights is governance power correspond to each seedId.
     * @return seedIds is ID for each seed category.
     * @return rewards is total amount of claimable tokens corresponding to each nft the token owner deposited.
     */
    function getClaimableRewards(address owner)
        external
        view
        hasStarted
        returns (
            uint256[] memory,
            uint256[] memory,
            uint256[] memory,
            uint256[] memory
        )
    {
        Stakers memory stakers;
        uint256 totalAmount;
        uint256 arrayLength = stakerTokenId[owner].getArray().length;

        uint256[] memory tokenIds = new uint256[](arrayLength);
        uint256[] memory weights = new uint256[](arrayLength);
        uint256[] memory seedIds = new uint256[](arrayLength);
        uint256[] memory rewards = new uint256[](arrayLength);

        tokenIds = stakerTokenId[owner].getArray();

        for (uint256 i = 0; i < tokenIds.length; i++) {
            stakers = staker[tokenIds[i]];

            uint256 totalAmount;
            uint256 totRew;
            require(stakers.owner == owner, "You are not currently staking");

            seedIds[i] = collection.seedIdByTokenAttribute(tokenIds[i]);
            weights[i] = collection.weightByTokenAttribute(seedIds[i]);

            if (stakers.isFinished == true) {
                rewards[i] = totalAmount;
                continue;
            }

            uint256 totalStakedTime = (block.timestamp - stakers.lastClaimed);
            if (block.timestamp < stakers.stageEndTime) {
                totRew += _calculateTotalRew(stakers);

                totalAmount += (totRew * totalStakedTime) / 1e36;
            } else {
                totalStakedTime = (stakers.stageEndTime - stakers.lastClaimed);
                for (uint256 j = 0; j < 3; j++) {
                    totRew = _calculateTotalRew(stakers);

                    totalAmount += (totRew * totalStakedTime) / 1e36;

                    totalStakedTime = block.timestamp - stakers.stageEndTime;
                    stakers.currentStage++;
                    stakers.stageEndTime = stageEndTimes[tokenIds[i]][
                        stakers.currentStage
                    ];
                    if (totalStakedTime > 30 days) {
                        totalStakedTime = 30 days;
                    }

                    if (stakers.stageEndTime > block.timestamp) {
                        totalStakedTime =
                            block.timestamp -
                            stageEndTimes[tokenIds[i]][
                                stakers.currentStage - 1
                            ];

                        totRew = _calculateTotalRew(stakers);

                        totalAmount += (totRew * totalStakedTime) / 1e36;
                        break;
                    }
                }
            }
            rewards[i] = totalAmount;
        }

        return (tokenIds, rewards, seedIds, weights);
    }

    /**
     * @notice Each stage corresponds to 1 month.
     * @return tokenIds is corresponding to each nft deposited by the 'owner'.
     * @return amounts is how much he will earn in total at the end of the current month.
     * @return times is current stage(month) ends time.
     * @return weights is token governance power correspond to each seedId.
     * @return seedIds is ID for each seed category.
     */
    function getCurrentStageInf(address owner)
        public
        view
        returns (
            uint256[] memory,
            uint256[] memory,
            uint256[] memory,
            uint256[] memory,
            uint256[] memory
        )
    {
        Stakers memory stakers;
        uint256 arrayLength = stakerTokenId[owner].getArray().length;

        uint256[] memory tokenIds = new uint256[](arrayLength);
        uint256[] memory amounts = new uint256[](arrayLength);
        uint256[] memory times = new uint256[](arrayLength);
        uint256[] memory weights = new uint256[](arrayLength);
        uint256[] memory seedIds = new uint256[](arrayLength);

        tokenIds = stakerTokenId[owner].getArray();

        for (uint256 i = 0; i < tokenIds.length; i++) {
            stakers = staker[tokenIds[i]];

            require(stakers.owner == owner, "You are not currently staking");
            seedIds[i] = collection.seedIdByTokenAttribute(tokenIds[i]);
            weights[i] = collection.weightByTokenAttribute(seedIds[i]);

            if (stakers.isFinished == false) {
                amounts[i] = getTotalMintPerMonths[stakers.seedId][
                    1 + stakers.currentStage
                ];
                if (block.timestamp >= stakers.stageEndTime) {
                    times[i] = block.timestamp - stakers.stageEndTime;
                } else {
                    times[i] = stakers.stageEndTime - block.timestamp;
                }
            } else {
                amounts[i] = 0;
                times[i] = 0;
            }
        }
        return (tokenIds, amounts, times, weights, seedIds);
    }

    /**
     * @return the NFTs that the 'owner' has staked on this contract.
     */
    function getUserTokenIds(address owner)
        external
        view
        returns (uint256[] memory)
    {
        return stakerTokenId[owner].getArray();
    }

    /**
     * @return tokenIds is number of tokens that the 'owner' has.
     * @return minted amount is so far according to the NFT they deposited.
     */
    function getMinted(address owner)
        external
        view
        returns (uint256[] memory, uint256[] memory)
    {
        Stakers memory stakers;
        uint256 length = stakerTokenId[owner].getArray().length;

        uint256[] memory tokenIds = new uint256[](length);
        uint256[] memory minted = new uint256[](length);

        tokenIds = stakerTokenId[owner].getArray();
        uint256 totalAmount;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 totRew;
            minted[i] += claimedRewByTokenId[tokenIds[i]];
            stakers = staker[tokenIds[i]];

            if (stakers.isFinished == true) {
                continue;
            }
            require(stakers.owner == owner, "You are not currently staking");

            uint256 totalStakedTime = (block.timestamp - stakers.lastClaimed);
            if (block.timestamp < stakers.stageEndTime) {
                totRew += _calculateTotalRew(stakers);

                totalAmount += (totRew * totalStakedTime) / 1e36;
                minted[i] += ((totRew * totalStakedTime) / 1e36);
            } else {
                totalStakedTime = (stakers.stageEndTime - stakers.lastClaimed);
                for (uint256 j = 0; j < 3; j++) {
                    totRew = _calculateTotalRew(stakers);

