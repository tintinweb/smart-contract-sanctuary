contract Liquidity {

    address private constant uniswapV2Router = 0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3;
    address private constant BUSD = 0xA480F47Bcae560Aa53b067b237BDb3833B846521;
    IBEP20 public Token_Address;
    address public owner;

    function Liquify() public {
        uint256 balance = Token_Address.balanceOf(address(this));
        swapTokensForBUSD(balance);
    }


    receive() payable external {}


    constructor(){
        owner = msg.sender;
        Token_Address = IBEP20(0xa263b091391b6D616995d57DC2c4190A65115Da8);
    }

        function swapTokensForBUSD(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> busd
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = BUSD;
        require(Token_Address.approve(address(uniswapV2Router), tokenAmount), 'approve failed.');

        // make the swap
        IUniswapV2Router02(uniswapV2Router).swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of BUSD
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 busdAmount) private {
        // approve token transfer to cover all possible scenarios
        IBEP20(Token_Address).approve(uniswapV2Router, tokenAmount);
        IBEP20(BUSD).approve(uniswapV2Router, busdAmount);


        // add the liquidity
        IUniswapV2Router02(uniswapV2Router).addLiquidity(
            address(this),
            BUSD,
            tokenAmount,
            busdAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner,
            block.timestamp
        );
    }

}