contract Admin {
    using SafeMathV2 for uint256;
    address public _gov;

    mapping(address => address) public _inviters;
    mapping(address => uint256) public _taxes;
    mapping(address => mapping(address => uint256)) public _rewards;

    uint256[] public _eightLevelRebate;

    mapping(address => bool) private _tokenMap;
    address[] private _tokenList;

    address public _router;
    Oracle private _oracle;

    constructor () public {
        _gov = msg.sender;
        _eightLevelRebate = [10, 5, 3, 2, 1, 1, 1, 1];
    }

    function setOracle(address _oAdd) public {
        require(_gov == msg.sender, "access denied");
        _oracle = Oracle(_oAdd);
    }

    function setRouter(address _routerAddr) public {
        require(_gov == msg.sender, "access denied");
        _router = _routerAddr;
    }

    function setInviter(address _inviter) public {
        _inviters[msg.sender] = _inviter;
    }

    function getInviter(address _addr) public view returns (address) {
        return _inviters[_addr];
    }

    function getTokenTax(address _token) public view returns (uint256){
        return _taxes[_token];
    }

    function setTokenTax(address _token, uint256 _tax) public {
        require(_gov == msg.sender, "access denied");
        _taxes[_token] = _tax;
    }

    function getEightLevelRebate() public view returns (uint256[] memory) {
        return _eightLevelRebate;
    }

    function setEightLevelRebate(uint256[] memory _rebate) public {
        require(_gov == msg.sender, "access denied");
        require(_rebate.length == 8, "_rebate length not allow");
        _eightLevelRebate = _rebate;
    }

    function triggerRefferrRewards(address _giver, address _token, uint256 _fee) public returns (bool) {
        require(msg.sender == _router, "access denied");
        uint256 step = 1;
        address last = _giver;
        uint256 sendFee = 0;
        for (;;) {
            if (step > _eightLevelRebate.length) {
                break;
            }
            address inviter = getInviter(last);
            if (inviter == address(0)) {
                break;
            }
            uint256 rew = _fee.mul(_eightLevelRebate[step - 1]).div(100);
            _rewards[inviter][_token] = _rewards[inviter][_token].add(rew);

            sendFee = sendFee.add(rew);
            last = inviter;
            step++;
        }
        _rewards[address(this)][_token] = _rewards[address(this)][_token].add(_fee.sub(sendFee));


        if (!_tokenMap[_token]) {
            _tokenMap[_token] = true;
            _tokenList.push(_token);
        }
        return true;
    }

    function estimateWBNBRewards() public view returns (uint256) {
        uint256 total = 0;
        for (uint256 i = 0; i < _tokenList.length; i++) {
            uint256 wBNBPrice = _oracle.getCurrentPrice(_tokenList[i]);
            total = total.add(_rewards[msg.sender][_tokenList[i]].mul(wBNBPrice));
        }
        return total;
    }

    function claimRewards() public {
        require(estimateWBNBRewards() > 0, "rewards < 0");
        for (uint256 i = 0; i < _tokenList.length; i++) {

            address token = _tokenList[i];
            uint256 tokenRewards = _rewards[msg.sender][token];
            if (tokenRewards <= 0) {
                continue;
            }
            if (token == _oracle.anchorToken()) {
                IERC20(_oracle.anchorToken()).transfer(msg.sender, tokenRewards);
                _rewards[msg.sender][token] = 0;
                continue;
            }
            address[] memory path;
            path[0] = token;
            path[1] = _oracle.anchorToken();
            //(uint amountIn, address[] calldata path)
            uint256[] memory amountsOut = IPancakeRouter01(_router).getAmountsOut(tokenRewards, path);
            //function swapExactTokensForETH(uint amountIn, uint amountOutMin, address[] calldata path, address to, uint deadline)
            IPancakeRouter01(_router).swapExactTokensForETH(
                tokenRewards,
                amountsOut[amountsOut.length - 1],
                path,
                msg.sender,
                block.timestamp.add(60 * 10)
            );

            _rewards[msg.sender][token] = 0;
        }
    }

    function govWithdrawl() public {
        require(msg.sender == _gov, "!access");
        for (uint256 i = 0; i < _tokenList.length; i++) {
            IERC20(_tokenList[i]).transfer(_gov, _rewards[msg.sender][_tokenList[i]]);
            _rewards[msg.sender][_tokenList[i]] = 0;
        }
    }
}