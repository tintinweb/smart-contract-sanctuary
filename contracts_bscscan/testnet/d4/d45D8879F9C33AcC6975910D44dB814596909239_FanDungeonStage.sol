pragma solidity ^0.8.2;

import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./interfaces/IFanDungeonLand.sol";
import "./interfaces/IFanDungeonDecoration.sol";
import "./libs/DecorationInfo.sol";
import "./libs/StageInfo.sol";

contract FanDungeonStage is StageInfo,ERC721Holder {
    using SafeERC20 for IERC20;

    IERC20 public crystalToken;
    IFanDungeonLand public fanDungeonLand;
    IFanDungeonDecoration public fanDungeonDecoration;
    address public treasuryAddress;

    mapping(uint256 => Stage) internal _stages;
    uint256 internal _levelCap = 20;
    mapping (uint256 => mapping(uint8 => Decoration)) internal _decorations;

    constructor(address _crystalToken,address _fanDungeonLand,address _fanDungeonDecoration,address _treasuryAddress) {
        require(
            _crystalToken != address(0) &&
            _treasuryAddress != address(0) &&
            _fanDungeonLand != address(0) &&
            _fanDungeonDecoration != address(0),
            "Stage: Cannot be zero address"
        );
        crystalToken = IERC20(_crystalToken);
        fanDungeonLand = IFanDungeonLand(_fanDungeonLand);
        fanDungeonDecoration = IFanDungeonDecoration(_fanDungeonDecoration);
        treasuryAddress = _treasuryAddress;
    }
    
    function upgradeDungeonLevel(uint256 _dungeonId) external {
        require(levelCapOf(_dungeonId) + 1 <= _levelCap,"Stage: Level cap");
        crystalToken.safeTransferFrom(msg.sender, treasuryAddress, 1000 ether);
        _stages[_dungeonId].levelCap = levelCapOf(_dungeonId) + 1;
    }

    function setFee(uint256 _dungeonId,uint64 _fee) external {
        require(_fee<=10000,"Stage: fee");
        _stages[_dungeonId].fee = _fee;
    }

    function addDecoration(uint256 _dungeonId,uint256 _decorationId) external {
        require(fanDungeonDecoration.ownerOf(_decorationId)==msg.sender,"Stage: Decoration");
        require(fanDungeonLand.ownerOf(_dungeonId)==msg.sender,"Stage: Land");

        DecorationInfo.Decoration memory d = fanDungeonDecoration.decorationOf(_decorationId);
        require(isActive(_dungeonId,d.position)==false,"Stage: Position");
        fanDungeonDecoration.safeTransferFrom(msg.sender, address(this), _decorationId);

        DecorationInfo.Bonus memory b = fanDungeonDecoration.bonusOf(_decorationId);
        _decorations[_dungeonId][d.position].tokenId = _decorationId;
        _decorations[_dungeonId][d.position].isActive = true;

        _stages[_dungeonId].bonus.xp += b.xp;
        _stages[_dungeonId].bonus.crystal += b.crystal;
        _stages[_dungeonId].bonus.duration += b.duration;
    }

    function removeDecoration(uint256 _dungeonId,uint256 _decorationId) external {
        require(fanDungeonLand.ownerOf(_dungeonId)==msg.sender,"Stage: Land");

        IFanDungeonDecoration.Decoration memory d = fanDungeonDecoration.decorationOf(_decorationId);
        require(isActive(_dungeonId,d.position),"Stage: No decoration");

        fanDungeonDecoration.decorationOf(_decorationId);

        fanDungeonDecoration.safeTransferFrom(address(this), msg.sender, _decorationId);
        
        DecorationInfo.Bonus memory b = fanDungeonDecoration.bonusOf(_decorationId);
        _stages[_dungeonId].bonus.xp -= b.xp;
        _stages[_dungeonId].bonus.crystal -= b.crystal;
        _stages[_dungeonId].bonus.duration -= b.duration;
        _decorations[_dungeonId][d.position].isActive = false;
    }

    // view function
    function bonusOf(uint256 _dungeonId) public view returns(Bonus memory) {
        return _stages[_dungeonId].bonus;
    }

    function decorationOf(uint256 _dungeonId,uint8 _position) public view returns(uint256) {
        return _decorations[_dungeonId][_position].tokenId;
    }

    function isActive(uint256 _dungeonId,uint8 _position) public view returns(bool) {
        return _decorations[_dungeonId][_position].isActive;
    }

    function feeOf(uint256 _dungeonId) external view returns(uint256) {
        return _stages[_dungeonId].fee;
    }

    function levelCapOf(uint256 _dungeonId) public view returns(uint64) {
        if (_stages[_dungeonId].levelCap==0) {
            return 1;
        }

        return _stages[_dungeonId].levelCap;
    }

    function stageOf(uint256 _dungeonId) public view returns(Stage memory) {
        return _stages[_dungeonId];
    }
    
    // @custom:internal_function
}

