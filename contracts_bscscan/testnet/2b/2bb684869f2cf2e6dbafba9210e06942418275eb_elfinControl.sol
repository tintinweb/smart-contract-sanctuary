// File: @openzeppelin/contracts/token/ERC20/ERC20.sol

contract elfinControl is Ownable {
    address private _owner;
    address private _cmo;
    //uint256 public gameRewardBalance;
    address  public blackHole;

    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    // Info of each user.
    struct UserInfo {
        uint256 totalReward; // How many tokens the user get.
        uint256 withdrawnReward; //How many tokens the user has withdrawn.
        uint256 latestWithdrawnTimestamp; // The latest withdrawn timestamp.
    }



    // Info of each user that rewards tokens.
    mapping(address => UserInfo) public userInfo;

    uint256 public withdrawWaitTime = 14 * 86400;
    uint256 public minimumWithdrawAmount = 100e18;

    // The elfin king token
    IERC20 public _kingToken;
    IERC20 public _elfinToken;
    IERC721 public _elfin721Token;
    uint256 public globalElfinKingRewards;






    mapping(uint => IERC20) public token20Array;



    event userWithdraw(address indexed user, uint256 indexed amount, uint256 timestamp);
    event setUserTotalRewards(address indexed user, uint256 indexed amount);
    event buyPropRecord(address indexed user, uint256 indexed id, uint256 indexed number);



    constructor(address kingToken, address elfinToken, address elfin721Token, uint256[] memory _id, uint256[] memory _price, uint256[] memory _limit) public {
        _owner  = msg.sender;
        _cmo    = msg.sender;
        _kingToken  = IERC20(kingToken);
        _elfinToken = IERC20(elfinToken);
        token20Array[0] = _kingToken;
        token20Array[1] = _elfinToken;
        _elfin721Token = IERC721(elfin721Token);
        require(_id.length == _price.length && _id.length == _limit.length);
        for (uint i = 0; i < _id.length; i++) {
            PropInfo storage info = propInfo[_id[i]];
            info.price = _price[i];
            info.limit = _limit[i];
        }
        blackHole = address(0x0000000000000000000000000000000000000001);




    }

    function setTokenArray(uint index, address _newToken) external onlyCmo {
        token20Array[index] = IERC20(_newToken);
    }


    function setWithdrawWaitTime(uint256 _time) external onlyCmo {
        require(_time > 0);
        withdrawWaitTime = _time;
    }


    function setMinimumWithdrawAmount(uint256 _minimumWithdrawAmount) external onlyCmo {
        require(_minimumWithdrawAmount > 0);
        minimumWithdrawAmount = _minimumWithdrawAmount;
    }

    // View function to see pending elfin King reward.
    function getUserPendingReward(address _user)
    public
    view
    returns (uint256)
    {
        UserInfo storage info = userInfo[_user];
        uint256 totalReward = info.totalReward;
        uint256 withdrawnReward = info.withdrawnReward;
        return totalReward.sub(withdrawnReward);
    }

    function canWithdraw(address _user)
    public
    view
    returns (bool)
    {
        return now.sub(withdrawWaitTime) >= userInfo[_user].latestWithdrawnTimestamp && userInfo[_user].totalReward.sub(userInfo[_user].withdrawnReward) >= minimumWithdrawAmount;
    }

    function injectRewards(uint256 injectReward) public {
        _kingToken.safeTransferFrom(msg.sender,address(this),injectReward);
    }

    function cmoWithdrawKingToken(uint256 amount) public onlyCmo() {
        _kingToken.safeTransfer(msg.sender,amount);
    }



    //User withdraw tokens.
    function withdrawRewards() external {
        UserInfo storage info = userInfo[msg.sender];
        uint256 pendingReward = getUserPendingReward(msg.sender);
        require(pendingReward >= minimumWithdrawAmount,"Withdraw amount must gather than minimumWithdrawAmount");
        require(canWithdraw(msg.sender),"Not withdraw times");

        require(_kingToken.balanceOf(address(this)) >= pendingReward,"Not enough reward from contract, please connect cmo to inject rewards");

        _kingToken.safeTransfer(msg.sender,pendingReward);
        info.latestWithdrawnTimestamp = now;
        info.withdrawnReward = info.withdrawnReward.add(pendingReward);
        globalElfinKingRewards = globalElfinKingRewards.add(pendingReward);

        emit userWithdraw(msg.sender,pendingReward,info.latestWithdrawnTimestamp);
    }

    function setUserTotalReward(address[] memory _user, uint256[] memory _newTotalReward) external onlyCmo() {
        require(_user.length == _newTotalReward.length,"Length is not equal");
        for (uint i = 0; i < _user.length; i++) {
            _setUserTotalReward(_user[i], _newTotalReward[i]);
        }
    }

    function _setUserTotalReward(address _user, uint256 _newTotalReward) internal  {
        UserInfo storage info = userInfo[_user];
        require(_newTotalReward > info.totalReward,"New total reward must gather then user current total reward.");
        info.totalReward = _newTotalReward;
        if(info.latestWithdrawnTimestamp == 0) {
            info.latestWithdrawnTimestamp = now - 86400;
        }
        emit setUserTotalRewards(_user,_newTotalReward);
    }









    event evolutionRecord(address indexed user, uint256 indexed evolutionType, uint256 indexed nftId, uint targetLevel, uint256 dealId,
        uint256[] burnAmount,  uint256[] nftArray,  uint[] propList);

    function evolution(uint evolutionType, uint256 nftId, uint targetLevel,
        uint256[] memory burnAmount, uint256[] memory nftArray, uint[] memory propList, uint256 dealId) public {
        require(_elfin721Token.ownerOf(nftId) == _msgSender(),"Nft owner is wrong");


        for (uint i = 0; i < burnAmount.length; i++) {
            token20Array[i].transferFrom(msg.sender,blackHole,burnAmount[i]);
        }

        for (uint i = 0; i < nftArray.length; i++) {
            _elfin721Token.safeTransferFrom(msg.sender,blackHole,nftArray[i]);
        }

        for (uint i = 0; i < propList.length; i++) {
            require(propInfo[i].limit > 0, "Not exist prop");
        }

        emit evolutionRecord(_msgSender(), evolutionType, nftId, targetLevel, dealId, burnAmount, nftArray, propList);


    }

















    // Info of each prop.
    struct PropInfo {
        uint256 id; // Prop id
        uint256 price; //Prop price
        uint256 limit; // Prop limit
    }

    // Info of buy prop info.
    struct BuyInfo {
        uint256 buyAmount; //Today this user has buy how many prop of this id.
        uint256 lastTimestamp;
        uint256 ownedAmount;
    }

    mapping(address => mapping(uint256 => BuyInfo)) public buyInfo;

    // Info of each prop.
    mapping(uint256 => PropInfo) public propInfo;


    uint256 public buyWaitTime = 86400;


    function getBuyLimit(address _user, uint256 _id) public view returns(uint256) {
        if(propInfo[_id].price == 0) {
            return 0;
        }
        if((now.div(buyWaitTime)).sub(buyInfo[_user][_id].lastTimestamp.div(buyWaitTime)) > 0) {
            return propInfo[_id].limit;
        } else {
            return propInfo[_id].limit.sub(buyInfo[_user][_id].buyAmount);
        }
    }

    function batchBuyProp(address _user, uint256[] memory _id, uint256[] memory _number) external {
        require(_user == msg.sender, "Only msg.sender can buy prop");
        require(_id.length == _number.length, "Length are not equal");

        for (uint i = 0; i < _id.length; i++) {
            _buyProp(_user, _id[i], _number[i]);
        }

    }

    function _buyProp(address _user, uint256 _id, uint256 _number) internal {

        require(getBuyLimit(_user,_id) >= _number, "Buy number gather then limit today");

        uint256 amount = _number.mul(propInfo[_id].price);

        BuyInfo storage info = buyInfo[_user][_id];
        info.buyAmount = propInfo[_id].limit.sub(getBuyLimit(_user,_id)).add(_number);
        //info.buyAmount = info.buyAmount;
        info.lastTimestamp = now;

        info.ownedAmount = info.ownedAmount.add(_number);

        _kingToken.transferFrom(msg.sender,blackHole,amount);

        emit buyPropRecord(_user,_id,_number);




    }

    function setPropInfo(uint256 id, uint256 price, uint256 limit) external onlyCmo {
        PropInfo storage info = propInfo[id];
        info.price = price;
        info.limit = limit;
    }


    function setBuyWaitTime(uint256 _time) external onlyCmo {
        require(_time > 0);
        buyWaitTime = _time;
    }










    function getCmo() public  view returns(address) {
        return _cmo;
    }


    modifier onlyCmo() {
        require(msg.sender == _cmo);
        _;
    }

    function setCmo(address newCmo) public {
        require(msg.sender == _owner);
        _cmo   = newCmo;
    }


    function setElfin721Token(address elfin721token) public onlyCmo {
        _elfin721Token = IERC721(elfin721token);
    }





}