// One TeleportSwap is a facade proxy for one pair BUSD-<TOKEN>
// Create multiple TeleportSwap and use the same router and factory.
contract TeleportSwap is Ownable {
    using SafeMath for uint256;

    address public router_address;
    IPancakeSimple public teleportRouter;
    address public factory_address;
    IPancakeFactory public teleportFactory;
    address private _busd;
    address private _token;
    IERC20 private busd_contract;
    IERC20 private token_contract;
    address public dev_address;
    uint256 public _fee; // in thousand

    constructor(
        address busd,
        address token,
        address router,
        address factory,
        address devAddress
    ) public Ownable() {
        router_address = router;
        teleportRouter = IPancakeSimple(router);
        factory_address = factory;
        teleportFactory = IPancakeFactory(factory_address);
        _busd = busd;
        _token = token;
        busd_contract = IERC20(busd);
        token_contract = IERC20(token);
        dev_address = devAddress;
        _fee = 3;
    }

    function updateFee(uint256 fee) public onlyOwner returns (bool) {
        _fee = fee;
    }

    function addLiquidity(
        uint256 amountBUSDDesired,
        uint256 amountTELEDesired,
        uint256 amountBUSDMin,
        uint256 amountTELEMin
    )
        public
        returns (
            uint256 amountBUSD,
            uint256 amountTELE,
            uint256 liquidity
        )
    {
        require(
            busd_contract.allowance(_msgSender(), address(this)) >= amountBUSDMin,
            'Teleport swap has no enough min busd allowance'
        );
        require(
            token_contract.allowance(_msgSender(), address(this)) >= amountTELEMin,
            'Teleport swap has no enough min tele allowance'
        );
        require(
            busd_contract.allowance(_msgSender(), address(this)) >= amountBUSDDesired,
            'Teleport swap has no enough desired busd allowance'
        );
        require(
            token_contract.allowance(_msgSender(), address(this)) >= amountTELEDesired,
            'Teleport swap has no enough desired tele allowance'
        );
        TransferHelper.safeTransferFrom(_busd, _msgSender(), address(this), amountBUSDMin);
        TransferHelper.safeApprove(_busd, router_address, amountBUSDMin);
        TransferHelper.safeTransferFrom(_token, _msgSender(), address(this), amountTELEMin);
        TransferHelper.safeApprove(_token, router_address, amountTELEMin);
        return
            teleportRouter.addLiquidity(
                _busd,
                _token,
                amountBUSDDesired,
                amountTELEDesired,
                amountBUSDMin,
                amountTELEMin,
                _msgSender(),
                block.timestamp
            );
    }

    function swapBUSDForToken(uint256 amountIn, uint256 amountOutMin) public returns (uint256[] memory amounts) {
        require(amountIn >= 10**12, 'Too low value to swap');
        require(
            busd_contract.allowance(_msgSender(), address(this)) >= amountIn,
            'Teleport swap has no enough min busd allowance'
        );
        uint256 devAmount = amountIn.mul(_fee).div(1000);
        uint256 swapAmount = amountIn.sub(devAmount);
        busd_contract.transferFrom(_msgSender(), address(this), amountIn);
        busd_contract.transfer(dev_address, devAmount);
        // busd_contract.transferFrom(_msgSender(), dev_address, devAmount);
        busd_contract.approve(router_address, amountIn);
        uint256 devAmountOut = amountOutMin.mul(_fee).div(1000);
        uint256 swapAmountOut = amountOutMin.sub(devAmountOut);
        return
            teleportRouter.swapExactTokensForTokens(
                swapAmount,
                swapAmountOut,
                getPathForBUSDtoToken(),
                _msgSender(),
                block.timestamp
            );
    }

    function swapTokenForBUSD(uint256 amountIn, uint256 amountOutMin) public returns (uint256[] memory amounts) {
        require(amountIn >= 10**15, 'Too low value to swap');
        require(
            token_contract.allowance(_msgSender(), address(this)) >= amountIn,
            'Teleport swap has no enough min busd allowance'
        );
        token_contract.transferFrom(_msgSender(), address(this), amountIn);
        token_contract.approve(router_address, amountIn);
        uint256 devAmount = amountIn.mul(_fee).div(1000);
        uint256 devAmountOut = amountOutMin.mul(_fee).div(1000);
        uint256 swapAmount = amountIn.sub(devAmount);
        uint256 swapAmountOut = amountOutMin.sub(devAmountOut);
        teleportRouter.swapExactTokensForTokens(
            devAmount,
            devAmountOut,
            getPathForTokentoBUSD(),
            dev_address,
            block.timestamp
        );
        return
            teleportRouter.swapExactTokensForTokens(
                swapAmount,
                swapAmountOut,
                getPathForTokentoBUSD(),
                _msgSender(),
                block.timestamp
            );
    }

    function removeLiquidity(
        uint256 liquidity,
        uint256 amountAMin,
        uint256 amountBMin
    ) public returns (uint256 amountA, uint256 amountB) {
        IPancakePair pair = IPancakePair(teleportFactory.getPair(_busd, _token));
        pair.sync();
        pair.transferFrom(_msgSender(), address(this), liquidity);
        pair.approve(router_address, liquidity);
        (amountA, amountB) = teleportRouter.removeLiquidity(
            _busd,
            _token,
            liquidity,
            amountAMin,
            amountBMin,
            _msgSender(),
            block.timestamp
        );
        return (amountA, amountB);
    }

    function getPair() public view returns (address) {
        address pairAddress = teleportFactory.getPair(_busd, _token);
        return pairAddress;
    }

    function getBUSDLiquidityAmount(address provider) public view returns (uint256) {
        address pairAddress = teleportFactory.getPair(_busd, _token);
        require(pairAddress != address(0), 'no liquidity in this pair');
        IPancakePair pair = IPancakePair(pairAddress);
        uint256 lpAmount = pair.balanceOf(provider);
        uint256 busdAmount = lpAmount.mul(lpAmount).div(getTeleReserve());
        return busdAmount;
    }

    function getTokenLiquidityAmount(address provider) public view returns (uint256) {
        address pairAddress = teleportFactory.getPair(_busd, _token);
        require(pairAddress != address(0), 'no liquidity in this pair');
        IPancakePair pair = IPancakePair(pairAddress);
        uint256 lpAmount = pair.balanceOf(provider);
        uint256 tokenAmount = lpAmount.mul(lpAmount).div(getBUSDReserve());
        return tokenAmount;
    }

    function getBUSDTokenLiquidityAmount(uint256 lpAmount) public view returns (uint256, uint256) {
        address pairAddress = teleportFactory.getPair(_busd, _token);
        require(pairAddress != address(0), 'no liquidity in this pair');
        IPancakePair pair = IPancakePair(pairAddress);
        (uint256 token0reserve, uint256 token1reserve, ) = pair.getReserves();
        uint256 supply = pair.totalSupply();
        require(supply >= lpAmount, 'the amount calculation is overflow');
        uint256 ratio = supply.div(lpAmount);
        uint256 lpAmount2 = supply.mul(supply);
        if (pair.token0() == _busd) {
            return (lpAmount2.div(token1reserve).div(ratio), lpAmount2.div(token0reserve).div(ratio));
        } else {
            return (lpAmount2.div(token0reserve).div(ratio), lpAmount2.div(token1reserve).div(ratio));
        }
    }

    function getTokenAmountRequire(uint256 amountIn) public view returns (uint256 amountOut) {
        return amountIn.mul(getTeleReserve()).div(getBUSDReserve());
    }

    function getBUSDAmountRequire(uint256 amountIn) public view returns (uint256 amountOut) {
        return amountIn.mul(getBUSDReserve()).div(getTeleReserve());
    }

    function getTokenAmountOut(uint256 amountIn) public view returns (uint256 amountOut) {
        uint256 devAmount = amountIn.mul(_fee).div(1000);
        uint256 swapAmount = amountIn.sub(devAmount);
        return PancakeLibrary.getAmountOut(swapAmount, getBUSDReserve(), getTeleReserve());
    }

    function getBUSDAmountOut(uint256 amountIn) public view returns (uint256 amountOut) {
        uint256 devAmount = amountIn.mul(_fee).div(1000);
        uint256 swapAmount = amountIn.sub(devAmount);
        return PancakeLibrary.getAmountOut(swapAmount, getTeleReserve(), getBUSDReserve());
    }

    function getBUSDReserve() public view returns (uint256) {
        address pairAddress = teleportFactory.getPair(_busd, _token);
        require(pairAddress != address(0), 'no liquidity in this pair');
        IPancakePair pair = IPancakePair(pairAddress);
        (uint256 token0reserve, uint256 token1reserve, ) = pair.getReserves();
        if (pair.token0() == _busd) {
            return token0reserve;
        } else {
            return token1reserve;
        }
    }

    function getTeleReserve() public view returns (uint256) {
        address pairAddress = teleportFactory.getPair(_busd, _token);
        require(pairAddress != address(0), 'no liquidity in this pair');
        IPancakePair pair = IPancakePair(pairAddress);
        (uint256 token0reserve, uint256 token1reserve, ) = pair.getReserves();
        if (pair.token0() == _token) {
            return token0reserve;
        } else {
            return token1reserve;
        }
    }

    function getPathForBUSDtoToken() private view returns (address[] memory) {
        address[] memory path = new address[](2);
        path[0] = _busd;
        path[1] = _token;

        return path;
    }

    function getPathForTokentoBUSD() private view returns (address[] memory) {
        address[] memory path = new address[](2);
        path[0] = _token;
        path[1] = _busd;

        return path;
    }
}

