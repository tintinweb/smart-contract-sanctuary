contract Dashboard{
    using SafeMath for uint;
     using SafeDecimal for uint;
    using SafeERC20 for IERC20;

    mapping(address => address) private pairAddresses;
    address public USDT;
    address public WBNB;
    address public PancakeMasterchef;
    address public UpFarm;
    address public PancakeFactory;
    address public CAKE;
    address public UP;
    uint constant public BLOCK_PER_YEAR = 1120000;
    IMasterChef public  master;
    IFarm public upmaster;
    constructor(
        address _usdt,
        address _wbnb,
        address _masterchef,
        address _factory,
        address _cake,
        address _up,
        address _upfarm
    )public{
        USDT = _usdt;
        WBNB = _wbnb;
        PancakeMasterchef = _masterchef;
        PancakeFactory = _factory;
        CAKE = _cake;
        UP = _up;
        UpFarm = _upfarm;
        master = IMasterChef(PancakeMasterchef);
        upmaster = IFarm(UpFarm);
    }
    function priceOfBNB() view public returns (uint) {
        address BNB_BUSD_POOL = IPancakeFactory(PancakeFactory).getPair(USDT,WBNB);
        return IERC20(USDT).balanceOf(BNB_BUSD_POOL).mul(1e18).div(IERC20(WBNB).balanceOf(BNB_BUSD_POOL));
    }
    function tokenPriceInWbnb(address _token) public view returns (uint256){
        if(_token == WBNB){
            return 1e18;
        }else if(IPancakeFactory(PancakeFactory).getPair(_token, WBNB) != address(0)){
            address pair = IPancakeFactory(PancakeFactory).getPair(_token, WBNB);

            (uint112 token0Rs,uint112 token1Rs,) = IPancakePair(pair).getReserves();
            if (IPancakePair(pair).token0() == _token){
                return uint256(token1Rs).mul(1e18).div(token0Rs);
            }else{
                return uint256(token0Rs).mul(1e18).div(token1Rs);
            }
        }else {
            return 0;
        }
    }
    function tokenPriceInUsd(address _token)public view returns (uint256){
        if(_token == USDT){
            return 1e18;
        }else if(IPancakeFactory(PancakeFactory).getPair(_token, USDT) != address(0)){
            address pair = IPancakeFactory(PancakeFactory).getPair(_token, USDT);

            (uint112 token0Rs,uint112 token1Rs,) = IPancakePair(pair).getReserves();
            if (IPancakePair(pair).token0() == _token){
                return uint256(token1Rs).mul(1e18).div(token0Rs);
            }else{
                return uint256(token0Rs).mul(1e18).div(token1Rs);
            }
        }else{
            // uint256 usdtPriceInWbnb = tokenPriceInWbnb(USDT);
            // uint256 tokenPriceInWbnb = tokenPriceInWbnb(_token);
            return tokenPriceInWbnb(_token).mul(1e18).div(tokenPriceInWbnb(USDT));
        }
    }

    function lpPriceInUsd(address _lptoken)public view returns (uint256){
        IPancakePair pair = IPancakePair(_lptoken);
        address token0 = pair.token0();
        address token1 = pair.token1();
        (uint112 token0Res, uint112 token1Res,) = pair.getReserves();    
        uint token0Price = tokenPriceInUsd(token0);
        uint token1Price = tokenPriceInUsd(token1);      
        return uint(token0Res).mul(token0Price).add(uint(token1Res).mul(token1Price)).div(pair.totalSupply());
    }



    /* ========== APY Calculation ========== */
    //APR of pool
    function basicCompound(uint256 pid) public view returns (uint) {
        (address token, uint allocPoint,,) = master.poolInfo(pid);
        uint valueInBNB = lpPriceInUsd(token).mul(1e18).div(priceOfBNB()).mul(IERC20(token).balanceOf(address(master))).div(1e18);

        uint cakePriceInBNB = tokenPriceInWbnb(address(CAKE));
        uint cakePerYearOfPool = master.cakePerBlock().mul(BLOCK_PER_YEAR).mul(allocPoint).div(master.totalAllocPoint());
        return cakePriceInBNB.mul(cakePerYearOfPool).div(valueInBNB);

        //uint apy = apr.div(compound).add(1e18).power(compound).sub(1e18);
    }
    function basicCompoundOfUPFarm(uint256 pid) public view returns (uint) {
        (address token, uint allocPoint,,) = upmaster.poolInfo(pid);
        uint valueInBNB = lpPriceInUsd(token).mul(1e18).div(priceOfBNB()).mul(IERC20(token).balanceOf(address(master))).div(1e18);

        uint UPPriceInBNB = tokenPriceInWbnb(address(UP));
        uint UPPerYearOfPool = upmaster.UPPerBlock().mul(BLOCK_PER_YEAR).mul(allocPoint).div(upmaster.totalAllocPoint());
        return UPPriceInBNB.mul(UPPerYearOfPool).div(valueInBNB);

        //uint apy = apr.div(compound).add(1e18).power(compound).sub(1e18);
    }
    function valueOfBNB(uint256 pid) public view returns (uint) {
        (address token, uint allocPoint,,) = master.poolInfo(pid);
        uint valueInBNB = lpPriceInUsd(token).mul(1e18).div(priceOfBNB()).mul(IERC20(token).balanceOf(address(master))).div(1e18);
        return valueInBNB;
    }

    function cakePerYearOfPool(uint256 pid)public view returns (uint) {
        (address token, uint allocPoint,,) = master.poolInfo(pid);
        uint valueInBNB = lpPriceInUsd(token).mul(1e18).div(priceOfBNB()).mul(IERC20(token).balanceOf(address(master)));

        uint cakePriceInBNB = tokenPriceInWbnb(address(CAKE));
        uint cakePerYearOfPool = master.cakePerBlock().mul(BLOCK_PER_YEAR).mul(allocPoint).div(master.totalAllocPoint());
        return cakePerYearOfPool;
    }

    //APY OF Pool
    //compound compound = 365
    //TVL  xx   compound = 365 * s
    function apyOfPool(uint256 pid, uint256 compound) public view returns (uint){
        uint apr = basicCompound(pid);
        return apr.div(compound).add(1e18).power(compound).sub(1e18);
    }
    
    function tvlOfPool(address pool) public view returns (uint){
        IStrategy strategy = IStrategy(pool);
        return lpPriceInUsd(strategy.wantAddress()).mul(strategy.wantLockedTotal()).div(1e18);
    }
}