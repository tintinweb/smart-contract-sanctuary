// File contracts/8/Horse721.sol

pragma solidity ^0.8.0;
pragma abicoder v2;



contract Horse721 is ERC721Enumerable, IHorse {

    using SafeMath for uint256;

    Horse[] public Horses;

    mapping (string => uint256) public nameToTokenId;


    event Birth(address owner, uint256 kittyId, uint256 matronId, uint256 sireId, uint256 genes);

    constructor() ERC721("Horse", "Horse") {
        
    }

    function _getRandom(uint256 randomN, uint256 baseN) private pure returns (uint256) {
        return randomN.mod(baseN);
    }
    
    function _getRandomGender() private view returns (Gender){
        uint256 _gender = _getRandom(uint256(blockhash(block.number-1)),2);
        return Gender(_gender);
    }

    function _getRandomColor() private view returns (uint256){
        uint256 r = _getRandom(uint256(blockhash(block.number-3)),256);
        // console.log("Color:r:",r);
        uint256 g = _getRandom(uint256(blockhash(block.number-2)),256);
        // console.log("Color:g:",g);
        uint256 b = _getRandom(uint256(blockhash(block.number-1)),256);
        // console.log("Color:b:",b);

        uint256 color = r + (g << 8) + (b << 16);
        // console.log("Color:color:",color);

        return color;
    }


    function _getRandomGenes() private view returns (uint256){
        uint256 r = _getRandom(uint256(blockhash(block.number-3)),5);
        // console.log("Genes:r:",r);
        uint256 s = _getRandom(uint256(blockhash(block.number-2)),5);
        // console.log("Genes:s:",s);
        uint256 y = _getRandom(uint256(blockhash(block.number-1)),5);
        // console.log("Genes:y:",y);

        uint256 genes = r + (s << 8) + (y << 16);
        // console.log("Genes:genes:",genes);

        return genes;
    }


    function _createHorse(
        uint256 _matronId,
        uint256 _sireId,
        bool _setGender,
        Gender _gender,
        Bloodline _bloodline,
        uint256 _genotype,
        BreedType _breedtype,
        address _owner
    ) 
    internal 
    virtual 
    returns (uint256)
    {
        if(!_setGender){
            _gender = _getRandomGender();
        }

        uint256 _color = _getRandomColor();
        uint256 _genes = _getRandomGenes();
        
        Horse memory _horse = Horse({
            name: "",
            gender: _gender,
            bloodline:_bloodline,
            genotype:_genotype,
            breedtype:_breedtype,
            coatcolour: _color,
            birthTime: block.timestamp,
            matronId: _matronId,
            sireId: _sireId,
            // siringWithId:0,
            genes: _genes
            // breedCount: 0,
            // lastBreedTime: 0
        });
        Horses.push(_horse);
        uint256 tokenId =  Horses.length - 1;

        _mint(_owner, tokenId);

        // emit Birth(_owner, tokenId, _matronId, _sireId, _gender);

        return tokenId;
    }

    function setHorseName(uint256 _horseId, string memory _name) external {
        require(ownerOf(_horseId) == msg.sender, "Horse721:ownerOf(_horseId) != msg.sender");
        Horse storage _horse = Horses[_horseId];
        console.log("origin name:",_horse.name);
        console.log("set name:", _name);
        console.log('_name length:', bytes(_name).length);
        require(bytes(_horse.name).length == 0, "Horse721:bytes(_horse.name).length != 0");
        require(bytes(_name).length <= 25, "Horse721:bytes(_name).length > 25");
        require(nameToTokenId[_name] == 0, "Horse721:nameToTokenId[_name] > 0");
        _horse.name = _name;
        nameToTokenId[_name] = _horseId;
    }


    function _isMatron(uint256 _horseId) internal view returns(bool){
        Horse memory horse = Horses[_horseId];
        return horse.gender == Gender.mare;
    }

    function _isSire(uint256 _horseId) internal view returns(bool){
        Horse memory horse = Horses[_horseId];
        return horse.gender == Gender.stallion;
    }


    function transfer(address _to,uint256 _tokenId) external override{
        _transfer(msg.sender, _to, _tokenId);
    }

    function getHorse(uint256 _tokenId) external view override returns(Horse memory){
        return Horses[_tokenId];
    }

    // function getGenes(uint256 _tokenId) external view override returns(uint256, uint256, uint256){
    function getGenes(uint256 _genes) external view override returns(uint256, uint256, uint256){
        // Horse memory _horse = Horses[_tokenId];
        // uint256 _genes = _horse.genes;
        console.log('Genes:genes:',_genes);

        uint256 y = _genes >> 16;
        console.log("Genes:y:",y);
        uint256 s = (_genes - (y << 16)) >> 8;
        console.log("Genes:s:",s);
        uint256 r = _genes - (y << 16) - (s << 8);
        console.log("Genes:r:",r);

        return (r,s,y);
    }

    function getCoatcolour(uint256 _tokenId) external view returns(uint256, uint256, uint256){
        Horse memory _horse = Horses[_tokenId];
        uint256 _coatcolour = _horse.coatcolour;
        console.log('Genes:genes:',_coatcolour);

        uint256 b = _coatcolour >> 16;
        console.log("Color:b:",b);
        uint256 g = (_coatcolour - (b << 16)) >> 8;
        console.log("Color:g:",g);
        uint256 r = _coatcolour - (b << 16) - (g << 8);
        console.log("Color:r:",r);

        return (r,g,b);
    }
    
}


