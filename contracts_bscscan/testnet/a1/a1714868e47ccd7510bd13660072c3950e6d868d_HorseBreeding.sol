        Horse memory _horse = Horses[_horseId];
        Bloodline _bloodline = _horse.bloodline;
        BreedType _breedtype = _horse.breedtype;

        uint256 prefix = bloodlineWeighting[uint256(_bloodline)].mul(80);
        uint256 prefix2 = breedTypeWeighting[uint256(_breedtype)].mul(20);
        uint256 price = basePrice.mul(prefix.add(prefix2)).div(10000);

        return price;
    }


