        uint256 _price,
        uint256 _duration
    )
        external
    {
        require(ownerOf(_horseId) == msg.sender,"HorseBreeding:ownerOf(_horseId) == msg.sender");
        require(_isSire(_horseId),"HorseBreeding:!_isSire(_horseId)");
        require(_isReadyToBreed(_horseId),"HorseBreeding:!_isReadyToBreed(_horseId)");
        require((_duration == auctionDuration1) || (_duration == auctionDuration2) || (_duration == auctionDuration3), "!_duration");
        uint256 miniBreedPrice = getMiniBreedingPrice(_horseId);
