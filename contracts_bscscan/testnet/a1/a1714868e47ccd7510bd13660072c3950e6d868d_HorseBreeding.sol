        require(_isSire(_sireId),"HorseBreeding:!_isSire(_sireId)");
        require(_isMatron(_matronId),"HorseBreeding:!_isMatron(_matronId)");

        require(_isValidMatingPair(_matronId,_sireId),"HorseBreeding:!_isValidMatingPair(_matronId,_sireId)");

        require(siringAuction.isOnAuction(_sireId),"HorseBreeding:!siringAuction._isOnAuction(_sireId)");


