pragma solidity ^0.8.2;


contract FanDungeonStage {
    IFanDungeonLand public fanDungeonLand;
    IFanDungeonDecoration public fanDungeonDecoration;

    constructor(address _fanDungeonLand,address _fanDungeonDecoration) {
        require(
            _fanDungeonLand != address(0) &&
            _fanDungeonDecoration != address(0),
            "Stage: Cannot be zero address"
        );

        fanDungeonLand = IFanDungeonLand(_fanDungeonLand);
        fanDungeonDecoration = IFanDungeonDecoration(_fanDungeonDecoration);
    }
    
    mapping(uint256 => uint256) internal _fees;
    mapping(uint256 => uint256) internal _levelCaps;
    uint256 internal _levelCap = 20;
    
    function upgradeDungeonLevel(uint256 _dungeonId) public {
        require(_levelCaps[_dungeonId] + 1 <= _levelCap,"Stage: Reach the limit");
        _levelCaps[_dungeonId]++;
    }

    function setFee(uint256 _dungeonId,uint256 _fee) public {
        require(_fee<=10000,"Stage: fee");
        _fees[_dungeonId] = _fee;
    }

    function viewFee(uint256 _dungeonId) external view returns(uint256) {
        return _fees[_dungeonId];
    }

    function viewLevelCap(uint256 _dungeonId) external view returns(uint256) {
        if (_levelCaps[_dungeonId]==0) {
            return 1;
        }

        return _levelCaps[_dungeonId];
    }

    // private and internal function
    
}