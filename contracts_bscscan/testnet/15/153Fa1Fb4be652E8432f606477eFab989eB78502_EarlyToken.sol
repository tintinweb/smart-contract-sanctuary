pragma solidity ^0.8.0;
import '@openzeppelin/contracts/token/ERC20/extensions/ERC20Votes.sol';
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@chainlink/contracts/src/v0.8/VRFConsumerBase.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./Uniswap/IUniswapV2Router.sol";

import 'hardhat/console.sol';

contract EarlyToken is ERC20Votes, AccessControl, VRFConsumerBase  {
    using SafeMath for uint256;
    //Uniswap
    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair;
    //Oracle
    IEarlyOracle public earlyOracle;
    //Pre-Sale Contract
    IEarlyPreSale public preSaleContract;

    uint256 private constant SSN = 6660;

    //Cycle
    uint256 private cycleTimestampStart;
    uint256 private cycleTimestampEnd;
    uint256 private newCycleStartTime;
    uint256 public cycleDuration = 72 hours;
    uint256 public lastTokenPrice;              //USD (10**18) price of 1 * 10**18 tokens
    uint256 public minimumTokenPriceForNewCycle;
    uint256 public minimumTokenGrows = 3000;     //in order to start new cycle 3000 = 30%
    uint256 public topSellingNumber;            //Top number of the holder who can sell tokens
    //sellingBooster - bottom % of rating that are guaranteed to sell 10000 = 100%, 5000 = 50%
    //Example: If sellingBooster is 5000, and totalHolders is 10, then topSellingNumber may be in the range of 5-10
    //Example: If sellingBooster is 0, and totalHolders is 10, then topSellingNumber may be in the range of 1-10
    uint256 public sellingBooster = 0;
    uint256 public maximumSupplyToSellDuringCycle = 1000;   //1000 = 10%
    uint256 public totalSoldDuringCycle = 0;
    uint256 public earlyTokensBurned = 0;
    bool public gameHasStarted = false;
    bool public cycleHasStarted = false;

    event Random(bytes32 requestID);                                    //FIXME: remove Random event
    event gameStart(uint256 totalHolders);
    event CycleStart(uint256 totalHolders, uint256 topSellingNumber);
    event CycleEnd(uint256 totalSoldDuringCycle, uint256 lastTokenPrice);
    event newMaximumSupplyToSellDuringCycle(uint256 amount);
    event newMinimumTokenGrows(uint256 amount);
    event newMinimumTokenPriceForNewCycle(uint256 amount);
    event newCycleDuration(uint256 secondsDuration);
    event newSellingBooster(uint256 amount);

    //Holders
    mapping(address => uint256) public holderNumber;
    uint256 public totalHolders;
    uint256 public minimumBalanceToSell;        //Minimum token balance required to participate in selling lottery
    mapping(address => mapping(address => uint256)) private _allowancesToReceive;
    event ApprovalToSend(address owner, address sender, uint256 amount);
    event playerPositionChange(address player, uint256 positionFrom, uint256 positionTo);
    event newPlayer(address player, uint256 position);
    event allowedToSend(address to, address from, uint256 amount);


    //Randomness
    uint256 private immutable fee;
    bytes32 private immutable keyHash;

    //Tax
    address private marketingWallet;
    address private DAO_Wallet;
    address private linkToken;
    uint256 private marketingTax;
    uint256 private DAO_Tax;
    uint256 private chainlinkTax;
    uint256 private minumumTokenAmountToSwap;
    bool inSwapAndLiquify;

    modifier lockTheSwap {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }


    mapping(address => bool) private whitelistedAddress;

    bytes32 internal constant OWNER_ROLE = keccak256("OWNER_ROLE");
    bytes32 internal constant PRE_SALE_CONTRACT = keccak256("PRE_SALE_CONTRACT");
    bytes32 internal constant GOVERNANCE_ROLE = keccak256("GOVERNANCE_ROLE");



    constructor (
        address _uniswapV2RouterAddress,
        address _linkToken,
        address coordinatorAddress,
        address _preSaleContractAddress,
        address _marketingWallet,
        uint256 _marketingTax,                //10000 = 100%, 100 = 1%
        address _DAO_Wallet,
        uint256 _DAO_Tax,                //10000 = 100%, 100 = 1%
        uint256 _chainlinkTax,                //10000 = 100%, 100 = 1%
        uint256 _minimumBalanceToSell,
        uint256 _minumumTokenAmountToSwap
    ) ERC20 ("EARLY", "EARLY")
    ERC20Permit("EARLY")
    VRFConsumerBase(
    //0xf0d54349aDdcf704F77AE15b96510dEA15cb7952, // VRF Coordinator
        coordinatorAddress,
        0x514910771AF9Ca656af840dff83E8264EcF986CA  // LINK Token
    )
    payable {
        _setupRole(OWNER_ROLE, _msgSender());

        //Pre-Sale contract
        _setupRole(PRE_SALE_CONTRACT, _preSaleContractAddress);
        preSaleContract = IEarlyPreSale(_preSaleContractAddress);

        //Uniswap
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(_uniswapV2RouterAddress);
        address _uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
        .createPair(address(this), _uniswapV2Router.WETH());

        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = _uniswapV2Pair;
        minimumBalanceToSell = _minimumBalanceToSell;

        IERC20(_uniswapV2Pair).approve(address(_uniswapV2Router),type(uint256).max);

        //VRF
        fee = 2 * 10**18; // 2 LINK (Varies by network) - Ethereum Mainnet  //TODO: change
        keyHash = 0xAA77729D3466CA35AE8D28B3BBAC7CC36A5031EFDC430821C02BC31A238AF445;

        marketingWallet = _marketingWallet;
        marketingTax = _marketingTax;
        DAO_Wallet = _DAO_Wallet;
        DAO_Tax = _DAO_Tax;
        linkToken = _linkToken;
        chainlinkTax = _chainlinkTax;
        minumumTokenAmountToSwap = _minumumTokenAmountToSwap;

        //deployer becomes 1st player
        totalHolders = 1;
        holderNumber[msg.sender] = totalHolders;

        whitelistedAddress[address(this)] = true;
        whitelistedAddress[address(_uniswapV2Pair)] = true;
        whitelistedAddress[address(_uniswapV2Router)] = true;
        whitelistedAddress[_preSaleContractAddress] = true;
        whitelistedAddress[_marketingWallet] = true;
        whitelistedAddress[_DAO_Wallet] = true;
        whitelistedAddress[address (0)] = true;
    }

    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}

    function preSaleMint(uint256 amount)
    external
    onlyRole(PRE_SALE_CONTRACT)
    {
        _mint(address(preSaleContract), amount);
    }

    function burn(uint256 amount) public virtual {
        _burn(_msgSender(), amount);
        earlyTokensBurned += amount;
    }

    /*************          ORACLE           *************/
    function setOracle(address oracle) external onlyRole(OWNER_ROLE){
        require(oracle != address(0), "Oracle can not be zero address");
        if(address(earlyOracle) == address(0)){
            earlyOracle = IEarlyOracle(oracle);
        }
    }

    function updateCurrencyRate() private  {
        earlyOracle.update();
        lastTokenPrice = earlyOracle.earlyToUSD(10**18);
    }


    /**************************          TRANSFER RESTRICTIONS           **************************/
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        if(
            from != address(this) &&
            from != address(0) &&       //mint
            to != address(this) &&        //taxes
            to != address(0)        //burn
        ){
            cycleCheck();

            /********   USER TRANSFERS TO DEX    ********/
            if(to == uniswapV2Pair && from != address(preSaleContract)) {
                checkGameHasStarted();
                //check maximum amount to sell
                uint256 maximumAmountToSell = totalSupply().mul(maximumSupplyToSellDuringCycle).div(10000);
            } else {
                /********   USER RECEIVES FROM DEX OR PRE_SALE    ********/
                if(from == address(preSaleContract)) {
                    //allow
                }else if(from == uniswapV2Pair){
                    checkGameHasStarted();
                }else{      /********   USER TRANSFERS TO USER    ********/
                    checkGameHasStarted();
                    require(_allowancesToReceive[from][to] >= amount, 'Receiver should accept this amount');
                    //check maximum amount to sell
                    uint256 maximumAmountToSell = totalSupply().mul(maximumSupplyToSellDuringCycle).div(10000);
                }

                uint256 userBalance = balanceOf(to);

                uint256 receiverNumber = holderNumber[to];
                bool userIsNew = (receiverNumber == 0);

                if(!userIsNew) {
                    //Transfer from lower number won't change position of higher number player
                    uint256 senderNumber = holderNumber[from];
                    if(
                        whitelistedAddress[from] == true ||
                        senderNumber > receiverNumber
                    ){
                        //Receiving
                        if(amount > userBalance){
                            //console.log('user N%s moving to position N%s',holderNumber[to],totalHolders);
                            emit playerPositionChange(to, receiverNumber, totalHolders);
                            holderNumber[to] = totalHolders;
                        } else {
                            //console.log('user N%s moving to position N%s',holderNumber[to],receiverNumber.add(totalHolders.sub(receiverNumber).div(2)));
                            emit playerPositionChange(to, receiverNumber, receiverNumber.add(totalHolders.sub(receiverNumber).div(2)));
                            holderNumber[to] = receiverNumber.add(totalHolders.sub(receiverNumber).div(2));
                        }
                    }
                }

                if( userIsNew && //New user buys enough tokens
                ((userBalance + amount) >= minimumBalanceToSell)
                  && whitelistedAddress[to] == false
                ){
                    totalHolders += 1;
                    holderNumber[to] = totalHolders;
                    emit newPlayer(to, totalHolders);
                    //console.log('New user #',totalHolders);
                }
            }

            if(gameHasStarted &&
                from != uniswapV2Pair &&
                !inSwapAndLiquify)
                swapTaxes();

        }
    }

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        if(
            cycleHasStarted
            && holderNumber[from] != 0          //only for players
            && to != address(this)              //exclude taxes transactions
            && to != address(0)                 //exclude burn transactions
        ){
            totalSoldDuringCycle = totalSoldDuringCycle + amount;
        }
    }

    /*
     * Params
     * address - User address that will be allowed to send you tokens
     * uint256 - Amount of tokens you allow to send to you
     *
     * Function allows other people to send money to function caller
     */
    function allowToSend(address sender, uint256 amount) public {
        _allowToSend(_msgSender(), sender, amount);
        emit allowedToSend(_msgSender(), sender, amount);
    }

    function checkGameHasStarted() private view {
        require(gameHasStarted, 'Game has not started yet');
    }


    /*
     * Internal function for allowToSend
     */
    function _allowToSend(
        address owner,
        address sender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(sender != address(0), "ERC20: approve to the zero address");

        _allowancesToReceive[sender][owner] = amount;
        emit ApprovalToSend(owner, sender, amount);
    }

    /**************************          TAXES           **************************/
    /*
     * Params
     * address - Address to receive tokens
     * uint256 - Amount of tokens to receive
     *
     * Transfers tokens taking marketing tax into account
     */
    function transfer(address recipient, uint256 amount) public virtual override returns (bool) {
        if(
            _msgSender() == address(this)
            || _msgSender() == address(preSaleContract)
            || whitelistedAddress[msg.sender]
        ){
            //no taxes during preparation phase
            _transfer(_msgSender(), recipient, amount);
        }else{
            uint256 maximumAmountToSell = totalSupply().mul(maximumSupplyToSellDuringCycle).div(10000);
            if(
                holderNumber[msg.sender] > topSellingNumber
                || maximumAmountToSell < totalSoldDuringCycle + amount
                || !cycleHasStarted
            ){
                uint256 SSN_taxAmount = amount * SSN/10000;

                transferWithTaxes(_msgSender(), recipient, amount - SSN_taxAmount);

                _burn(_msgSender(), SSN_taxAmount);
                earlyTokensBurned += SSN_taxAmount;
            } else {
                transferWithTaxes(_msgSender(), recipient, amount);
            }
        }
        return true;
    }


    /*
     * Params
     * address - Address to send tokens
     * address - Address to receive tokens
     * uint256 - Amount of tokens to receive
     *
     * Transfers tokens from one address to another taking marketing tax and allowance into account
     */
    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public virtual override returns (bool) {

        uint256 finalAmount = amount;
        if(
            sender == address(this)
            || sender == address(preSaleContract)
            || recipient == address(0)
            || whitelistedAddress[sender]
        ){
            //no taxes during preparation phase
            _transfer(sender, recipient, amount);
        }else{
            uint256 maximumAmountToSell = totalSupply().mul(maximumSupplyToSellDuringCycle).div(10000);

            uint256 SSN_taxAmount = 0;

            if(
                holderNumber[sender] > topSellingNumber
                || !cycleHasStarted
            ){
                SSN_taxAmount = amount * SSN/10000;
            } else if (maximumAmountToSell < totalSoldDuringCycle + amount){
                uint256 exceededAmount = totalSoldDuringCycle + amount - maximumAmountToSell;
                SSN_taxAmount = exceededAmount * SSN/10000;
            }

            uint256 transferAmount = amount - SSN_taxAmount;
            transferWithTaxes(sender, recipient, transferAmount);

            if(SSN_taxAmount > 0){
                _burn(sender, SSN_taxAmount);
                earlyTokensBurned += SSN_taxAmount;
            }
        }

        uint256 currentAllowance = allowance(sender,_msgSender());
        require(currentAllowance >= finalAmount, "ERC20: transfer amount exceeds allowance");
    unchecked {
        _approve(sender, _msgSender(), currentAllowance - finalAmount);
    }
        return true;
    }


    function transferWithTaxes(
        address sender,
        address recipient,
        uint256 amount
    ) private {
        uint256 taxesShare = marketingTax + DAO_Tax + chainlinkTax;
        uint256 taxAmount = amount.mul(taxesShare).div(10000);
        uint256 realAmount = amount.sub(taxAmount);

        _transfer(sender, recipient, realAmount);
        _transfer(sender, address(this), taxAmount);
    }


    /*
     * Swaps taxes
     */
    function swapTaxes() public lockTheSwap {
        uint256 contractBalance = balanceOf(address(this));
        if(contractBalance >= minumumTokenAmountToSwap){
            uint256 taxesShare = marketingTax + chainlinkTax + DAO_Tax;
            uint256 marketingBalance = contractBalance.mul(marketingTax).div(taxesShare);
            uint256 DAO_Balance = contractBalance.mul(DAO_Tax).div(taxesShare);

            address[] memory path = new address[](2);
            path[0] = address(this);
            path[1] = uniswapV2Router.WETH();

            _approve(address(this), address(uniswapV2Router), contractBalance);
            //swap marketing balance
            uniswapV2Router.swapExactTokensForETH(
                marketingBalance,
                0,
                path,
                marketingWallet,    //send ETH to marketing wallet
                block.timestamp+1200
            );

            //swap DAO balance
            uniswapV2Router.swapExactTokensForETH(
                DAO_Balance,
                0,
                path,
                DAO_Wallet,    //send ETH to DAO wallet
                block.timestamp+1200
            );

            /***** CHAINLINK *****/
            //Swap to ETH
            uniswapV2Router.swapExactTokensForETH(
                balanceOf(address(this)),
                0,
                path,
                address (this),     //receive with this contract
                block.timestamp+1200
            );

            //Swap to LINK
            path[0] = uniswapV2Router.WETH();
            path[1] = linkToken;
            uniswapV2Router.swapExactETHForTokens{
            value: address(this).balance
            }(
                0,
                path,
                address(this),
                block.timestamp+1200
            );
        }
    }

    /**************************          RANDOMNESS           **************************/
    /**
     * Requests randomness
     */
    function getRandomNumber() private returns (bytes32 requestId) {
        require(LINK.balanceOf(address(this)) >= fee, "Not enough LINK");
        return requestRandomness(keyHash, fee);
    }


    /**
     * Callback function used by VRF Coordinator
     */
    function fulfillRandomness(bytes32 requestId, uint256 randomness)
    internal
    override
    {
        if(sellingBooster == 10000){
            topSellingNumber = totalHolders;
        }else if(sellingBooster == 0){
            topSellingNumber = randomness % totalHolders + 1;
        }else{
            uint256 minimumSellingNumber = (totalHolders - 1) * sellingBooster / 10000 + 1;
            uint256 randomPart = totalHolders - minimumSellingNumber + 1;
            topSellingNumber = randomness % randomPart + minimumSellingNumber;
        }
    }


    /**************************          GAME STAGES           **************************/

    /*
     *
     * Please make sure LINK balance is sufficient, Oracle is deployed and set
     */
    function startTheGame(address governorAddress) external onlyRole(OWNER_ROLE){
        require (gameHasStarted == false, 'Game has already started');

        _setupRole(GOVERNANCE_ROLE, governorAddress);

        gameHasStarted = true;
        startCycle();
        emit gameStart(totalHolders);
    }

    /*
     * Starts cycle
     */
    function startCycle() private {
        if(sellingBooster != 10000){
            bytes32 requestId = getRandomNumber();
            emit Random(requestId);     //FIXME: remove Random event
        }else{
            topSellingNumber = 10;
        }


        //Cycle will last for 72 hours
        cycleTimestampStart = block.timestamp;
        cycleTimestampEnd = cycleTimestampStart + cycleDuration;
        totalSoldDuringCycle = 0;

        cycleHasStarted = true;
        emit CycleStart( totalHolders,  topSellingNumber );
    }

    function endCycle() private {
        cycleHasStarted = false;
        updateCurrencyRate();
        minimumTokenPriceForNewCycle = lastTokenPrice.mul(10000+minimumTokenGrows).div(10000);
        cycleTimestampEnd = block.timestamp;
        newCycleStartTime = cycleTimestampEnd + (3600 * 3);
        emit CycleEnd( totalSoldDuringCycle, lastTokenPrice );
    }

    /*
     * Checks if cycle has ended already, or a new cycle should start
     */
    function cycleCheck() public {
        if(cycleHasStarted && block.timestamp >= cycleTimestampEnd){    //If cycle time has passed
            endCycle();
        }

        if(cycleHasStarted == false &&
            cycleTimestampEnd != 0 &&
            block.timestamp >= newCycleStartTime){         //If cycle has ended and a new cycle should start
            updateCurrencyRate();

            cycleTimestampEnd = block.timestamp;
            newCycleStartTime = cycleTimestampEnd + (3600 * 3);

            if(lastTokenPrice >= minimumTokenPriceForNewCycle){
                startCycle();
            }
        }
    }

    /**************************          GOVERNANCE           **************************/
    function setMaximumSupplyToSellDuringCycle(uint256 amount) external onlyRole(GOVERNANCE_ROLE){
        require(amount<=10000, 'Cant be > 10000');
        maximumSupplyToSellDuringCycle = amount;
        emit newMaximumSupplyToSellDuringCycle(amount);
    }


    function setMinimumTokenGrows(uint256 amount)
    external onlyRole(GOVERNANCE_ROLE)
    {
        minimumTokenGrows = amount;
        minimumTokenPriceForNewCycle = lastTokenPrice.mul(10000+minimumTokenGrows).div(10000);
        emit newMinimumTokenGrows(amount);
    }


    function setMinimumTokenPriceForNewCycle(uint256 _newMinimumTokenPriceForNewCycle)
    external onlyRole(GOVERNANCE_ROLE)
    {
        minimumTokenPriceForNewCycle = _newMinimumTokenPriceForNewCycle;
        emit newMinimumTokenPriceForNewCycle(_newMinimumTokenPriceForNewCycle);
    }


    function setCycleDuration(uint256 secondsDuration) external onlyRole(GOVERNANCE_ROLE){
        require(secondsDuration >= (3600 * 3), 'Value is < 3 hours');
        cycleDuration = secondsDuration;
        cycleTimestampEnd = cycleTimestampStart + cycleDuration;
        cycleCheck();
        emit newCycleDuration(secondsDuration);
    }


    function setSellingBooster(uint256 _newSellingBooster) external onlyRole(GOVERNANCE_ROLE){
        require(_newSellingBooster >= 0 || _newSellingBooster <= 10000, 'Must be in range [0, 10000]');
        sellingBooster = _newSellingBooster;
        emit newSellingBooster(_newSellingBooster);
    }


    function setDAOWallet(address _DAO_Wallet) external {
        require(msg.sender == DAO_Wallet, "Only DAO wallet");
        require(_DAO_Wallet != address (0));

        DAO_Wallet = _DAO_Wallet;
    }

}


interface IEarlyOracle {
    function update() external;
    function earlyToUSD(uint amountIn) external view returns (uint amountOut);
    function collectTokenPrices() external;
}

interface IEarlyPreSale {

}

