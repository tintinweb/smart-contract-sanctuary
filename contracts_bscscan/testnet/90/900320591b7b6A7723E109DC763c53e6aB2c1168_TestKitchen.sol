// File: contracts/BKIT.sol

pragma solidity ^0.8.0;





contract TestKitchen is AccessControlEnumerable {
    using SafeMath for uint256;
    // Variables
    address[] tokenPool;
    address wbnb;
    address bbft;
    address busd;
    address burnWallet;

    uint256 buyThreshold;
    uint256 sellPercentage;

    IUniswapV2Router02 router;

    mapping(address => bool) sellExclusion;

    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");

    modifier onlyOperator() {
        require(hasRole(OPERATOR_ROLE, msg.sender), "Not authorized");
        _;
    }

    modifier onlyAdmin() {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "Not authorized");
        _;
    }

    // Router mainnet: 0x10ED43C718714eb63d5aA57B78B54704E256024E
    // Router testnet: 0xD99D1c33F9fC3444f8101754aBC46c52416550D1
    // BUSD mainnet: 0xe9e7CEA3DedcA5984780Bafc599bD69ADd087D56
    // BUSD testnet: 0x8301F2213c0eeD49a7E28Ae4c3e91722919B8B47
    // BNB mainnet: 0xbb4CdB9CBd36B01bD1cBaEBF2De08d9173bc095c
    // BNB testnet: 0x094616F0BdFB0b526bD735Bf66Eca0Ad254ca81F

    constructor(
        address _bbft
    ) {
        router = IUniswapV2Router02(0xD99D1c33F9fC3444f8101754aBC46c52416550D1);
        bbft = _bbft;
        burnWallet = 0x000000000000000000000000000000000000dEaD;
        wbnb = 0x094616F0BdFB0b526bD735Bf66Eca0Ad254ca81F;
        busd = 0x8301F2213c0eeD49a7E28Ae4c3e91722919B8B47;
        _setupRole(OPERATOR_ROLE, msg.sender);
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    // Admin functions

    function addAuthAddress(address authorizedAddress) external onlyAdmin {
        grantRole(OPERATOR_ROLE, authorizedAddress);
    }

    function removeAuthAddress(address unauthorizedAddress) external onlyAdmin {
        _checkRole(OPERATOR_ROLE, unauthorizedAddress);
        revokeRole(OPERATOR_ROLE, unauthorizedAddress);
    }

    function viewAuthAddress() external onlyOperator view returns (address[] memory) {
        uint256 roleCount = getRoleMemberCount(OPERATOR_ROLE);
        address[] memory roleList = new address[](roleCount);

        for (uint256 i = 0; i < roleCount; i++) {
            roleList[i] = getRoleMember(OPERATOR_ROLE, i);
        }

        return (roleList);
    }

    // Buy functions

    // Add tokens to the BKIT Buy Pool
    function addPoolToken(address token) external onlyOperator {
        for (uint256 i = 0; i < tokenPool.length; i++) {
            require(tokenPool[i] != token, "token already in pool");
        }

        tokenPool.push(token);
    }

    // Remove tokens from the BKIT Buy Pool
    function removePoolToken(address token) external onlyOperator {
        for (uint256 i = 0; i < tokenPool.length; i++) {
            if (tokenPool[i] == token) {
                tokenPool[i] = tokenPool[tokenPool.length - 1];
                tokenPool.pop();

                break;
            }
        }
    }

    // Set the minimum number of BNB required to trigger the automatic BKIT Buy Pool buys
    function setPoolBNBMin(uint256 amount) external onlyOperator {
        buyThreshold = amount;
    }

    // Manually invoke the  BKIT Buy Pool buying process
    function buyPoolTokensManual() external onlyOperator {
        uint256 amountInWallet = IERC20(wbnb).balanceOf(address(this));
        uint256 amountBuyPerToken = (amountInWallet - 1) / tokenPool.length;

        for (uint256 i = 0; i < tokenPool.length; i++) {
            buySingleToken(tokenPool[i], amountBuyPerToken);
        }
    }

    // Manually buy a specified amount of a single token
    function buySingleToken(address token, uint256 amount) public onlyOperator {
        require(IERC20(wbnb).balanceOf(address(this)) >= amount, "Not enough BNB");

        address[] memory path = new address[](2);
        path[0] = wbnb;
        path[1] = token;
        router.swapExactETHForTokensSupportingFeeOnTransferTokens{
            value: amount
        }(0, path, address(this), block.timestamp + 100);
    }

    // View all of the tokens currently in the BKIT Buy Pool
    function viewPoolTokens() external onlyOperator view returns (address[] memory) {
        uint256 tokenCount = tokenPool.length;
        address[] memory tokenList = new address[](tokenCount);

        for (uint256 i = 0; i < tokenCount; i++) {
            tokenList[i] = tokenPool[i];
        }

        return (tokenList);
    }

    // Buyback and Burn functions

    // Set the percentage of token to sell during the BKIT Pool sell operation
    function setBNBSellPercent(uint256 percentage) external onlyOperator {
        require(percentage <= 100);
        sellPercentage = percentage;
    }

    // Exclude a specified token from being sold during the BKIT Pool sell operation
    function excludeBNBSell(address token, bool exclude) external onlyOperator {
        sellExclusion[token] = exclude;
    }

    // Set the address for the BBFT token
    function setBBFTaddress(address token) external onlyOperator {
        bbft = token;
    }

    // Swap a percentage of a specific token from the BKIT Pool for BNB
    function liquidateTokenBNB(address token, uint256 percentage)
        public
        onlyOperator
    {
        require(percentage <= 100, "percentage can't be more than 100");
        uint256 amountOfTokens = IERC20(token).balanceOf(address(this));
        uint256 amountToSell = (amountOfTokens * percentage) / 100;

        IERC20(token).approve(address(router), amountToSell);

        address[] memory path = new address[](2);
        path[0] = token;
        path[1] = wbnb;
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amountToSell,
            0,
            path,
            address(this),
            block.timestamp + 100
        );
    }

    // Swap a percentage of a specific token from the BKIT Pool for BUSD
    function liquidateTokenBUSD(address token, uint256 percentage)
        public
        onlyOperator
    {
        require(percentage <= 100, "percentage can't be more than 100");
        uint256 amountOfTokens = IERC20(token).balanceOf(address(this));
        uint256 amountToSell = (amountOfTokens * percentage) / 100;

        IERC20(token).approve(address(router), amountToSell);

        address[] memory path = new address[](2);
        path[0] = token;
        path[1] = wbnb;
        path[2] = busd;
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            amountToSell,
            0,
            path,
            address(this),
            block.timestamp + 100
        );
    }

    // Swaps a percentage of every token in the BKIT Pool for BBFT tokens
    function doBNBSell() external onlyOperator {
        for (uint256 i = 0; i < tokenPool.length; i++) {
            if (!sellExclusion[tokenPool[i]]) {
                address[] memory path = new address[](3);
                path[0] = tokenPool[i];
                path[1] = wbnb;
                path[2] = bbft;

                uint256 amountToSell = (IERC20(tokenPool[i]).balanceOf(
                    address(this)
                ) * sellPercentage) / 100;

                IERC20(tokenPool[i]).approve(address(router), amountToSell);

                router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
                    amountToSell,
                    0,
                    path,
                    address(this),
                    block.timestamp + 100
                );
            }
        }
    }

    // Sends all available BBFT tokens to the dead wallet
    function doBurnBBFT() external onlyOperator {
        uint256 amountToSend = IERC20(bbft).balanceOf(address(this));
        IERC20(bbft).transferFrom(address(this), burnWallet, amountToSend);
    }

    // Sends all available BUSD to the BBFT wallet for use in holder dsitribution
    function doDistribute() external onlyOperator {
        uint256 amountToSend = IERC20(busd).balanceOf(address(this));
        IERC20(busd).transferFrom(address(this), bbft, amountToSend);
    }
}