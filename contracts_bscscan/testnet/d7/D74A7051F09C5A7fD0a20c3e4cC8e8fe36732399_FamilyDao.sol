pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./interfaces/IERC20.sol";
import "./Family.sol";

contract FamilyDao is Ownable {
    using SafeMath for uint256;

    IERC20 public FMV;
    uint256 public minProposeAmount = 50 * 10 ** 18;
    uint256 public createAmount = 85 * 10 ** 18;
    uint256 public nftPrice = 100 * 10 ** 18;

    Family[] families;

    struct ProposalEntity {
        address addr;
        uint256 amount;
        uint256 time;
    }

    mapping(address => mapping(address => bool)) public hasProposal;
    mapping(address => ProposalEntity[]) public comingProposals;
    mapping(address => ProposalEntity[]) public sentProposals;

    mapping(address => Family) public familyMap;

    event ProposalSent(address from, address to, uint256 amount, uint256 time);
    event ProposalAccepted(address from, address to, uint256 amount, uint256 time);
    event ProposalCancelled(address from, address to, uint256 amount, uint256 time);
    event ProposalRejected(address from, address to, uint256 amount, uint256 time);
    event FamilyCreated(address father, address mother, uint256 time);

    constructor(IERC20 _FMV) {
        FMV = _FMV;
    }

    function getComingProposals(address account) external view returns(ProposalEntity[] memory proposals) {
        proposals = comingProposals[account];
    }

    function getSentProposals(address account) external view returns(ProposalEntity[] memory proposals) {
        proposals = sentProposals[account];
    }

    function sendProposal(address _to, uint256 _amount) external {
        require(msg.sender != _to, "Can't send proposal myself");
        require(_amount >= minProposeAmount, "send at least minimum amount");
        require(hasProposal[msg.sender][_to] == false, "proposal already exists");
        require(hasProposal[_to][msg.sender] == false, "proposal already exists");
        require(address(familyMap[msg.sender]) == address(0), "Family already exsits");
        require(address(familyMap[_to]) == address(0), "Family already exsits");
        
        FMV.transferFrom(msg.sender, address(this), _amount);
        
        hasProposal[msg.sender][_to] = true;

        uint256 time = block.timestamp;

        comingProposals[_to].push(
            ProposalEntity({
                addr: msg.sender,
                amount: _amount,
                time: time
            })
        );
        sentProposals[msg.sender].push(
            ProposalEntity({
                addr: _to,
                amount: _amount,
                time: time
            })
        );

        emit ProposalSent(msg.sender, _to, _amount, time);
    }

    function findProposal(address _from, address _to) internal view returns (uint256) {
        ProposalEntity memory proposal;
        for (uint i = 0; i < comingProposals[_to].length; i ++) {
            proposal = comingProposals[_to][i];
            if (proposal.addr == _from) {
                return i;
            }
        }
        return 0;
    }

    function clearSentProposal(address _from, address _to) internal {
        ProposalEntity memory proposal;
        for (uint i = 0; i < sentProposals[_from].length; i ++) {
            proposal = sentProposals[_from][i];
            if (proposal.addr == _to) {
                delete sentProposals[_from][i];
                return;
            }
        }
    }

    function acceptProposal(address _from) external {
        address to = msg.sender;

        require(hasProposal[_from][to] == true, "proposal does not exist");

        uint256 index = findProposal(_from, to);
        ProposalEntity memory proposal = comingProposals[to][index];

        uint256 amount = proposal.amount;
        FMV.transferFrom(msg.sender, address(this), amount);

        // refund to coming proposals
        for (uint i = 0; i < comingProposals[to].length; i ++) {
            proposal = comingProposals[to][i];
            if (proposal.addr != _from && proposal.time > 0) {
                FMV.transfer(proposal.addr, proposal.amount);
            }
        }
        delete comingProposals[to];

        for (uint i = 0; i < comingProposals[_from].length; i ++) {
            proposal = comingProposals[_from][i];
            if (proposal.addr != to && proposal.time > 0) {
                FMV.transfer(proposal.addr, proposal.amount);
            }
        }
        delete comingProposals[_from];

        // get refund from proposals sent
        for (uint i = 0; i < sentProposals[to].length; i ++) {
            proposal = sentProposals[to][i];
            if (proposal.addr != _from && proposal.time > 0) {
                FMV.transfer(to, proposal.amount);
            }
        }
        delete sentProposals[to];

        for (uint i = 0; i < sentProposals[_from].length; i ++) {
            proposal = sentProposals[_from][i];
            if (proposal.addr != to && proposal.time > 0) {
                FMV.transfer(to, proposal.amount);
            }
        }
        delete sentProposals[_from];

        // reset proposal
        hasProposal[_from][to] = false;
        hasProposal[to][_from] = false;

        emit ProposalAccepted(_from, msg.sender, amount, block.timestamp);

        // create family
        createFamily(_from, to, amount);
    }

    // cancel proposal sent
    function cancelProposal(address _to) external {
        address from = msg.sender;
        require(hasProposal[from][_to] == true, "proposal does not exist");

        uint256 index = findProposal(from, _to);
        uint256 amount = comingProposals[_to][index].amount;
        FMV.transfer(from, amount);
        delete comingProposals[_to][index];

        hasProposal[from][_to] = false;
        clearSentProposal(from, _to);

        emit ProposalCancelled(from, _to, amount, block.timestamp);
    }

    // reject coming proposal
    function rejectProposal(address _from) external {
        address to = msg.sender;
        require(hasProposal[_from][to] == true, "proposal does not exist");

        uint256 index = findProposal(_from, to);
        uint256 amount = comingProposals[to][index].amount;
        FMV.transfer(_from, amount);
        delete comingProposals[to][index];

        hasProposal[_from][to] = false;
        clearSentProposal(_from, to);

        emit ProposalRejected(_from, to, amount, block.timestamp);
    }

    function hasFamily(address account) external view returns (bool) {
        return address(familyMap[account]) != address(0);
    }

    function createFamily(address _from, address _to, uint256 _amount) internal {
        uint256 amount = _amount * 2 * 85 / 100;
        Family family = new Family(_from, _to, createAmount, amount);
        families.push(family);
        familyMap[_from] = family;
        familyMap[_to] = family;
        emit FamilyCreated(_from, _to, block.timestamp);
    }

    function childCreatable(address sender) external view returns (bool, address) {
        return familyMap[sender].creatable();
    }

    function createChild() external {
        Family family = familyMap[msg.sender];
        family.createChild(msg.sender);
    }

    function acceptChild() external {
        Family family = familyMap[msg.sender];
        family.acceptChild(msg.sender);
    }

    function cancelChild() external {
        Family family = familyMap[msg.sender];
        family.cancelChild();
    }

    function getChildCount(address sender) external view returns (uint256) {
        return familyMap[sender].childCount();
    }

    function getFamilyBalance(address sender) external view returns (uint256) {
        return familyMap[sender].balanceOf();
    }

    function getFamilyClaimable(address sender) external view returns (uint256) {
        return familyMap[sender].claimable();
    }

    function claimFromFamily() external {
        Family family = familyMap[msg.sender];
        family.claim();
    }

    function withdrawFromFamily(uint256 amount) external {
        Family family = familyMap[msg.sender];
        require (family.balanceOf() >= amount * 2, "Not enough balance to withdraw");

        family.withdraw(amount);

        (address father, address mother) = family.getPair();
        FMV.transfer(father, amount);
        FMV.transfer(mother, amount);
    }

    function depositToFamily(uint256 amount) external {
        Family family = familyMap[msg.sender];

        FMV.transferFrom(msg.sender, address(this), amount);
        family.deposit(msg.sender, amount);
    }

    function accetDepositToFamily() external {
        Family family = familyMap[msg.sender];

        (address sender, uint256 amount) = family.acceptDeposit(msg.sender);
        FMV.transferFrom(sender, address(this), amount);
    }

    function cancelDepositToFamily() external {
        Family family = familyMap[msg.sender];
        (address depositer, uint256 amount) = family.cancelDeposit();
        FMV.transfer(depositer, amount);
    }

    function depositInfo(address sender) external view returns (address, uint256) {
        return familyMap[sender].depositInfo();
    }

    function familyFarmed(address sender) external view returns (uint256) {
        return familyMap[sender].totalFarmed();
    }
}

