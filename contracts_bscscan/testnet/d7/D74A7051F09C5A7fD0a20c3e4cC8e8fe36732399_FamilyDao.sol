pragma solidity ^0.8.0;

import "./interfaces/IFamily.sol";

contract Family is IFamily {
    address dao;
    address father;
    address mother;

    uint256 balance = 0;

    struct ChildInfo {
        uint256 lastUpdated;
        uint256 amountFarmed;
    }
    ChildInfo[] childs;
    uint count = 0;
    address createSender = address(0);
    address depositSender = address(0);
    uint256 depositAmount;

    uint256 createAmount;
    uint256 farmPerDay;

    modifier onlyDao() {
        require(dao == msg.sender, "caller is not the dao");
        _;
    }

    constructor(address _father, address _mother, uint256 _createAmount, uint256 _balance) {
        dao = msg.sender;
        father = _father;
        mother = _mother;
        createAmount = _createAmount;
        farmPerDay = createAmount * 4 / 100;
        balance = _balance;
    }

    function createChild(address sender) override external onlyDao {
        // require(sender == father || sender == mother, "Not Family Member");
        require(createSender == address(0), "Request already created");
        require(balance >= createAmount, "Not enough family balance");

        createSender = sender;
    }

    function acceptChild(address sender) override external onlyDao {
        require(createSender != address(0), "Request not created");
        require(sender != createSender, "Need approval on other side");

        childs.push(ChildInfo({
            lastUpdated: block.timestamp,
            amountFarmed: 0
        }));
        count ++;

        createSender = address(0);

        balance -= createAmount;
    }

    function cancelChild() override external onlyDao {
        require(createSender != address(0), "Request not created");
        createSender = address(0);
    }

    function deposit(address sender, uint256 amount) override external onlyDao returns (address, uint256) {
        require(depositSender == address(0), "Request already exists");
        depositSender = sender;
        depositAmount = amount;

        return (sender, amount);
    }

    function acceptDeposit(address sender) override external onlyDao returns (address, uint256) {
        require(depositSender != address(0), "Request not created");
        require(depositSender != sender, "Need approval on other side");

        uint256 amount = depositAmount;
        balance = balance + amount * 2;

        depositSender = address(0);
        depositAmount = 0;

        return (sender, amount);
    }

    function cancelDeposit() override external onlyDao returns (address, uint256) {
        require(depositSender != address(0), "Request not created");

        address sender = depositSender;
        uint256 amount = depositAmount;

        depositSender = address(0);
        depositAmount = 0;

        return (sender, amount);
    }

    function claimAmount(uint i) internal view returns (uint256) {
        uint256 amount = farmPerDay * (block.timestamp - childs[i].lastUpdated) / 1 days;
        return amount;
    }

    function claimable() override external view returns (uint256) {
        uint256 amount = 0;
        for (uint i = 0; i < childs.length; i ++) {
            amount += claimAmount(i);
        }
        return amount;
    }

    function claim() override external onlyDao {
        for (uint i = 0; i < childs.length; i ++) {
            uint256 amount = claimAmount(i);
            balance += amount;
            childs[i].amountFarmed += amount;
            childs[i].lastUpdated = block.timestamp;
        }
    }
    
    function balanceOf() override external view returns(uint256) {
        return balance;
    }

    function childCount() override external view returns(uint256) {
        return count;
    }

    function getPair() override external view returns(address, address) {
        return (father, mother);
    }

    function withdraw(uint256 amount) override external onlyDao {
        balance = balance - amount * 2;
    }

    function depositInfo() override external view returns (address, uint256) {
        return (depositSender, depositAmount);
    }

    function creatable() override external view returns (bool, address) {
        return (createSender == address(0), createSender);
    }

    function totalFarmed() override external view returns (uint256) {
        uint256 amount = 0;
        for (uint i = 0; i < childs.length; i ++) {
            amount += childs[i].amountFarmed;
        }
        return amount;
    }
}

