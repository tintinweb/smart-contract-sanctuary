pragma solidity 0.8.10;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "../interfaces/IPancakePair.sol";
import "../interfaces/IPancakeRouter01.sol";

contract Zap is Ownable {
    using SafeMath for uint;
    using SafeERC20 for IERC20;

    // Settings
    IPancakeRouter01 public router;
    address public WBNB;
    address[] public pathFromToken0ToToken1;
    address[] public pathFromToken1ToToken0;
    // ten thousand
    uint256 percentMinAMount = 9600; // 96%

    // pancake route from token to token0
    mapping(address => address[]) pathFromTokenToToken0Paths;
    // pancake route from token to token1
    mapping(address => address[]) pathFromTokenToToken1Paths;

    constructor (address _router, address _WBNB) {
        router = IPancakeRouter01(_router);
        WBNB = _WBNB;
    }

    function zapIn(
        address token,
        address pair,
        uint256 amount
    ) external returns (uint256 liquidityBought) {
        address token0 = IPancakePair(pair).token0();
        address token1 = IPancakePair(pair).token1();

        IERC20(token).safeTransferFrom(msg.sender, address(this), amount);

        (uint256 halfAmount, uint256 halfSwapAmount) = _processSwap(token, amount, token0, token1);

        if (token1 == WBNB) {
            uint256 _amountETHMin;
            uint256 _amountTokenDesired;

            if (token == WBNB) {
                _amountETHMin = _minAmount(halfAmount);
                _amountTokenDesired = halfSwapAmount;
                // _approveTokenIfNeeded(IERC20(token0), halfSwapAmount, router);
                // _approveTokenIfNeeded(IERC20(token1), halfAmount, router);
            } else {
                _amountETHMin = _minAmount(halfSwapAmount);
                _amountTokenDesired = halfAmount;
                // _approveTokenIfNeeded(IERC20(token0), halfAmount, router);
                // _approveTokenIfNeeded(IERC20(token1), halfSwapAmount, router);
            }

            (,,liquidityBought) =  router.addLiquidityETH(token0,
                _amountTokenDesired,
                _minAmount(_amountTokenDesired),
                _amountETHMin,
                address(this),
                block.number);
    
        } else {         
            uint256 _amountTokenADesired;
            uint256 _amountTokenBDesired;
            if (token == token0) {
                _amountTokenADesired = halfAmount; 
                _amountTokenBDesired = halfSwapAmount;
            //     _approveTokenIfNeeded(IERC20(token0), halfAmount, router);
            //     _approveTokenIfNeeded(IERC20(token1), halfSwapAmount, router);
            // } else {
                _amountTokenADesired = halfSwapAmount; 
                _amountTokenBDesired = halfAmount;
                // _approveTokenIfNeeded(IERC20(token0), halfSwapAmount, router):
                // _approveTokenIfNeeded(IERC20(token1), halfAmount, router);
            }
            (,,liquidityBought) =  router.addLiquidity(token0,
                token1,
                _amountTokenADesired,
                _amountTokenBDesired,
                _minAmount(_amountTokenADesired),
                _minAmount(_amountTokenBDesired),
                address(this),
                block.number);                
        }
        IERC20(pair).safeTransfer(msg.sender, liquidityBought);
    }

    function zapOut(
        address token,
        address pair,
        uint256 liquidity,
        bool isWantPair
    ) external returns (uint256 token0Amount, uint256 token1Amount) {
        // find pair token
        address token0 = IPancakePair(pair).token0();
        address token1 = IPancakePair(pair).token1();

        // get those lp token
        IERC20(pair).safeTransferFrom(msg.sender, address(this), liquidity);

        // calculate liquidity
        (uint256 balance0, uint256 balance1,) = IPancakePair(pair).getReserves();
        uint256 amount0 = liquidity.mul(balance0) / IPancakePair(pair).totalSupply();
        uint256 amount1 = liquidity.mul(balance1) / IPancakePair(pair).totalSupply(); 

        // check if native token, then remove liquidity from pair for user
        if (token1 == WBNB) {
            (amount0, amount1) =  router.removeLiquidityETH(token0,
                liquidity,
                _minAmount(amount0),
                _minAmount(amount1),
                address(this),
                block.number);
        } else {
            (amount0, amount1) =  router.removeLiquidity(token0,
                token1,
                liquidity,
                _minAmount(amount0),
                _minAmount(amount1),
                address(this),
                block.number);                
        }

        // want pair, transfer each
        if (isWantPair) {
            IERC20(token0).safeTransfer(msg.sender, amount0);
            IERC20(token1).safeTransfer(msg.sender, amount1);
            token0Amount = amount0;
            token1Amount = amount1;
        } else {
            // want single token, proceed to swap
            uint256[] memory swapAmounts;
            if (token == token0) {
                swapAmounts = _swap(
                    amount1,
                    0,
                    pathFromToken1ToToken0,
                    address(this)
                );
                // transfer the desire token
                IERC20(token0).safeTransfer(msg.sender, amount0.add(swapAmounts[swapAmounts.length - 1]));
                token0Amount = amount0.add(swapAmounts[swapAmounts.length - 1]);
                token1Amount = 0;
            } else {
                swapAmounts = _swap(
                    amount0,
                    0,
                    pathFromToken0ToToken1,
                    address(this)
                );
                
                // transfer the desire token
                IERC20(token1).safeTransfer(msg.sender, amount1.add(swapAmounts[swapAmounts.length - 1]));
                token0Amount = 0;
                token1Amount = amount1.add(swapAmounts[swapAmounts.length - 1]);
            }
        }
    }

    function addPathTokenToToken0(address token, address[] calldata pathToToken) external onlyOwner {
        pathFromTokenToToken0Paths[token] = pathToToken;
    }

    function addPathTokenToToken1(address token, address[] calldata pathToToken) external onlyOwner {
        pathFromTokenToToken1Paths[token] = pathToToken;
    }

    function setPathFromToken0ToToken1(address[] memory _path) external onlyOwner {
        pathFromToken0ToToken1 = _path;
    }

    function setPathFromToken1ToToken0(address[] memory _path) external onlyOwner {
        pathFromToken1ToToken0 = _path;
    }

    function _minAmount(uint256 amount) private view returns (uint256) {
        return amount.mul(percentMinAMount.mul(1e6)).div(1e10);
    } 

    function _processSwap(address _token, uint256 _amount, address token0, address token1) private returns (uint256, uint256) {

        if (_token != token0 && _token != token1) {
            // not token0 or token1, swap each half to complete the pair
            uint256[] memory amountArr0 = _swap(
                _amount.div(2),
                0,
                pathFromTokenToToken0Paths[_token],
                address(this)
            );

            uint256[] memory amountArr1 = _swap(
                _amount.div(2),
                0,
                pathFromTokenToToken1Paths[_token],
                address(this)
            );
            return (amountArr0[amountArr0.length -1], amountArr1[amountArr1.length -1]);
        } else {
            // swap other half to complete the pair
            address[] memory pathToOther;

            if (_token == token0) {
                pathToOther = pathFromToken0ToToken1; 
            } else {
                pathToOther = pathFromToken1ToToken0;
            }
        
            uint256[] memory amounts = _swap(
                _amount.div(2),
                0,
                pathToOther,
                address(this)
            );
            return (_amount.div(2), amounts[amounts.length] - 1);
        }
    }

    function _swap(
        uint256 _inputAmount,
        uint256 _minOutputAmount,
        address[] memory _path,
        address _to
    ) private returns (uint256[] memory) {
        _approveTokenIfNeeded(
            IERC20(_path[0]),
            _inputAmount,
            address(router)
        );
        return router.swapExactTokensForTokens(
            _inputAmount,
            _minOutputAmount,
            _path,
            _to,
            block.timestamp
        );
    }

    function swapMock(
        uint256 _inputAmount,
        uint256 _minOutputAmount,
        address[] memory _path
    ) private returns (uint256[] memory) {
        return router.swapExactTokensForTokens(
            _inputAmount,
            _minOutputAmount,
            _path,
            msg.sender,
            block.timestamp
        );
    }

    function _getAmountsOut(uint256 amount, address[] memory path) private view returns (uint256) {
        uint256[] memory amountOuts = router.getAmountsOut(amount, path);
        return amountOuts[amountOuts.length - 1];
    }

    function _approveTokenIfNeeded(
        IERC20 _token,
        uint256 _amount,
        address _spender
    ) private {
        if (_token.allowance(address(this), _spender) < _amount) {
            _token.safeIncreaseAllowance(_spender, _amount);
        }
    }
}

