pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./GardenItem.sol";

contract Market is ERC1155Holder, Ownable {
    GardenItem public immutable  gardenItem;
    uint256 public immutable createdAt;

    mapping(uint256 => uint256) private _prices;
    mapping(address => bool) private _freeClaims;

    uint256 public constant FREE_CROPS = 2;
    uint256 public constant MAX_CROPS = 24;
    uint256 public constant DUNG_INCREASE_PRICE_PER_DAY_PERCENT = 2;
    uint256 public constant WATER_INCREASE_PRICE_PER_DAY_PERCENT = 2;
    uint256 public constant WEATHER_INCREASE_PRICE_PER_DAY_PERCENT = 2;
    uint256 public constant SECONDS_IN_A_DAY = 86400;

    constructor(address _gardenItem) {
        gardenItem = GardenItem(_gardenItem);
        createdAt = block.timestamp;
    }

    function price(uint256 id) external view virtual returns (uint256) {
        return _prices[id];
    }

    function setPrice(uint256 id, uint256 _price) external onlyOwner {
        _prices[id] = _price;
    }

    function hasClaimed(address _address) public view virtual returns (bool) {
        return _freeClaims[_address];
    }

    function claimFreeCrops() external {
        require(!_freeClaims[msg.sender], 'Already claimed.');

        uint256 senderCrops = gardenItem.balanceOf(msg.sender, 0);
        require(senderCrops < MAX_CROPS, 'Crops limit exceeded.');

        gardenItem.mint(address(this), 0, FREE_CROPS);
        gardenItem.safeTransferFrom(address(this), msg.sender, 0, FREE_CROPS, "");

        _freeClaims[msg.sender] = true;
    }

    function calculateCropPrice(address _address, uint256 amount) public view virtual returns (uint256) {
        require(amount > 0, 'Amount should be more than 0.');
        require(hasClaimed(_address), 'Must claim free.');

        uint256 _price = _prices[gardenItem.CROP()];
        require(_price > 0, 'Price not defined.');

        uint256 senderCrops = gardenItem.balanceOf(msg.sender, 0);
        require(senderCrops + amount <= MAX_CROPS, 'Exceeds crop limit.');

        uint256 start = senderCrops - FREE_CROPS + 1;

        uint256 startPrice = _price;
        for (uint i = 0; i < start - 1; i++) {
            startPrice = startPrice + startPrice * 50 / 100;
        }

        uint256 lastPrice = startPrice;
        uint256 total = lastPrice;

        for (uint i = 1; i <= amount - 1; i++) {
            lastPrice = lastPrice + lastPrice * 50 / 100;
            total += lastPrice;
        }

        return total;
    }

    function buyCrop(uint256 amount) external payable {
        uint256 _price = calculateCropPrice(msg.sender, amount);

        require(msg.value == _price, 'Invalid amount.');

        gardenItem.mint(address(this), gardenItem.CROP(), amount);
        gardenItem.safeTransferFrom(address(this), msg.sender, gardenItem.CROP(), amount, "");
    }

    function calculateDungPrice(uint256 amount) public view virtual returns (uint256) {
        require(amount > 0, 'Amount should be more than 0.');
        uint256 _price = _prices[gardenItem.DUNG()];
        require(_price > 0, 'Price not defined.');

        uint256 daysPassed = (block.timestamp - createdAt) / SECONDS_IN_A_DAY;

        uint256 lastPrice = _price;
        for (uint i = 0; i < daysPassed; i++) {
            lastPrice += lastPrice * DUNG_INCREASE_PRICE_PER_DAY_PERCENT / 100;
        }

        uint256 total = lastPrice * amount;

        return total;
    }

    function buyDung(uint256 amount) external payable {
        uint256 _price = calculateDungPrice(amount);

        require(msg.value == _price, 'Invalid amount.');

        uint256 balance = gardenItem.balanceOf(address(this), gardenItem.DUNG());
        require(balance >= amount, "Item out of stock.");

        gardenItem.safeTransferFrom(address(this), msg.sender, gardenItem.DUNG(), amount, "");
    }

    function calculateWaterPrice(uint256 amount) public view virtual returns (uint256) {
        require(amount > 0, 'Amount should be more than 0.');
        uint256 _price = _prices[gardenItem.WATER()];
        require(_price > 0, 'Price not defined.');

        uint256 daysPassed = (block.timestamp - createdAt) / SECONDS_IN_A_DAY;

        uint256 lastPrice = _price;
        for (uint i = 0; i < daysPassed; i++) {
            lastPrice += lastPrice * WATER_INCREASE_PRICE_PER_DAY_PERCENT / 100;
        }

        uint256 total = lastPrice * amount;

        return total;
    }

    function buyWater(uint256 amount) external payable {
        uint256 _price = calculateWaterPrice(amount);

        require(msg.value == _price, 'Invalid amount.');

        uint256 balance = gardenItem.balanceOf(address(this), gardenItem.WATER());
        require(balance >= amount, "Item out of stock.");

        gardenItem.safeTransferFrom(address(this), msg.sender, gardenItem.WATER(), amount, "");
    }

    function calculateWeatherPrice(uint256 amount) public view virtual returns (uint256) {
        require(amount > 0, 'Amount should be more than 0.');
        uint256 _price = _prices[gardenItem.WEATHER()];
        require(_price > 0, 'Price not defined.');

        uint256 daysPassed = (block.timestamp - createdAt) / SECONDS_IN_A_DAY;

        uint256 lastPrice = _price;
        for (uint i = 0; i < daysPassed; i++) {
            lastPrice += lastPrice * WEATHER_INCREASE_PRICE_PER_DAY_PERCENT / 100;
        }

        uint256 total = lastPrice * amount;

        return total;
    }

    function buyWeather(uint256 amount) external payable {
        uint256 _price = calculateWeatherPrice(amount);

        require(msg.value == _price, 'Invalid amount.');

        uint256 balance = gardenItem.balanceOf(address(this), gardenItem.WEATHER());
        require(balance >= amount, "Item out of stock.");

        gardenItem.safeTransferFrom(address(this), msg.sender, gardenItem.WEATHER(), amount, "");
    }

    function withdraw(uint256 value) external payable onlyOwner {
        require(value > 0, 'Wrong value.');
        require(address(this).balance >= value, 'Not enough funds.');

        address payable wallet = payable(owner());
        wallet.transfer(value);
    }
}

