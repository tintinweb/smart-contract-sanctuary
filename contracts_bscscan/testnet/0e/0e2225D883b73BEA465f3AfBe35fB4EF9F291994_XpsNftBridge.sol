pragma solidity 0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/introspection/IERC165.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";

contract XpsNftBridge is Ownable, ERC721Holder, ERC1155Holder {
    using SafeMath for uint;
    bool public paused;

    bytes4 private constant INTERFACE_SIGNATURE_ERC165 = 0x01ffc9a7;
    
    bytes4 private constant INTERFACE_SIGNATURE_ERC1155 = 0xd9b67a26;

    //whitelist on
    uint8 private constant WL_ON = 1;

    // Map from nft address to nft type.
    //auctionIdToAuction[auctionId]
    mapping(address => uint8) whitelistNft;

    event SwapNFT(address nftAddress, uint256 nftId,string receiver);

    modifier whenNotPause() {
        require(!paused, "Paused");
        _;
    }

    address private constant BINANCE_SC_ADDRESS = 0x333B52BADCF7C53c60Ac1a352eA2EEf188B3d131;

    address private constant SOTA_SC_ADDRESS = 0x22b57acbb5F28Ce1D447B2b0a1a7C5f8765A7dA5;
    // uint256 private constant HYPERPAY_TOKEN_ID = 1;
    // uint256 private constant ONTO_TOKEN_ID = 2;
    // uint256 private constant CHAIR_TOKEN_ID = 3;

    address private constant LIQUIDITFY_SC_ADDRESS = 0xF176f55c102Db5c2F9988D4d4A0004Fbb31f8D8E;
    // uint256 private constant LIQUIDITFY_TOKEN_ID = 1793573;

    modifier whenPause() {
        require(paused, "!Paused");
        _;
    }

     constructor() {
          whitelistNft[BINANCE_SC_ADDRESS]  = WL_ON;
          whitelistNft[SOTA_SC_ADDRESS]  = WL_ON;
          whitelistNft[LIQUIDITFY_SC_ADDRESS]  = WL_ON;
     }

    /// @dev Swap NFT from BSC to WAX, assigning ownership to this contract.
    /// @param nftAddress - Address of token whose approval to verify.
    /// @param nftId - ID of token whose approval to verify.
    /// @param receiver - WAX account name
    function swapNFT(address nftAddress, uint256 nftId,string memory receiver) external {
        require(whitelistNft[nftAddress] == WL_ON, "Invalid.");
        require(IERC165(nftAddress).supportsInterface(INTERFACE_SIGNATURE_ERC165) == true);
        // Check if NFT is ERC1155
        if (IERC165(nftAddress).supportsInterface(INTERFACE_SIGNATURE_ERC1155) == true){
            IERC1155(nftAddress).safeTransferFrom(msg.sender, address(this), nftId, 1, abi.encodePacked(keccak256("onERC1155Received(address,address,uint256,uint256,bytes)")));
        }else{
            //NFT is ERC721
            IERC721(nftAddress).safeTransferFrom(msg.sender, address(this), nftId, abi.encodePacked(keccak256("onERC721Received(address,address,uint256,bytes)")));
        }

        emit SwapNFT(nftAddress, nftId, receiver);
            
    }
    //Set NFT whitelist
    function setNFTWhitelist(address nftAddress, uint256 status) external onlyOwner() {
        require(IERC165(nftAddress).supportsInterface(INTERFACE_SIGNATURE_ERC165) == true);
        whitelistNft[nftAddress] = uint8(status);
    }

    //Check NFT is whitelist ot not
    function isNFTWhitelist(address nftAddress) external view returns (bool){
        return (whitelistNft[nftAddress] == WL_ON);
    }


}

