pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

contract AuthController is Initializable{
    enum AdminType{ NONE, OWNER, ADMIN, TRADERS }

    mapping(address => AdminType) adminMap;

    modifier onlyOwner() {
        require(adminMap[msg.sender] == AdminType.OWNER, 'access deny');
        _;
    }

    modifier onlyAdmin() {
        require(adminMap[msg.sender] == AdminType.ADMIN || adminMap[msg.sender] == AdminType.OWNER, 'access deny');
        _;
    }

    function addAuthority(address admin, AdminType auth) public onlyOwner {
        adminMap[admin] = auth;
    }

    function showAuthority(address admin) public view virtual returns (uint) {
        return uint(adminMap[admin]);
    }

    function __AuthController_init() internal initializer {
        __AuthController_init_unchained();
    }

    function __AuthController_init_unchained() internal initializer {
        adminMap[msg.sender] = AdminType.OWNER;
    }
}

