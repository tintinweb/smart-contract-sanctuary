// File: readygo67/contracts/OfflineOTC/contracts/Arbitrator.sol


pragma solidity ^0.8.0;





// An extension of Access Control to include multi owner approval to execute key actions
// in this first version every signatorie has the same privileges and after collecting
// the minimum signatures necessary will give a ticket to any admins to execute the given
// action.
// TODO: Specify the inputs of the function to be executed

contract Arbitrator is Context{
    using SafeMath for uint256;

    enum Status {
        NON_EXIST,
        PENDING,
        ARBITRATED
    }
    struct Arbitration{
        uint256 sellerWinCount;
        uint256 buyerWinCount;
        uint256 minQuorum;   //snapshot
        address[] arbitrators; //snapshot
        Status status;
    }

    address public governance;
    address public pendingGovernance;
    address[] public arbitrators;

    address public dex;
    mapping(address => mapping(uint256 => Arbitration)) private arbitrations;
    mapping(address => mapping(uint256 => mapping(address => bool))) public hasVoted;
    uint256 public minQuorum;

    event PendingGovernanceUpdated(address);
    event GovernanceUpdated(address);
    event MinQuorumChange(uint256, uint256);
    event ArbitratorAdded(address indexed);
    event ArbitratorRemoved(address indexed);
    event ArbitrationRaised(address indexed, uint256);
    event ArbitrationVoted(address indexed, uint256, address, bool);

    modifier onlyGovernance() {
        require(governance == _msgSender(), "only governance");
        _;
    }

    modifier onlyDex(){
        require(dex == _msgSender(), "only dex");
        _;
    }

    constructor(
        address _dex,
        address[] memory _arbitrators,
        uint256 _minQuorum
    ) {
        // register arbitrators
        for (uint256 i = 0; i < _arbitrators.length; i++) {
            arbitrators.push(_arbitrators[i]);
            emit ArbitratorAdded(_arbitrators[i]);
        }

        governance = _msgSender();
        minQuorum = _minQuorum;
        dex = _dex;
        emit MinQuorumChange(0, minQuorum);
    }

    function setPendingGovernance(address _pendingGovernance) external onlyGovernance {
        require(_pendingGovernance != address(0), "pending governance address cannot be 0x0");
        pendingGovernance = _pendingGovernance;
        emit PendingGovernanceUpdated(pendingGovernance);
    }

    function acceptGovernance() external {
        require(msg.sender == pendingGovernance, "only pending governance");
        governance = pendingGovernance;
        emit GovernanceUpdated(governance);
    }

    function setMinQuorum(uint256 _minQuorum) external onlyGovernance {
        require(_minQuorum >= arbitrators.length.div(2), "minQuorum must equal or greater than 1/2 of arbitrators number");
        require(_minQuorum <= arbitrators.length, "minQuorum must equal or less than arbitrators number");

        emit MinQuorumChange(minQuorum, _minQuorum);
        minQuorum = _minQuorum;
    }

    function addArbitrator(address _account) external onlyGovernance returns(bool) {
        for (uint256 i =0; i< arbitrators.length; i++){
            if (arbitrators[i] == _account){
                return false;
            }
        }
        arbitrators.push(_account);
        emit ArbitratorAdded(_account);
        return true;
    }


    function removeArbitrator(address _account) external onlyGovernance returns(bool) {
        for (uint256 i = 0; i < arbitrators.length; i++){
            if (arbitrators[i] == _account){
                require(arbitrators.length > minQuorum, "arbitrators number must greater than or equal to minQuorum");
                arbitrators[i] = arbitrators[arbitrators.length - 1];
                arbitrators.pop();
                emit ArbitratorRemoved(_account);
                return true;
            }
        }

        return false;
    }

    function raiseArbitration(address _token, uint256 _id) external onlyDex {
        require(arbitrations[_token][_id].status == Status.NON_EXIST, "arbitration already exists");
        arbitrations[_token][_id].status = Status.PENDING;
        arbitrations[_token][_id].minQuorum = minQuorum;

        for (uint256 i = 0; i < arbitrators.length; i++){
            arbitrations[_token][_id].arbitrators.push(arbitrators[i]);
        }

        emit ArbitrationRaised(_token, _id);
    }

    function voteArbitration(address _token, uint256 _id, bool _sellerWin) external {
        Arbitration storage arb = arbitrations[_token][_id];

        require(arb.status != Status.NON_EXIST, "status must be PENDING and ARBITRATED");
        require(!hasVoted[_token][_id][_msgSender()], "already voted");
        bool isArbitrator = false;
        for (uint256 i = 0; i < arbitrators.length; i++){
            if (arb.arbitrators[i] == _msgSender()){
                isArbitrator = true;
            }
        }

        require(isArbitrator, "must be a arbitrator");
        if (_sellerWin){
            arb.sellerWinCount ++;
        }else{
            arb.buyerWinCount ++;
        }

        hasVoted[_token][_id][_msgSender()] =true;
        emit ArbitrationVoted(_token, _id, _msgSender(), _sellerWin);

        if (arb.status == Status.ARBITRATED){
            return;
        }

        if (arb.sellerWinCount >= arb.minQuorum){
            IDex(dex).resolveArbitration(_token, _id, true);
            arb.status = Status.ARBITRATED;
        }else if (arb.buyerWinCount >= arb.minQuorum){
            IDex(dex).resolveArbitration(_token, _id, false);
            arb.status = Status.ARBITRATED;
        }
    }

    receive() external payable {}

    function allArbitrators( ) external view returns (address[] memory) {
        return arbitrators;
    }

    function arbitration(address _token, uint256 _id ) external view returns (Arbitration memory) {
        return arbitrations[_token][_id];
    }
}