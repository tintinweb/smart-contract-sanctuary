  event Approval(address indexed owner, address indexed spender, uint value);
  event Transfer(address indexed from, address indexed to, uint value);
  event Sync(uint256 reserveSDB, uint256 reserveUSD);
  event replanishmentSDB(uint sdbamount);
  // event Approval(address indexed owner, address indexed spender, uint value);
  uint private unlocked = 1;
  modifier lock(){
    require(unlocked == 1, 'LOCKED');
    unlocked = 0;
    _;
    unlocked = 1;
  }
  
  constructor(address _sdbtoken, address _usdtoken,address sdbowner) public {
    sdbtoken = _sdbtoken;
    usdtoken = _usdtoken;
    _transferOwnership(sdbowner);
  }
   
  function freezeAccount(address target, bool freeze) onlyOwner public {
        frozenAccount[target] = freeze;
        emit FrozenFunds(target, freeze);
    }

  function freezeContract(bool freeze) onlyOwner public {
        isContractFrozen = freeze;
        emit FrozenContract(freeze);           // triggers network event
    }

  function getReserves() public view returns(uint256 _reserveSDB,uint256 _reserveUSD, uint32 _blockTimestampLast){
    _reserveSDB = reserveSDB;
    _reserveUSD = reserveUSD;
    _blockTimestampLast = blockTimestampLast;
  }
  function _update(uint balanceSDB, uint balanceUSD, uint256 _reserveSDB,uint256 _reserveUSD)private{
    require(balanceSDB<=uint256(-1)&& balanceUSD <=uint256(-1), 'OVERFLOW');
    uint32 blockTimestamp = uint32(block.timestamp%2**32);
    uint32 timeElapsed = blockTimestamp - blockTimestampLast;
    require (timeElapsed > 0 );
    reserveSDB = uint256(balanceSDB);
    reserveUSD = uint256(balanceUSD);
    blockTimestampLast = blockTimestamp;
    emit Sync(reserveSDB, reserveUSD);
  }
  
  function changeTokenPerUSD(uint256 newtokensPerUSD) public onlyOwner{
    tokensPerUSD = newtokensPerUSD;
  }
  

  function buyTokens(address recipient, uint256 amountUSD) external  returns(uint amountSDB) {
    require (amountUSD > 0 ,'not enought amount USD');
    require (reserveSDB > 0, 'not enought amount SDB');
    uint amountSDBforUSD;
    amountSDBforUSD = amountUSD * tokensPerUSD;
    reserveSDB -=amountSDBforUSD;
    require (reserveSDB >amountSDBforUSD , 'not enought SDB to pay');  
    uint balanceSDB;
    uint balanceUSD;
    address _sdbtoken = sdbtoken;
    address _usdtoken = usdtoken;
    if (amountUSD > 0)_safeTransfer(sdbtoken, recipient, amountSDBforUSD);
    reserveUSD += amountUSD;
    reserveSDB -= amountSDB;
    balanceSDB = IERC20(_sdbtoken).balanceOf(address(this));
    balanceUSD = IERC20(_usdtoken).balanceOf(address(this));   
    // IERC20(sdbtoken).transfer(recipient, amountSDB);
    
  }
    
  function transferUSD(address owner, uint256 amountUSD) external onlyOwner returns(bool){
  require (reserveUSD !=0);
  reserveUSD-=amountUSD;
  _safeTransfer(usdtoken, owner, amountUSD);
  }

  function giveSDB( uint sdbamount) public {
    if (sdbamount > 0) _safeTransfer(sdbtoken, address(this), sdbamount);
    reserveSDB += sdbamount;
    emit replanishmentSDB(sdbamount);
  }







  function skim(address to) external lock {
   address _sdbtoken = sdbtoken; // gas savings
   address _usdtoken = usdtoken; // gas savings
    _safeTransfer(_sdbtoken, to, IERC20(_sdbtoken).balanceOf(address(this)).sub(reserveSDB));
    _safeTransfer(_usdtoken, to, IERC20(_usdtoken).balanceOf(address(this)).sub(reserveUSD));
    }

    // force reserves to match balances
  function sync() external lock {
        _update(IERC20(sdbtoken).balanceOf(address(this)), IERC20(usdtoken).balanceOf(address(this)), reserveSDB, reserveSDB);
    }
    function _safeTransfer(address token, address to, uint value) private{
      (bool success, bytes memory data) = token.call(abi.encodeWithSelector(SELECTOR, to, value));
      require(success && (data.length == 0 || abi.decode(data, (bool))), 'Vendor : Transfer_failed');
    }
  
  
}