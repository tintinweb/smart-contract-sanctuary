contract Forwarder is Ownable{
    using SafeMath for uint256;
    using SafeBEP20 for IBEP20;
    
    address[] public token;
    
    struct tokenStore{
        uint256 deposit;
        uint256 withdraw;
        uint256 lastTimeStamp;
        bool status;
    }
    
    mapping (address => tokenStore) private tokenData;
    
    address private immutable masterWallet;
    
    constructor (address _masterWallet) {
        masterWallet = _masterWallet;
        
        transferOwnership(_masterWallet);
    }

    receive() external payable {}
  
    function deposit(address tokenAddr,uint256 amount) external {
        IBEP20(tokenAddr).safeTransferFrom(msg.sender,address(this),amount);
        
        if(!tokenData[tokenAddr].status){
            token.push(tokenAddr);
        }
        
        tokenData[tokenAddr].deposit = tokenData[tokenAddr].deposit.add(amount);
        tokenData[tokenAddr].lastTimeStamp = block.timestamp;
        tokenData[tokenAddr].status = true;
    }
    
    function withdraw(address tokenAddr,uint256 amount) public {
        require(msg.sender == masterWallet, "unable to access");
        
        IBEP20(tokenAddr).safeTransfer(masterWallet,amount);
    }
    
    function contractTokenBalance(IBEP20 tokenAddr) public view returns (uint256) {
        return tokenAddr.balanceOf(address(this));
    }
    
    function allLength() public view returns (uint256){
        return token.length;
    }
    
    function tokenDetails(address tokenAddr) public view returns (
        uint256 _deposit,
        uint256 _withdraw,
        uint256 _balanceOf,
        uint256 _lastTimeStamp,
        bool _status){
            
         _deposit = tokenData[tokenAddr].deposit;
         _withdraw = tokenData[tokenAddr].withdraw;
         _balanceOf = IBEP20(tokenAddr).balanceOf(address(this));
         _lastTimeStamp = tokenData[tokenAddr].lastTimeStamp;
         _status = tokenData[tokenAddr].status;
    }
}

contract Factory is Ownable{
     
     address[] public allPair;
     mapping (address => address) public pair;
     
     
     function pairLength() public view returns (uint256) {
         return allPair.length;
     }
     
     function create(address masterWallet) public onlyOwner {
         require(pair[masterWallet] == address(0), "Already have pair");
         address implAddr = address(new Forwarder{salt: keccak256(abi.encode(masterWallet, address(this)))}(masterWallet));
         
         allPair.push(implAddr);
         pair[masterWallet] = implAddr;
     }
}