    event Withdrawn(uint256 amount, address indexed wallet);
    mapping(address => uint256) public redeemBalanceMap;
    uint256 public totalRedeemAmount;
    
    constructor(address _newBaseToken) {
        baseTokenAddress = _newBaseToken;
        totalRedeemAmount = 0;
    }
    
    function addRedeemCandidates(address[] memory candidates, uint256[] memory amounts) external onlyOwner {
        require(candidates.length > 0, "Empty candidates");
        require(candidates.length == amounts.length, "data is not mismatched");
        for(uint i = 0; i < candidates.length; i ++) {
            redeemBalanceMap[candidates[i]] = redeemBalanceMap[candidates[i]].add(amounts[i]);
            totalRedeemAmount = totalRedeemAmount.add(amounts[i]);
        }
    }
    // set baseTokenAddress
    function setBaseTokenAddress(address _newBaseTokenAddress) external onlyOwner {
        baseTokenAddress = _newBaseTokenAddress;
    }

    /**
     * Redeem tokens from contract. Only owner can call this function
     * @param realAmountTokens token amount to withdraw
    */
    function redeemToken(uint256 realAmountTokens) external {
        require(realAmountTokens > 0, "You need to withdraw at least some tokens");
        require(redeemBalanceMap[msg.sender] > realAmountTokens, "insufficient redeem balance");
        uint256 contractTokenBalance = IERC20(baseTokenAddress).balanceOf(address(this));
        require(contractTokenBalance > realAmountTokens, "insufficient contract balances");
        
