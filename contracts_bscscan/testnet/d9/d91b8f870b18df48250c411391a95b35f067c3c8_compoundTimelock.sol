contract compoundTimelock is Ownable, Pausable {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    struct Staking {
        uint256 principal;
        uint256 block;
        uint256 timelock;
        uint256 reward;
    }

    uint256 public constant RATE = 100;
    uint256 public constant AVG_BLOCK_PER_DAY = 28800;
    uint256 public constant DURATION = 10080; // 7 days

    IERC20 public token;

    mapping(address => Staking) public account;

    event Staked(address indexed user, uint256 amount, uint256 principal); // staked recorded.
    event Interest(address indexed user, uint256 interest); // reward distributed.
    event Withdrawn(address indexed user, uint256 amount); // total amount withdrawn, including reward.

    constructor(address _token) {
        token = IERC20(_token);
    }

    function stake(uint256 amount) external whenNotPaused {
        require(token.balanceOf(msg.sender) > amount, "Insufficient balance");
        require(token.allowance(msg.sender, address(this)) >= amount, "Insufficient allowances");

        uint256 pReward = reward(msg.sender); // calculate previous interest.
        uint256 sum = amount.add(pReward); // previous interest added to principal.
        account[msg.sender].principal = account[msg.sender].principal.add(sum);
        account[msg.sender].block = block.number;
        account[msg.sender].timelock = block.timestamp;
        account[msg.sender].reward = 0;

        token.safeTransferFrom(msg.sender, address(this), amount);
        emit Staked(msg.sender, amount, account[msg.sender].principal);
        emit Interest(msg.sender, pReward);
    }

    function withdraw() external whenNotPaused {

        require(account[msg.sender].principal > 0, "Nothing to withdraw.");
        require(account[msg.sender].timelock.add(DURATION * 1 minutes) < block.timestamp, "Unable to withdraw before the timelock");
        
        uint256 pReward = reward(msg.sender);
        uint256 principal = account[msg.sender].principal;
        uint256 amount = principal.add(pReward);

        require(token.balanceOf(address(this)) >= amount, "Insufficient balance in contract.");

        account[msg.sender].principal = 0;
        account[msg.sender].reward = 0;
        account[msg.sender].block = block.number;

        token.safeTransfer(msg.sender, amount);

        emit Withdrawn(msg.sender, amount);
        emit Interest(msg.sender, pReward);
    }

    function reward(address _address) public view returns (uint256) {
        uint256 principal = account[msg.sender].principal;
        uint256 day = (block.number.sub(account[msg.sender].block)).div(AVG_BLOCK_PER_DAY);

        return ( PRBMathUD60x18.pow(  ( PRBMathUD60x18.mul(principal, (RATE.div(100)) ) ).add(1), day) ).sub(principal);

        // return (principal.mul((RATE.div(100)).add(1)).pow(day)).sub(principal);
    }

    function exit(address _token) external onlyOwner {
        IERC20(_token).transfer(owner(), IERC20(_token).balanceOf(address(this)));
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

}