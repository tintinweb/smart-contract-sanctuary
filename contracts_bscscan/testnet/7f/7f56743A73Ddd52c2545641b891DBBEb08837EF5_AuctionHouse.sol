pragma solidity 0.6.8;

pragma experimental ABIEncoderV2;
import "@openzeppelin/contracts/access/Ownable.sol";
import {SafeMath} from "@openzeppelin/contracts/math/SafeMath.sol";
import {Math} from "@openzeppelin/contracts/math/Math.sol";
import {IERC721, IERC165} from "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import {ReentrancyGuard} from "@openzeppelin/contracts/utils/ReentrancyGuard.sol";
import {IERC20} from "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import {SafeERC20} from "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import {Counters} from "@openzeppelin/contracts/utils/Counters.sol";
import {IMarket, Decimal} from "@zoralabs/core/dist/contracts/interfaces/IMarket.sol";
import {IMedia} from "@zoralabs/core/dist/contracts/interfaces/IMedia.sol";
import {IAuctionHouse} from "./interfaces/IAuctionHouse.sol";
import "./HitchensUnorderedKeySet.sol";
import "hardhat/console.sol";

interface IWETH {
    function deposit() external payable;

    function withdraw(uint256 wad) external;

    function transfer(address to, uint256 value) external returns (bool);
}

interface IMediaExtended is IMedia {
    function marketContract() external returns (address);
}

/**
 * @title An open auction house, enabling collectors and curators to run their own auctions
 */
contract AuctionHouse is IAuctionHouse, ReentrancyGuard, Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    using Counters for Counters.Counter;
    using HitchensUnorderedKeySetLib for HitchensUnorderedKeySetLib.Set;

    // A mapping of all of the auctions currently running.
    // mapping(uint256 => IAuctionHouse.Auction) public auctions;

    // Note: Auction ID's start at 0x01 and do not start at 0x00.
    HitchensUnorderedKeySetLib.Set auctionSet;
    mapping(bytes32 => IAuctionHouse.Auction) public auctions;

    // Mapping of nfts to auction ids
    mapping (address => mapping (uint256 => uint256)) public auctionIdOfToken;

    // The minimum amount of time left in an auction after a new bid is created
    uint256 public timeBuffer;

    // The minimum percentage difference between the last bid amount and the current bid.
    uint8 public minBidIncrementPercentage;

    // / The address of the WETH contract, so that any ETH transferred can be handled as an ERC-20
    address public wethAddress;

    // A mapping of autoApproved nft collections for auctions.
    mapping(address => bool) public approvedCollections;

    bytes4 constant interfaceId = 0x80ac58cd; // 721 interface id

    Counters.Counter private _auctionIdTracker;

    uint8 public curatorFeePercentage;

    address payable platformWallet;

    bool public approvedAuctionsOnly;

    /**
     * @notice Require that the specified auction exists
     */
    modifier auctionExists(uint256 auctionId) {
        require(auctionSet.exists(bytes32(auctionId)), "Auction doesn't exist");
        _;
    }

    /*
     * Constructor
     */
    constructor(address _weth) public {
        wethAddress = _weth;
        timeBuffer = 15 * 60; // extend 15 minutes after every bid made in last 15 minutes
        minBidIncrementPercentage = 5; // 5%
        curatorFeePercentage = 10; // 10%
        platformWallet = payable(owner());
        _auctionIdTracker.increment(); // always start auction ids at 1
        approvedAuctionsOnly = false;
    }

    function setCollectionApproval(
        address collectionAddress,
        bool isApproved,
        bool _approvedAuctionsOnly
    ) public onlyOwner {
        approvedCollections[collectionAddress] = isApproved;
        approvedAuctionsOnly = _approvedAuctionsOnly;
    }

    function setPlatformWallet(
        address _platformWallet,
        uint8 _curatorFeePercentage
    ) public onlyOwner {
        platformWallet = payable(_platformWallet);
        curatorFeePercentage = _curatorFeePercentage;
    }

    /**
     * @notice Create an auction.
     * @dev Store the auction details in the auctions mapping and emit an AuctionCreated event.
     * If there is no curator, or if the curator is the auction creator, automatically approve the auction.
     */
    function createAuction(
        uint256 tokenId,
        address tokenContract,
        uint256 duration,
        uint256 reservePrice,
        address auctionCurrency,
        uint256 buyNowPrice
    ) public override nonReentrant returns (uint256) {
        require(
            IERC165(tokenContract).supportsInterface(interfaceId),
            "tokenContract does not support ERC721 interface"
        );
        require(
            buyNowPrice == 0 || buyNowPrice > reservePrice,
            "Buy now price must be higher than reservePrice"
        );
        address tokenOwner = IERC721(tokenContract).ownerOf(tokenId);
        require(
            msg.sender == IERC721(tokenContract).getApproved(tokenId) ||
                msg.sender == tokenOwner,
            "Caller must be approved or owner for token id"
        );
        if (approvedAuctionsOnly) {
            require(
                approvedCollections[tokenContract],
                "Approved collections only"
            );
        }

        uint256 auctionId = _auctionIdTracker.current();

        auctionSet.insert(bytes32(auctionId));
        IAuctionHouse.Auction storage a = auctions[bytes32(auctionId)];
        a.tokenId = tokenId;
        a.tokenContract = tokenContract;
        a.approved = false;
        a.amount = 0;
        a.duration = duration;
        a.firstBidTime = 0;
        a.reservePrice = reservePrice;
        a.tokenOwner = tokenOwner;
        a.bidder = address(0);
        a.auctionCurrency = auctionCurrency;
        a.buyNowPrice = buyNowPrice;

        IERC721(tokenContract).transferFrom(tokenOwner, address(this), tokenId);

        _auctionIdTracker.increment();
        
        auctionIdOfToken[address(tokenContract)][tokenId] = auctionId;

        emit AuctionCreated(
            auctionId,
            tokenId,
            tokenContract,
            duration,
            reservePrice,
            tokenOwner,
            auctionCurrency,
            buyNowPrice
        );

        if (approvedCollections[tokenContract]) {
            _approveAuction(auctionId, true);
        }
        return auctionId;
    }

    /**
     * @notice Approve an auction, opening up the auction for bids.
     * @dev Only callable by the curator. Cannot be called if the auction has already started.
     */
    function setAuctionApproval(uint256 auctionId, bool approved)
        external
        override
        auctionExists(auctionId)
        onlyOwner
    {
        IAuctionHouse.Auction storage a = auctions[bytes32(auctionId)];
        require(a.firstBidTime == 0, "Auction has already started");
        _approveAuction(auctionId, approved);
    }

    function setAuctionReservePrice(
        uint256 auctionId,
        uint256 reservePrice,
        uint256 buyNowPrice
    ) external override auctionExists(auctionId) {
        IAuctionHouse.Auction storage a = auctions[bytes32(auctionId)];
        require(
            msg.sender == owner() || msg.sender == a.tokenOwner,
            "Must be auction curator or token owner"
        );
        require(a.firstBidTime == 0, "Auction has already started");
        require(
            buyNowPrice == 0 || buyNowPrice > reservePrice,
            "Buy now price must be higher than reservePrice"
        );

        a.reservePrice = reservePrice;

        emit AuctionReservePriceUpdated(
            auctionId,
            a.tokenId,
            a.tokenContract,
            reservePrice,
            buyNowPrice
        );
    }

    /**
     * @notice Create a bid on a token, with a given amount.
     * @dev If provided a valid bid, transfers the provided amount to this contract.
     * If the auction is run in native ETH, the ETH is wrapped so it can be identically to other
     * auction currencies in this contract.
     */
    function createBid(uint256 auctionId, uint256 amount)
        external
        payable
        override
        auctionExists(auctionId)
        nonReentrant
    {
        IAuctionHouse.Auction storage a = auctions[bytes32(auctionId)];
        address payable lastBidder = a.bidder;
        require(a.approved, "Auction must be approved by curator");
        require(
            a.firstBidTime == 0 ||
                block.timestamp < a.firstBidTime.add(a.duration),
            "Auction expired"
        );
        require(amount >= a.reservePrice, "Must send at least reservePrice");
        require(
            amount >=
                a.amount.add(a.amount.mul(minBidIncrementPercentage).div(100)),
            "Must send more than last bid by minBidIncrementPercentage amount"
        );

        // If this is the first valid bid, we should set the starting time now.
        // If it's not, then we should refund the last bidder
        if (a.firstBidTime == 0) {
            a.firstBidTime = block.timestamp;
        } else if (lastBidder != address(0)) {
            _handleOutgoingBid(lastBidder, a.amount, a.auctionCurrency);
        }

        _handleIncomingBid(amount, a.auctionCurrency);

        a.amount = amount;
        a.bidder = msg.sender;

        bool extended = false;
        // at this point we know that the timestamp is less than start + duration (since the auction would be over, otherwise)
        // we want to know by how much the timestamp is less than start + duration
        // if the difference is less than the timeBuffer, increase the duration by the timeBuffer
        if (a.firstBidTime.add(a.duration).sub(block.timestamp) < timeBuffer) {
            // Playing code golf for gas optimization:
            // uint256 expectedEnd = auctions[auctionId].firstBidTime.add(auctions[auctionId].duration);
            // uint256 timeRemaining = expectedEnd.sub(block.timestamp);
            // uint256 timeToAdd = timeBuffer.sub(timeRemaining);
            // uint256 newDuration = auctions[auctionId].duration.add(timeToAdd);
            uint256 oldDuration = a.duration;
            a.duration = oldDuration.add(
                timeBuffer.sub(
                    a.firstBidTime.add(oldDuration).sub(block.timestamp)
                )
            );
            extended = true;
        }

        emit AuctionBid(
            auctionId,
            a.tokenId,
            a.tokenContract,
            msg.sender,
            amount,
            lastBidder == address(0), // firstBid boolean
            extended
        );

        if (extended) {
            emit AuctionDurationExtended(
                auctionId,
                a.tokenId,
                a.tokenContract,
                a.duration
            );
        }

        // If amount is higher than buy now price, then end the auction immediately.
        if (amount >= a.buyNowPrice && a.buyNowPrice != 0) {
            _endAuction(auctionId, true);
        }
    }

    /**
     * @notice End an auction, finalizing the bid on Zora if applicable and paying out the respective parties.
     * @dev If for some reason the auction cannot be finalized (invalid token recipient, for example),
     * The auction is reset and the NFT is transferred back to the auction creator.
     */
    function endAuction(uint256 auctionId)
        external
        override
        auctionExists(auctionId)
        nonReentrant
    {
        _endAuction(auctionId, false);
    }

    /**
     * @notice Cancel an auction.
     * @dev Transfers the NFT back to the auction creator and emits an AuctionCanceled event
     */
    function cancelAuction(uint256 auctionId)
        external
        override
        nonReentrant
        auctionExists(auctionId)
    {
        IAuctionHouse.Auction storage a = auctions[bytes32(auctionId)];
        require(
            a.tokenOwner == msg.sender || owner() == msg.sender,
            "Can only be called by auction creator or curator"
        );
        require(
            uint256(a.firstBidTime) == 0,
            "Can't cancel an auction once it's begun"
        );
        _cancelAuction(auctionId);
    }

    function getAuctionsCount() public view returns (uint256 count) {
        return auctionSet.count();
    }

    function getAuction(uint256 auctionId)
        public
        view
        returns (IAuctionHouse.Auction memory auction)
    {
        IAuctionHouse.Auction memory a = auctions[bytes32(auctionId)];
        return a;
    }

    function fetchAuctions(uint256 offset, uint256 length)
        public
        view
        returns (
            IAuctionHouse.Auction[] memory auctions,
            uint256[] memory auctionIds
        )
    {
        uint256 lastItem = Math.min(getAuctionsCount(), offset.add(length));
        IAuctionHouse.Auction[]
            memory returnedAuctions = new IAuctionHouse.Auction[](
                lastItem.sub(offset)
            );
        uint256[] memory returnedAuctionIds = new uint256[](
            lastItem.sub(offset)
        );
        for (uint256 i = offset; i < lastItem; i++) {
            IAuctionHouse.Auction memory a = getAuction(
                uint256(auctionSet.keyAtIndex(i))
            );

            returnedAuctions[i.sub(offset)] = a;
            returnedAuctionIds[i.sub(offset)] = uint256(
                auctionSet.keyAtIndex(i)
            );
        }
        return (returnedAuctions, returnedAuctionIds);
    }

    function _endAuction(uint256 auctionId, bool buyNowReached) internal {
        IAuctionHouse.Auction storage a = auctions[bytes32(auctionId)];
        if (!buyNowReached) {
            require(uint256(a.firstBidTime) != 0, "Auction hasn't begun");
            require(
                block.timestamp >= a.firstBidTime.add(a.duration),
                "Auction hasn't completed"
            );
        }

        address currency = a.auctionCurrency == address(0)
            ? wethAddress
            : a.auctionCurrency;
        uint256 curatorFee = 0;

        uint256 tokenOwnerProfit = a.amount;

        // transfer the token to the winner and pay out the participants below
        try
            IERC721(a.tokenContract).safeTransferFrom(
                address(this),
                a.bidder,
                a.tokenId
            )
        {} catch {
            _handleOutgoingBid(a.bidder, a.amount, a.auctionCurrency);
            _cancelAuction(auctionId);
            return;
        }
        // }

        curatorFee = tokenOwnerProfit.mul(curatorFeePercentage).div(100);

        tokenOwnerProfit = tokenOwnerProfit.sub(curatorFee);

        _handleOutgoingBid(platformWallet, curatorFee, a.auctionCurrency);

        _handleOutgoingBid(a.tokenOwner, tokenOwnerProfit, a.auctionCurrency);

        emit AuctionEnded(
            auctionId,
            a.tokenId,
            a.tokenContract,
            a.tokenOwner,
            a.bidder,
            tokenOwnerProfit,
            curatorFee,
            currency
        );
        auctionSet.remove(bytes32(auctionId));
        delete auctions[bytes32(auctionId)];
        delete auctionIdOfToken[address(a.tokenContract)][a.tokenId];
    }

    /**
     * @dev Given an amount and a currency, transfer the currency to this contract.
     * If the currency is ETH (0x0), attempt to wrap the amount as WETH
     */
    function _handleIncomingBid(uint256 amount, address currency) internal {
        // If this is an ETH bid, ensure they sent enough and convert it to WETH under the hood
        if (currency == address(0)) {
            require(
                msg.value == amount,
                "Sent ETH Value does not match specified bid amount"
            );
            IWETH(wethAddress).deposit{value: amount}();
        } else {
            // We must check the balance that was actually transferred to the auction,
            // as some tokens impose a transfer fee and would not actually transfer the
            // full amount to the market, resulting in potentally locked funds
            IERC20 token = IERC20(currency);
            uint256 beforeBalance = token.balanceOf(address(this));
            token.safeTransferFrom(msg.sender, address(this), amount);
            uint256 afterBalance = token.balanceOf(address(this));
            require(
                beforeBalance.add(amount) == afterBalance,
                "Token transfer call did not transfer expected amount"
            );
        }
    }

    function _handleOutgoingBid(
        address to,
        uint256 amount,
        address currency
    ) internal {
        // If the auction is in ETH, unwrap it from its underlying WETH and try to send it to the recipient.
        if (currency == address(0)) {
            IWETH(wethAddress).withdraw(amount);

            // If the ETH transfer fails (sigh), rewrap the ETH and try send it as WETH.
            if (!_safeTransferETH(to, amount)) {
                IWETH(wethAddress).deposit{value: amount}();
                IERC20(wethAddress).safeTransfer(to, amount);
            }
        } else {
            IERC20(currency).safeTransfer(to, amount);
        }
    }

    function _safeTransferETH(address to, uint256 value)
        internal
        returns (bool)
    {
        (bool success, ) = to.call{value: value}(new bytes(0));
        return success;
    }

    function _cancelAuction(uint256 auctionId) internal {
        IAuctionHouse.Auction storage a = auctions[bytes32(auctionId)];
        address tokenOwner = a.tokenOwner;
        IERC721(a.tokenContract).safeTransferFrom(
            address(this),
            tokenOwner,
            a.tokenId
        );

        emit AuctionCanceled(auctionId, a.tokenId, a.tokenContract, tokenOwner);
        auctionSet.remove(bytes32(auctionId));
        delete auctions[bytes32(auctionId)];
        delete auctionIdOfToken[address(a.tokenContract)][a.tokenId];
    }

    function _approveAuction(uint256 auctionId, bool approved) internal {
        IAuctionHouse.Auction storage a = auctions[bytes32(auctionId)];
        a.approved = approved;
        emit AuctionApprovalUpdated(
            auctionId,
            a.tokenId,
            a.tokenContract,
            approved
        );
    }

    // TODO: consider reverting if the message sender is not WETH
    receive() external payable {}

    fallback() external payable {}
}

