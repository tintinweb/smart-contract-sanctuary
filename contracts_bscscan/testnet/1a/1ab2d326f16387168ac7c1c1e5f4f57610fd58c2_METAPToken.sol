    uint8 private _previousLiquidityFee = _liquidityFee;

    uint8 public _burnFee = 0; // Fee for burning
    uint8 private _previousBurnFee = _burnFee;

    uint8 public _walletFee = 0; // Fee to marketing/charity wallet
