interface ChestContract {
    function getChestDataIsOpen(uint256 p_nftId) external view returns (bool);
    function changeChestStatus(uint256 p_nftId) external returns (bool);
}

///Township main contract
contract Township is ERC721Enumerable, Ownable, WorkerFactory {
    uint256 _nftId = 1;


    ChestContract wtContractSettler     = ChestContract(0x877b4cDdF13AdB0fc673D32Cc52dDdaf8c3AA22d);
    IERC721 wtContractSettler721        = IERC721(0x877b4cDdF13AdB0fc673D32Cc52dDdaf8c3AA22d);

    ChestContract wtContractMigrant     = ChestContract(0xF6697f5D31fF4670b13ED2430a179AD9a1f1Aa3c);
    IERC721 wtContractMigrant721        = IERC721(0xF6697f5D31fF4670b13ED2430a179AD9a1f1Aa3c);

    ChestContract wtContractChristmas   = ChestContract(0xEDD77F9599EC3984e3f4BF83A5990d51C69d4fD5);
    IERC721 wtContractChristmas721      = IERC721(0xEDD77F9599EC3984e3f4BF83A5990d51C69d4fD5);

    constructor(string memory name, string memory symbol)
        ERC721(name, symbol)
    {}

    // bnb address tesnet = 0x2514895c72f50D8bd4B4F9b1110F0D6bD2c97526
    // bnb address mainnet = 0x0567F2323251f0Aab15c8dFb1967E4e8A7D42aeE
    AggregatorV3Interface private immutable PRICE_FEED = AggregatorV3Interface(0x0567F2323251f0Aab15c8dFb1967E4e8A7D42aeE);

    // Returns the latest price BNB / USD
    // Returns how many Dollars for a 1 BNB
    function _getBNBPrice() public view returns (uint256) {
        (, int256 price, , , ) = PRICE_FEED.latestRoundData();

        return uint256(price);
    }

    //0xbb4cdb9cbd36b01bd1cbaebf2de08d9173bc095c bnb         11
    //0xa0c8c80ed6b7f09f885e826386440b2349f0da7e   ssg     10

    //we determine the value of the chests
    function value() public view returns (uint256) {
        // How many Dollars for a 1 BNB without decimals
        uint256 BNB_USD_Price = _getBNBPrice() / 10**8; // 565
        uint256 amountBNBReward = (50 * 1 ether) / BNB_USD_Price; //  cuantos bnb son
        // return amountBNBReward;
        return (amountBNBReward * 1 ether); //  amounBNBReward = weis cuantos bnb son la cantidad de dolares deseada    erc20_bnb_price = weis de bnb
    }

    function compareStrings(string memory a, string memory b) public pure returns (bool) {
        return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
    }

    // we obatin the human data
    function getWorkerData(uint256 p_nftId)
        public
        view
        returns (Worker memory)
    {
        require(_exists(p_nftId), "NFT dont exists");
        uint256 position = getWorkerPosition(p_nftId);
        return workers[position];
    }

    // function rarity(uint256 p_nftId) public view returns (uint256) {
    //     require(_exists(p_nftId), "NFT dont exists");
    //     uint256 position = getWorkerPosition(p_nftId);
    //     return workers[position].rarity;
    // }

    /**
     * @dev function that converts the integer received from the oracle and converts it to a uint
     */
    function convert() public pure returns (uint256) {
        return 963300000000000 / 100000000;
        //return uint256(_getBNBPrice()) / 100000000;
    }

    /**
     * @dev receives the data in uint and converts it to weis to set the price for chest
    */
    function oracleSetBnbPrice() public view returns (uint256) {
        uint256 bnbPrice = convert();
        uint256 cost = 5000000; // 50
        return (cost / bnbPrice) * 10000000000000;
    }

    //validation that the user has the amount to pay
    function canPayChest() internal view returns (bool) {
        if (msg.value == oracleSetBnbPrice()) {
            return true;
        } else {
            return false;
        }
    }

    //create worker from chests
    function createWorker(string memory _name, uint256 _chestId) external payable {
        // hay q deployar a la testnet todo el kit para probar sino no vamos a poder
        // hay q armar el test bien esta la anotacion en el etest
        ChestContract actualContract;
        IERC721 actualContract721;
        uint actualChest;
        if(compareStrings(_name, "Settler")) {
            actualContract      = wtContractSettler;
            actualContract721   = wtContractSettler721;
            actualChest         = 1;
        } else if (compareStrings(_name, "Migrant")) {
            actualContract      = wtContractMigrant;
            actualContract721   = wtContractMigrant721;
            actualChest         = 2;
        } else if (compareStrings(_name, "Christmas")) {
            actualContract      = wtContractChristmas;
            actualContract721   = wtContractChristmas721;
            actualChest         = 3;
        } else {
            return;
        }
        
        require(
            msg.sender == actualContract721.ownerOf(_chestId),
            "you are not the owner of this chest"
        );
        require(
            actualContract.getChestDataIsOpen(_chestId) == false,
            "this chest is already open"
        );
        require(canPayChest(), "you dont have enough BNB to open the chest");
        _safeMint(msg.sender, _nftId);
        
        uint[] memory _class = _randomClass(actualChest);

        uint _class_id  = _class[0];
        uint _gender_id = _randomGender(_class_id);
        uint rarity     = _class[1];

        _createWorker(_class_id, rarity, _gender_id, _nftId);
        actualContract.changeChestStatus(_chestId);
        _nftId++;
    }

    // Setters of chest contracts
    function setWtContractSettler(address _address) external onlyOwner {
        wtContractSettler       = ChestContract(_address);
        wtContractSettler721    = IERC721(_address);
    }

    function setWtContractMigrant(address _address) external onlyOwner {
        wtContractMigrant       = ChestContract(_address);
        wtContractMigrant721    = IERC721(_address);
    }

    function setWtContractChristmas(address _address) external onlyOwner {
        wtContractChristmas     = ChestContract(_address);
        wtContractChristmas721  = IERC721(_address);
    }

    //workers by owner
    function walletOfOwner(address _owner)
        public
        view
        returns (uint256[] memory)
    {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i; i < ownerTokenCount; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokenIds;
    }
}

