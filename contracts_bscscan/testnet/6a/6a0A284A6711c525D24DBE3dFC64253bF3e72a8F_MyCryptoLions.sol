pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/ERC1155.sol";
import "hardhat/console.sol";

contract MyCryptoLions is ERC721 {
    constructor(string memory name, string memory symbol) ERC721(name, symbol) {
    }

    struct Artwork {
        uint256 tokenId;
        string metadata_url;
        SubArtworks[] childs;
    }

    struct SubArtworks {
        uint256 parentId;
        uint256 tokenId;
        string metadata_url;
    }

    mapping(uint256 => Artwork) public artworkById;
    mapping(uint256 => SubArtworks) public childById;

    mapping(address => bool) public frozenAccount;

    uint256 original_tokenId = 0;
    uint256 watermark_tokenId = 1;
    uint256 artist_tokenId = 2;
    uint256 token_count = 0;

    event Log(string log);




    function mint(address to, string memory watermark_url, string memory artist_url, string memory original_url) public virtual {
        token_count+=1;
        original_tokenId = token_count;
        super._safeMint(to, original_tokenId);
        super._setTokenURI(original_tokenId, original_url);
        super._setFreezeTokenId(original_tokenId);
        token_count+=1;
        watermark_tokenId = token_count;
        super._safeMint(to, watermark_tokenId);
        super._setTokenURI(watermark_tokenId, watermark_url);
        token_count+=1;
        artist_tokenId = token_count;
        super._safeMint(to, artist_tokenId);
        super._setFreezeTokenId(artist_tokenId);
        super._setTokenURI(artist_tokenId, artist_url);

        childById[original_tokenId] = SubArtworks(watermark_tokenId, original_tokenId, watermark_url);
        childById[artist_tokenId] = SubArtworks(watermark_tokenId, artist_tokenId, artist_url);

        artworkById[watermark_tokenId].tokenId = watermark_tokenId;
        artworkById[watermark_tokenId].metadata_url = original_url;
        artworkById[watermark_tokenId].childs.push(childById[original_tokenId]);
        artworkById[watermark_tokenId].childs.push(childById[artist_tokenId]);
    }


    function getArtworkById(uint256 tokenId) public view virtual returns (Artwork memory) {
        return artworkById[tokenId];
    }

    function getChildById(uint256 tokenId) public view virtual returns (SubArtworks memory) {
        return childById[tokenId];
    }

    function getArtworkChildsById(uint256 tokenId) public view virtual returns (SubArtworks[] memory) {
        return artworkById[tokenId].childs;
    }

    function getChildArtworkIdByChildId(uint256 tokenId) public view virtual returns (uint256){
        return childById[tokenId].parentId;
    }


    function getChildPairIdByChildId(uint256 tokenId) public view virtual returns (uint256){
        uint256 parentId = childById[tokenId].parentId;
        Artwork memory artwork = artworkById[parentId];
        for (uint i = 0; i < artwork.childs.length; i++) {
            if (artwork.childs[i].tokenId != tokenId) {
                return artwork.childs[i].tokenId;
            }
        }
        return 0;
    }

    function isContract(address account) internal view returns (bool) {
        uint256 size;
        assembly {size := extcodesize(account)}
        return size > 0;
    }

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 tokenId) internal virtual override {

        if (from != address(0)){
            Artwork memory artwork = artworkById[tokenId];
            for (uint i = 0; i< artwork.childs.length; i++) {
                // burn tokens
                console.log("BurnID", artwork.childs[i].tokenId);
                super._burn(artwork.childs[i].tokenId);
                // mint tokens
                console.log("mintID", artwork.childs[i].tokenId);
                super._safeMint(to, artwork.childs[i].tokenId);
            }
        }


    }


}

