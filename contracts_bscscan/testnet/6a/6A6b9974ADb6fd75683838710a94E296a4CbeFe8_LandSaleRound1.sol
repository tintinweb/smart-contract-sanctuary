    pragma solidity 0.8.10;

    import "@openzeppelin/contracts/security/Pausable.sol";
    import "@openzeppelin/contracts/access/AccessControl.sol";
    import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
    import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
    import "./InvertLand.sol";

    contract LandSaleRound1 is Pausable, AccessControl {
        using SafeERC20 for IERC20;

        bytes32 private immutable OPERATOR_ROLE = keccak256("OPERATOR_ROLE");
        uint256 public whitelistedCount;
        uint256 public whitelistedPartnersCount;
        address public immutable owner;
        address public nftAddress;
        IERC20 public immutable busd;
        InvertLand private iLand;
                
        // Mapping from buyer address to listed status
        mapping(address => bool) private _whitelisted;
        // Mapping from partner address to listed status
        mapping(address => bool) private _whitelistedPartners;
        // Mapping from land type to land price
        mapping(uint256 => uint256) private usdLandPrices;
        // Mapping from land type to sales
        mapping(uint256 => uint256) public typesSales;
        
        event TransferSent(address from, address to, uint256 amount);
        event AddressWhitelisted(address addressWhitelisted);
        event AddressRemoved(address addressRemoved);
        event TokenMinted(address indexed to, uint256 amount);
            
        modifier whitelisted(address _buyer) {
            require(_whitelisted[_buyer], "LANDSALE: wallet address not whitelisted.");
            _;
        }
        
        modifier whitelistedPartner(address _partner) {
            require(_whitelistedPartners[_partner], "LANDSALE: partner address not whitelisted.");
            _;
        }
        
        /**
        * @dev Initializes the contract by setting an erc20 tokean address and a list of land prices.
        */
        constructor(address _owner, address _busdAddress, uint[] memory _usdLandPrices) {
            require(_owner != address(0), "LANDSALE: Invalid Zero Address informed");
            require(_busdAddress != address(0), "LANDSALE: Invalid Zero Address informed");
            
            for(uint i = 0; i < _usdLandPrices.length; i++) {
                //require(%)
                usdLandPrices[i] = _usdLandPrices[i];
            }
            _grantRole(DEFAULT_ADMIN_ROLE, _owner);
            _grantRole(OPERATOR_ROLE, _owner);
            owner = _owner;
            busd = IERC20(_busdAddress);
        }

        /**
        * @dev pauses the contract.
        */
        function pause() public onlyRole(OPERATOR_ROLE) {
            _pause();
        }
        
        /**
        * @dev unpauses the contract.
        */
        function unpause() public onlyRole(OPERATOR_ROLE) {
            _unpause();
        }
        
        /**
        * @dev sets the contract address for nft token.
        */
        function setNftAddress(address _nftAddress) public onlyRole(OPERATOR_ROLE) {
            require(_nftAddress != address(0), "LANDSALE: Invalid Zero Address informed");
            nftAddress = _nftAddress;
            iLand = InvertLand(nftAddress); 
        }

        /**
        * @dev Safely mint and transfers token from nft contract to sender, checking first if `tokenId` has not been minted.
        *
        * This function verifies the correct price for the land type of the nft, 
        * mints the token and remove sender address from the whitelist. 
        *
        * Requirements:
        *
        * - sender erc20 balance has to be greater or equal to`amount`.
        * - `tokenId` token should not minted before.
        * - `_landType` has to be defined on nft contract and has to have equivalent price set on `landPrice` mapping.
        *
        * Emits a {TransferSent} event.
        * Emits a {TokenMinted} event.
        */
        function buyLand( 
            uint256 _usdAmount, 
            uint256 _landType,
            uint256 _id
        ) public whenNotPaused whitelisted(msg.sender) {
            require(_usdAmount > 0, "LANDSALE: USD amount should be greater than zero");
            IERC20 token = IERC20(busd); 
            uint amount =  _usdAmount * 1e18;
            require(_usdAmount == getLandPriceInUsd(_landType), "LANDSALE: invalid price for the land type");
            // receive busb payment
            token.safeTransferFrom(msg.sender, owner, amount);
            emit TransferSent(msg.sender, owner, amount);
            // remove from whitelist
            _whitelisted[msg.sender] = false;
            whitelistedCount--; 
            // mint nft to buyer
            iLand.mintToken(msg.sender, _landType, _id);
            emit TokenMinted(msg.sender, _usdAmount);
            // add to sales
            typesSales[_landType] = typesSales[_landType] + _usdAmount;         
        }

        /**
        * @dev Safely mint and transfers token from nft contract to partners, checking first if `tokenId` has not been minted.
        *
        * This function mints the token without charging costs, and remove sender address from the whitelist. 
        *   
        * Requirements:
        *
        * - `tokenId` token should not minted before.
        * - `_landType` has to be defined on nft contract and has to have equivalent price set on `landPrice` mapping.
        *
        * Emits a {TransferSent} event.
        * Emits a {TokenMinted} event.
        */
    
        function mintToPartner(
            address _partner,
            uint256 _landType,
            uint256 _landId
        ) public whenNotPaused whitelistedPartner(_partner) 
            onlyRole(OPERATOR_ROLE) 
        {
            // remove from whitelist
            _whitelistedPartners[_partner] = false;
            whitelistedPartnersCount--; 
            // mint to partner
            iLand.mintToken(_partner, _landType, _landId);
        }
        
        /**
        * @dev Returns land price for `_type`.
        */
        function getLandPriceInUsd(uint256 _type) public view returns (uint256){        
            return usdLandPrices[_type];
        }

        /**
        * @dev Returns usd sales by `_type`.
        */
        function getUsdSalesByType(uint256 _type) public view returns (uint256){        
            return typesSales[_type];
        } 

        /**
        * @dev Adds a number of wallets to the whitelist, checking first that wallet is not being added before.
        * `whitelistedCount` is incremented by th number of wallets added.
        *
        * Requirements:
        *
        * - `_addresses` is a list of valid addresses to be added to the whitelist.
        *
        * Emits a {AddressWhitelisted} event.
        */
        function addToWhitelist( address[] calldata _addresses) public onlyRole(OPERATOR_ROLE){
            uint256 repeated = 0;
            for(uint256 i = 0; i < _addresses.length; i++) {
                // if address is already in the whitelist, skip
                if(_whitelisted[_addresses[i]] == true) {
                    repeated++;
                    continue;
                } else {
                    // add to the whitelist
                    _whitelisted[_addresses[i]] = true;
                    whitelistedCount++;
                    emit AddressWhitelisted(_addresses[i]);
                }
            }   
        }

        /**
        * @dev Removes a number of wallets from the whitelist.
        * `whitelistedCount` is decremented by the number of wallets removed.
        *
        * Requirements:
        *
        * - `_addresses` is a list of valid addresses to be remove from the whitelist.
        *
        * Emits a {AddressRemoved} event.
        */
        function removeFromWhitelist(address[] memory _addresses) public onlyRole(OPERATOR_ROLE) {
            for(uint256 i = 0; i < _addresses.length; i++) {
                if(_whitelisted[_addresses[i]] == true) {
                    delete _whitelisted[_addresses[i]];
                    whitelistedCount--;
                    emit AddressRemoved(_addresses[i]);
                }
            }
        }

        /**
        * @dev Returns true if `buyer` is whitelisted.
        */
        function getIsWhitelisted(address _buyer) public view returns(bool) {
            return _whitelisted[_buyer];
        }

        /**
        * @dev Returns whitelisted count.
        */
        function getWhitelistedCount() public view returns(uint256) {
            return whitelistedCount;
        }

        /**
        * @dev Adds a number of partners addresses to the whitelist,
        * `whitelistedPartnerCount` is incremented by th number of wallets added.
        *
        * Requirements:
        *
        * - `_addresses` is a list of valid addresses to be added to the whitelist.
        *
        * Emits a {AddressWhitelisted} event.
        */
        function addToPartnersWhitelist( address[] calldata _addresses) public onlyRole(OPERATOR_ROLE){
            uint256 repeated=0;
            for(uint256 i = 0; i < _addresses.length; i++) {
                // if address is already in the whitelist, skip
                if(_whitelistedPartners[_addresses[i]] == true) {
                    repeated++;
                    continue;
                } else {
                    // add to the whitelist
                    _whitelistedPartners[_addresses[i]] = true;
                    whitelistedPartnersCount++;
                    emit AddressWhitelisted(_addresses[i]);
                }
            }
        }

        /**
        * @dev Removes a number of wallets from the whitelist.
        * `whitelistedCount` is decremented by the number of wallets removed.
        *
        * Requirements:
        *
        * - `_addresses` is a list of valid addresses to be remove from the whitelist.
        *
        * Emits a {AddressRemoved} event.
        */
        function removeFromPartnersWhitelist(
            address[] memory _addresses
        ) public onlyRole(OPERATOR_ROLE) {
            for(uint256 i = 0; i < _addresses.length; i++) {
                if(_whitelistedPartners[_addresses[i]] == true) {
                    delete _whitelistedPartners[_addresses[i]];
                    whitelistedPartnersCount--;
                    emit AddressRemoved(_addresses[i]);
                }
            }
        }

        /**
        * @dev Returns true if `_partner` is whitelisted.
        */
        function getIsWhitelistedPartner(address _partner) public view returns(bool) {
            return _whitelistedPartners[_partner];
        }

        /**
        * @dev Returns whitelisted partners count.
        */
        function getWhitelistedPartnersCount() public view returns(uint256) {
            return whitelistedPartnersCount;
        }
    }

