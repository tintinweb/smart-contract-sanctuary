pragma solidity 0.8.10;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

/// @custom:security-contact [emailÂ protected]
contract InvertLand is ERC721, Pausable, AccessControl {
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIdCounter;
    uint256 totalMaxSupply;    
    bytes32 private immutable OPERATOR_ROLE = keccak256("OPERATOR_ROLE");
    address public immutable owner;
    mapping(uint256 => uint256) public tokenIdToType;
    mapping(uint256 => uint256) public typeToMaxSupply;
    mapping(uint256 => uint256) public typeToMinted;
    mapping(uint256 => bool) public landIdToMinted;

    event LandMinted(address indexed to, uint256 landType, uint256 indexed tokenId, uint256 indexed landId);

    modifier validType(uint256 _type) {
        require(isValidType(_type), "INVERTLAND: invalid land type");
        _;
    }
        
    /**
     * @dev Initializes the contract by setting the owner address and sets Name and Symbol for the token.
     */
    constructor(address _owner, uint256[] memory _typesMaxSupply) ERC721("Forest Test2", "FORST2") {
        require(_owner != address(0), "INVERTLAND: Invalid Zero Address informed");
        require(_typesMaxSupply.length == 5, "INVERTLAND: needs initially 5 max supply values for types");
        
        _grantRole(DEFAULT_ADMIN_ROLE, _owner);
        _grantRole(OPERATOR_ROLE, _owner);
        owner = _owner;

        // set land types supply
        for(uint256 i = 0; i < _typesMaxSupply.length; i++) {
            typeToMaxSupply[i] = _typesMaxSupply[i];
            typeToMinted[i] = 0;
            totalMaxSupply += _typesMaxSupply[i];    
        } 
    }

    function _baseURI() internal pure override returns (string memory) {
        return "https://letsinvert.io/lands/";
    }
    
    /**
     * @dev pauses the contract.
     */
    function pause() public onlyRole(OPERATOR_ROLE) {
        _pause();
    }
    
    /**
     * @dev unpauses the contract.
     */
    function unpause() public onlyRole(OPERATOR_ROLE) {
        _unpause();
    }

    /**
     * @dev Safely mint the nft to buyer, checking first for correct land type.
     *
     * This function verifies the correct type for the land of the nft, and mints the token.  
     *
     * Requirements:
     * - `_type` has to be a valide type defined on `landTypes`
     */
    function mintToken(address _to, uint256 _type, uint256 _landId)
        public 
        onlyRole(OPERATOR_ROLE) 
        whenNotPaused
        validType(_type)
    {
        require(_to != address(0), "INVERTLAND: Invalid Zero Address informed");
        require(getTypeMinted(_type) < getTypeMaxSupply(_type), "INVERTLAND: max supply reached for the land type");
        require(landIdToMinted[_landId] == false, "INVERTLAND: land for this id already minted");
        uint256 _tokenId = _tokenIdCounter.current();
        _tokenIdCounter.increment();

        tokenIdToType[_tokenId] = _type;
        typeToMinted[_type] = typeToMinted[_type] + 1;
        landIdToMinted[_landId] = true;
        _safeMint(_to, _tokenId);
        emit LandMinted(_to, _type, _tokenId, _landId);
    }

    /**
     * @dev Verifies if land type is valid.
     */
    function isValidType(uint256 _type) public view returns(bool) {
        return (typeToMaxSupply[_type] > 0);
    }

    /**
     * @dev add new land type with max supply.
     */
    function addLandType(uint256 _type, uint256 _maxSupply) public onlyRole(OPERATOR_ROLE) {
        require(!isValidType(_type), "INVERTLAND: land type already exists");
        typeToMaxSupply[_type] = _maxSupply;
        totalMaxSupply += _maxSupply;
    }

    /**
     * @dev Returns the land type for the tokenId.
     */
    function getTokenLandType(uint tokeinId) public view returns (uint256) {
        return tokenIdToType[tokeinId];
    }
    
    function getTypeMinted(uint256 _type) public view validType(_type) returns (uint256)  {
        return  typeToMinted[_type];
    }

    function getTypeMaxSupply(uint256 _type) public view validType(_type) returns (uint256) {
        return  typeToMaxSupply[_type];
    }

    function getTotalMaxSupply() public view returns(uint256) {
        return totalMaxSupply;
    }
    
    function addTypeSupply(uint256 _type, uint256 _supply) public validType(_type) onlyRole(OPERATOR_ROLE) {
        typeToMaxSupply[_type] = typeToMaxSupply[_type] + _supply; 
    }
  
    // The following functions are overrides required by Solidity.
    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, AccessControl)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

