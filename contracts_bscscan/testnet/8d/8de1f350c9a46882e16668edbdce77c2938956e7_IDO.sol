contract IDO is Ownable {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;
    address SERVICE_ADDRESS;
    struct Ido{
        IERC20 tokenIdo;
        IERC20 tokenBase;
        uint256 startTime;
        uint256 endTime;
        uint256 claimTime;
        uint256 idoDuration;
        uint256 idoRate; //  tokenBase/tokenIdo
        uint256 maxContribution;
        uint256 totalTokenBaseDeposited;
        uint256 totalTokenIdoForSale; 
        uint256 tokenRemain;
        string idoType;
        address devAddress;
        bool devClaimed;
        mapping(address => uint256) depositors;
        mapping(address => bool) isClaimed;
    }
    Ido public idoInfo;
    event Transfer(address indexed from, address indexed to, uint256 value);

    constructor(address _tokenIdo, address _tokenBase, uint256 _idoDuration,uint256 _idoRate, uint256 _maxContribution, uint256 _totalTokenBaseDeposited, uint256 _totalTokenIdoForSale,string memory _idoType, address _devAddress) public payable {
        SERVICE_ADDRESS = address(0xbe44ddE2875D023D8de11518b4b4e351d6A7bC58);
        idoInfo = Ido({tokenIdo: IERC20(_tokenIdo),tokenBase: IERC20(_tokenBase),startTime: 0, endTime: 0 , claimTime: 0, idoDuration: _idoDuration, idoRate: _idoRate, maxContribution: _maxContribution, totalTokenBaseDeposited: _totalTokenBaseDeposited,totalTokenIdoForSale: _totalTokenIdoForSale, tokenRemain: _totalTokenBaseDeposited, idoType: _idoType, devAddress: _devAddress, devClaimed: false});
         payable(SERVICE_ADDRESS).transfer(msg.value);
        emit Transfer(address(0), _devAddress, _totalTokenBaseDeposited);
    }

   
    function getCurrentIDO() external view returns(IERC20, IERC20, uint256, uint256, uint256, uint256, uint256, uint256, uint256,uint256, uint256,string memory, address, bool) {
      return (idoInfo.tokenIdo, idoInfo.tokenBase,idoInfo.startTime, idoInfo.endTime, idoInfo.claimTime, idoInfo.idoDuration, idoInfo.idoRate, idoInfo.totalTokenBaseDeposited, idoInfo.totalTokenIdoForSale, idoInfo.tokenRemain, idoInfo.maxContribution, idoInfo.idoType, idoInfo.devAddress, idoInfo.devClaimed);
    }
    
    // function add(address _tokenBase,address _tokenIdo,uint256 _idoDuration,uint256 _claimTime,uint256 _idoRate, uint256 _individualCap,uint256 _totalTokenIdoForSale, address _devAddress) public onlyOwner{
    //     idoInfo.push(Ido({
    //         startTime: 0,
    //         endTime: 0,
    //         claimTime: _claimTime,
    //         tokenBase: IERC20(_tokenBase),
    //         tokenIdo: IERC20(_tokenIdo),
    //         idoDuration: _idoDuration,
    //         idoRate: _idoRate,
    //         individualCap: _individualCap,
    //         totalTokenIdoForSale: _totalTokenIdoForSale,
    //         totalTokenBaseDeposited: 0,
    //         tokenRemain: _totalTokenIdoForSale,
    //         devAddress: _devAddress,
    //         devClaimed: false
    //     })) ; 
    // }
    modifier inIDO() {
        require(idoInfo.startTime > 0,'ido has not yet started');
        require(idoInfo.endTime > block.timestamp,'ido has ended');
        _;
    }

    modifier validClaim() {
        Ido storage ido = idoInfo;
        require(ido.endTime.add(ido.claimTime) < block.timestamp,'ido claim has not yet started');
        require(ido.depositors[msg.sender] > 0,'account not funded');
        require(!ido.isClaimed[msg.sender],'account has already claim');
        _;
    }

    function start() public returns(uint, uint) {
        require(idoInfo.devAddress == msg.sender,'you are not ido s dev');
        require(idoInfo.startTime == 0, "IDO is started!");
        idoInfo.startTime = block.timestamp;
        idoInfo.endTime = idoInfo.startTime.add(idoInfo.idoDuration);
        return (idoInfo.startTime, idoInfo.endTime);
    }

    function deposit( uint256 amount) public inIDO() {
        Ido storage ido = idoInfo;
        require(
            ido.maxContribution == 0 || ido.depositors[msg.sender].add(amount) <= ido.maxContribution,
            "Over individualCap"
        );
        ido.totalTokenBaseDeposited = ido.totalTokenBaseDeposited.add(amount);
        ido.depositors[msg.sender] = ido.depositors[msg.sender].add(amount);
        ido.tokenBase.safeTransferFrom(msg.sender, address(this), amount);
    }
    function amountDeposited( address _user) public view returns(uint256){
        return idoInfo.depositors[_user];
    }
    function isClaimed(address _user) public view returns(bool){
        return idoInfo.isClaimed[_user];
    }
    function userClaimToken() public validClaim() {
        Ido storage ido = idoInfo;
        require(ido.depositors[msg.sender]>0);
        uint256 amountToClaim = ido.depositors[msg.sender].mul(ido.idoRate);
        uint256 excessTokenBase = 0;
        uint256 maxAmountToClaim = ido.totalTokenIdoForSale.mul(ido.depositors[msg.sender]).div(ido.totalTokenBaseDeposited);
        if(amountToClaim > maxAmountToClaim){
            excessTokenBase = amountToClaim.sub(maxAmountToClaim).div(ido.idoRate);
            amountToClaim = maxAmountToClaim;
        }
        require(amountToClaim <= ido.tokenIdo.balanceOf(address(this)),'amountToClaim: not enough balance');
        require(excessTokenBase <= ido.tokenBase.balanceOf(address(this)),'excessTokenBase: not enough balance');
        ido.isClaimed[msg.sender] = true;
        ido.tokenIdo.safeTransfer(msg.sender, amountToClaim);
        if(excessTokenBase > 0){
            ido.tokenBase.safeTransfer(msg.sender, excessTokenBase);
        }
    }
    function devClaimToken() public{
        Ido storage ido = idoInfo;
        require(msg.sender == ido.devAddress,'you are not ido s dev');
        require(!ido.devClaimed,'has already claim' );
        require(ido.endTime < block.timestamp,'ido is not ended');
        require(ido.tokenBase.balanceOf(address(this)) > 0,'No balance');
        uint256 amoutToClaim =  ido.totalTokenIdoForSale.div(ido.idoRate);
        uint256 excessTokenIdo = 0;
        if(amoutToClaim > ido.totalTokenBaseDeposited){
            amoutToClaim = ido.totalTokenBaseDeposited;
            excessTokenIdo = ido.totalTokenIdoForSale.sub(ido.totalTokenBaseDeposited.mul(ido.idoRate));
        }
        require(amoutToClaim>0,'not thing to claim');
        ido.devClaimed = true;
        ido.tokenBase.safeTransfer(ido.devAddress, amoutToClaim);
        if(excessTokenIdo>0){
            ido.tokenIdo.safeTransfer(ido.devAddress, excessTokenIdo);
        }
    }
    
    function saveFund() public onlyOwner{
        IERC20 tokenBase = idoInfo.tokenBase;
        IERC20 tokenIdo = idoInfo.tokenIdo;
        tokenBase.safeTransfer(msg.sender, tokenBase.balanceOf(address(this)));
        tokenIdo.safeTransfer(msg.sender, tokenIdo.balanceOf(address(this)));
    }
}