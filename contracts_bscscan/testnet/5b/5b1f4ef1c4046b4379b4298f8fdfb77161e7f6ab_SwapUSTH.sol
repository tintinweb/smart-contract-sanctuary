}


contract SwapUSTH{
    
    using SafeMath for uint256;
    
     // The swap router, modifiable. Will be changed to Pancake's router when our own AMM release
    IUniswapV2Router02 public PancakeRouter;
    
    // The trading pair
    address public PancakePair;
    
    event _buyToken(uint256 pid, address buyer, uint256 bep20Amount, uint256 usthAmount);
    
    event _transferOwnership(address oldowner, address newOwner);
    
    event _withdraw(address owner, uint256 amount);
    
    event PancakeRouterUpdated(address indexed operator, address indexed router, address indexed pair);

    uint256 minAmount = 0.1 ether;
    
    BEP20 public usth;
    
    address public ownerAddress;
    
    address public presaleAddress;
    
    uint256 public tokenPrice = 0.1 ether;
    
    uint256 public usdtPid = 0;
    
    // Info of each pool.
    struct PoolInfo {
        BEP20 token;           // Address of LP token contract.
    }
    
     // Info of each pool.
    PoolInfo[] public poolInfo;
    
    
    constructor(BEP20 _usth, address owner, address _presaleAddress, IUniswapV2Router02 _pancakeRouter) public {
        usth = _usth;
        ownerAddress = owner;
        presaleAddress = _presaleAddress;
        PancakeRouter = _pancakeRouter;
    }
    
    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(ownerAddress == msg.sender, 'Ownable: caller is not the owner');
        _;
    }
    
    // add pool
    function add(BEP20 _token) public onlyOwner {
        poolInfo.push(PoolInfo({
            token: _token
        }));
    }

    // Buy USTH with any coin
    function buyToken(uint256 pid, uint256 bep20Amount) public {
        // require(bep20 == wbnb || bep20 == usdt || bep20 == usdc, "Can not swap with this wrong token"); 
        require(bep20Amount > 0, "Can not transfer zero amount");
        require(bep20Amount > minAmount, "Invalid transfer amount");
        
        swapTokenForUSDT(pid, bep20Amount);
        
        uint256 usdtAmount = poolInfo[usdtPid].token.balanceOf(address(this));
        
        poolInfo[pid].token.transferFrom(msg.sender, presaleAddress, usdtAmount);
        
        uint256 usthAmount = usdtAmount.div(tokenPrice) * 1 ether;
        usth.transfer(msg.sender, usthAmount);
        
        emit _buyToken(pid, msg.sender, usdtAmount, usthAmount);
    }
    
    
    // Buy USTH with USDT
    function buyTokenWithUSDT(uint256 pid, uint256 bep20Amount) public {
        // require(bep20 == wbnb || bep20 == usdt || bep20 == usdc, "Can not swap with this wrong token"); 
        require(bep20Amount > 0, "Can not transfer zero amount");
        require(bep20Amount > minAmount, "Invalid transfer amount");
        
        poolInfo[pid].token.transferFrom(msg.sender, presaleAddress, bep20Amount);
        
        uint256 usthAmount = bep20Amount.div(tokenPrice) * 1 ether;
        usth.transfer(msg.sender, usthAmount);
        
        emit _buyToken(pid, msg.sender, bep20Amount, usthAmount);
    }
    
    
    // Buy USTH with BNB
    function buyTokenWithBNB(uint256 pid) public payable{
        require(msg.value > 0, "Can not transfer zero amount");
        
        swapBNBForUSDT(msg.value);
        
        uint256 bep20Amount = poolInfo[usdtPid].token.balanceOf(address(this));
        poolInfo[usdtPid].token.transferFrom(msg.sender, presaleAddress, bep20Amount);

        uint256 usthAmount = bep20Amount.div(tokenPrice) * 1 ether;
        usth.transfer(msg.sender, usthAmount);
        
        emit _buyToken(pid, msg.sender, bep20Amount, usthAmount);
    }
    
    // swap BNB to USDT
    function swapBNBForUSDT(uint256 BNBAmount) public {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[1] = address(poolInfo[usdtPid].token);
        path[0] = PancakeRouter.WETH();

        // make the swap
        PancakeRouter.swapExactETHForTokensSupportingFeeOnTransferTokens{value:BNBAmount}(
            0,
            path,
            address(this),
            block.timestamp.add(300)
        );
    }
    
    // swap token to USDT
    function swapTokenForUSDT(uint256 pid, uint256 tokenAmount) public {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[1] = address(poolInfo[usdtPid].token);
        path[0] = address(poolInfo[pid].token);

        // make the swap
        PancakeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp.add(300)
        );
    }
    
    // Withdraw USTH onlyOwner
    function withdraw(uint256 withdrawAmount) public onlyOwner{
        require(withdrawAmount > 0, "Can not transfer zero amount");
        usth.transfer(msg.sender, withdrawAmount);
        emit _withdraw(msg.sender, withdrawAmount);
    }
    
    // transfer ownership
    function transferOwnership(address newOwner) public onlyOwner{
        ownerAddress = newOwner;
        emit _transferOwnership(msg.sender, newOwner);
    }
    
    // update usth
    function updateUSTH(BEP20 _usth) public onlyOwner{
        usth = _usth;
    }
    
    // update presaleAddress
    function updatePresaleAddress(address _presaleAddress) public onlyOwner{
        presaleAddress = _presaleAddress;
    }
    
    // update token price
    function updateTokenPrice(uint256 _tokenPrice) public onlyOwner{
        tokenPrice = _tokenPrice;
    }
    
    // update usdt pid
    function udpateUSDTPid(uint256 _pid) public onlyOwner{
        usdtPid = _pid;
    }
    
    
    /**
     * @dev Update the swap router.
     * Can only be called by the current operator.
     */
    function updatePancakeRouter(address _router) public onlyOwner {
        PancakeRouter = IUniswapV2Router02(_router);
        emit PancakeRouterUpdated(msg.sender, address(PancakeRouter), PancakePair);
    }
    
    
}