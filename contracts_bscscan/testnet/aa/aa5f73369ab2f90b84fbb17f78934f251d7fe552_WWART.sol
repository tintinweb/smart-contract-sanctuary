// File: contracts/WWART.sol

// ERC721URIStorage is the contract from openzeppelin for v0.8 that includes the metadata


contract WWART is  ERC721URIStorage, Ownable {
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;
    using SafeMath for uint256;

    mapping(string => bool) _artExists;

    mapping(address => uint256)  ownedTokens;
    mapping(address => uint256)  createdTokens;
    mapping(uint256 => bool)  tokensForSale;
    // Data of each NFT
    struct TokenData {
        address payable creator; // creator of the NFT
        uint256 royalties;       // royalties to be paid to NFT creator on a resale. In basic points
        bool isFeatured;
        bool isSaleEnabled;
        uint256 priceInWei;
        uint256 createCommission;
        uint256 resellCommission;
        uint256 firstsellCommission;
    }
    mapping(uint256 => TokenData) tokens;
    string private _baseTokenURI;
    string private _baseContractURI;


    uint commissionDenominator = 1000;



    constructor() ERC721("WWART NFT", "WWART") {}



    /*
    */
    function baseTokenURI() public view returns (string memory) {
        return _baseTokenURI;
    }
    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenURI();
    }

    // 1. mints the NFT and save the data in the "tokens" map.
    event WWARTCreated(uint256 _tokenId, address owner,uint256 originalPrice,uint256 firstsellCommission, uint256 commission, uint256 resellCommission, uint now);

    /**
    *  tokenURI - the url for the json file containing name, url of original art and description
    *  _royalties - _royalty set by creator for resell
    *  _price - price for the art excluding platform commission
    *  _createCommission - platform commission for first time sell
    *  _resellCommission -  platform commission for resell
    */
    function createNFT(string memory tokenURI, uint256 _royalties, uint256 _price, uint256 _firstsellCommission, uint256 _createCommission, uint256 _resellCommission)
    public
    returns (uint256)
    {
        require(_royalties <=1000 && _royalties >=500, "Royalty should be within 5-10%");
        require(_price > 0.0, "Price must be grater than 0.");
        require(_createCommission > 0.0, "Platform charge must be grater than 0.");
        require(_resellCommission > 0.0, "Platform charge for resell must be grater than 0.");
        require(_firstsellCommission > 0.0, "MarketPlace charge for resell must be grater than 0.");
        require(_artExists[tokenURI]  != true, "Token Already Exists");
        _tokenIds.increment();

        uint256 newItemId = _tokenIds.current();
        _mint(msg.sender, newItemId);
        _setTokenURI(newItemId, tokenURI);

        _price = (_price.mul(1e18).div(10000) ); // convert to wei
        uint256 commissionToPay = _price.mul(_createCommission).div(commissionDenominator);

        _price= _price.add(commissionToPay);

        tokens[newItemId] = TokenData({
                    creator:payable(msg.sender) ,
                    royalties: _royalties,
                    isFeatured:false ,
                    priceInWei:_price,
                    createCommission: _createCommission,
                    resellCommission: _resellCommission,
                    firstsellCommission:_firstsellCommission,
                    isSaleEnabled:true
                    });
        _artExists[tokenURI] = true;

        createdTokens[msg.sender]=newItemId;
        ownedTokens[msg.sender]=newItemId;
        enableSale(newItemId);
        emit WWARTCreated(newItemId, msg.sender, _price, _firstsellCommission, _createCommission, _resellCommission, block.timestamp);
        return newItemId;
    }

    function getCreator(uint256 _tokenId)
    public
    view
    returns (address)
    {
        return tokens[_tokenId].creator;
    }

    // returns in basic points the royalties of a token
    function getRoyalties(uint256 _tokenId)
    public
    view
    returns (uint256)
    {
        return tokens[_tokenId].royalties;
    }


    // returns price in wei including the platform charge
    function getPrice(uint256 _tokenId)
    public
    view
    returns (uint256)
    {
        return tokens[_tokenId].priceInWei;
    }

    function contractBalance() external view returns (uint256) {
        return address(this).balance;
    }

    function contractURI() public view returns (string memory) {
        return _baseContractURI;
    }


    function totalSupply() public view returns (uint256) {
        return _tokenIds.current();
    }


    /***
    * customer can change the price for the contract
    */
    event PriceChanged(uint256 indexed _tokenId, uint256 _oldPrice, uint256 _newPrice, address owner, uint now);

    /***
    *  _tokenId = id of the token
    *  _price = price in 10000 ether for the token. For example, for 1 ether = send 10000
    */
    function changePrice(uint256 _tokenId, uint256 _price) external {
        // Token must exist
        require(_exists(_tokenId), "This is not a valid TokenId");
        require(ownerOf(_tokenId) == msg.sender, "Only owner can change the price.");
        require(_price != uint256(0));
        uint256 _oldPrice = tokens[_tokenId].priceInWei;
        require(_price != _oldPrice, "New price can not be same as old price.");
        address  tokenOwner = ownerOf(_tokenId);
        address creator = tokens[_tokenId].creator;
        uint256 commission= 0;
        if(creator == tokenOwner) {
            commission= tokens[_tokenId].createCommission;

        } else {
            commission= tokens[_tokenId].resellCommission;
        }
        _price = (_price.mul(1e18).div(10000) ).add(commission.mul(1e16));
        tokens[_tokenId].priceInWei = _price;
        emit PriceChanged(_tokenId, _oldPrice, _price, msg.sender, block.timestamp);
    }

    event CommissionChanged(uint256 indexed _tokenId, uint256 _oldCommission, uint256 _newCommission, address owner, uint stage, uint now);
    /**
    * stage = 1 , change createCommission
    * stage = 2 , change resell com
    */
    function changeCommission(uint256 _tokenId, uint256 _commission, uint stage) external {
        // Token must exist
        require(_exists(_tokenId), "This is not a valid TokenId");
        require(ownerOf(_tokenId) == msg.sender, "Only owner can change the price.");
        require(_commission != uint256(0), 'Platform Charge must be greater than 0');
        uint256 _oldCommission = 0;
        if(stage == 1){
            _oldCommission = tokens[_tokenId].createCommission;
            require(_commission != _oldCommission, "New Commission can not be same as old Commission.");
            tokens[_tokenId].createCommission = _commission;
        }

        if (stage == 1){
            _oldCommission = tokens[_tokenId].resellCommission;
            require(_commission != _oldCommission, "New Commission can not be same as old Commission.");
            tokens[_tokenId].resellCommission = _commission;
        }

        emit CommissionChanged(_tokenId, _oldCommission, _commission, msg.sender, stage, block.timestamp);
    }

    /*
    Events
    */
    event ArtFeatured( uint256  _tokenId, uint dateFeatured);
    event SaleEnabled( uint256  _tokenId, uint date);
    event FeatureWithdrawn(uint256  _tokenId, uint dateWithdrawn);
    event SaleDisabled( uint256  _tokenId, uint date);

    function featureWWArt(uint256  _tokenId) onlyOwner public returns (bool) {
        // Token must exist
        require(_exists(_tokenId), "This is not a valid TokenId");
        require(ownerOf(_tokenId) == msg.sender, "Only owner can feature an art item.");
        tokens[_tokenId].isFeatured= true;
        _increaseRoyalities(_tokenId, 300);
        emit ArtFeatured( _tokenId, block.timestamp);
        return true;
    }
    /***
     * customer can enable resale for the wwart contract
     */
    function enableSale(uint256  _tokenId)  public returns (bool) {
        // Token must exist
        require(_exists(_tokenId), "This is not a valid TokenId");
        require(ownerOf(_tokenId) == msg.sender, "Only owner can enable sale");
        require((tokens[_tokenId].priceInWei>0), "Price for the token is not set, please change Price.");
        tokens[_tokenId].isSaleEnabled = true;
        emit SaleEnabled( _tokenId, block.timestamp);
        return true;
      }
    /***
     * customer can enable resale for the wwart contract
     */
      function disableSale(uint256  _tokenId) public returns (bool) {
          // Token must exist
          require(_exists(_tokenId), "This is not a valid TokenId");
          require(ownerOf(_tokenId) != msg.sender, "Only owner can disable sale.");
          tokens[_tokenId].isSaleEnabled= false;
          tokens[_tokenId].priceInWei =0;
          emit SaleDisabled( _tokenId, block.timestamp);
          return false;
      }

    function isSaleEnabled(uint256 _tokenId) public view returns (bool) {
        return tokens[_tokenId].isSaleEnabled;
    }
    function isFeatured(uint256 _tokenId) public view returns (bool) {
        return tokens[_tokenId].isFeatured;
    }

    function setFeatured(uint256 _tokenId, bool _featured) public  returns (bool) {
        require(ownerOf(_tokenId) == msg.sender, "Not the owner");

        tokens[_tokenId].isFeatured= _featured;
        if(_featured){
            _increaseRoyalities(_tokenId, 300);
            emit ArtFeatured( _tokenId, block.timestamp);
        } else {
            _decreaseRoyalities(_tokenId, 300);
            emit FeatureWithdrawn(_tokenId, block.timestamp);
        }
        return _featured;
    }


    function _increaseRoyalities(uint256 _tokenId , uint256 _value) internal returns(uint256){
        uint256 currentRoyalties = getRoyalties(_tokenId);
        uint256 postFeatureRoyalities=  currentRoyalties+_value;
        if(postFeatureRoyalities <=1000){
            tokens[_tokenId].royalties = postFeatureRoyalities;
            return postFeatureRoyalities;
        } else {
            return currentRoyalties;
        }
    }

    function _decreaseRoyalities(uint256 _tokenId , uint256 _value) internal returns(uint256){
        uint256 currentRoyalties = getRoyalties(_tokenId);
        uint256 postFeatureRoyalities=  currentRoyalties - _value;
        if(postFeatureRoyalities <=1000){
            tokens[_tokenId].royalties = postFeatureRoyalities;
            return postFeatureRoyalities;
        } else {
            return currentRoyalties;
        }
    }


    // sale nft

    event Sale(uint256 indexed _tokenId, uint256 _price, uint now);
    event SendRoyalty(uint256 indexed _tokenId, uint256 _roaylty,uint now);
    event Payment( uint _amount, address from, address to, uint now);
    event SentComission(uint256 _tokenId, uint256 amount, uint now);


    function transferFromOwner(address _seller, address _buyer, uint256 _tokenId) internal returns(bool){
        _safeTransfer(_seller, _buyer, _tokenId, "");
        return true;
    }

    function sellWWART( uint256  _tokenId)  payable external{
        // Token must exist
        require(_exists(_tokenId), "This is not a valid TokenId");
        //require(getApproved(_tokenId) == address(this), 'address is not apporved');
        address  customer = msg.sender;
        address payable _platform= payable(owner());
        // sale isSaleEnabled

        require(tokens[_tokenId].isSaleEnabled, "Token is not enabled for sale.");


        // Token must be owned by message sender
        address payable  tokenOwner = payable(ownerOf(_tokenId));
        //require(tokenOwner == msg.sender, "To sale a token you must have ownership of it.");
        require(tokenOwner != address(0) && customer != address(0) && _platform != address(0), "Invalid address.");
        require(tokenOwner != customer, "Invalid customer address.");
        require(tokenOwner != msg.sender, "Invalid owner address.");
        require(tokenOwner != _platform, "Invalid MarketPlace address.");
        // Price must be non zero

        uint256 productPriceinWei = tokens[_tokenId].priceInWei;
        uint256 price = msg.value;
        uint256 customerBalance = address(customer).balance;//balanceOf(customer);
        require(customerBalance >=productPriceinWei, 'Insufficient Balance. Balance should be more than or equal to price.');
        // send ether is same as price
        require(price == productPriceinWei, "Invalid amount.");

        address payable  creator =  payable(tokens[_tokenId].creator);
        disableSale(_tokenId);
        _safeTransfer(tokenOwner, customer, _tokenId, "");
        uint256 commissionN = 0;
        if(creator == tokenOwner){
            commissionN = tokens[_tokenId].createCommission;
        } else {
            commissionN = tokens[_tokenId].resellCommission;
        }
        uint256 platformCut=0;
        uint256 commissionToPay = 0 ;
        // first time sale by artist
        if(creator == tokenOwner){
            commissionToPay = (msg.value.mul(commissionN)).div(commissionDenominator);
            // Transfer share amount for marketplace Owner
            _platform.transfer(commissionToPay);
            emit SentComission(_tokenId, commissionToPay, block.timestamp);
            uint256 payment = msg.value.sub(commissionToPay);
            //10% will go to platform
            uint256 platformPercentage = tokens[_tokenId].createCommission;
            platformCut = (payment.mul(platformPercentage)).div(commissionDenominator);
            //market place commission
            _platform.transfer(platformCut);
            uint256 ownerCut1 = payment.sub(commissionToPay);
            tokenOwner.transfer(ownerCut1);
            emit Payment(ownerCut1, tokenOwner, customer, block.timestamp);
        }
        // resale by customer
        if(creator != tokenOwner){
            commissionToPay = (msg.value.mul(commissionN)).div(commissionDenominator);
            _platform.transfer(commissionToPay);
            emit SentComission(_tokenId, commissionToPay, block.timestamp);
            uint256 royalty = getRoyalties(_tokenId);
            uint256 royaltiesToPay = (msg.value.mul(royalty)).div(10000);
            creator.transfer(royaltiesToPay);
            emit SendRoyalty(_tokenId, royaltiesToPay, block.timestamp);
            uint256 ownerCut = msg.value.sub(royaltiesToPay.add(commissionToPay));
            tokenOwner.transfer(ownerCut);
            emit Payment( ownerCut, tokenOwner, customer, block.timestamp);
        }


    }
    event  SentMoney(address payable _payee, uint256 _amount, uint256 balance);
    /**
     *  send / withdraw _amount to _payee
     */
    function sendTo(address payable _payee, uint256 _amount) public onlyOwner {
        require(_payee != address(0) && _payee != address(this));
        require(_amount > 0 && _amount <= address(this).balance);
        _payee.transfer(_amount);
        emit SentMoney(_payee, _amount, address(this).balance);
    }
}