pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "hardhat/console.sol";

interface ITokenVault {
    function transfer(
        address from,
        address to,
        uint256 amount
    ) external;
}

interface IEliteWorld {
    function getItemId(uint256 _id) external view returns (uint256);

    function getItemIdLocal(uint256 index) external view returns (uint256);

    function itemRegistry(uint256 tokenId) external view returns (uint256);

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) external;

    function ownerOf(uint256 tokenId) external view returns (address);

    function itemOf(uint256 tokenId, address owner)
        external
        view
        returns (bool);

    function itemOfOwnerByToken(uint256 tokenId)
        external
        view
        returns (uint256);

    function itemBlock(uint256 id, address owner) external view returns (bool);

    function setItemOf(
        address _oldOwner,
        address _tokenOwner,
        uint256 _tokenId
    ) external;

    function prizeAuction(uint256 id, address owner) external;
}

contract EliteWorldMarketAuctionSystem is Initializable, OwnableUpgradeable {
    using SafeMath for uint256;

    ITokenVault public tokenVault;
    IEliteWorld public eliteWorld;

    uint256 public auctionFee;

    uint256 public balanceOfOwner;
    mapping(uint256 => uint256) private _ownedItemId;

    mapping(uint256 => uint256) public tokenAuctionStartingPrice;

    mapping(uint256 => uint256) public tokenAuctionStep;
    mapping(uint256 => uint256) public tokenAuctionEnd;

    mapping(uint256 => address) public tokenAuctionAddress;
    mapping(uint256 => uint256) public tokenAuctionAddressAmount;

    mapping(address => uint256) public balanceOfBuyer;
    mapping(address => mapping(uint256 => uint256)) public _buyTokens;
    mapping(uint256 => uint256) private _buyTokensIndex;

    mapping(address => mapping(uint256 => bool)) public ownedItemIdAuction;

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(address _tokenVault, address _eliteWorld)
        public
        initializer
    {
        tokenVault = ITokenVault(_tokenVault);
        eliteWorld = IEliteWorld(_eliteWorld);
        __Ownable_init();
    }

    function setAuctionFee(uint256 _fee) public onlyOwner {
        auctionFee = _fee;
    }

    function setTokenVault(address _tokenVault) public onlyOwner {
        tokenVault = ITokenVault(_tokenVault);
    }

    function setEliteWorld(address _eliteWorld) public onlyOwner {
        eliteWorld = IEliteWorld(_eliteWorld);
    }

    // ============ Private ============

    function _addTokenToOwnerEnumeration(uint256 _idItem) private {
        _ownedItemId[balanceOfOwner] = _idItem;
        balanceOfOwner += 1;
    }

    function _removeTokenFromOwnerEnumeration(uint256 _indexBalance) private {
        uint256 lastIndexBalance = balanceOfOwner - 1;

        // When the token to delete is the last token, the swap operation is unnecessary
        if (_indexBalance != lastIndexBalance) {
            uint256 lastItemId = _ownedItemId[lastIndexBalance];
            _ownedItemId[_indexBalance] = lastItemId; // Move the last token to the slot of the to-delete token
            tokenAuctionStartingPrice[
                _indexBalance
            ] = tokenAuctionStartingPrice[lastIndexBalance];
            tokenAuctionStep[_indexBalance] = tokenAuctionStep[
                lastIndexBalance
            ];
            tokenAuctionEnd[_indexBalance] = tokenAuctionEnd[lastIndexBalance];
            tokenAuctionAddressAmount[
                _indexBalance
            ] = tokenAuctionAddressAmount[lastIndexBalance];
            if (tokenAuctionAddress[_indexBalance] != address(0)) {
                tokenAuctionAddress[_indexBalance] = tokenAuctionAddress[
                    lastIndexBalance
                ];
            }
        }

        delete _ownedItemId[lastIndexBalance];
        delete tokenAuctionStartingPrice[lastIndexBalance];
        delete tokenAuctionStep[lastIndexBalance];
        delete tokenAuctionEnd[lastIndexBalance];
        delete tokenAuctionAddressAmount[lastIndexBalance];
        delete tokenAuctionAddress[lastIndexBalance];
        balanceOfOwner -= 1;
    }

    function _addTokenToBuyerEnumeration(address to, uint256 _indexBalanceOwner)
        private
    {
        _buyTokens[to][balanceOfBuyer[to]] = _indexBalanceOwner;
        _buyTokensIndex[_indexBalanceOwner] = balanceOfBuyer[to];
        balanceOfBuyer[to] += 1;
    }

    function _removeTokenFromBuyerEnumeration(address from, uint256 _index)
        private
    {
        uint256 lastTokenIndex = balanceOfBuyer[from] - 1;
        uint256 tokenIndex = _buyTokensIndex[_index];

        // When the token to delete is the last token, the swap operation is unnecessary
        if (tokenIndex != lastTokenIndex) {
            uint256 lastTokenId = _buyTokens[from][lastTokenIndex];

            _buyTokens[from][tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
            _buyTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index
        }

        // This also deletes the contents at the last position of the array
        delete _buyTokensIndex[_index];
        delete _buyTokens[from][lastTokenIndex];
        balanceOfBuyer[from] -= 1;
    }

    // ============ Helper ============

    function tokenOfOwnerByIndex(uint256 index)
        public
        view
        virtual
        returns (uint256)
    {
        require(index < balanceOfOwner, "owner index out of bounds");
        return _ownedItemId[index];
    }

    function tokenOfBuyerByIndex(address buyer, uint256 index)
        public
        view
        virtual
        returns (uint256)
    {
        require(index < balanceOfBuyer[buyer], "buyer index out of bounds");
        return _ownedItemId[_buyTokens[buyer][index]];
    }

    // ============ Market ============

    function auctionToken(
        uint256 _idItem,
        uint256 _startingPrice,
        uint256 _step,
        uint256 _days
    ) external onlyOwner {
        require(_startingPrice > 0, "Price must greater than zero");
        require(_step > 0, "Step must greater than zero");
        require(_days > 0, "Days must greater than zero");

        tokenAuctionStartingPrice[balanceOfOwner] = _startingPrice;
        tokenAuctionStep[balanceOfOwner] = _step;
        tokenAuctionEnd[balanceOfOwner] = block.timestamp + _days * 86400;
        tokenAuctionAddressAmount[balanceOfOwner] = _startingPrice;

        _addTokenToOwnerEnumeration(_idItem);
    }

    function cancelAuctionToken(uint256 _indexBalance) external onlyOwner {
        require(_indexBalance < balanceOfOwner, "index balance out of bounds");
        require(
            tokenAuctionAddressAmount[_indexBalance] ==
                tokenAuctionStartingPrice[_indexBalance],
            "Token already in auction"
        );

        _removeTokenFromOwnerEnumeration(_indexBalance);
    }

    function auctionBidToken(uint256 _indexBalance, uint256 _price) external {
        require(_indexBalance < balanceOfOwner, "index out of bounds");
        require(
            !eliteWorld.itemOf(_ownedItemId[_indexBalance], msg.sender),
            "Item purchased"
        );
        require(
            msg.sender != tokenAuctionAddress[_indexBalance],
            "You are the highest bidder"
        );
        require(
            !ownedItemIdAuction[msg.sender][_ownedItemId[_indexBalance]],
            "Already in auction same item"
        );
        require(
            _price >=
                tokenAuctionAddressAmount[_indexBalance].add(
                    tokenAuctionStep[_indexBalance]
                ),
            "Bid price must greater than current bid by a step"
        );
        require(
            tokenAuctionEnd[_indexBalance] > block.timestamp,
            "Auction already end"
        );
        address lastBuyer = tokenAuctionAddress[_indexBalance];
        uint256 lastAmount = tokenAuctionAddressAmount[_indexBalance];

        tokenAuctionAddress[_indexBalance] = msg.sender;
        tokenAuctionAddressAmount[_indexBalance] = _price;
        ownedItemIdAuction[msg.sender][_ownedItemId[_indexBalance]] = true;

        //return bun for previous bid address
        if (lastBuyer != address(0)) {
            delete ownedItemIdAuction[lastBuyer][_ownedItemId[_indexBalance]];
            tokenVault.transfer(address(tokenVault), lastBuyer, lastAmount);
            _removeTokenFromBuyerEnumeration(lastBuyer, _indexBalance);
        }
        tokenVault.transfer(address(msg.sender), address(tokenVault), _price);
        _addTokenToBuyerEnumeration(msg.sender, _indexBalance);
    }

    function collectAuctionToken(uint256 _indexBalance) external {
        require(
            tokenAuctionEnd[_indexBalance] < block.timestamp,
            "Auction not end"
        );
        require(
            tokenAuctionAddress[_indexBalance] == msg.sender,
            "Not token buyer"
        );

        _removeTokenFromBuyerEnumeration(msg.sender, _indexBalance);
        _removeTokenFromOwnerEnumeration(_indexBalance);
        delete ownedItemIdAuction[msg.sender][_ownedItemId[_indexBalance]];
        eliteWorld.prizeAuction(_ownedItemId[_indexBalance], msg.sender);
    }
}

