        swapAndLiquifyEnabled = val;
    }

    function setBuyAllowed(bool val) external onlyOwner {
        buyAllowed = val;
    }

    //call when presale in progress
    function setPresale() external onlyOwner {
        _marketingFee = 0;
        _liquidityFee = 0;
        _totalFee = 0;
        _maxTxLimitEnabled = false;
        swapAndLiquifyEnabled = false;
        inPresale = true;
    }

    //call when presale over
    function endPresale() external onlyOwner {
        _marketingFee = 1;
        _liquidityFee = 2;
        _totalFee = 3;
        _maxTxLimitEnabled = true;
        swapAndLiquifyEnabled = true;
        inPresale = false;
    }

    function updateUniswapV2Router(address newAddress) external onlyOwner {
        require(
            newAddress != address(uniswapV2Router),
            "The router already has that address"
        );
        emit UpdateUniswapV2Router(newAddress, address(uniswapV2Router));
        uniswapV2Router = IUniswapV2Router02(newAddress);
        address _uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory())
            .createPair(address(this), uniswapV2Router.WETH());
        uniswapV2Pair = _uniswapV2Pair;
    }

    /**
