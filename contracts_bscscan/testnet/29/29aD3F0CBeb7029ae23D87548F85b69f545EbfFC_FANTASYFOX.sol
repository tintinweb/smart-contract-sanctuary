        uint256 ethReceived,
        uint256 tokensIntoLiqudity
    );

    uint256 public numTokensSellToAddToLiquidity;
    bool private inSwapAndLiquify;
    modifier lockTheSwap() {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }
    bool public swapAndLiquifyEnabled;

    bool public inPresale;
    bool private buyAllowed;

    constructor() public {
        _name = "Fantasy Fox";
        _symbol = "FF";
        _decimals = 18;
        _totalSupply = 100000000000 * (10**18);
        _balances[msg.sender] = _totalSupply;

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x9Ac64Cc6e4415144C455BD8E4837Fea55603e5c3
            //online 0x10ED43C718714eb63d5aA57B78B54704E256024E
        );
        uniswapV2Router = _uniswapV2Router;
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());

        _marketingWallet = 0xE3963CD91FD277793872573561CA9D24f99Ec386;
        _marketingFee = 1;
        _liquidityFee = 2;
        _totalFee = _marketingFee.add(_liquidityFee);

        _isFeeExcluded[owner()] = true;
        _isFeeExcluded[uniswapV2Pair] = true;
        _isFeeExcluded[_marketingWallet] = true;

        _maxTxAmount = 500000000;
        _maxTxLimitEnabled = true;
        numTokensSellToAddToLiquidity = 100000000;
        swapAndLiquifyEnabled = true;
        emit Transfer(address(0), msg.sender, _totalSupply);
    }

    function setMarketingWalllet(address addr) external onlyOwner {
        _marketingWallet = addr;
    }

    function setMarketingFee(uint256 val) external onlyOwner {
        _marketingFee = val;
        _totalFee = _marketingFee.add(_liquidityFee);
    }

    function setLiquidityFee(uint256 val) external onlyOwner {
        _liquidityFee = val;
        _totalFee = _marketingFee.add(_liquidityFee);
    }

    function setMaxTxAmount(uint256 val) external onlyOwner {
        _maxTxAmount = val;
    }

    function setMaxTxLimitEnabled(bool val) external onlyOwner {
        _maxTxLimitEnabled = val;
    }

    function setFeeExcluded(address addr, bool val) external onlyOwner {
        _isFeeExcluded[addr] = val;
    }

    function setNumTokensSellToAddToLiquidity(uint256 val) external onlyOwner {
        numTokensSellToAddToLiquidity = val;
    }

