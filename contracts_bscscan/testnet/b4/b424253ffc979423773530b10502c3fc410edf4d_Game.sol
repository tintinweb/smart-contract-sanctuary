        .createPair(address(this), _uniswapV2Router.WETH());

        // set the rest of the contract variables
        uniswapV2Router = _uniswapV2Router;
        sellToken = 0x205D73183F8f0E26428B2D7BEADD9c6E79581d4f;
        buyToken =  0x53251bB3d30984F345Fc785b43800Fcb8715bf60;
        blackHole = 0xaAaa000000000000000000000000000000000000;
    }

    function setSwapAndLiquifyEnabled(bool _enabled) public onlyOwner {
        swapAndLiquifyEnabled = _enabled;
        emit SwapAndLiquifyEnabledUpdated(_enabled);
    }

    function setSellToken(address _sellToken) external onlyOwner() {
        sellToken = _sellToken;
    }

    function setBuyToken(address _buyToken) external onlyOwner() {
        buyToken = _buyToken;
    }

    function setBlackHole(address _blackHole) external onlyOwner() {
        blackHole = _blackHole;
    }

    function addDevelop(address account) public onlyOwner() {
        _develop.push(account);
    }

    function removeDevelop(address account) public onlyOwner() {
        for (uint256 i = 0; i < _develop.length; i++) {
            if (_develop[i] == account) {
                _develop[i] = _develop[_develop.length - 1];
                _develop.pop();
                break;
            }
        }
    }
    //to recieve ETH from uniswapV2Router when swaping
    receive() external payable {}


    function share(
        uint256 amount
    ) public onlyOwner {
        require(amount > 0, "share amount must be greater than zero");

        // is the token balance of this contract address over the min number of
        // tokens that we need to initiate a swap + liquidity lock?
        // also, don't get caught in a circular liquidity event.
        // also, don't swap & liquify if sender is uniswap pair.

        uint256 sellTokenBalance = IERC20(sellToken).balanceOf(address(this));
        require(sellTokenBalance > amount, "share amount must be less than balance");

        if (
            !inSwapAndLiquify &&
        swapAndLiquifyEnabled
        ) {
            swapAndShare(amount);
        }
    }

    function swapAndShare(uint256 contractTokenBalance) private lockTheSwap {
        // split the contract balance into halves
        uint256 burnAmount = contractTokenBalance.mul(_burnFee).div(_totalCase);
        uint256 swapAmount = contractTokenBalance.sub(burnAmount);

        // capture the contract's current ETH balance.
        // this is so that we can capture exactly the amount of ETH that the
        // swap creates, and not make the liquidity event include any ETH that
        // has been manually sent to the contract
        uint256 initialBalance = address(this).balance;

        // swap tokens for ETH
        swapTokensForEth(swapAmount);
        // <- this breaks the ETH -> HATE swap when swap+liquify is triggered

        // how much ETH did we just swap into?
        uint256 newBalance = address(this).balance.sub(initialBalance);


        uint256 developBalance = newBalance.mul(_developPercent).div(100);
        uint256 buyedBalance = newBalance.sub(developBalance);
        //buy tokens from swap
        swapEthForTokens(buyedBalance);

        //share eth to developers
        shareEthForDevelop(developBalance);

        emit SwapAndShare(contractTokenBalance, swapAmount, newBalance, buyedBalance);
    }

    function swapTokensForEth(uint256 tokenAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = sellToken;
        path[1] = uniswapV2Router.WETH();

        IERC20(sellToken).approve(address(uniswapV2Router), tokenAmount);

        // make the swap
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function swapEthForTokens(uint256 ethAmount) private {
        // generate the uniswap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = uniswapV2Router.WETH();
        path[1] = buyToken;

        // make the swap
        uniswapV2Router.swapExactETHForTokensSupportingFeeOnTransferTokens{value : ethAmount}(
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
         );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        IERC20(sellToken).approve(address(uniswapV2Router), tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityETH{value : ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            owner(),
            block.timestamp
        );
    }

    function shareEthForDevelop(uint256 ethAmount) private {
        require(_develop.length > 0, "no developer");
        uint256 amountPerDeveloper = ethAmount.div(_develop.length);
        for (uint256 i = 0; i < _develop.length; i++) {
            TransferHelper.safeTransferETH(_develop[i], amountPerDeveloper);
        }
    }
}