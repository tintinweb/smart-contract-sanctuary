contract CryptoFarmBank is Context, Ownable {
    address private cryptofarmTokenAddress;
    address private cryptoFarmPairAddress;
    address private cryptoFarmRouterAddress;

    event ClaimToken (address _inWallet, uint256 _amount);
    
    constructor(address _cryptofarmTokenAddress, address _cryptofarmPairAddress){
        payable(address(this));
        cryptofarmTokenAddress = _cryptofarmTokenAddress;
        cryptoFarmPairAddress = _cryptofarmPairAddress;
        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(
            0x4e9Fc8C34A78997d3705340401Aa76439Ee54B33
            
        );
        //0x10ED43C718714eb63d5aA57B78B54704E256024E
        cryptoFarmRouterAddress = address(_uniswapV2Router);
        ICryptofarmToken(cryptofarmTokenAddress).approve(cryptoFarmRouterAddress,~uint256(0));
        
    }
    
    function setCryyptofarmTokenAddress(address _cryptofarmTokenAddress, address _cryptofarmPairAddress) public onlyOwner{
        cryptofarmTokenAddress = _cryptofarmTokenAddress;
        cryptoFarmPairAddress = _cryptofarmPairAddress;
    }
    
    
    function claimToken(address _inWallet , uint256 _amount) public onlyOwner{
        ICryptofarmToken(cryptofarmTokenAddress).transfer(_inWallet,_amount);
        emit ClaimToken(_inWallet,_amount);
    }
}

contract CryptoFarmActivity is Context,Ownable{
    using SafeMath for uint256;
    uint256[10000] public activityID;
    mapping(uint256 => uint) public price;
    mapping(uint256 => uint256) public maxNumber;
    mapping(uint256 => uint256) public lastNumber;
    mapping(uint256 => bool) public isSelling;  
    address public activityWallet;
 
    constructor(address _activityWallet){
        activityWallet = _activityWallet;
        payable(activityWallet);
        
    }
    function setActivityDetail(uint256 _index,uint256 _maxNumber,uint _price,uint256 _lastNumber) public onlyOwner  returns (bool){
        activityID[_index] = _index;
        maxNumber[_index] = _maxNumber;
        price[_index] = _price ;
        lastNumber[_index] = _lastNumber;
        return true;
        
    }
    
    function setSelliingStaus(uint256 _index,bool _isSelling) public onlyOwner returns (bool){
        isSelling[_index] = _isSelling;
        return true;
    }
    
    
    function setLastNumber(uint256 _index, uint256 _lastNumber) public onlyOwner returns (bool){
        lastNumber[_index] = _lastNumber;
        return true;
    }
    
    function transferForActivity(uint256 _index) public payable {
        require(activityID[_index] == _index, "ActivityID wrong");
        require(lastNumber[_index] > 0, "Insufficient inventory");
        require(isSelling[_index] == true,"Sale has not commenced or has ended");
        require(msg.value == price[_index],"value not equal");
    
    }
    
    function setActivityWallet(address _activityWallet) public onlyOwner{
        activityWallet = _activityWallet;
    }
    
    function getEthBalance() public view returns (uint){
        return address(this).balance;
    }
    
    function withdraw() public onlyOwner payable{
        payable(activityWallet).transfer(address(this).balance-10000000);
    }
    
    
    
}