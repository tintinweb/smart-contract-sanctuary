pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/math/MathUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";

import "../CreatorExtensionTokenURI.sol";
import "../../ERC721Creator.sol";

contract AmbassadorExtension is
    CreatorExtensionTokenURI,
    Initializable,
    OwnableUpgradeable,
    ReentrancyGuardUpgradeable,
    PausableUpgradeable,
    UUPSUpgradeable
{
    using AddressUpgradeable for address payable;
    using StringsUpgradeable for uint256;
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using SafeERC20 for IERC20;

    /* ========== ENUMS ========== */
    enum Rarity {
        Normal,
        Rare,
        SuperRare,
        Legendary,
        Epic
    }

    /* ========== STRUCTS ========== */

    struct NFTInfo {
        uint8 level; // 1 - 5
        Rarity rarity;
        uint256 printEdition;
    }

    struct BlindBoxSaleInfo {
        uint256 startTime;
        uint256 endTime;
        uint256 maxSaleNumber;
        EnumerableSetUpgradeable.UintSet records;
    }

    /* ========== STATE VARIABLES ========== */

    // global
    // NFT contract
    ERC721Creator public nft;
    uint256 public unrevealed;

    // sale
    uint256[5] public presalePrices;
    uint256[5] public presaleTracker;
    uint256[5] public maxPresaleNumbers;
    uint256[][5] public nftNumbers;
    uint256[5] public nftNumbersPerLevel;
    string public nftBaseURI;
    NFTInfo[378] public nftHelperArray;
    uint256 public total;
    uint256 public presaleStartTime;
    uint256 public presaleEndTime;
    mapping(uint256 => NFTInfo) public tokenNFTInfo;

    // blind box
    IERC20 public pctToken;
    uint256 public blindBoxPrice;
    string public blindBoxURI;
    uint256 public nextBindBoxSaleId;
    mapping(uint256 => BlindBoxSaleInfo) private blindBoxSaleInfos;
    mapping(uint256 => uint256) public tokenIdToBlindBoxSaleId;

    /* ========== EVENTs ========== */

    event PresaleTimeUpdated(uint256 startTime, uint256 endTime);
    event PresalePurchased(address indexed buyer, uint256 indexed tokenId, NFTInfo nftInfo);
    event BlindBoxRevealed(uint256 indexed tokenId, NFTInfo nftInfo);
    event LogRefund(address indexed caller, uint256 indexed amount);
    event BlindBoxSaleCreated(uint256 indexed id, uint256 startTime, uint256 endTime, uint256 maxSaleNumber);
    event BlindBoxSaleUpdated(uint256 indexed id, uint256 startTime, uint256 endTime, uint256 maxSaleNumber);
    event BlindBoxPriceUpdated(uint256 newPrice);
    event BlindBoxClaimed(uint256 indexed tokenId);
    /* ========== MODIFIERS ========== */

    /// @dev Require that the caller must be an EOA account if not whitelisted.
    modifier onlyEOA() {
        require(msg.sender == tx.origin, "not eoa");
        _;
    }

    /* ========== INITIALIZER ========== */

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(address nftAddress, address pctAddress) public initializer {
        __Ownable_init();
        __ReentrancyGuard_init();
        __Pausable_init();
        __UUPSUpgradeable_init();

        require(nftAddress != address(0), "bad nftAddress");
        nft = ERC721Creator(nftAddress);

        // initial params
        presalePrices = [0.5 ether, 1.2 ether, 3.2 ether, 6.8 ether, 13.8 ether];
        maxPresaleNumbers = [28, 18, 8, 4, 1];
        // rarity distribution
        nftNumbers[0] = [57, 47, 38, 28, 19];
        nftNumbers[1] = [28, 24, 19, 14, 9];
        nftNumbers[2] = [17, 14, 11, 9, 6];
        nftNumbers[3] = [10, 9, 7, 5, 3];
        nftNumbers[4] = [4];
        total = 378;
        nftBaseURI = "ipfs://bafybeibskwbozfdceriycqhoxtpa2u2kz5jkdts3fejnr3u27lqle6azkm/";
        blindBoxURI = "ipfs://bafybeibskwbozfdceriycqhoxtpa2u2kz5jkdts3fejnr3u27lqle6azkm/blind_box.json";
        // initialize random helper array
        uint256 _index = 0;
        uint256 numberPerLevel;
        for (uint8 i = 0; i < nftNumbers.length; i++) {
            numberPerLevel = 0;
            for (uint8 j = 0; j < nftNumbers[i].length; j++) {
                uint256 num = nftNumbers[i][j];
                for (uint256 k = 0; k < num; k++) {
                    nftHelperArray[_index++] = NFTInfo({
                        level: i + 1,
                        rarity: i != 4 ? Rarity(j) : Rarity.Epic, // only epic for level 5
                        printEdition: _index
                    });
                }
            }
            nftNumbersPerLevel[i] = numberPerLevel;
        }
        // init unrevealed
        unrevealed = total;
        require(_index == total, "bad nftNumbers");

        // init blind box info
        nextBindBoxSaleId = 1;
        pctToken = IERC20(pctAddress);
        blindBoxPrice = 5000 ether;
    }

    /* ========== VIEW FUNCTIONS ========== */
    ///
    /// @notice Get the uri for a given project/tokenId
    ///
    function tokenURI(
        address, /* project */
        uint256 tokenId
    ) public view override returns (string memory) {
        uint256 blindBoxSaleId = tokenIdToBlindBoxSaleId[tokenId];
        // is blind box and not revealed
        if (blindBoxSaleId != 0 && blindBoxSaleInfos[blindBoxSaleId].records.contains(tokenId)) {
            return blindBoxURI;
        }
        NFTInfo storage nftInfo = tokenNFTInfo[tokenId];
        require(nftInfo.level > 0, "PresaleExtension: nonexistent token");
        return string(abi.encodePacked(nftBaseURI, nftInfo.printEdition.toString()));
    }

    function presaleLeftAmount() public view returns (uint256[5] memory res) {
        for (uint8 i = 0; i < 5; i++) {
            res[i] = maxPresaleNumbers[i] - presaleTracker[i];
        }
    }

    /* ========== MUTATIVE FUNCTIONS ========== */

    /// @notice claim blind box nft
    /// @param num uint256 number of blind boxes to claim
    function claimBlindBox(uint256 blindBoxSaleId, uint256 num) external nonReentrant onlyEOA {
        // 1. sanity check
        BlindBoxSaleInfo storage blindBoxInfo = blindBoxSaleInfos[blindBoxSaleId];
        uint256 alreadyMinted = blindBoxInfo.records.length();
        uint256 left = blindBoxInfo.maxSaleNumber - alreadyMinted;
        uint256 nftAmount = MathUpgradeable.min(num, left);
        require(nftAmount != 0, "blind box sold out");
        // 2. transfer pct
        uint256 _checkoutCost = nftAmount * blindBoxPrice;
        pctToken.safeTransferFrom(_msgSender(), address(this), _checkoutCost);
        // 3. mint blind box
        uint256 tokenId;
        for (uint256 i = 0; i < nftAmount; i++) {
            tokenId = nft.mintExtension(_msgSender(), "");
            blindBoxInfo.records.add(tokenId);
            tokenIdToBlindBoxSaleId[tokenId] = blindBoxSaleId;
            emit BlindBoxClaimed(tokenId);
        }
    }

    /// @notice buy sale NFT
    /// @param level uint8 nft level to purchase 1-5
    /// @param num uint256 number of NFTs to purchase
    function purchasePresale(uint8 level, uint256 num) external payable nonReentrant onlyEOA {
        // 1.1 check level sanity
        require(level > 0 && level < 6, "bad level");
        uint8 levelIndex = level - 1;
        // 1.2 check left
        uint256 alreadySold = presaleTracker[levelIndex];
        uint256 left = maxPresaleNumbers[levelIndex] - alreadySold;
        require(left > 0, "sold out");
        // 2. real amount can be purchased
        uint256 nftAmount = MathUpgradeable.min(left, num);
        // 3. check funds
        uint256 _checkoutCost = nftAmount * presalePrices[levelIndex];
        require(msg.value >= _checkoutCost, "insufficient funds");
        // 4. determine start of the position
        uint256 startsFrom = 0;
        for (uint256 i = 0; i < levelIndex; i++) {
            startsFrom += nftNumbersPerLevel[i];
        }
        startsFrom += presaleTracker[levelIndex];
        // 5. increase tracker before mint
        presaleTracker[levelIndex] += nftAmount;
        // 6. mint loop
        uint256 tokenId;
        uint256 position;
        for (uint256 i = 0; i < nftAmount; i++) {
            // 6.1 call nft contract to mint token to msg.sender
            tokenId = nft.mintExtension(_msgSender(), "");
            position = startsFrom + i;
            // 6.2 find and consume NFT, swap to tail
            NFTInfo memory info = _consumeNFT(position);
            tokenNFTInfo[tokenId] = info;
            emit PresalePurchased(_msgSender(), tokenId, info);
        }
        // 7. Refund unused fund
        uint256 _changes = msg.value - _checkoutCost;
        if (_changes > 0) {
            AddressUpgradeable.sendValue(payable(_msgSender()), _changes);
            //            payable(_msgSender()).sendValue(_changes);
            emit LogRefund(_msgSender(), _changes);
        }
    }

    /* ========== RESTRICTED FUNCTIONS ========== */

    /// @notice admin reveal blind box
    /// @param blindBoxSaleId uint256 sale id
    /// @param batchSize uint256 batch size, prevent revealing too much causing out of gas
    function revealBlindBox(uint256 blindBoxSaleId, uint256 batchSize) external onlyOwner {
        BlindBoxSaleInfo storage blindBoxInfo = blindBoxSaleInfos[blindBoxSaleId];
        require(blindBoxInfo.records.length() > 0, "no more blind boxes to reveal");
        uint256 batch = MathUpgradeable.min(blindBoxInfo.records.length(), batchSize);
        uint256 tokenId;
        for (uint256 i = 0; i < batch; i++) {
            // get token id from records, and remove it after revealing
            tokenId = blindBoxInfo.records.at(i);
            blindBoxInfo.records.remove(tokenId);
            // assign a random nft info to this token
            uint256 randomIndex = _getRandomNumber(unrevealed - 1);
            NFTInfo memory nftInfo = _consumeNFT(randomIndex);
            tokenNFTInfo[tokenId] = nftInfo;
            emit BlindBoxRevealed(tokenId, nftInfo);
        }
    }

    function updateBlindBoxPrice(uint256 price) external onlyOwner {
        blindBoxPrice = price;
        emit BlindBoxPriceUpdated(price);
    }

    function updatePresaleTime(uint256 _startTime, uint256 _endTime) external onlyOwner {
        presaleStartTime = _startTime;
        presaleEndTime = _endTime;
        emit PresaleTimeUpdated(_startTime, _endTime);
    }

    function createBlindBoxSaleInfo(
        uint256 _startTime,
        uint256 _endTime,
        uint256 _maxSaleNumber
    ) external onlyOwner {
        require(_endTime == 0 || _endTime > _startTime, "bad endTime");
        require(_maxSaleNumber > 0, "bad maxSaleNumber");
        uint256 id = nextBindBoxSaleId++;
        BlindBoxSaleInfo storage info = blindBoxSaleInfos[id];
        info.startTime = _startTime;
        info.endTime = _endTime;
        info.maxSaleNumber = _maxSaleNumber;
        emit BlindBoxSaleCreated(id, _startTime, _endTime, _maxSaleNumber);
    }

    function updateBlindBoxSaleInfo(
        uint256 _blindBoxSaleId,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _maxSaleNumber
    ) external onlyOwner {
        BlindBoxSaleInfo storage info = blindBoxSaleInfos[_blindBoxSaleId];
        require(info.maxSaleNumber != 0, "blind box sale not exists");
        require(_endTime == 0 || _endTime > _startTime, "bad endTime");
        require(_maxSaleNumber > 0, "bad maxSaleNumber");
        info.startTime = _startTime;
        info.endTime = _endTime;
        info.maxSaleNumber = _maxSaleNumber;
        emit BlindBoxSaleUpdated(_blindBoxSaleId, _startTime, _endTime, _maxSaleNumber);
    }

    /// @dev Withdraw native funds
    /// @param _to The address to received funds
    function withdrawNative(address payable _to) external onlyOwner {
        _to.sendValue(address(this).balance);
    }

    function withdrawToken(
        address _tokenAddress,
        address _to,
        uint256 _amount
    ) external onlyOwner {
        IERC20(_tokenAddress).safeTransfer(_to, _amount);
    }

    /// @dev pause the contract
    function pause() external onlyOwner {
        _pause();
    }

    /// @dev unpause the contract
    function unpause() external onlyOwner {
        _unpause();
    }

    /* ========== INTERNAL FUNCTIONS ========== */

    /**
     * @dev Generates a pseudo-random number.
     */
    function _getRandomNumber(uint256 _upper) private view returns (uint256) {
        uint256 random = uint256(
            keccak256(
                abi.encodePacked(
                    total - unrevealed,
                    blockhash(block.number - 1),
                    block.coinbase,
                    block.difficulty,
                    _msgSender()
                )
            )
        );

        return random % _upper;
    }

    function _consumeNFT(uint256 i) internal returns (NFTInfo memory) {
        // swap the used nft info to the tail
        NFTInfo memory info = nftHelperArray[i];
        // unrevealed - 1 is the tail index
        uint256 tailIndex = --unrevealed;
        nftHelperArray[i] = nftHelperArray[tailIndex];
        nftHelperArray[tailIndex] = info;
        return info;
    }

    function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}
}

