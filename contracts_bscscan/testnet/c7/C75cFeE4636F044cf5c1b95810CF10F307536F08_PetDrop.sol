pragma solidity 0.8.7;





struct PetData {
    uint256 _type;
    uint256 _attack;
    uint256 _life;
    uint256 _energy;
    uint256 _agility;
    uint256 _skin;
    uint256 _power;
    bool _male;
}

contract ArkerPets is ERC721Enumerable, AccessControl {
    
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIds;
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    // Token Id => PetData
    mapping(uint256 => PetData) public _petData;

    string public baseURI;

    constructor() ERC721("ArkerPets", "ARPT") {
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);
    }

    function mint(
        address player,
        uint256 petType,
        uint256 attack,
        uint256 life,
        uint256 energy,
        uint256 agility,
        uint256 skin,
        uint256 power,
        bool male
    )
        public
        returns (uint256)
    {
        require(hasRole(MINTER_ROLE, msg.sender), "Caller is not a minter");
        require(attack <= 100, "Attack can't be more than 100");
        require(life <= 100, "Life can't be more than 100");
        require(energy <= 100, "Energy can't be more than 100");
        require(agility <= 100, "Agility can't be more than 100");

        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();
        _safeMint(player, newItemId);

        PetData memory data;
        data._type = petType;
        data._attack = attack;
        data._life = life;
        data._energy = energy;
        data._agility = agility;
        data._male = male;
        data._skin = skin;
        data._power = power;
        _petData[newItemId] = data;
        return newItemId;
    }

    function getPetData(uint256 tokenId)
        public
        view
        returns (PetData memory)
    {
        return _petData[tokenId];
    }

    function setBaseURI(string memory _newBaseURI) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), 'Only for role DEFAULT_ADMIN_ROLE');
        baseURI = _newBaseURI;
    }

    function _baseURI() internal view override(ERC721) returns (string memory) {
        return baseURI;
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721Enumerable, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

}

