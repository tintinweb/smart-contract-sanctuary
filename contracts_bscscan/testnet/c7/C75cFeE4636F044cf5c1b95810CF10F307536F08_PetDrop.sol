pragma solidity 0.8.7;











contract ArkerBreeder is ERC721Enumerable, Pausable, Ownable, AccessControl {

    using SafeMath for uint256;

    using Counters for Counters.Counter;

    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");

    Counters.Counter private _eggIds;
    uint256 private constant maskLast8Bits = uint256(0xff);
    uint256 private constant maskFirst248Bits = ~uint256(0xff);

    // Token Id => EggData
    mapping(uint256 => EggData) public _eggData;

    // Pet Id => Number of births
    mapping(uint256 => uint256) public _breedCount;

    // Pet Id => Father Id
    mapping(uint256 => uint256) _fathers;
    // Pet Id => Mother Id
    mapping(uint256 => uint256) _mothers;

    // Pet Id => Time to breed again
    mapping(uint256 => uint256) _breedCooldown;
    uint256 public _breedCooldownTime = 432000; // 5 days

    uint256[] public _breedFoAFees = [100 ether];
    uint256[] public _breedArkerFees = [1 ether];
    uint256 public _maxBreeds = 1;


    address[] public _feesDestinators;
    uint256[] public _feesPercentages;
    address public _feeArkerDestinator;
    uint256 public _blocksUntilHatch = 144000; // 5 days

    string public baseURI;

    IERC20 public _foa;
    IERC20 public _arker;
    ArkerPets public _pets;
    IGeneMixer public _geneMixer;

    constructor(IERC20 foa, IERC20 arker, ArkerPets pets, IGeneMixer geneMixer) ERC721("ArkerEggs", "EARK") {
        _foa = foa;
        _arker = arker;
        _pets = pets;
        _geneMixer = geneMixer;
        _feesDestinators = [msg.sender];
        _feeArkerDestinator = msg.sender;
        _feesPercentages = [100];
        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _setupRole(MINTER_ROLE, msg.sender);
    }

    function getBreedCost(uint256 mother) external view returns (uint256 foa, uint256 arker) {
        return (
            _breedFoAFees[_breedCount[mother]],
            _breedArkerFees[_breedCount[mother]]
        );
    }

    function getParents(uint256 petId) external view returns (uint256 father, uint256 mother) {
        return (
            _fathers[petId],
            _mothers[petId]
        );
    }

    function getEggData(uint256 eggId)
        public
        view
        returns (EggData memory)
    {
        return _eggData[eggId];
    }

    function breed(uint256 father, uint256 mother, address eggReceiver) external onlyHumans whenNotPaused {
        require(_pets.ownerOf(father) == msg.sender, "You're not the owner of the father");
        require(_pets.ownerOf(mother) == msg.sender, "You're not the owner of the mother");

        PetData memory fatherData = _pets.getPetData(father);
        PetData memory motherData = _pets.getPetData(mother);

        require(fatherData._type == motherData._type, "You're not god. You can't breed cross types.");
        require(fatherData._male, "Father must be male");
        require(!motherData._male, "Mother must be female");

        require(block.timestamp > _breedCooldown[father], "Father is in cooldown");
        require(block.timestamp > _breedCooldown[mother], "Mother is in cooldown");

        require(_breedCount[father] < _maxBreeds, "Father reached max births");
        require(_breedCount[mother] < _maxBreeds, "Mother reached max births");

        require(_foa.transferFrom(msg.sender, address(this), _breedFoAFees[_breedCount[mother]]), "FoA transfer from failed");
        require(_arker.transferFrom(msg.sender, address(this), _breedArkerFees[_breedCount[mother]]), "ARKER transfer from failed");

        _breedCount[mother] = _breedCount[mother] + 1;
        _breedCount[father] = _breedCount[father] + 1;
        
        _breedCooldown[father] = block.timestamp + _breedCooldownTime;
        _breedCooldown[mother] = block.timestamp + _breedCooldownTime;

        _eggIds.increment();
        uint256 newEggId = _eggIds.current();
        _safeMint(eggReceiver, newEggId);

        EggData memory data;
        data._fatherData = fatherData;
        data._motherData = motherData;
        data._targetBlock = block.number + _blocksUntilHatch;
        data._fatherId = father;
        data._motherId = mother;
        _eggData[newEggId] = data;
    }

    function createEgg(PetData calldata fatherData, PetData calldata motherData, uint256 blocksUntilHatch, address eggReceiver) external whenNotPaused {
        require(hasRole(MINTER_ROLE, msg.sender), "Caller is not a minter");
        require(blocksUntilHatch > 0, "Blocks until hatch should be at least 1");
        _eggIds.increment();
        uint256 newEggId = _eggIds.current();
        _safeMint(eggReceiver, newEggId);

        EggData memory data;
        data._fatherData = fatherData;
        data._motherData = motherData;
        data._targetBlock = block.number + blocksUntilHatch;
        _eggData[newEggId] = data;
    }

    function hatch(uint256 eggId) external onlyHumans whenNotPaused {
        EggData memory egg = getEggData(eggId);
        require(block.number > egg._targetBlock, "Target block not reached");
        uint256 seed = getRandomSeed(
            egg._fatherData._life.add(egg._motherData._energy), // Generate a salt with the parent genes
            egg._targetBlock
        );

        PetData memory data = _geneMixer.getSonGenes(egg, seed);

        uint256 petId = _pets.mint(
            ownerOf(eggId),
            data._type,
            data._attack,
            data._life,
            data._energy,
            data._agility,
            data._skin,
            data._power,
            data._male
        );
        _burn(eggId);

        _fathers[petId] = egg._fatherId;
        _mothers[petId] = egg._motherId;
    }

    function getRandomSeed(uint256 salt, uint256 _targetBlock) private view returns (uint256) {
        require(block.number > _targetBlock, "Target block not reached");

        // Try to grab the hash of the "target block". This should be available, it will only fail if no-one 
        // calls generateRandom() within 256 blocks of the target block
        uint256 randomN = uint256(blockhash(_targetBlock));

        if (randomN == 0) {
            // We don't want to completely bail if the target block is no-longer available,
            // nor do we want to just use the current block's hash (since it could allow a
            // caller to game the random result). Compute the most recent block that has the
            // the same value modulo 256 as the target block. The hash for this block will
            // still be available, and  while it can still change as time passes  it will
            // only change every 256 blocks. Again, someone is very likely to jump in with
            // the getRandomSeed() call before it can cycle too many times.
            _targetBlock = (block.number & maskFirst248Bits) + (_targetBlock & maskLast8Bits);

            // The computation above could result in a block LARGER than the current block,
            // if so, subtract 256.
            if (_targetBlock >= block.number) _targetBlock -= 256;

            randomN = uint256(blockhash(_targetBlock));
        }

        return uint256(keccak256(
            abi.encodePacked(randomN, block.timestamp - 42 * salt, block.difficulty, _targetBlock)
        ));
    }


    // Verifies that no other smart contracts can call this function
    modifier onlyHumans {
        require(msg.sender == tx.origin, "Only wallets can call this");
        _;
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function setBaseURI(string memory _newBaseURI) external onlyOwner {
        baseURI = _newBaseURI;
    }

    function _baseURI() internal view override(ERC721) returns (string memory) {
        return baseURI;
    }

    function setBreedConfig(uint256 maxBreeds, uint256[] memory breedFoAFees, uint256[] memory breedArkerFees) external onlyOwner {
        require(breedFoAFees.length == maxBreeds, "Invalid fee length");
        require(breedArkerFees.length == maxBreeds, "Invalid fee length");
        _maxBreeds = maxBreeds;
        _breedFoAFees = breedFoAFees;
        _breedArkerFees = breedArkerFees;
    }

    function setBreedCooldown(uint256 _seconds) external onlyOwner {
        _breedCooldownTime = _seconds;
    }

    function setGeneMixer(IGeneMixer geneMixer) external onlyOwner {
        _geneMixer = geneMixer;
    }

    function setBlocksUntilHatch(uint256 blocksUntilHatch) external onlyOwner {
        _blocksUntilHatch = blocksUntilHatch;
    }

    function setFeeArkerDestinator(address feeArkerDestinator) external onlyOwner {
        _feeArkerDestinator = feeArkerDestinator;
    }

    function setFeesDestinatorsWithPercentages(
        address[] memory _destinators,
        uint256[] memory _percentages
    )
        external
        onlyOwner
    {
        require(_destinators.length == _percentages.length, "Destinators and percentageslenght are not equals");

        uint256 total = 0;
        for (uint8 i = 0; i < _percentages.length; i++) {
            total += _percentages[i];
        }
        require(total == 100, "Percentages sum must be 100");

        _feesDestinators = _destinators;
        _feesPercentages = _percentages;
    }

    function withdrawAcumulatedFees() external onlyOwner {
        uint256 total = _foa.balanceOf(address(this));
        _arker.transfer(_feeArkerDestinator, _arker.balanceOf(address(this)));
        
        for (uint8 i = 0; i < _feesDestinators.length; i++) {
            _foa.transfer(
                _feesDestinators[i],
                total.mul(_feesPercentages[i]).div(100)
            );
        }
    }

    function supportsInterface(bytes4 interfaceId) public view override(ERC721Enumerable, AccessControl) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
}

// File: contracts/PetDrop.sol


pragma solidity 0.8.7;







struct DropData {
    uint256 _skins;
    uint256 _totalSupply;
    uint256 _price;
    uint256 _hatchBlocks;
    uint256 _endDate;
    uint256 _maxAllocation;
}

contract PetDrop is Pausable, Ownable {

    using Counters for Counters.Counter;

    Counters.Counter private _dropIds;

    // Drop Id => Drop Info
    mapping(uint256 => DropData) public _drops;


    // Drop Id => Gen1
    mapping(uint256 => PetData) public _gen1;

    // Drop Id => Gen2
    mapping(uint256 => PetData) public _gen2;

    // Drop Id => Number of sells
    mapping(uint256 => uint256) public _saleCount;

    // Drop Id => => Address => Number of sells
    mapping(uint256 => mapping(address => uint256)) public _saleCountByWallet;

    ArkerBreeder public _breeder;
    IERC20 public _currency;
    address public _feeReceiver;

    constructor (ArkerBreeder breeder, IERC20 currency) {
        _breeder = breeder;
        _currency = currency;
        _feeReceiver = msg.sender;
    }

    function buy(uint256 dropId) external whenNotPaused onlyHumans {
        DropData memory drop = _drops[dropId];
        require(drop._totalSupply > 0, "Drop not found");
        require(_saleCount[dropId] < drop._totalSupply, "Limit reached");
        require(_currency.transferFrom(msg.sender, _feeReceiver, drop._price), "Transfer from failed");
        require(block.timestamp < drop._endDate, "Drop ended");
        require(_saleCountByWallet[dropId][msg.sender] < drop._maxAllocation, "Max buys per wallet reached");
        
        _saleCount[dropId] = _saleCount[dropId] + 1;

        PetData memory gen1 = _gen1[dropId];
        gen1._skin = getSkin(drop._skins, dropId + _saleCount[dropId]);

        PetData memory gen2 = _gen2[dropId];
        gen2._skin = getSkin(drop._skins, dropId + _saleCount[dropId] + 1);

        _breeder.createEgg(gen1, gen2, drop._hatchBlocks, msg.sender);
        _saleCountByWallet[dropId][msg.sender]++;
    }

    function getSkin(uint256 maxSkins, uint256 salt) private view returns (uint256) {
        uint256 firstRand = uint256(keccak256(
            abi.encodePacked(uint256(blockhash(block.number - 1)), block.timestamp - 66 * (salt + 44), block.difficulty)
        )) % 3;
        if (firstRand < 2) { // 66,6% of getting a pet without skin
            return 0;
        }
        return uint256(keccak256(
            abi.encodePacked(uint256(blockhash(block.number - 1)), block.timestamp - 42 * salt, block.difficulty)
        )) % (maxSkins + 1);
    }

    function createDrop(
        uint256 _skins,
        uint256 _totalSupply,
        uint256 _price,
        uint256 _hatchBlocks,
        uint256 _endDate,
        uint256 _maxAllocation,
        PetData calldata gen1,
        PetData calldata gen2
    ) external onlyOwner {
        _dropIds.increment();
        uint256 dropId = _dropIds.current();

        DropData memory drop;
        drop._skins = _skins;
        drop._totalSupply = _totalSupply;
        drop._price = _price;
        drop._hatchBlocks = _hatchBlocks;
        drop._endDate =  _endDate;
        drop._maxAllocation = _maxAllocation;

        _drops[dropId] = drop;
        _gen1[dropId] = gen1;
        _gen2[dropId] = gen2;
    }

    function totalDrops() external view returns (uint256) {
        return _dropIds.current();
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function setFeeReceiver(address feeReceiver) external onlyOwner {
        _feeReceiver = feeReceiver;
    }

    // Verifies that no other smart contracts can call this function
    modifier onlyHumans {
        require(msg.sender == tx.origin, "Only wallets can call this");
        _;
    }

}