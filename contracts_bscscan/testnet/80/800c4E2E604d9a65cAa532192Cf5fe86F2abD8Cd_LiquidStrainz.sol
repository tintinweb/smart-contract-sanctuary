pragma solidity ^0.8.0;

import "./IStrainzNFT.sol";
import "../StrainzNFT/StrainMetadata.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import '@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol';
import '@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol';

contract LiquidStrainz is Ownable, IStrainMetadata, ERC20Burnable, IERC721Receiver {

    IStrainzNFT public strainzNFT;
    IERC20 public strainzToken;
    IUniswapV2Factory public pancakeFactory = IUniswapV2Factory(0x6725F303b657a9451d8BA641348b6761A6CC7a17); // Mainnet: IUniswapV2Factory(0xcA143Ce32Fe78f1f7019d7d551a6402fC5350c73);
    IUniswapV2Router02 public pancakeRouter = IUniswapV2Router02(0xD99D1c33F9fC3444f8101754aBC46c52416550D1);
    IERC20 public lpToken;

    constructor(address strainzNFTAddress, address strainzTokenAddress) ERC20('Liquid Strainz', 'LSTRAINZ') {
        strainzNFT = IStrainzNFT(strainzNFTAddress);
        strainzToken = IERC20(strainzTokenAddress);


    }
    function decimals() public pure override returns(uint8) {
        return 4;
    }

    function addLiquidity(uint strainzNftId, uint strainzAmount, uint slippageInPercent) external {
        // get plant + strainz from user
        strainzNFT.transferFrom(msg.sender, address(this), strainzNftId);
        strainzToken.transferFrom(msg.sender, address(this), strainzAmount);
        // liquidize the plant
        uint liquidAmount = liquidize(strainzNftId);
        _mint(address(this), liquidAmount);

        if (address(lpToken) == address(0)) {
            lpToken = IERC20(pancakeFactory.createPair(address(strainzToken), address(this)));
        }

        strainzToken.approve(address(pancakeRouter), strainzAmount);
        _approve(address(this), address(pancakeRouter), liquidAmount);

        uint minStrainzAmount = 1; //TODO: slippage calculation
        uint minLiquidAmount = 1; //TODO: slippage calculation
        pancakeRouter.addLiquidity(address(strainzToken), address(this), strainzAmount, liquidAmount, minStrainzAmount, minLiquidAmount, address(this), block.timestamp + 1 days);
    }

    function buyPlantWithStrainz(uint strainzNftId, uint maxPaymentAmount) external {
        require(strainzNFT.ownerOf(strainzNftId) == address(this));

        uint liquidAmount = liquidize(strainzNftId);

        address[] memory path = new address[](2);
        path[0] = address(strainzToken);
        path[1] = address(this);
        pancakeRouter.swapTokensForExactTokens(liquidAmount, maxPaymentAmount, path, address(this), block.timestamp + 1 days);



    }


    function liquidize(uint tokenId) public view returns (uint) {
        StrainMetadata memory metadata = getV3Strain(tokenId);


        return metadata.growRate * 10 ** decimals();
    }

    function getV3Strain(uint strainId) private view returns (StrainMetadata memory) {
        (uint id,
        string memory prefix,
        string memory postfix,
        uint dna,
        uint generation,
        uint growRate, // 0-255
        uint lastHarvest,
        uint breedingCost) = strainzNFT.strainData(strainId);

        return StrainMetadata(id, prefix, postfix, dna, generation, growRate, lastHarvest, breedingCost);
    }

    bytes4 constant ERC721_RECEIVED = 0xf0b9e5ba;

    function onERC721Received(address, address, uint256, bytes calldata) public pure override returns (bytes4) {
        return ERC721_RECEIVED;
    }

}

