pragma solidity 0.7.6;
pragma experimental ABIEncoderV2;

import "@openzeppelin/contracts/token/ERC721/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Pausable.sol";
import "@openzeppelin/contracts/utils/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/SafeERC20.sol";
import "@openzeppelin/contracts/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/EnumerableSet.sol";

contract FatMarketPlace is ERC721Holder, Ownable, Pausable, ReentrancyGuard {
  using EnumerableSet for EnumerableSet.AddressSet;
  using EnumerableSet for EnumerableSet.UintSet;
  using SafeERC20 for IERC20;
  using SafeMath for uint256;

  struct Offer {
    address from;
    address onlySellTo;
    uint256 price;
    uint256 expiration;
  }

  IERC20 public immutable fat;

  uint256 public constant MAX_TAX_FEE = 3000; // 3%;
  uint256 public constant MAX_BATCH = 20;

  bool private _freeListed;
  address private _admin;
  uint256 private _listingFee;
  uint256 private _taxFee;
  uint256 private _timeToExpiry;
  address private _treasury;

  mapping(address => bool) public isNFTSupported;
  mapping(address => mapping(uint256 => uint256)) public lastActive;
  mapping(address => mapping(uint256 => Offer)) public tokenOffer;

  EnumerableSet.AddressSet private _nftAddressSet;
  mapping(address => EnumerableSet.UintSet) private _offerNFTIds;
  mapping(address => mapping(address => EnumerableSet.UintSet)) private _NFTIdsOfSeller;

  event PlaceOffer(address indexed from, address indexed nftAddress, uint256 indexed nftId, uint256 price);
  event PlaceOfferTo(address indexed to, address indexed nftAddress, uint256 indexed nftId, uint256 price);
  event ModifyOffer(address indexed nftAddress, uint256 indexed nftId, uint256 priceFrom, uint256 priceTo);
  event CancelOffer(address indexed from, address indexed nftAddress, uint256 indexed nftId);
  event Buy(address indexed buyer, address indexed nftAddress, uint256 indexed nftId, uint256 price);
  event Expire(address indexed from, address indexed nftAddress, uint256 indexed nftId);

  event SetSupportNFT(address indexed nftAddress, bool isSupported);
  event SetFeeTo(address indexed treasury);
  event SetFee(uint256 fee);
  event SetAdmin(address admin);
  event SetDefaultExpiration(uint256 expiration);

  event SetListingFee(uint256 fee);
  event ListNFT(address indexed nft);

  constructor(
    IERC20 _fat
  ) {
    fat = _fat;
    _taxFee = 0;
    _timeToExpiry = 28800; // ~ 1 day
    _freeListed = true;
    _listingFee = 0;
  }

  modifier onlyAdmin() {
    require(_msgSender() == _admin, "FatMarket: not an admin");
    _;
  }

  modifier onlySupportedNFT(address _nft) {
    require(_freeListed || isNFTSupported[_nft], "FatMarket: not a supported nft");
    _;
  }

  modifier validAddress(address _addr) {
    require(_addr != address(0), "FatMarket: Invalid address");
    _;
  }

  modifier validPrice(uint256 _price) {
    require(_price > 0, "FatMarket: price is too low");
    _;
  }

  modifier onlyEOA() {
    require(_msgSender() == tx.origin, "FatMarket: not EOA");
    _;
  }

  modifier isExpired(address _nftAddress, uint256 _nftId) {
    require(tokenOffer[_nftAddress][_nftId].expiration < block.number, "FatMarket: not expired");
    _;
  }

  modifier isNotExpired(address _nftAddress, uint256 _nftId) {
    require(tokenOffer[_nftAddress][_nftId].expiration > block.number, "FatMarket: expired");
    _;
  }

  function _removeOffer(address _seller, address _nftAddress, uint256 _nftId) internal {
    delete tokenOffer[_nftAddress][_nftId];
    _NFTIdsOfSeller[_seller][_nftAddress].remove(_nftId);
    _offerNFTIds[_nftAddress].remove(_nftId);
    IERC721(_nftAddress).safeTransferFrom(address(this), _msgSender(), _nftId);
  }

  function _placeOfferTo(address _nftAddress, uint256 _nftId, uint256 _price, address _to) internal {
    require(_to != _msgSender(), "FatMarket: Wash trading");
    Offer storage offer = tokenOffer[_nftAddress][_nftId];
    offer.from = _msgSender();
    offer.onlySellTo = _to;
    offer.price = _price;
    offer.expiration = block.number + _timeToExpiry;

    _NFTIdsOfSeller[_msgSender()][_nftAddress].add(_nftId);
    _offerNFTIds[_nftAddress].add(_nftId);

    IERC721(_nftAddress).safeTransferFrom(_msgSender(), address(this), _nftId);
  }

  function placeOffer(address _nftAddress, uint256 _nftId, uint256 _price)
    external
    onlySupportedNFT(_nftAddress)
    validPrice(_price)
    onlyEOA
    nonReentrant
    whenNotPaused
  {
    _placeOfferTo(_nftAddress, _nftId, _price, address(0));

    emit PlaceOffer(_msgSender(), _nftAddress, _nftId, _price);
  }

  function placeOfferTo(address _nftAddress, uint256 _nftId, uint256 _price, address _to)
    external
    onlySupportedNFT(_nftAddress)
    validPrice(_price)
    onlyEOA
    nonReentrant
    whenNotPaused
  {
    require(_price > 0, "FatMarket: price is too low");
    _placeOfferTo(_nftAddress, _nftId, _price, _to);

    emit PlaceOfferTo(_to, _nftAddress, _nftId, _price);
  }

  function batchPlaceOffer(address _nftAddress, uint256[] calldata _nftId, uint256[] calldata _price)
    external
    onlySupportedNFT(_nftAddress)
    onlyEOA
    nonReentrant
    whenNotPaused
  {
    require(_nftId.length <= MAX_BATCH, "FatMarket: Invalid batch");
    require(_nftId.length == _price.length, "FatMarket: Invalid offer");
    for (uint256 i = 0; i < _nftId.length; i++) {
      require(_price[i] > 0, "FatMarket: price is too low");
      _placeOfferTo(_nftAddress, _nftId[i], _price[i], address(0));

      emit PlaceOffer(_msgSender(), _nftAddress, _nftId[i], _price[i]);
    }
  }

  function batchPlaceOfferTo(address _nftAddress, uint256[] calldata _nftId, uint256[] calldata _price, address _to)
    external
    onlySupportedNFT(_nftAddress)
    onlyEOA
    nonReentrant
    whenNotPaused
  {
    require(_nftId.length <= MAX_BATCH, "FatMarket: Invalid batch");
    require(_nftId.length == _price.length, "FatMarket: Invalid offer");
    for (uint256 i = 0; i < _nftId.length; i++) {
      require(_price[i] > 0, "FatMarket: price is too low");
      _placeOfferTo(_nftAddress, _nftId[i], _price[i], _to);

      emit PlaceOfferTo(_to, _nftAddress, _nftId[i], _price[i]);
    }
  }

  function _modifyOffer(address _nftAddress, uint256 _nftId, uint256 _price, address _to)
    internal
  {
    Offer storage offer = tokenOffer[_nftAddress][_nftId];
    require(offer.from == _msgSender(), "FatMarket: not owner");
    require(_to != _msgSender(), "FatMarket: Wash trading");

    uint256 _priceFrom = offer.price;
    offer.onlySellTo = _to;
    offer.price = _price;
    offer.expiration = block.number + _timeToExpiry;

    emit ModifyOffer(_nftAddress, _nftId, _priceFrom, _price);
  }

  function modifyOffer(address _nftAddress, uint256 _nftId, uint256 _price)
    external
    onlySupportedNFT(_nftAddress)
    validPrice(_price)
    onlyEOA
    isNotExpired(_nftAddress, _nftId)
    nonReentrant
    whenNotPaused
  {
    _modifyOffer(_nftAddress, _nftId, _price, address(0));
  }

  function modifyOfferTo(address _nftAddress, uint256 _nftId, uint256 _price, address _to)
    external
    onlySupportedNFT(_nftAddress)
    validPrice(_price)
    onlyEOA
    isNotExpired(_nftAddress, _nftId)
    nonReentrant
    whenNotPaused
  {
    _modifyOffer(_nftAddress, _nftId, _price, _to);
  }

  function _cancelOffer(address _nftAddress, uint256 _nftId) internal {
    Offer memory offer = tokenOffer[_nftAddress][_nftId];
    require(offer.from == _msgSender(), "FatMarket: not owner");
    _removeOffer(_msgSender(), _nftAddress, _nftId);

    emit CancelOffer(_msgSender(), _nftAddress, _nftId);
  }

  function cancelOffer(address _nftAddress, uint256 _nftId)
    external
    onlySupportedNFT(_nftAddress)
    onlyEOA
    nonReentrant
  {
    _cancelOffer(_nftAddress, _nftId);
  }

  function batchCancelOffer(address _nftAddress, uint256[] calldata _nftId)
    external
    onlySupportedNFT(_nftAddress)
    onlyEOA
    nonReentrant
  {
    require(_nftId.length <= MAX_BATCH, "FatMarket: Invalid batch");
    for (uint256 i = 0; i < _nftId.length; i++) {
      _cancelOffer(_nftAddress, _nftId[i]);
    }
  }

  function _claimExpiredOffer(address _nftAddress, uint256 _nftId)
    internal
    isExpired(_nftAddress, _nftId)
  {
    Offer memory offer = tokenOffer[_nftAddress][_nftId];
    require(offer.from == _msgSender(), "FatMarket: not owner");
    _removeOffer(_msgSender(), _nftAddress, _nftId);

    emit Expire(_msgSender(), _nftAddress, _nftId);
  }

  function claimExpiredOffer(address _nftAddress, uint256 _nftId)
    external
    onlySupportedNFT(_nftAddress)
    onlyEOA
    nonReentrant
  {
    _claimExpiredOffer(_nftAddress, _nftId);
  }

  function batchClaimExpiredOffer(address _nftAddress, uint256[] calldata _nftId)
    external
    onlySupportedNFT(_nftAddress)
    onlyEOA
    nonReentrant
  {
    require(_nftId.length <= MAX_BATCH, "FatMarket: Invalid batch");
    for (uint256 i = 0; i < _nftId.length; i++) {
      _claimExpiredOffer(_nftAddress, _nftId[i]);
    }
  }

  function getOffer(address _nftAddress, uint256 _nftId) external view returns (Offer memory){
    return tokenOffer[_nftAddress][_nftId];
  }

  function isOfferExpired(address _nftAddress, uint256 _nftId) external view returns (bool){
    return tokenOffer[_nftAddress][_nftId].expiration < block.number;
  }

  function _buy(address _nftAddress, uint256 _nftId)
    internal
    isNotExpired(_nftAddress, _nftId)
  {
    Offer memory offer = tokenOffer[_nftAddress][_nftId];
    uint256 _price = offer.price;
    require(offer.from != _msgSender(), "FatMarket: self-buying");
    require(offer.onlySellTo == _msgSender() || offer.onlySellTo == address(0), "FatMarket: can not buy");
    _removeOffer(offer.from, _nftAddress, _nftId);

    fat.safeTransferFrom(_msgSender(), offer.from, _price.mul(1e5 - _taxFee).div(1e5));
    if (_taxFee > 0) {
      fat.safeTransferFrom(_msgSender(), _treasury, _price.mul(2 * _taxFee).div(1e5));
    }

    emit Buy(_msgSender(), _nftAddress, _nftId, _price);
  }

  function buy(address _nftAddress, uint256 _nftId)
    external
    onlySupportedNFT(_nftAddress)
    onlyEOA
    nonReentrant
    whenNotPaused
  {
    _buy(_nftAddress, _nftId);
  }

  function batchBuy(address _nftAddress, uint256[] calldata _nftId)
    external
    onlySupportedNFT(_nftAddress)
    onlyEOA
    nonReentrant
    whenNotPaused
  {
    require(_nftId.length <= MAX_BATCH, "FatMarket: Invalid batch");
    for (uint256 i = 0; i < _nftId.length; i++) {
      _buy(_nftAddress, _nftId[i]);
    }
  }

  function setAdmin(address _newAdmin) external onlyOwner validAddress(_newAdmin) {
    _admin = _newAdmin;

    emit SetAdmin(_newAdmin);
  }

  function setDefaultExpiration(uint256 _expiration) external onlyAdmin {
    require(_expiration > 0, "FatMarket: expiration is too low");
    _timeToExpiry = _expiration;

    emit SetDefaultExpiration(_expiration);
  }

  function setFee(uint256 _fee) external onlyAdmin {
    require(_fee >= 0, "FatMarket: fee is too low");
    require(_fee <= MAX_TAX_FEE, "FatMarket: fee is too high");
    _taxFee = _fee;

    emit SetFee(_fee);
  }

  function setFeeTo(address _feeTo) external onlyOwner validAddress(_feeTo) {
    _treasury = _feeTo;

    emit SetFeeTo(_feeTo);
  }

  function setFreeListed(bool _free) external onlyAdmin {
    _freeListed = _free;
  }

  function setListingFee(uint256 _fee) external onlyAdmin {
    require(_fee >= 0, "FatMarket: fee is too low");
    _listingFee = _fee;

    emit SetListingFee(_fee);
  }

  function setSupportNFT(address[] calldata _nftAddress, bool _isSupported) external onlyAdmin {
    for (uint256 i = 0; i < _nftAddress.length; i++) {
      require(IERC721(_nftAddress[i]).supportsInterface(0x80ac58cd), "FatMarket: Not ERC721");
      isNFTSupported[_nftAddress[i]] = _isSupported;
      if (_isSupported) {
        _nftAddressSet.add(_nftAddress[i]);
      } else {
        _nftAddressSet.remove(_nftAddress[i]);
      }

      emit SetSupportNFT(_nftAddress[i], _isSupported);
    }
  }

  function freeListed() external view returns (bool) {
    return _freeListed;
  }

  function listingFee() external view returns (uint256) {
    return _listingFee;
  }

  function admin() external view returns (address) {
    return _admin;
  }

  function taxFee() external view returns (uint256) {
    return _taxFee;
  }

  function timeToExpiry() external view returns (uint256) {
    return _timeToExpiry;
  }

  function treasury() external view returns (address) {
    return _treasury;
  }

  function listNFT(address _nftAddress) external {
    require(!_freeListed, "FatMarket: Do not allowed");
    require(_listingFee > 0, "FatMarket: No listing fee");
    require(isNFTSupported[_nftAddress] == false, "FatMarket: Listed");
    require(IERC721(_nftAddress).supportsInterface(0x80ac58cd), "FatMarket: Not ERC721");

    uint256 fatBalance = fat.balanceOf(_msgSender());
    if (fatBalance >= _listingFee) {
      isNFTSupported[_nftAddress] = true;
      _nftAddressSet.add(_nftAddress);
      fat.safeTransferFrom(_msgSender(), _treasury, _listingFee);

      emit ListNFT(_nftAddress);
    }
  }

  function viewNFTs(
    uint256 cursor,
    uint256 size
  ) external view returns (address[] memory nftAddresses, uint256)
  {
    uint256 length = size;
    if (length > _nftAddressSet.length() - cursor) {
      length = _nftAddressSet.length() - cursor;
    }

    nftAddresses = new address[](length);
    for (uint256 i = 0; i < length; i++) {
      nftAddresses[i] = _nftAddressSet.at(cursor + i);
    }

    return (nftAddresses, cursor + length);
  }

  function getOffersByNFTs(
    address _nftAddress,
    uint256 _cursor,
    uint256 _size
  ) external view returns (uint256[] memory _tokenIds, Offer[] memory _offerInfo, uint256)
  {
    uint256 _length = _size;
    if (_length > _offerNFTIds[_nftAddress].length() - _cursor) {
      _length = _offerNFTIds[_nftAddress].length() - _cursor;
    }

    _tokenIds = new uint256[](_length);
    _offerInfo = new Offer[](_length);

    for (uint256 i = 0; i < _length; i++) {
      _tokenIds[i] = _offerNFTIds[_nftAddress].at(_cursor + i);
      _offerInfo[i] = tokenOffer[_nftAddress][_tokenIds[i]];
    }

    return (_tokenIds, _offerInfo, _cursor + _length);
  }

  function getOffersByNFTsAndSeller(
    address _seller,
    address _nftAddress,
    uint256 _cursor,
    uint256 _size
  ) external view returns (uint256[] memory _tokenIds, Offer[] memory _offerInfo, uint256)
  {
    uint256 _length = _size;
    if (_length > _NFTIdsOfSeller[_seller][_nftAddress].length() - _cursor) {
      _length = _NFTIdsOfSeller[_seller][_nftAddress].length() - _cursor;
    }

    _tokenIds = new uint256[](_length);
    _offerInfo = new Offer[](_length);

    for (uint256 i = 0; i < _length; i++) {
      _tokenIds[i] = _NFTIdsOfSeller[_seller][_nftAddress].at(_cursor + i);
      _offerInfo[i] = tokenOffer[_nftAddress][_tokenIds[i]];
    }

    return (_tokenIds, _offerInfo, _cursor + _length);
  }

  function pause() external whenNotPaused nonReentrant onlyAdmin {
    _pause();
  }

  function unpause() external whenPaused onlyAdmin {
    _unpause();
  }
}

