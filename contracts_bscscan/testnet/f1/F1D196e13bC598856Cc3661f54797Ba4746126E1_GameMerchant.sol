import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/math/Math.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "hardhat/console.sol";
import "./interface/IGameMerchant.sol";

pragma solidity ^0.8.0;

contract GameMerchant is IGameMerchant, Ownable {
    using SafeMath for uint256;
    using Counters for Counters.Counter;
    using Address for address payable;
    using SafeERC20 for IERC20;
    
    address public factory;
    uint256 public lastId = 1;
    uint256 public total = 0;
    bool public activated = false;
    mapping(address => Merchant) public  _merchants;
    mapping(address => Game[]) private  _games;
    mapping(address => Game) private  _game;
    mapping(address => NFTToken[]) private  _nftTokens;
    mapping(address => mapping(uint256 => mapping(uint256 => NFTToken))) private  _gameNftTokens;

    mapping(uint256 => address) public idToAddress;
            
    event CreateMerchant(
        uint256 id,
        address addr,
        uint256 erc721Total,
        uint256 erc1155Total,
        uint256 gameTotal,
        uint256 joinTime
    );

    event CreateGame(
        uint256 id,
        address addr,
        string name,
        uint256 merchantId,
        uint256 createTime
    );

    event CreatNftToken(
        uint256 id,
        address addr,
        uint256 gameId,
        address nftAddress,
        uint256 nftType,
        uint256 createTime
    );

    
    constructor(address _factory) {
        factory = _factory;
    }

    function createMerchant() external  {
        require(_merchants[msg.sender].addr==address(0), "Is already merchant.");
        _merchants[msg.sender]  =  Merchant(total.add(1),msg.sender,0,0,0,block.timestamp);

        total = total.add(1);
        idToAddress[lastId] = msg.sender;
        lastId += 1;
        emit CreateMerchant(total.add(1),msg.sender,0,0,0,block.timestamp);
    }
    
    function createGame(string memory name) external  {
        require(_merchants[msg.sender].addr!=address(0), "Merchant does not exist.");
        Merchant storage  merchant = _merchants[msg.sender];

        Game[] storage gameArray =  _games[msg.sender];
        gameArray.push(Game(gameArray.length.add(1),name,merchant.id,block.timestamp));
        merchant.gameTotal = merchant.gameTotal.add(1);

        emit CreateGame(gameArray.length.add(1),msg.sender,name,merchant.id,block.timestamp);
    }

    function creatNftToken(address merchantAddr,uint256 gameId,address nftAddress,uint256 nftType) external override {
        require(msg.sender==factory, "Permission denied.");
        Merchant storage merchant = _merchants[merchantAddr];
        require(merchant.addr!=address(0), "Merchant does not exist.");

        Game[] storage gameArray =  _games[merchantAddr];
        require(gameId<gameArray.length, "Game id does not exist.");

        NFTToken[] storage nftTokenArray =  _nftTokens[merchantAddr];
        nftTokenArray.push(NFTToken(gameArray.length.add(1),gameId,nftAddress,nftType,block.timestamp));

        _gameNftTokens[merchantAddr][gameId][gameArray.length.add(1)] = NFTToken(gameArray.length.add(1),gameId,nftAddress,nftType,block.timestamp);
        if(nftType==0){
            merchant.erc721Total = merchant.erc721Total.add(1);
        }else{
            merchant.erc1155Total = merchant.erc1155Total.add(1);
        }
        emit CreatNftToken(gameArray.length.add(1),merchantAddr,gameId,nftAddress,nftType,block.timestamp);
    }

    function merchants(address addr) external view  override returns (Merchant memory){
        return _merchants[addr];
    }

    function games(address addr) external view override returns (Game[] memory){
        return _games[addr];
    }

    function gameItem(address addr,uint256 gameId) external view override returns (Game memory){
        return  _games[addr].length>gameId?_games[addr][gameId]:Game(0,"",0,0);
    }
    
    function nftTokens(address addr) external view override returns (NFTToken[] memory){
        return _nftTokens[addr];
    }

    function gameNftTokens(address addr,uint256 gameId,uint256 nftId) external view override returns (NFTToken memory){
        return _gameNftTokens[addr][gameId][nftId];
    }   

}

