import "./libraries/Context.sol";
import "./libraries/Auth.sol";

contract BabyToken is Context, Auth, IERC20 {
    using SafeMath for uint256;

    //ERC20
    uint8 private _decimals;
    uint256 private _totalSupply;
    string private _name;
    string private _symbol;
    mapping(address => uint256) private _balances;
    mapping(address => mapping(address => uint256)) private _allowances;

    //Tokenomic Buy
    uint256 public percentBuyMarketing = 200;
    uint256 public percentBuyLiquidity = 100;
    uint256 public percentBuyDevelopment = 200;
    //Tokenomic Sell
    uint256 public percentSellMarketing = 400;
    uint256 public percentSellLiquidity = 200;
    uint256 public percentSellDevelopment = 400;
    
    uint256 public percentTaxDenominator = 10000;
    uint256 public minimumSwapForWeth;
    uint256 public minimumWethForBuybackAndBurn = 1000000000000000000; //1 BNB
    uint256 public minimumTokenForAddLiquidity = 1 * (10**18);
    bool public isAutoSwapForWeth = true;
    bool public isTaxBuyEnable = true;
    bool public isTaxSellEnable = true;
    bool public isAutoBuybackAndBurn = false;
    bool public isAutoAddLiquidity = true;
    // uint256
    mapping(address => bool) public isExcludeFromFee;

    //address
    address public factoryAddress;
    address public wethAddress;
    address public routerAddress;
    address public marketingAddress;
    address public developmentAddress;
    address ZERO = 0x0000000000000000000000000000000000000000;
    address DEAD = 0x000000000000000000000000000000000000dEaD;

    mapping(address => bool) public isPair;

    event BuybackAndBurn(uint256 amountETH, uint256 amountToken);
    event AddLiquidity(uint256 amountETH, uint256 amountToken);

    bool inSwap;

    modifier swapping() {
        inSwap = true;
        _;
        inSwap = false;
    }

    constructor(
      string memory tokenName,
      string memory tokenSymbol,
      uint8 tokenDecimal,
      uint256 tokenSupply,
      uint256[] memory percentBuy, //0: Marketing, 1: Development, 2: Liquidity
      uint256[] memory percentSell,
      address _routerAddress,
      address _marketingAddress,
      address _developmentAddress
    ) Auth(msg.sender) {
      require(percentBuy.length==3,"BabyToken: Invalid Percent Buy");
      require(percentSell.length==3,"BabyToken: Invalid Percent Sell");
      _name = tokenName;
      _symbol = tokenSymbol;
      _decimals = tokenDecimal;
      _totalSupply = tokenSupply * (10**_decimals);

      percentBuyMarketing = percentBuy[0];
      percentBuyDevelopment = percentBuy[1];
      percentBuyLiquidity = percentBuy[2];

      percentSellMarketing = percentSell[0];
      percentSellDevelopment = percentSell[1];
      percentSellLiquidity = percentSell[2];

      // initialize minimum swap for weth 0.05% of total supply
      minimumSwapForWeth = _totalSupply.mul(5).div(percentTaxDenominator);
      minimumTokenForAddLiquidity = 1 * (10 ** _decimals);

      routerAddress = _routerAddress;
      marketingAddress = _marketingAddress;
      developmentAddress = _developmentAddress;

      wethAddress = IUniswapV2Router02(routerAddress).WETH();
      factoryAddress = IUniswapV2Router02(routerAddress).factory();
      IUniswapV2Factory(factoryAddress).createPair(address(this), wethAddress);
      address pairWETH = IUniswapV2Factory(factoryAddress).getPair(address(this), wethAddress);
      isPair[pairWETH] = true;

      isExcludeFromFee[msg.sender] = true;
      isExcludeFromFee[routerAddress] = true;

      _approve(address(this), routerAddress, _totalSupply);

      _balances[msg.sender] = _totalSupply;
      emit Transfer(address(0), msg.sender, _totalSupply);
    }

    receive() external payable {}

    function name() public view override returns (string memory) {
        return _name;
    }

    function symbol() public view virtual override returns (string memory) {
        return _symbol;
    }

    function decimals() public view virtual override returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }

    function getOwner() public view virtual override returns (address) {
        return _getOwner();
    }

    function balanceOf(address account)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return _balances[account];
    }

    function transfer(address recipient, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender)
        public
        view
        virtual
        override
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount)
        public
        virtual
        override
        returns (bool)
    {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) public virtual override returns (bool) {
        if (_allowances[sender][msg.sender] != _totalSupply) {
            _allowances[sender][msg.sender] = _allowances[sender][msg.sender]
                .sub(amount, "BabyToken: Insufficient Allowance");
        }
        _transfer(sender,recipient,amount);
        return true;
    }

    
    function increaseAllowance(address spender, uint256 addedValue)
        public
        virtual
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender] + addedValue
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        public
        virtual
        returns (bool)
    {
        uint256 currentAllowance = _allowances[_msgSender()][spender];
        require(
            currentAllowance >= subtractedValue,
            "BabyToken: decreased allowance below zero"
        );
        unchecked {
            _approve(_msgSender(), spender, currentAllowance - subtractedValue);
        }

        return true;
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal virtual {
        require(owner != address(0), "BabyToken: approve from the zero address");
        require(spender != address(0), "BabyToken: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual {
      if(shouldTakeFee(sender, recipient)){
        _complexTransfer(sender, recipient, amount);
      } else {
        _basicTransfer(sender, recipient, amount);
      }
    }

    function _basicTransfer(address sender, address recipient, uint256 amount) internal {
      _balances[sender] = _balances[sender].sub(amount);
      _balances[recipient] = _balances[recipient].add(amount);
      emit Transfer(sender, recipient, amount);
    }

    function _complexTransfer(address sender, address recipient, uint256 amount) internal {
      uint256 amountTransfer = getAmountTransfer(amount, sender);

      if(shouldSwapForWeth(sender)) _swapForWeth(_balances[address(this)]);
      if(shouldAutoAddLiquidity(sender)) _swapAndLiquify();
      
      _balances[sender] = _balances[sender].sub(amount);
      _balances[recipient] = _balances[recipient].add(amountTransfer);
      emit Transfer(sender, recipient, amount);
    }

    function getAmountTransfer(uint256 amount, address sender) internal returns(uint256){
      uint256 percentTotalTax;
      uint256 amountTax;
      if(isPair[sender]) {
        percentTotalTax = percentBuyMarketing.add(percentBuyDevelopment).add(percentBuyLiquidity);
      } else {
        percentTotalTax = percentSellMarketing.add(percentSellDevelopment).add(percentSellLiquidity);
      }
      amountTax = amount.mul(percentTotalTax).div(percentTaxDenominator);
      _balances[address(this)] = _balances[address(this)].add(amountTax);
      emit Transfer(sender, address(this), amount);

      return amount.sub(amountTax);
    }

    function burn(uint256 amount) external {
        require(_balances[_msgSender()] >= amount,"BabyToken: Insufficient Amount");
        _burn(_msgSender(), amount);
    }

    function _burn(address account, uint256 amount) internal virtual {
        _balances[account] = _balances[account].sub(amount);
        _totalSupply.sub(amount);
        emit Transfer(account, DEAD, amount);
    }

    function shouldTakeFee(address sender, address recipient) internal view returns(bool){
      if(inSwap) return false;
      if(isExcludeFromFee[sender]) return false;
      if(isExcludeFromFee[recipient]) return false;
      if(isPair[sender] && !isTaxBuyEnable) return false;
      if(isPair[recipient] && !isTaxSellEnable) return false;
      return true;
    }

    function shouldSwapForWeth(address sender) internal view returns(bool){
      return (isAutoSwapForWeth && !isPair[sender] && !inSwap && _balances[address(this)] >= minimumSwapForWeth); 
    }

    function shouldAutoAddLiquidity(address sender) internal view returns(bool){
      return (isAutoAddLiquidity && !inSwap && !isPair[sender] && address(this).balance >= minimumTokenForAddLiquidity);
    }

    function shouldBuybackAndBurn(address sender) internal view returns(bool){
      return (isAutoBuybackAndBurn && !inSwap && !isPair[sender] && address(this).balance >= minimumWethForBuybackAndBurn);
    }

    function setIsPair(address pairAddress, bool state) external onlyOwner {
      isPair[pairAddress] = state;
    }

    function setTaxReceiver(address _marketingAddress, address _developmentAddress) external onlyOwner {
      marketingAddress = _marketingAddress;
      developmentAddress = _developmentAddress;
    }

    function setIsTaxEnable(bool taxBuy, bool taxSell) external onlyOwner {
      isTaxBuyEnable = taxBuy;
      isTaxSellEnable = taxSell;
    }

    function setIsExcludeFromFee(address account, bool state) external authorized {
      isExcludeFromFee[account] = state;
    }

    function setAutoSwapForWeth(bool state,uint256 amount) external onlyOwner {
      require(amount <= _totalSupply,"BabyToken: Amount Swap For Weth max total supply");
      isAutoSwapForWeth = state;
      minimumSwapForWeth = amount;
    }

    function setAutoBuybackAndBurn(bool state,uint256 amountWETH) external onlyOwner {
      isAutoBuybackAndBurn = state;
      minimumWethForBuybackAndBurn = amountWETH;
    }

    function setAutoAddLiquidity(bool state, uint256 amountToken) external onlyOwner {
      isAutoAddLiquidity = state;
      minimumTokenForAddLiquidity = amountToken;
    }

    function _buyBackAndBurn(uint256 amount) internal { 
      if(amount >= address(this).balance) {
        uint256 amountDeadBefore = _balances[DEAD];

        _buyToken(amount,DEAD);

        uint256 amountDeadAfter = _balances[DEAD];
        uint256 amountDeadDiff = amountDeadAfter.sub(amountDeadBefore);
        _totalSupply = _totalSupply.sub(amountDeadDiff);
        emit BuybackAndBurn(amount,amountDeadDiff);
      }
    }

    function _buyToken(uint256 amount, address to) internal swapping {
      IUniswapV2Router02 router = IUniswapV2Router02(routerAddress);
      address[] memory path = new address[](2);
      path[0] = wethAddress;
      path[1] = address(this);
      
      router.swapExactETHForTokensSupportingFeeOnTransferTokens{value:amount}(
        0,
        path, 
        to, 
        block.timestamp.add(300)
      );
    }

    function buybackAndBurn(uint256 amount) external onlyOwner {
      _buyBackAndBurn(amount);
    }

    function _swapForWeth(uint256 amount) internal swapping {
      if(amount > 0) {
        uint256 totalTax = percentSellMarketing.add(percentSellDevelopment).add(percentSellLiquidity);
        uint256 amountTokenForLiquify = amount.mul(percentSellLiquidity.mul(percentTaxDenominator).div(totalTax)).div(percentTaxDenominator);
        uint256 amountHalfTokenForLiquify = amountTokenForLiquify.div(2); //half token for swap to WETH
        uint256 amountSwapForWeth = amount.sub(amountHalfTokenForLiquify); //total amount token for liquify

        IUniswapV2Router02 router = IUniswapV2Router02(routerAddress);

        uint256 balanceETHBefore = address(this).balance;

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = wethAddress;
        
        uint256[] memory estimate = router.getAmountsOut(amountSwapForWeth, path);

        uint256 amountForSwap = estimate[1];

        router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            amountSwapForWeth,
            amountForSwap,
            path,
            address(this),
            block.timestamp
        );
    
        uint256 balanceETHAfter = address(this).balance.sub(balanceETHBefore);

        //distribute
        uint256 amountMarketing = balanceETHAfter.mul(percentSellMarketing.mul(percentTaxDenominator).div(totalTax)).div(percentTaxDenominator);
        payable(marketingAddress).transfer(amountMarketing);
        uint256 amountDev = balanceETHAfter.mul(percentSellDevelopment.mul(percentTaxDenominator).div(totalTax)).div(percentTaxDenominator);
        payable(developmentAddress).transfer(amountDev);        
      }
    }

    function swapForWeth(uint256 amount) external onlyOwner {
      _swapForWeth(amount);
    }

    function _swapAndLiquify() internal swapping {
      uint256 amountETH = address(this).balance;
      uint256 amountToken = _balances[address(this)];
      _addLiquidity(amountToken,amountETH);
      emit AddLiquidity(amountETH, amountToken);
    }

    function _addLiquidity(uint256 amountToken, uint256 amountETH) internal {
      IUniswapV2Router02 router = IUniswapV2Router02(routerAddress);
      router.addLiquidityETH{value: amountETH}(
        address(this),
        amountToken,
        0,
        0,
        owner,
        block.timestamp
      );
    }
}

