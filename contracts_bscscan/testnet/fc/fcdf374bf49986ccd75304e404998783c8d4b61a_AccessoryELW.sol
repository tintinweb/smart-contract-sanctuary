pragma solidity ^0.8.2;

import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "hardhat/console.sol";

interface ITokenVault {
    function initialize(address token) external;

    function buy(address from, uint256 amount) external;

    function transfer(address to, uint256 amount) external;
}

contract TokenRewardVault is Initializable, OwnableUpgradeable {
    using SafeERC20Upgradeable for IERC20Upgradeable;

    address public token;

    function initialize(address _token) public initializer {
        token = _token;
        __Ownable_init();
    }

    function buy(address from, uint256 amount) external onlyOwner {
        IERC20Upgradeable(token).safeTransferFrom(from, address(this), amount);
    }

    function transfer(
        address from,
        address to,
        uint256 amount
    ) external onlyOwner {
        IERC20Upgradeable(token).safeTransferFrom(from, to, amount);
    }
}

interface IEliteWorld {
    function levels(uint256 tokenId) external view returns (uint256);

    function itemOfOwnerByToken(uint256 tokenId)
        external
        view
        returns (uint256);

    function items(uint256 index)
        external
        view
        returns (
            string memory tokenURI,
            uint256 idType,
            uint256 id,
            uint256 price,
            uint256 point,
            uint256 maxLevel,
            bool isSold
        );

    function increaseLevel(uint256 tokenId, uint256 nextLevel) external;

    function ownerOf(uint256 tokenId) external view returns (address);

    function itemExist(uint256 accessoryId) external view;

    function getItemId(uint256 accessoryId) external view returns (uint256);
}

contract AccessoryELW is Initializable, OwnableUpgradeable {
    using SafeMath for uint256;

    address public agent;
    address public tokenVault;
    IEliteWorld public eliteWorld;

    uint256 public constant DICE = 0;
    uint256 public constant CHAR = 2;
    uint256 public constant ACCESSORY = 6;

    // store quantity of each item
    mapping(address => mapping(uint256 => uint256)) public ownedAccessories;

    mapping(uint256 => mapping(uint256 => uint256)) private _rateLevel;

    mapping(uint256 => mapping(uint256 => uint256))
        private _totalAccessoryUpgradeOf;
    mapping(uint256 => mapping(uint256 => mapping(uint256 => uint256)))
        private _accessoryUpgrade;
    mapping(uint256 => mapping(uint256 => mapping(uint256 => uint256)))
        private _accessoryUpgradeIndex;

    mapping(uint256 => mapping(uint256 => mapping(uint256 => uint256)))
        private _accessoryQuantity;

    mapping(address => uint256) private _balanceOf;
    mapping(address => mapping(uint256 => uint256)) private _ownedAccessories;
    mapping(uint256 => uint256) private _ownedAccessoriesIndex;

    event BuyAccessory(
        address indexed account,
        uint256 id,
        uint256 price,
        uint256 quantity
    );

    event IncreaseLevel(
        address indexed account,
        uint256 tokenId,
        uint256 level
    );

    event UseAccessories(
        address indexed account,
        uint256[] accessoryIds,
        string roomId
    );

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize(
        address _tokenVault,
        address _eliteWorld,
        address _agent
    ) public initializer {
        tokenVault = address(new TokenRewardVault());
        ITokenVault(tokenVault).initialize(_tokenVault);

        eliteWorld = IEliteWorld(_eliteWorld);
        agent = _agent;

        __Ownable_init();
    }

    // ============ Helper ============

    function validType(uint256 _type) private pure {
        require(_type == CHAR || _type == DICE, "Invalid type");
    }

    function rateLevel(uint256 _type, uint256 _level)
        public
        view
        returns (uint256)
    {
        return _rateLevel[_type][_level];
    }

    function sumRateByLevel(uint256 _type, uint256 _level)
        external
        view
        returns (uint256)
    {
        uint256 sum = 0;
        for (uint256 i = 1; i <= _level; i++) {
            sum += _rateLevel[_type][i];
        }
        return sum;
    }

    function balanceOf(address owner) public view returns (uint256) {
        return _balanceOf[owner];
    }

    // ============ Ownable ============
    function safeTransferToken(address account, uint256 amount) internal {
        ITokenVault(tokenVault).buy(account, amount);
    }

    function setEliteWorld(address _eliteWorld) external onlyOwner {
        eliteWorld = IEliteWorld(_eliteWorld);
    }

    function setAgent(address _agent) external onlyOwner {
        agent = _agent;
    }

    function withdraw(address to, uint256 amount) external onlyOwner {
        ITokenVault(tokenVault).transfer(to, amount);
    }

    // set accessory which item need to upgradeable
    function setAccessoryUpgradeLevel(
        uint256 _type,
        uint256 _level,
        uint256 _accessoryId
    ) external onlyOwner {
        validType(_type);
        require(_level > 0, "Invalid level");
        eliteWorld.itemExist(_accessoryId);
        require(
            _totalAccessoryUpgradeOf[_type][_level] < 3,
            "Maximum accessory upgrade level"
        );
        _accessoryUpgrade[_type][_level][
            _totalAccessoryUpgradeOf[_type][_level]
        ] = _accessoryId;

        _accessoryUpgradeIndex[_type][_level][
            _accessoryId
        ] = _totalAccessoryUpgradeOf[_type][_level];

        _totalAccessoryUpgradeOf[_type][_level] += 1;
    }

    // remove accessory which item need to upgradeable
    function removeAccessoryUpgradeLevel(
        uint256 _type,
        uint256 _level,
        uint256 _accessoryId
    ) external onlyOwner {
        validType(_type);
        require(
            _totalAccessoryUpgradeOf[_type][_level] > 0,
            "Minimum accessory upgrade level"
        );
        uint256 lastIndexTotal = _totalAccessoryUpgradeOf[_type][_level] - 1;
        uint256 indexTotal = _accessoryUpgradeIndex[_type][_level][
            _accessoryId
        ];
        // When the index to delete is the last index, the swap operation is unnecessary
        if (indexTotal != lastIndexTotal) {
            uint256 lastAccessory = _accessoryUpgrade[_type][_level][
                lastIndexTotal
            ];
            _accessoryUpgrade[_type][_level][indexTotal] = lastAccessory; // Move the last index to the slot of the to-delete index
            _accessoryUpgradeIndex[_type][_level][lastIndexTotal] = indexTotal; // Update the moved index's index
        }
        // // This also deletes the contents at the last position of the array
        delete _accessoryUpgradeIndex[_type][_level][_accessoryId];
        delete _accessoryUpgrade[_type][_level][lastIndexTotal];
        _totalAccessoryUpgradeOf[_type][_level] -= 1;
    }

    // set quantity of accessory which item need to upgradeable
    function setQuantityUpgradeLevel(
        uint256 _type,
        uint256 _level,
        uint256 _accessoryId,
        uint256 _quantity
    ) external onlyOwner {
        validType(_type);
        require(_level > 1, "Invalid level");
        eliteWorld.itemExist(_accessoryId);
        _accessoryQuantity[_type][_level][_accessoryId] = _quantity;
    }

    // set rate percent increase when upgrade level item
    function setRateLevel(
        uint256 _type,
        uint256 _level,
        uint256 _percent
    ) external onlyOwner {
        validType(_type);
        require(_level > 0, "Invalid level");
        require(_percent > 0 && _percent < 1000, "Invalid percent");
        _rateLevel[_type][_level] = _percent;
    }

    function _addAccessoryToOwnerEnumeration(address to, uint256 _accessoryId)
        private
    {
        uint256 length = balanceOf(to);
        _ownedAccessories[to][length] = _accessoryId;
        _ownedAccessoriesIndex[_accessoryId] = length;
        _balanceOf[to] += 1;
    }

    function _removeAccessoryFromOwnerEnumeration(
        address from,
        uint256 accessoryId
    ) private {
        uint256 lastAccessoryIndex = balanceOf(from) - 1;
        uint256 tokenIndex = _ownedAccessoriesIndex[accessoryId];

        if (tokenIndex != lastAccessoryIndex) {
            uint256 lastAccessoryId = _ownedAccessories[from][
                lastAccessoryIndex
            ];

            _ownedAccessories[from][tokenIndex] = lastAccessoryId;
            _ownedAccessoriesIndex[lastAccessoryId] = tokenIndex;
        }

        delete _ownedAccessoriesIndex[accessoryId];
        delete _ownedAccessories[from][lastAccessoryIndex];
    }

    // ============ Transaction ============
    function buyAccessory(uint256 _accessoryId, uint256 _quantity) external {
        uint256 indexItem = eliteWorld.getItemId(_accessoryId);
        uint256 idType;
        uint256 maxLevel;
        uint256 price;
        uint256 id;
        bool isSold;

        (, idType, id, price, , maxLevel, isSold) = eliteWorld.items(indexItem);
        require(isSold, "Item not sold");
        require(idType == ACCESSORY, "Invalid item accessory");
        require(_quantity > 0, "Quantity minimum 1");

        safeTransferToken(msg.sender, price * _quantity);

        if (ownedAccessories[msg.sender][_accessoryId] == 0) {
            _addAccessoryToOwnerEnumeration(msg.sender, _accessoryId);
        }
        ownedAccessories[msg.sender][_accessoryId] += _quantity;

        emit BuyAccessory(msg.sender, id, price, _quantity);
    }

    function increaseLevel(uint256 _tokenId) external {
        require(
            eliteWorld.ownerOf(_tokenId) == msg.sender,
            "Not owner of this token"
        );
        uint256 currentLevel = eliteWorld.levels(_tokenId);
        uint256 indexItem = eliteWorld.itemOfOwnerByToken(_tokenId);

        uint256 idType;
        uint256 maxLevel;
        (, idType, , , , maxLevel, ) = eliteWorld.items(indexItem);
        validType(idType);

        uint256 nextLevel = currentLevel + 1;
        uint256 nextRate = rateLevel(idType, nextLevel);
        require(nextRate > 0 && nextLevel < maxLevel, "Maximum level");

        uint256 total = _totalAccessoryUpgradeOf[idType][nextLevel];

        for (uint256 i = 0; i < total; i++) {
            uint256 accessoryId = _accessoryUpgrade[idType][nextLevel][i];
            uint256 quantityNeeded = _accessoryQuantity[idType][nextLevel][
                accessoryId
            ];
            uint256 currentQuantity = ownedAccessories[msg.sender][accessoryId];
            require(
                quantityNeeded < currentQuantity,
                "Insufficient quantity accessory"
            );
            ownedAccessories[msg.sender][accessoryId] = currentQuantity.sub(
                quantityNeeded
            );

            if (ownedAccessories[msg.sender][accessoryId] == 0) {
                _removeAccessoryFromOwnerEnumeration(msg.sender, accessoryId);
            }
        }

        eliteWorld.increaseLevel(_tokenId, nextLevel);

        emit IncreaseLevel(msg.sender, _tokenId, nextLevel);
    }

    function useAccessories(
        address _player,
        uint256[] memory _accessoryIds,
        string memory roomId
    ) external {
        require(msg.sender == agent, "Caller is not a agent");
        require(
            _accessoryIds.length > 0 && _accessoryIds.length < 3,
            "Invalid accessories"
        );

        for (uint256 index = 0; index < _accessoryIds.length; index++) {
            uint256 indexItem = eliteWorld.getItemId(_accessoryIds[index]);
            uint256 idType;
            (, idType, , , , , ) = eliteWorld.items(indexItem);
            require(idType == ACCESSORY, "Invalid accessory");
            require(
                ownedAccessories[_player][_accessoryIds[index]] > 0,
                "Insufficient quantity accessory"
            );
            ownedAccessories[_player][_accessoryIds[index]] = ownedAccessories[
                _player
            ][_accessoryIds[index]].sub(1);

            if (ownedAccessories[_player][_accessoryIds[index]] == 0) {
                _removeAccessoryFromOwnerEnumeration(
                    _player,
                    _accessoryIds[index]
                );
            }
        }

        emit UseAccessories(_player, _accessoryIds, roomId);
    }
}

