pragma solidity ^0.8.0;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";

//import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "./NFTCollectible.sol";

import "./INFTCollectionManager.sol";
import "./INFTCollectible.sol";

import "hardhat/console.sol";

contract NFTCollectionManager is 
    AccessControlEnumerable, 
    Ownable,
    INFTCollectionManagerRole,
    ReentrancyGuard,
    IERC721Receiver
{
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.AddressSet;

    Counters.Counter private _collectionIds;

    //id => contract address
    mapping(uint256 => address) private contractList;

    // ContractAddress => Owner
    // mapping(address => address) private _owners;
    // owner => ContractList
    mapping(address => EnumerableSet.AddressSet) private _contractsByOwner;

    // contractList by contract names
    mapping(string => EnumerableSet.AddressSet) private _contractsByName;

    // Allowed roles for collection manager
    uint256 public ADMIN_ROLE_LIMIT     = 100;
    uint256 public CREATOR_ROLE_LIMIT   = 1000;

    bytes32 internal CREATOR_ROLE; 
    bytes32 internal ADMIN_ROLE;

    struct CollectionInfo {
        address addr;
        string name;
        string symbol;
        uint256 count;
        uint256 price;
    }

    event CollectionCreated(
        address createdAt,
        address creator
    );

    event WithDraw(
        uint256 amount,
        address to
    );

    
    constructor() {
        _initRoles();
    }

    function onERC721Received(
        address, 
        address, 
        uint256, 
        bytes calldata
    ) external override returns(bytes4) {
        return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    }

    function hasCreatorRole(address _to) external view override returns(bool) {
        return hasRole(CREATOR_ROLE, _to);
    }

    function hasAdminRole(address _to) external view override returns(bool) {
        return hasRole(ADMIN_ROLE, _to);
    }

    function getRoleMemberList(bytes32 role) private view returns(address[] memory) {
        uint length = getRoleMemberCount(role);
        address[] memory ret = new address[](length);
        for (uint i = 0; i < length; i++) {
            ret[i] = getRoleMember(role, i);
        }

        return ret;
    }

    function getAdminList() external view override returns(address[] memory) {
        return getRoleMemberList(ADMIN_ROLE);
    }

    function getCreatorList() external view override returns(address[] memory) {
        return getRoleMemberList(CREATOR_ROLE);
    }

    /**
     *@dev initialize roles.
     */
    function _initRoles() internal {
        _setupRole(DEFAULT_ADMIN_ROLE, owner());

        ADMIN_ROLE = keccak256("ADMIN_ROLE");
        CREATOR_ROLE = keccak256("MINTER_ROLE");
        grantRole(CREATOR_ROLE, owner());
        grantRole(ADMIN_ROLE, owner());
    }

    /**
     *@dev set up creators for CollectionManager.
     * minters can register ERC721 token to this.
     */
    function addCreator(address _to) external onlyOwner {
        require(!hasRole(CREATOR_ROLE, _to), "Already granted");
        require(_to != address(0), "Zero address invalid");
        require(getRoleMemberCount(CREATOR_ROLE) < CREATOR_ROLE_LIMIT, "No space");
        _setupRole(CREATOR_ROLE, _to);

        emit CreatorAdded(_to);
    }

    function revokeCreator(address _to) external onlyOwner {
        require(hasRole(CREATOR_ROLE, _to), "No creator");
        revokeRole(CREATOR_ROLE, _to);

        emit CreatorRevoked(_to);
    }

    function addAdmin(address _to) external onlyOwner {
        require(!hasRole(ADMIN_ROLE, _to), "Already granted");
        require(_to != address(0), "Zero address invalid");
        require(getRoleMemberCount(ADMIN_ROLE) < ADMIN_ROLE_LIMIT, "No space");
        _setupRole(ADMIN_ROLE, _to);

        emit AdminAdded(_to);
    }

    function revokeAdmin(address _to) external onlyOwner {
        require(hasRole(ADMIN_ROLE, _to), "No admin");
        revokeRole(ADMIN_ROLE, _to);

        emit AdminRevoked(_to);
    }

    /**
     *@dev create collection by minters or owner
     */
    function createCollection(
        string memory tokenName,
        string memory tokenSymbol,
        string memory tokenBaseUri,
        uint256 tokenPrice,
        uint256 tokenCount
    ) external returns (address) {
        require(hasRole(CREATOR_ROLE, _msgSender()), "Not allowed");
        uint newCollectionID = _collectionIds.current();
        NFTCollectible newCollection = new NFTCollectible(
            tokenName,
            tokenSymbol,
            tokenBaseUri,
            tokenPrice,
            address(this)
        );
        newCollection.createNFTs(tokenCount);

        contractList[newCollectionID] = address(newCollection);

        // _owners[address(newCollection)] = _msgSender(); //Not owner, in fact Minter

        console.log("Created at : ", address(newCollection));

        _contractsByOwner[_msgSender()].add(address(newCollection));
        _contractsByName[tokenName].add(address(newCollection));

        _collectionIds.increment();

        console.log("This address : ", address(this));
        console.log("Sender address : ", address(_msgSender()));
        console.log("Contract Owner address : ", newCollection.owner());

        console.log("Token Price : ", newCollection.getItemPrice());

        emit CollectionCreated(address(newCollection), _msgSender());

        return address(newCollection);
    }

    function getCollectionListByName(
        string memory tokenName
    ) external view returns (address[] memory)  {
        EnumerableSet.AddressSet storage _list = _contractsByName[tokenName];
        uint256 length = _list.length();

        address[] memory retList = new address[](length);
        for (uint256 i = 0; i < length; i++) {
            retList[i] = _list.at(i);
        }

        return retList;
    }

    function getCollectionListByOwner(
        address tokenOwner
    ) external view returns (address[] memory)  {
        EnumerableSet.AddressSet storage _list = _contractsByOwner[tokenOwner];
        uint256 length = _list.length();

        address[] memory retList = new address[](length);
        for (uint256 i = 0; i < length; i++) {
            retList[i] = _list.at(i);
        }

        return retList;
    }

    function getAllCollectionAddressList() external view returns (address[] memory) {
        uint256 length = _collectionIds.current();
        address[] memory retList = new address[](length);
        for (uint256 i = 0; i < length; i++) {
            retList[i] = contractList[i];
        }
        return retList;
    }

    function getAllDetailedCollectionList() external view returns (CollectionInfo[] memory) {
        uint256 length = _collectionIds.current();
        CollectionInfo[] memory retList = new CollectionInfo[](length);
        address addr;
        string memory name;
        string memory symbol;
        uint256 count;
        uint256 price;
        for (uint256 i = 0; i < length; i++) {
            address _addr = contractList[i];
            (addr, name, symbol, count, price, ) = INFTCollectible(_addr).getContractInfo();
            retList[i] = CollectionInfo(addr, name, symbol, count, price);
        }
        return retList;
    }

    function getAllTokensForUser(address _to) external view override returns(TokenListInfo[] memory) {
        uint256 length = _collectionIds.current();
       
        TokenListInfo[] memory ret = new TokenListInfo[](length);

        for (uint256 i = 0; i < length; i++) {
            INFTCollectible _contract = INFTCollectible(contractList[i]);
            uint256[] memory idList = _contract.tokensOfOwner(_to);
            if (idList.length > 0) {
                (, string memory tokenName, string memory tokenSymbol, , uint256 tokenPrice, string memory baseURI ) = _contract.getContractInfo();
                ret[i] = TokenListInfo(
                    tokenName, tokenSymbol, baseURI, tokenPrice, idList
                );
            }
        }

        return ret;
    }

    // Function to receive Ether. msg.data must be empty
    receive() external payable {}

    // Fallback function is called when msg.data is not empty
    fallback() external payable {}

    /**
     *@dev withdraw balance
     */
    function withdraw() external payable onlyOwner {
        uint balance = address(this).balance;
        require (balance > 0, "No ether left to withdraw");
        (bool success,) = (msg.sender).call{value: balance}("");
        require(success, "Transfer failed");
        emit WithDraw(balance, _msgSender());
    }

    // Purchase by general users
    function mintNFTs(address contractAddr, uint256 count) external payable nonReentrant {
        INFTCollectible destContract = INFTCollectible(contractAddr);

        uint256 _price = destContract.getItemPrice();
        console.log("Manager Price=", _price);
        
        require(msg.value >=_price * count, "Insufficient Ether");
        
        destContract.mintNFTs(count, _msgSender());
    }
}

