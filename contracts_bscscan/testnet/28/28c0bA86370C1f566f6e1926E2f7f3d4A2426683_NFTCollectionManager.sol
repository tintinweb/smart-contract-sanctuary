pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/Strings.sol";


import "./INFTCollectible.sol";
import "./INFTCollectionManager.sol";

import "hardhat/console.sol";

contract NFTCollectible is INFTCollectible, ERC721Enumerable, Ownable {
    using Counters for Counters.Counter;
    using Strings for uint256;

    Counters.Counter private _tokenIds;

    INFTCollectionManagerRole private _accessManager;

    // The maximum number of NFTs that can be minted in your collection
    uint public constant MAX_SUPPLY = 5000;
    uint public constant MAX_PER_MINT = 500;

    uint public itemPrice = 0.01 ether;

    uint private sellingItemIndex = 0;
    
    // The IPFS URL of the folder containing the JSON metada
    string public baseTokenURI;

    constructor (
        string memory tokenName,
        string memory tokenSymbol,
        string memory baseURI,
        uint tokenPrice,
        address collectionManager
    ) ERC721(tokenName, tokenSymbol) {
        _accessManager = INFTCollectionManagerRole(collectionManager);
        baseTokenURI = baseURI;
        itemPrice = tokenPrice;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(ERC721Enumerable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }

    /**
     *@dev withdraw balance
     */
    function withdraw() external payable override onlyOwner {
        uint balance = address(this).balance;
        require (balance > 0, "No ether left to withdraw");
        (bool success,) = (msg.sender).call{value: balance}("");
        require(success, "Transfer failed");
        emit WithDraw(balance, _msgSender());
    }

    /**
     *@dev set Item Price
     * not able to change item price once trading started
     */
    function setItemPrice(uint newItemPrice) external override{
        require(sellingItemIndex == 0, "Not able to set after trade");
        uint256 oldPrice = itemPrice;
        itemPrice = newItemPrice;
        emit ItemPriceUpdated(oldPrice, itemPrice);
    }

    function getItemPrice() external view override returns(uint256) {
        return itemPrice;
    }

    function totalCount() external view override returns(uint256) {
        return _tokenIds.current();
    }

    function getMintedCount() external view override returns (uint256) {
        return sellingItemIndex;
    }

    
    function _baseURI() internal view virtual override returns (string memory) {
        return baseTokenURI;
    }

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "ERC721Metadata: URI query for nonexistent token");

        string memory baseURI = _baseURI();
        if (bytes(baseURI).length > 0) {
            return string(abi.encodePacked(baseURI,tokenId.toString()));
        }
        return "";
    }

    function setBaseURI(string memory _baseTokenURI) external override {
        require(_accessManager.hasAdminRole(_msgSender()), "Only admin possible");
        baseTokenURI = _baseTokenURI;
        emit BaseURIUpdated(baseTokenURI);
    }

    function createNFTs(uint _count) external override onlyOwner {
        console.log("Creating NFT by", _msgSender());
        
        uint totalMinted = _tokenIds.current();
        require(
            (totalMinted + _count) <= MAX_SUPPLY, "Not enough space"
        );
        console.log("Creating NFT count : ", _count);
        require(
            (_count > 0 && _count <= MAX_PER_MINT),
            "Count invalid"
        );

        require(
            _tokenIds.current() == 0, "Add is not allowed"
        );

        for (uint i = 0; i < _count; i++) {
            _createSingleNFT();
        }

        console.log("NFT Contract : created ", _count);

        emit NFTCreated(_count);
    }

    function mintNFTs(uint256 count, address to) external onlyOwner override {
        uint totalSupply = _tokenIds.current();
        require(
            sellingItemIndex + count <= totalSupply, "Insufficient items"
        );
        for (uint i = 0; i < count; i++) {
            safeTransferFrom(owner(), to, sellingItemIndex);
            // TransferFrom(owner(), to, sellingItemIndex);
            sellingItemIndex++;
        }

        console.log("NFT Contract : minted ", count);
        emit NFTMinted(count, _msgSender(), totalSupply, sellingItemIndex);
    }

    function _createSingleNFT() private {
        uint newTokenID = _tokenIds.current();
        _safeMint(owner(), newTokenID); //Mint to CollectionManager
        _tokenIds.increment();
    }

    function tokensOfOwner(address _owner) external view override returns(uint[] memory) {
        uint tokenCount = balanceOf(_owner);
        uint[] memory tokensId = new uint256[](tokenCount);
        for (uint i = 0; i < tokenCount; i++) {
            tokensId[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokensId;
    }

    function getContractInfo() external view override returns (
        address, //token address
        string memory, // name
        string memory, // symbol
        uint256, // count
        uint256, //price
        string memory //base URI
    ) {
        return (
            address(this),
            name(),
            symbol(),
            _tokenIds.current(),
            itemPrice,
            _baseURI()
        );
    }

    // function reserveNFTs() public onlyOwner {
    //     uint totalMinted = _tokenIds.current();
    //     require(
    //         (totalMinted + 10) < MAX_SUPPLY, "Not enough NFTs"
    //     );
    //     for (uint i = 0; i < 10; i++) {
    //         _mintSingleNFT();
    //     }
    // }

}

