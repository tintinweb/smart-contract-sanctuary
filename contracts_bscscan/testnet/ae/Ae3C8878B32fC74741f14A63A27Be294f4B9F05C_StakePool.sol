     * present.
     */
    function remove(UintSet storage set, uint256 value) internal returns (bool) {
        return _remove(set._inner, bytes32(value));
    }

    /**
     * @dev Returns true if the value is in the set. O(1).
     */
    function contains(UintSet storage set, uint256 value) internal view returns (bool) {
        return _contains(set._inner, bytes32(value));
    }

    /**
     * @dev Returns the number of values on the set. O(1).
     */
    function length(UintSet storage set) internal view returns (uint256) {
        return _length(set._inner);
    }

   /**
    * @dev Returns the value stored at position `index` in the set. O(1).
    *
    * Note that there are no guarantees on the ordering of values inside the
    * array, and it may change when more values are added or removed.
    *
    * Requirements:
    *
    * - `index` must be strictly less than {length}.
    */
    function at(UintSet storage set, uint256 index) internal view returns (uint256) {
        return uint256(_at(set._inner, index));
    }
}

// SPDX-License-Identifier: MIT
contract StakePool is ReentrancyGuard, Ownable, Pausable {
    using SafeMath for uint;
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.AddressSet;

// variable definitions
    struct Tier {
        uint stake;
        uint32 weight;
        uint unlockTime;
    }

    IERC20 public stakeToken;

    uint public totalAllocPoint;

    mapping(address => uint) stakeBalances;

    mapping(address => uint) allocPoints;

    EnumerableSet.AddressSet private users;

    // EXPERIMENTAL !!  
     mapping(address => uint) timeLocks;
     uint public timeLock; //---> create a setter to be altered after deployment


    event Staked(address indexed user, uint amount);
    event Withdrawn(address indexed user, uint amount);

    Tier[] public tiers;
    
// constructor
    constructor(address _stakeToken) public {
        stakeToken = IERC20(_stakeToken);
        totalAllocPoint = 0;

        // Fill Tiers
        addTier(20000, 750, uint32(1 hours));
        addTier(10000, 300, uint32(1 hours));
        addTier(5000, 130, uint32(1 hours));
        addTier(2000, 40, uint32(1 hours));
        addTier(1000, 15, uint32(1 hours));

        // EXPERIMENTAL !!  
        timeLock = uint(1 seconds);
    }

    function addTier(uint _stake, uint32 _weight, uint _unlockTime) public onlyOwner {
        tiers.push(
            Tier({  
                stake: _stake,
                weight: _weight,
                unlockTime: _unlockTime
            })
        );
    }

// core functions
    function stake(uint _amount) external {
        // do some checks 
        require(stakeToken.balanceOf(msg.sender) >= _amount, "!amount");
        require(_amount >  10, "minimum 10!");
        require((_amount +  stakeBalances[msg.sender])  >= 1000, "user should stake a minimum 1000!");

        // transfer tokens to this stakePool
        stakeToken.safeTransferFrom(msg.sender, address(this), _amount);

      // change stakeBalance
        stakeBalances[msg.sender] += _amount; 


      // reset totalAllocPoint
        totalAllocPoint -= allocPoints[msg.sender];

      // calculate and assing new allocPoints
        allocPoints[msg.sender] = _reBalance(stakeBalances[msg.sender] );

      // update totalAllocPoint
        totalAllocPoint += allocPoints[msg.sender];

      // update other variables like unlockTime etc. 

      // add user to the list
        _checkOrAddUser(msg.sender);


        // EXPERIMENTAL !! 
        uint _unlock = block.timestamp.add(timeLock);
        timeLocks[msg.sender] = _unlock;

        emit Staked(msg.sender, _amount);
    }

    function withdraw(uint _amount) external virtual {
        // do some checks 
        require(balanceOf(msg.sender) >= _amount, "!StakeAmount");

       // if(timeLocks[msg.sender]  <= block.timestamp ) {

       // transfer tokens to user
        stakeToken.safeTransfer(msg.sender, _amount);

        // change stakeBalance
        stakeBalances[msg.sender] -= _amount; 

        // reset totalAllocPoint
        totalAllocPoint -= allocPoints[msg.sender];

        // calculate and assing new allocPoints
        allocPoints[msg.sender] = _reBalance(stakeBalances[msg.sender] );

        // remove from userList if no Tier
        if (allocPoints[msg.sender] == 0) _removeUser(msg.sender);

        // update totalAllocPoint
        totalAllocPoint += allocPoints[msg.sender];

        // update other variables
        
        // remove users if stakes == 0
         // allocPoints.remove(msg.sender);
         // stakeBalances.remove(msg.sender);

        emit Withdrawn(msg.sender, _amount);
    }


    function withdrawAll() external virtual{
        // do some checks 
        require(balanceOf(msg.sender) >= 0, "!StakeAmount");

        // EXPERIMENTAL !! 
       // if(timeLocks[msg.sender]  <= block.timestamp ) {
 
        // return all staked tokens back to user
        uint _amount = stakeBalances[msg.sender];
        stakeToken.safeTransfer(msg.sender, _amount);

        // reset allocPoints
        uint _points =  allocPoints[msg.sender] ;
       
        // zero stakeBalance
        stakeBalances[msg.sender] =0; 

        // zero allocPoints
        allocPoints[msg.sender] = 0;

        // reset totalAllocPoint
        totalAllocPoint -= _points;

        // remove from userList
        _removeUser(msg.sender);

        emit Withdrawn(msg.sender, _amount);
    }

// helper functions
    function _reBalance(uint _balance) internal view returns (uint _points) {
        // initiate
        _points = 0;
        
        //uint _smallest = 1000;    // tier 4 staked
        uint _smallest = tiers[tiers.length-1].stake; // use former routine if this fails

        // take the biggest tier possible
        // we can keep of numbers per tier in later stage like tier{god}++
        while (_balance >= _smallest) { 
            for (uint i = 0; i < tiers.length; i++) {   // iterate over tiers, order by biggest tier
                if (_balance >= tiers[i].stake) {  // check if we have enough stake left for this tier
                    _points += tiers[i].weight; // add weight points
                    _balance -= tiers[i].stake; // redduce balance of stakes
                    i = tiers.length;   // exit iteration loopo
                }
            }
        }
        return _points;
    }

    function _removeUser(address _user) internal {
        if (users.contains(_user) == true) {
            users.remove(_user);
        }
    }

    function _checkOrAddUser(address _user) internal {
        if (users.contains(_user) == false) {
            users.add(_user);
        }
    }

// getters 
    function balanceOf(address _sender) public view returns(uint) {

        return stakeBalances[_sender];
    }

    function allocPointsOf(address _sender) public view returns(uint) {

        return allocPoints[_sender];
    }

    function allocPercentageOf(address _sender) public view returns(uint) {

        uint points = allocPoints[_sender]*10**6;

        // calculated percentage of total Allowance in percPoints (100% = 1000000 points)
        uint millePercentage = points.div(totalAllocPoint);

        return millePercentage;
    }

    function tierCount() public view returns (uint8) {
        return uint8(tiers.length);
    }

    function getUserList() external view onlyOwner returns (address[] memory) {
        address[] memory userList = new address[](users.length());
        for (uint i = 0; i < users.length(); i++) {
            userList[i] = users.at(i);
        }
        return userList;
    }


// setters 
    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    
    /// @param _lockDuration must be in seconds
    function setLockDuration(uint32 _lockDuration) external onlyOwner {
            timeLock = _lockDuration;
    }
 
// filters
}