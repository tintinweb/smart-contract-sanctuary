pragma solidity 0.8.6;

import "../access/Ownable.sol";
import "../ERC/ERC721/IERC721.sol";

contract ERC721TransferProxy is Ownable {
    function erc721safeTransferFrom(
        IERC721 token,
        address from,
        address to,
        uint256 tokenId
    ) external onlyOwner {
        token.safeTransferFrom(from, to, tokenId);
    }
}

