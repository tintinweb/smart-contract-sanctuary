import "./interfaces/ITransferer.sol";
import "./interfaces/IMasterChef.sol";
import "./interfaces/IPancakeRouterV2.sol";

import "@pancakeswap/pancake-swap-lib/contracts/access/Ownable.sol";
import "@pancakeswap/pancake-swap-lib/contracts/token/BEP20/BEP20.sol";
import "@pancakeswap/pancake-swap-lib/contracts/token/BEP20/SafeBEP20.sol";

contract Strategy is Ownable, BEP20 {
    
    using SafeBEP20 for IBEP20;
    using SafeMath for uint256;

    uint256 pid;
    address token0;
    address token1;
    address transferer;
    address lpPair;
    address masterChefAddress;
    address rewardToken;

    constructor(
        uint256 _pid,
        address _token0,
        address _token1,
        address _lpPair,
        address _rewardToken,
        string memory _name,
        string memory _symbol
    ) public BEP20 (
        _name,
        _symbol
    )
    {
        pid = _pid;
        token0 = _token0;
        token1 = _token1;
        transferer = owner();
        lpPair = _lpPair;
        rewardToken = _rewardToken;
    }

    function deposit() external onlyOwner {
        uint256 lpBalance = IBEP20( lpPair ).balanceOf( address (this) );
        if ( lpBalance > 0 ) {
            IMasterChef( ITransferer( transferer ).masterChefAddress() ).deposit(pid, lpBalance);
        } 
    }

    function mintTokens( address _recipient, uint _amount ) external onlyOwner {
        _mint(_recipient, _amount);
    }

    function balanceOfStrategy() public view returns ( uint ) {
        return IBEP20 ( lpPair ).balanceOf( address(this) );
    }

    function balanceOfPool( uint _pid ) public view returns (uint256) {
        ( uint256 _amount, ) = IMasterChef( ITransferer( transferer ).masterChefAddress() ).userInfo( _pid, address (this) );
        return _amount;
    }

    function withdraw( uint256 _amount ) external onlyOwner {
        uint256 lpBalance = IBEP20( lpPair ).balanceOf( address (this) );

        if (lpBalance < _amount) {
            IMasterChef( ITransferer( transferer ).masterChefAddress() ).withdraw( pid, _amount.sub( lpBalance ) );
            lpBalance = IBEP20( lpPair ).balanceOf( address (this) );
        }

        if (lpBalance > _amount) {
            lpBalance = _amount;
        }

      
        IBEP20( lpPair ).safeTransfer( transferer, lpBalance );
        
    }

    function harvest() public onlyOwner {
        IMasterChef( ITransferer( transferer ).masterChefAddress() ).deposit( pid, 0 );
        addLiquidity( );
        this.deposit( );
    }

    function addLiquidity() internal onlyOwner {
        address router = ITransferer( transferer ).routerAddress();
        uint256 amount = IBEP20( rewardToken ).balanceOf( address(this) );

        address[] memory path0 = new address[](2);
        path0[0] = rewardToken;
        path0[1] = token0;

        address[] memory path1 = new address[](2);
        path1[0] = rewardToken;
        path1[1] = token1;
        
        uint[] memory expectedReturnToken0  = IPancakeRouterV2( router ).getAmountsOut( amount.div(2), path0 );
        IPancakeRouterV2( router ).swapExactTokensForTokens( amount.div(2), expectedReturnToken0[1], path0, address( this ), now );

        uint[] memory expectedReturnToken1  = IPancakeRouterV2( router ).getAmountsOut( amount.div(2), path1 );
        IPancakeRouterV2( router ).swapExactTokensForTokens( amount.div(2), expectedReturnToken1[1], path1 , address( this ), now );

        uint256 token0allowance = IBEP20( token0 ).allowance( address( this ), router );
        uint256 token1allowance = IBEP20( token1 ).allowance( address( this ), router );

        if (token0allowance == 0) {
            IBEP20( token0 ).approve( router, 2 ** 254 );
        }

        if (token1allowance == 0) {
            IBEP20( token1 ).approve( router, 2 ** 254 );
        }

        IPancakeRouterV2( router ).addLiquidity(
            token0,
            token1,
            expectedReturnToken0[1],
            expectedReturnToken1[1],
            expectedReturnToken0[1] * 99 / 100,
            expectedReturnToken1[1] * 99 / 100,
            address( this ),
            now
        );
    }

}

