contract Tokenv3 is ERC721URIStorage, Ownable {
    
    using SafeMath for uint256;
    using Counters for Counters.Counter;
    Counters.Counter private _tokenIds;

    uint256 private price = 25000000000000000;
    bool public isSaleActive = true;
    address payable private _owner;
    address public TOKEN_ADMIN_WALLET = 0xCB84511a1530D065176d90Cc0717e302DD9cDcA7;
    constructor() ERC721("Token", "TKN") {
    }
    
    function _baseURI() internal pure override returns (string memory) {
        return "ipfs://xxx/";
    }
    
    // original award item
    function awardItem() payable public returns (uint256) {
        require(isSaleActive, "Sale is not active" );
        require(msg.value >= price, "Amount is too low");
        payable(address(TOKEN_ADMIN_WALLET)).transfer(msg.value);
        _tokenIds.increment();
        _safeMint(TOKEN_ADMIN_WALLET, _tokenIds.current());
        return _tokenIds.current();
    }

    function mint(address customer) public onlyOwner returns (uint256) {
        require(isSaleActive, "Sale is not active" );
        _tokenIds.increment();
        _safeMint(customer, _tokenIds.current());
        return _tokenIds.current();
    }
    
    function flipSaleStatus() public onlyOwner {
        isSaleActive = !isSaleActive;
    }

    function setprice(uint256 newprice) public onlyOwner {
        price = newprice;
    }
    
    function withdraw(address payable recipient) public onlyOwner {
        uint256 balance = address(this).balance;
        recipient.transfer(balance);
    }
    
}