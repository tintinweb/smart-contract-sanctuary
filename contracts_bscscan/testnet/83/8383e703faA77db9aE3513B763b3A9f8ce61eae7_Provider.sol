contract Provider is Ownable,IERC721Receiver {
  using SafeMath for uint256;
  using EnumerableSet for EnumerableSet.AddressSet;

  EnumerableSet.AddressSet private _contentAggregatorsNFTBundles;
  mapping(address => address) public contentAggregatorForNFTBundle;
  mapping(address => mapping(uint256 => uint256)) public recievedPaymentsValuesEachNFTBundle;
  address public usdContract;
  AggregatorV3Interface internal priceFeed;
  uint256 public networkId;
  string public name;
  string public info;
  string public playerURL;
  address public governance;

  event Received(address indexed from, uint256 value);
  event PaymentReceivedFor(uint256 amount, address from, address nftBunble,uint256 nftBunbleId, uint256 orderId);
  event PayRevenueSharing(uint256 amount, address to);

  constructor (
    address _usdContract,
    uint256 _networkId,
    string memory _name,
    string memory _info,
    address _governance,
    string memory _playerURL
  ) public {
    name = _name;
    info = _info;
    governance = _governance;
    usdContract = _usdContract;
    networkId = _networkId;
    playerURL = _playerURL;

    if (_networkId == 1) {
      priceFeed = AggregatorV3Interface(0x5f4eC3Df9cbd43714FE2740f5E3616155c5b8419);// ETH mainnet
    }
    if (_networkId == 42) {
      priceFeed = AggregatorV3Interface(0x9326BFA02ADD2366b30bacB125260Af641031331);// ETH kovan
    }
    if (_networkId == 56) {
      priceFeed = AggregatorV3Interface(0x0567F2323251f0Aab15c8dFb1967E4e8A7D42aeE);// BCS mainnet
    }
    if (_networkId == 97) {
      priceFeed = AggregatorV3Interface(0x2514895c72f50D8bd4B4F9b1110F0D6bD2c97526);// BCS testnet
    }
    if (_networkId == 80001) {
      priceFeed = AggregatorV3Interface(0xd0D5e3DB44DE05E9F294BB0a3bEEaF030DE24Ada);// Matic testnet
    }
    if (_networkId == 137) {
      priceFeed = AggregatorV3Interface(0xAB594600376Ec9fD91F8e885dADF0CE036862dE0);// Matic mainnet
    }
  }

  function _updateProvider(
    string memory _name,
    string memory _info,
    string memory _playerURL
  ) public onlyOwner {
    name = _name;
    info = _info;
    playerURL = _playerURL;
  }

  function getLatestPrice() public view returns (uint256, uint8) {
    (,int256 price,,,) = priceFeed.latestRoundData();
    uint8 decimals = priceFeed.decimals();
    return (uint256(price), decimals);
  }

  function getContentAggregatorsNFTBundles() public view returns (address[] memory) {
    return _contentAggregatorsNFTBundles.collection();
  }

  receive() external payable {
    emit Received(msg.sender, msg.value);
  }


  function _addContentAggregator(address nftBunble,address contentAggregator) public onlyOwner {
    _contentAggregatorsNFTBundles.add(nftBunble);
    contentAggregatorForNFTBundle[nftBunble] = contentAggregator;
  }

  function _removeContentAggregator(address nftBunble) public onlyOwner {
    _contentAggregatorsNFTBundles.remove(nftBunble);
    delete contentAggregatorForNFTBundle[nftBunble];
  }

  /**
  * payFor(...) function to cover title payment or subscription payment. NFTBunble includes orderId inside JSON tokenURI, with uses for future verification.
  */
  function payFor(address nftBunble,uint256 nftBunbleId, uint256 orderId) public payable {
    require(msg.value > 0, "Provider: minimal purchase > 0");
    require(IControl721(nftBunble).ownerOfNFT(nftBunbleId) == address(this), "Provider: nftBunble must be on Provider balance");
    recievedPaymentsValuesEachNFTBundle[nftBunble][nftBunbleId] = recievedPaymentsValuesEachNFTBundle[nftBunble][nftBunbleId] + msg.value;
    emit PaymentReceivedFor(msg.value, msg.sender, nftBunble, nftBunbleId, orderId);
    if (IControl721(nftBunble).tokenRevShareOfNFT(nftBunbleId) > 0) {
      uint256 revShare = IControl721(nftBunble).tokenRevShareOfNFT(nftBunbleId);
      uint256 amount = (msg.value.mul(revShare)).div(10 ** 18);
      payable(contentAggregatorForNFTBundle[nftBunble]).transfer(amount);
      emit PayRevenueSharing(amount,contentAggregatorForNFTBundle[nftBunble]);
    }
  }


  function payForInUSD(address nftBunble,uint256 nftBunbleId, uint256 orderId, uint256 amountToPay) public {
    require(IERC20(usdContract).allowance(msg.sender,address(this)) > amountToPay, "Provider: please allow to spend USD first");
    IERC20(usdContract).transferFrom(msg.sender, address(this), amountToPay);
    uint256 priceMainToUSD;
    uint8 decimals;
    (priceMainToUSD,decimals) = getLatestPrice();
    uint256 amountInMainCoin = amountToPay.div(priceMainToUSD.div(10 ** uint256(decimals)));
    recievedPaymentsValuesEachNFTBundle[nftBunble][nftBunbleId] = recievedPaymentsValuesEachNFTBundle[nftBunble][nftBunbleId] + amountInMainCoin;
    emit PaymentReceivedFor(amountInMainCoin, msg.sender, nftBunble, nftBunbleId, orderId);
    if (IControl721(nftBunble).tokenRevShareOfNFT(nftBunbleId) > 0) {
      uint256 revShare = IControl721(nftBunble).tokenRevShareOfNFT(nftBunbleId);
      uint256 amount = (amountToPay.mul(revShare)).div(10 ** 18);
      IERC20(usdContract).transfer(contentAggregatorForNFTBundle[nftBunble], amount);
      emit PayRevenueSharing(amount,contentAggregatorForNFTBundle[nftBunble]);
    }
  }

  function _withdrawSuperAdmin(address payable sender,address token, uint256 amount) public onlyOwner returns (bool) {
    if (amount > 0) {
      if (token == address(0)) {
        sender.transfer(amount);
        return true;
      } else {
        IERC20(token).transfer(sender, amount);
        return true;
      }
    }
    return false;
  }


  /**
  * Always returns `IERC721Receiver.onERC721Received.selector`.
  */
  function onERC721Received(address, address, uint256, bytes memory) public virtual override returns (bytes4) {
    return this.onERC721Received.selector;
  }
}

