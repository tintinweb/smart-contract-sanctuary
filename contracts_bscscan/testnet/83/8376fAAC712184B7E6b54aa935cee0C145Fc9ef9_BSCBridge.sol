pragma solidity ^0.8.0;
import "./bridgeBase.sol";

contract BSCBridge is BridgeBase {
  constructor(address token, address _validator)
    BridgeBase(token, _validator)
  {}

  uint256 nonce_count;

  function depositTokens(uint256 amount, address recipient) external override {
    uint256 balBeforeTrans = token.balanceOf(address(this));

    token.transferFrom(msg.sender, address(this), amount);

    nonce_count += 1;

    uint256 balAfterTrans = token.balanceOf(address(this));

    uint256 transAmount = balAfterTrans - balBeforeTrans;
    bytes32 txHash = keccak256(
      abi.encode(transAmount, nonce_count, recipient, msg.sender)
    );

    bytes memory _transactionID = abi.encode(
      transAmount,
      nonce_count,
      recipient,
      msg.sender
    );

    emit TokenDeposit(txHash, _transactionID);
  }

  function withdrawTokens(
    bytes32 _txHash,
    bytes calldata _transactionID,
    bytes calldata signature
  ) external override {
    (uint256 _amount, uint256 _nonce, address _to, address _from) = abi.decode(
      _transactionID,
      (uint256, uint256, address, address)
    );

    require(msg.sender == _to, "BSC Bridge: Irrelevant reciever");
    address signAddress;

    bytes32 message = prefixed(
      keccak256(abi.encodePacked(_from, _to, _amount, _nonce))
    );

    signAddress = recoverSigner(message, signature);

    require(validator == signAddress, "BSC Bridge: wrong signature");

    uint256 adminBal = token.balanceOf(address(this));
    require(
      adminBal >= _amount,
      "BSC Bridge: insuffient funds in admin account."
    );

    require(
      !processedTransactions[_txHash],
      "BSCBridge: transaction already processed"
    );
    processedTransactions[_txHash] = true;

    token.transfer(_to, _amount);
    emit TokenWithdraw(_from, _to, _amount, _nonce, signature);
  }
}

