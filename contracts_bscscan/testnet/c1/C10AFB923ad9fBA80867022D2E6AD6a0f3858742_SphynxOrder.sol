import "./interfaces/ISphynxRouter.sol";
import "./interfaces/ISphynxFactory.sol";
import "hardhat/console.sol";

contract SphynxOrder is Ownable, ReentrancyGuard, KeeperCompatible {
    struct Order {
        uint256 id;
        uint256 amountA;
        uint256 amountB;
        address tokenA;
        address tokenB;
        address user;
        uint256 expiredAt;
        bool isSell;
        bool isCanceled;
    }

    uint256 public currentOrderId;
    mapping(uint256 => Order) orders;
    mapping(address => uint256[]) public userOrderIds;
    uint256[] public activeOrderIds;
    ISphynxRouter public router;
    ISphynxFactory public factory;

    event OrderCreated(
        uint256 id,
        uint256 amountA,
        uint256 amountB,
        address tokenA,
        address tokenB,
        bool isSell,
        address owner
    );

    event OrderCanceled(
        uint256 id
    );

    constructor(address _router) {
        router = ISphynxRouter(_router);
        factory = ISphynxFactory(router.factory());
    }

    function createOrder(uint256 _amountA, uint256 _amountB, address _tokenA, address _tokenB, bool _isSell, uint256 _expiredAt) external nonReentrant {
        address pair = factory.getPair(_tokenA, _tokenB);
        require(pair != address(0), "pair-does-not-exist");
        Order memory newOrder;
        newOrder.id = currentOrderId;
        newOrder.amountA = _amountA;
        newOrder.amountB = _amountB;
        newOrder.tokenA = _tokenA;
        newOrder.tokenB = _tokenB;
        newOrder.isSell = _isSell;
        newOrder.user = msg.sender;
        newOrder.expiredAt = _expiredAt;
        orders[currentOrderId] = newOrder;
        activeOrderIds.push(currentOrderId);
        userOrderIds[msg.sender].push(currentOrderId);
        emit OrderCreated(currentOrderId, _amountA, _amountB, _tokenA, _tokenB, _isSell, msg.sender);
        currentOrderId++;
    }

    function _removeActiveOrderId(uint256 _orderId) internal {
        for(uint256   i=0; i < activeOrderIds.length; i++) {
            if(activeOrderIds[i] == _orderId) {
                activeOrderIds[i] = orders[activeOrderIds.length - 1].id;
                activeOrderIds.pop();
                break;
            }
        }
    }

    function _removeOrderIds(uint256 _orderId) internal {
        uint i = 0;
        for(;i < userOrderIds[msg.sender].length;i++) {
            if(userOrderIds[msg.sender][i] == _orderId) {
                userOrderIds[msg.sender][i] = orders[userOrderIds[msg.sender].length - 1].id;
                userOrderIds[msg.sender].pop();
                break;
            }
        }
        _removeActiveOrderId(_orderId);
    }

    function removeBatchOrderIds(uint256[] memory _orderIds, uint256 _index) public {
        for(uint256 i = 0; i < _index; i++) {
            _removeActiveOrderId(_orderIds[i]);
        }
    }

    function checkUpkeep(bytes calldata) external override returns (bool upkeepNeeded, bytes memory performData) {
        uint256[] memory expiredOrderIds = new uint256[](activeOrderIds.length);
        uint256[] memory performOrderIds;
        uint256 expiredIndex;
        uint256 currentTime = block.timestamp;
        console.log("length", activeOrderIds.length);
        for (uint256 i = 0; i < activeOrderIds.length; i++) {
            Order memory currentOrder = orders[activeOrderIds[i]];
            if(currentOrder.expiredAt < currentTime) {
                expiredOrderIds[expiredIndex] = activeOrderIds[i];
                expiredIndex++;
                continue;
            }
        }

        console.log("expiredIdsLength", expiredOrderIds.length);

        if(expiredOrderIds.length > 0) {
            upkeepNeeded = true;
            performData = abi.encodeWithSignature("removeBatchOrderIds(uint256[],uint256)", expiredOrderIds, expiredIndex);
        }
    }

    function performUpkeep(bytes calldata performData) external override {
        (bool success, bytes memory data) = address(this).call(performData);
    }


    function cancelOrder(uint256 _orderId) external nonReentrant {
        require(orders[_orderId].user == msg.sender, "not-an-owner");
        orders[_orderId].isCanceled = true;
        _removeOrderIds(_orderId);
        emit OrderCanceled(_orderId);
    }
}

