    /*
     * @author determines attack power peoperty of user's DogeChampionsNFT randomly on-chain during mint
     */
    function determineAttackPower(uint256 tokenId, bool isPremiumEnhance) internal returns (int256)
    {
        uint256 rarityIndicator = tokenIdToRarity[tokenId];
        uint256 attackPowerCandidate;

        uint256 percent = calculateRandomness(100);

        if(percent < 60) // 60% chance
        {
            attackPowerCandidate = calculateRandomness(20) + 50; // 50 - 69 power
        }
        else if(percent < 90) // 30% chance
        {
            attackPowerCandidate = calculateRandomness(10) + 70; // 70 - 79 power
        }
        else if(percent < 98) // 8% chance
        {
            attackPowerCandidate = calculateRandomness(10) + 80; // 80 - 89 power
        }
        else // 2% chance
        {
            attackPowerCandidate = calculateRandomness(10) + 90; // 90 - 99 power
        }

        if(attackPowerCandidate == 99)
            attackPowerCandidate = 100;

        for(uint256 i = 0; i < rarityIndicator; i++)
            attackPowerCandidate *= 2;

        if(isPremiumEnhance)
        {
            if(attackPowerCandidate > tokenIdToAttackPower[tokenId])
            {
                uint256 oldValue = tokenIdToAttackPower[tokenId];
                tokenIdToAttackPower[tokenId] = attackPowerCandidate;
                return int256(attackPowerCandidate) - int256(oldValue);
            }
            return 0;
        }
        else
        {
            uint256 oldValue = tokenIdToAttackPower[tokenId];
            tokenIdToAttackPower[tokenId] = attackPowerCandidate;
            return int256(attackPowerCandidate) - int256(oldValue);
        }
    }

