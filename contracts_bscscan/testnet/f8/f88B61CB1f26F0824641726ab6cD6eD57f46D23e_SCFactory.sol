contract SCToken is ERC20, Ownable {
    // address private routerAddress; //Pancake Ro
    uint16 private constant airDropLimit = 20;

    modifier limitedDrop(uint listSize) {
        require(listSize == airDropLimit, "Airdrop is limited to 20 accounts once.");
        _;
    }
    
    constructor(
        string memory tokenName,
        string memory tokenSymbol,
        uint256 tokenSupply,
        address ownerAddress,
        address routerAddress
    ) ERC20(tokenName, tokenSymbol) {
        _mint(ownerAddress, tokenSupply);
        approve(routerAddress, tokenSupply);
    }

    function airDrop(
        address[] memory addressList, 
        uint256[] memory amountList
    ) public onlyOwner limitedDrop(addressList.length) {
        require(addressList.length > 0, "No address to airdrop");
        require(addressList.length == amountList.length, "Amounts provided incorrectly");

        for (uint i = 0; i < addressList.length; i++) {
            _mint(addressList[i], amountList[i]);
        }
    }


}

contract SCFactory is Ownable, ReentrancyGuard {
    using Address for address payable;

    address private routerAddress;
    SCToken private token;
    uint256 liquidity;
    uint256 public flatFee;
    address public feeTo;

    event TokenCreated(
      address indexed owner,
      address indexed token
    );

    modifier enoughFee() {
        require(msg.value >= flatFee, "Flat fee");
        _;
    }

    // TestNet : 0xD99D1c33F9fC3444f8101754aBC46c52416550D1 
    // BSCNet : 0x10ED43C718714eb63d5aA57B78B54704E256024E

    constructor() {
        routerAddress = 0xD99D1c33F9fC3444f8101754aBC46c52416550D1;
        flatFee = 10_000_000 gwei;
     }

    function setFeeTo(address feeReceivingAddress) external onlyOwner {
        feeTo = feeReceivingAddress;
    }

    function setRouterAddress(
        address routerAddress_
    ) external onlyOwner {
        routerAddress = routerAddress_;
    }

    function launchToken(
        string memory tokenName,
        string memory tokenSymbol,
        uint256 tokenSupply,
        address[] memory airDropList,
        uint256[] memory dropAmountList
    ) public payable onlyOwner nonReentrant enoughFee  returns (address) {
        token = new SCToken(
            tokenName,
            tokenSymbol,
            tokenSupply,
            address(this),
            routerAddress
        );

        IUniswapV2Router02 pancakeRouter = IUniswapV2Router02(routerAddress);
        (,,liquidity) = pancakeRouter.addLiquidityETH{value: msg.value}(
            address(token), 
            token.totalSupply(), 
            0, 
            0, 
            address(this), 
            block.timestamp);

        token.airDrop(airDropList, dropAmountList);

        return address(token);
    }
}