pragma solidity ^0.8.0;

import "./standard/Ownable.sol";
import "./interface/IERC20.sol";
import "hardhat/console.sol";

contract HoldMeStaking is Ownable {
    struct StakeInfo {
        uint256 time;
        uint256 amount;
    }

    uint256 constant YEAR_IN_SECONDS = 31556926;
    mapping(address => StakeInfo) public stakeMap;
    address public stakeToken;

    uint256 public rewardAmount;
    // 1% = 1e18
    uint256 public yearlyYield;

    event Staked(address indexed user, uint256 amount);
    event Withdrawn(address indexed user, uint256 amount);
    event RewardPaid(address indexed user, uint256 reward, address token);

    constructor(address _stakeToken, uint256 _yearlyYield) {
        stakeToken = _stakeToken;
        yearlyYield = _yearlyYield;
    }

    function setYearlyYield(uint256 amount) public onlyOwner {
        yearlyYield = amount;
    }

    function addReward(uint256 amount) public onlyOwner {
        rewardAmount += amount;
        IERC20(stakeToken).transferFrom(_msgSender(), address(this), amount);
    }

    function stake(uint256 amount) public {
        StakeInfo storage info = stakeMap[_msgSender()];
        if (info.amount > 0) {
            applyReward(_msgSender());
        }
        info.amount += amount;
        info.time = block.timestamp;
        IERC20(stakeToken).transferFrom(_msgSender(), address(this), amount);
    }

    function unstake() public {
        StakeInfo storage info = stakeMap[_msgSender()];
        rewardAmount -= applyReward(_msgSender());
        IERC20(stakeToken).transfer(_msgSender(), info.amount);
    }

    function applyReward(address sender) internal returns (uint256) {
        StakeInfo storage info = stakeMap[sender];
        uint256 elapsedTime = block.timestamp - info.time;
        uint256 reward = (elapsedTime * yearlyYield) / YEAR_IN_SECONDS;
        info.amount += reward;
        info.time = block.timestamp;
        return reward;
    }
}

