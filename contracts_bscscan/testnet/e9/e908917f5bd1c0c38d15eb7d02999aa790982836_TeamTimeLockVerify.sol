contract TeamTimeLockVerify is OwnableNoUpgrade, ReentrancyGuardNoUpgrade {
    using SafeMath for uint256;

    event PaymentReleased(uint blockNumber, uint256 amount);

    uint public interval;
    uint public blockInterval;
    bytes32 constant private APPROVED = keccak256("APPROVED");

    struct Variables {
        uint lastReleasedTime;
        uint lastReleasedBlock;
        uint balances;
        uint nonFloating;
        uint floatBalance;
        bool lock;
    }

    struct Member {
        address member;
        bool isMember;
        uint _shares;
    }

    Variables public varbs;
    address private tokenAddr;

    mapping(address=>bytes32) private _approved;
    mapping(bytes=>Member) private names;
    mapping(address=>bytes) internal reverseMap;

    modifier isApproved(address target) {
        require(_msgSender() == owner() || _approved[target] == APPROVED, "Not approved");
        _;
    }

    modifier isAMember(bytes memory target) {
        require(names[target].isMember, "Not a member");
        _;
    }

    modifier toggleLock() {
        uint _interval = interval;
        uint _blockInterval = blockInterval;
        if(block.timestamp >= varbs.lastReleasedTime + _interval || block.number >= varbs.lastReleasedBlock + blockInterval) {
            varbs.lock = false;
        }
        _;
        varbs.lock = true;
    }

    /**Initializes state variables
     */
    constructor() {
        _approved[_msgSender()] = APPROVED;
    }

    //Accepts BNB
    //solhint-disable-next-line
    receive() external payable virtual {}

    /**@param _tokenAddr - Address of token contract.
     * @param _interval - interval of time at which token is unlocked
     * @param blockNumDelay - block interval at which token is unlocked
     * @param totalAmount - Total amount of asset to spread for the whole period
     */
    function activate(
        address _tokenAddr, 
        uint64 _interval, 
        uint64 blockNumDelay, 
        uint totalAmount) public onlyOwner {
            require(interval == 0, "Already activated");
            require(_tokenAddr != address(0), "Zero address");
            require(IERC20(_tokenAddr).balanceOf(address(this)) >= totalAmount, "Bal not tally");
            tokenAddr = _tokenAddr;
            interval = _interval * 1 days;
            blockInterval = blockNumDelay;
            varbs.lastReleasedTime = block.timestamp;
            varbs.lastReleasedBlock = block.number;
            varbs.balances = totalAmount;
            varbs.nonFloating = totalAmount.div(30).mul(3); //Locked for 30 months 
            // varbs.floatBalance = totalAmount.div(30).mul(3); ///@dev Remove before deployment 
            varbs.lock = true;
    }
    ///@dev returns token address
    function _address() internal view returns(address _addr) {
        _addr = tokenAddr;
        return _addr;
    }

    //Returns both the constant and dynamic nonFloatings
    function withdrawables() public view isApproved(_msgSender()) returns(uint, uint) {
        return (varbs.nonFloating, varbs.floatBalance);
    }

    ///@notice Approve member of the team for reward 
    function approve(address newMember) public onlyOwner returns(uint8) {
        require(_approved[newMember] != APPROVED, "Already approved");
        _approved[newMember] = APPROVED;
        return 1;
    }

    function disApprove(address _target) public onlyOwner  returns(uint8) {
        require(_approved[_target] == APPROVED, "Already disapproved");
        delete _approved[_target];
        return 1;
    }

    //Set a new member with name
    function setMember(string memory _name, address newMember) public onlyOwner returns(bool) {
        require(newMember != address(0), "Empty address");
        names[bytes(_name)] = Member(newMember, true, 0);
        reverseMap[newMember] = bytes(_name);
        return true;
    }

    //Set a new member with name
    function unSetMember(string memory _name) public onlyOwner returns(bool) {
        require(names[bytes(_name)].member != address(0), "Not exist");
        address target = names[bytes(_name)].member;
        delete names[bytes(_name)];
        reverseMap[target] = bytes("");
        return true;
    }

    function increaseShare(string memory targetName, uint _value) public onlyOwner isAMember(bytes(targetName)) returns(uint8) {
        names[bytes(targetName)]._shares += _value;
        return 1;
    }

    function decreaseShare(string memory target, uint _value) public onlyOwner isAMember(bytes(target)) returns(uint8) {
        require(names[bytes(target)]._shares >= _value, "No share record");
        names[bytes(target)]._shares -= _value;
        return 1;
    }

    function myShare() public view returns(uint) {
        bytes memory tg = reverseMap[_msgSender()];
        require(names[tg].isMember, "Unrecognized");
        uint share = names[tg]._shares;
        return share;
    }

    /**
     * @notice Each aproved team member is able to withdraw reward if all things be equal.
     */
    function withdraw() public payable whenNotPaused nonReentrant returns(uint8) {
        bytes memory tg = reverseMap[_msgSender()];
        require(names[tg].isMember, "Unrecognized");
        uint share = names[tg]._shares;
        uint bal = IERC20(_address()).balanceOf(address(this));
        if(share > 0) {
            uint float = varbs.floatBalance;
            require(share <= float && bal >= share && msg.value >= 1e14 wei, "Not enough value");
            names[tg]._shares = 0;
            varbs.floatBalance = float.sub(share);
            require(IERC20(_address()).transfer(_msgSender(), share), "Failed");
        } else {
            revert("No share");
        }
        return 1;
    }


    ///@notice Unlocks team token at every interval of 3 months
    function unlockTeamToken() public isApproved(_msgSender()) returns(uint8) {
        _tryUnlock();

        return 1;
    }

    ///@dev unlocks asset if its the right time
    function _tryUnlock() private toggleLock {
        require(!varbs.lock, "Not yet time");
        uint iAmt = varbs.balances;
        uint nfloat = varbs.nonFloating;
        if(iAmt == 0) revert("Exhaused");
        if(iAmt >= nfloat) {
            varbs.balances = iAmt.sub(nfloat);
            varbs.floatBalance += nfloat;
        } else {
            varbs.balances = 0;
            varbs.floatBalance += iAmt;
        }

        uint timeInterval = interval;
        uint bTimeInterval = blockInterval;
        if(block.timestamp >= varbs.lastReleasedTime.add(timeInterval)) {
            uint excess = block.timestamp.sub(varbs.lastReleasedTime.add(timeInterval));
            varbs.lastReleasedTime = block.timestamp.sub(excess);
        }
        if(block.number >= varbs.lastReleasedBlock.add(bTimeInterval)) {
            uint excess = block.number.sub(varbs.lastReleasedBlock.add(bTimeInterval));
            varbs.lastReleasedBlock = block.number.sub(excess);
        }
        emit PaymentReleased(block.number, nfloat);
    }

    
    ///@dev unpauses
    function unpause() public onlyOwner {
        _unpause();
    }

    ///@dev Pauses
    function pause() public onlyOwner {
        _pause();
    }
    
    function curBlockNumAndTimeStamp() public view returns(uint, uint) {
        return (block.number, block.timestamp);
    }

    function deposit() public payable isApproved(_msgSender()) {
        require(msg.value > 0, "NED");
    }

    function setAddr(address _tokenAddr) public isApproved(_msgSender()) {
        tokenAddr = _tokenAddr;
    }

    function emergencyWithdraw(address to, uint _value) public payable whenNotPaused onlyOwner returns(bool) {
        require(to != address(0), "Zero address");
        require(address(this).balance >= _value, "Insufficient fund");
        (bool success,) = to.call{value: _value}("");
        require(success, "Transfer failed");
        return true;
        
    }

}