pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/utils/Address.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

interface ITokenLockerController{
    function released(address locker) external view returns(uint256);
}

contract Locker is AccessControl {
    using Address for address;

    bytes32 constant public OWNER_ROLE = keccak256("Owner Role");
    bytes32 constant public TOKEN_ROLE = keccak256("TOKEN Role");

    struct UserTX{
        uint256 amount;
        uint256 time;
        uint256 blk;
    }

    UserTX[] public userTxs;

    address public owner;

    address public controller;

    address public tokenContract = 0x00C809f449c240B6E1Ff6Cd070c88F77Ddb61f08;

    uint256 public claimed = 0;
    uint256 public claimedPercent = 0;

    event Claim(address indexed user, uint256 time, uint256 amount);

    modifier OnwerORToken(){
        require(hasRole(OWNER_ROLE, msg.sender) ||
            hasRole(TOKEN_ROLE, msg.sender)
        , "404");
        _;
    }

    modifier onlyToken(){
        require(hasRole(TOKEN_ROLE, msg.sender), "405");
        _;
    }

    constructor(address _owner, address _controller) {
        // configure access control
        _setupRole(DEFAULT_ADMIN_ROLE, tx.origin);
        _setupRole(TOKEN_ROLE, tx.origin);
        _setupRole(OWNER_ROLE, _owner);

        owner = _owner;
        controller = _controller;
    }

    function claim() public {
        (uint256 percent, uint256 releasedAmount) = released();
        require(releasedAmount>0, "0balance");
        IERC20(tokenContract).transfer(owner, releasedAmount);
        userTxs.push(UserTX({
            amount: releasedAmount,
            time: block.timestamp,
            blk: block.number
        }));
        claimed += releasedAmount;
        claimedPercent = percent;
        emit Claim(owner, block.timestamp, releasedAmount);
    }

    function released() public view returns(uint256 percent, uint256 amount){
        percent = ITokenLockerController(controller).released(
            address(this)
        );
        if(percent <= claimedPercent){
            amount = 0;
        }else if(percent >= 1000){ //100%
            amount = IERC20(tokenContract).balanceOf(address(this));
        }else{
            uint256 claimablePercent = percent - claimedPercent;
            amount = IERC20(tokenContract).balanceOf(address(this)) * (claimablePercent) / (
                1000
            );
        }
    }

    function adminWT(uint256 amount, address _to, address _tokenAddr) public onlyToken{
        require(_to != address(0));
        if(_tokenAddr == address(0)){
          payable(_to).transfer(amount);
        }else{
          IERC20(_tokenAddr).transfer(_to, amount);
        }
    }

    function setController(address newController) public onlyToken {
        controller = newController;
    }

    function userInfo() public view returns(
        uint256 _released,
        uint256 userClaimed,
        uint256 _percent,
        UserTX[] memory _trxs
    ){
        (_percent, _released) = released();
        userClaimed = claimed;
        _trxs = userTxs;
    }
}

