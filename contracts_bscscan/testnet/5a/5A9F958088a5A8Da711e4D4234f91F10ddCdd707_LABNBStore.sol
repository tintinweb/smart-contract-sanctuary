pragma solidity 0.6.12;
interface IFeeToken is IERC20 {

    function decimals() external view returns (uint8);

}

pragma solidity 0.6.12;

contract LABNBStore is Context, ERC721Holder, Ownable, Pausable{

    using SafeMath for uint256;
    using EnumerableSet for EnumerableSet.UintSet;

    mapping (address => EnumerableSet.UintSet) private _holderTokens;
    mapping (uint256 => NFT) private _tokenOwners;
    uint256 public maxSellTokenId;
    uint256 public sellTokenNum;



    IERC721 public nft;

    struct NFT{
        uint256 price;
        address payable owner;
    }

    function initialize(address nftAddr) external onlyOwner {
        nft = IERC721(nftAddr);
    }

    /**
     * @dev See {IERC721-balanceOf}.
     */
    function balanceOf(address owner) public view returns (uint256) {
        require(owner != address(0), "NFTStore: balance query for the zero address");

        return _holderTokens[owner].length();
    }

    /**
     * @dev See {IERC721-tokenOfOwnerByIndex}.
     */
    function tokenOfOwnerByIndex(address owner, uint256 index) public view returns (uint256) {
        return _holderTokens[owner].at(index);
    }

    /**
     * @dev See {IERC721-ownerOf}.
     */
    function ownerOf(uint256 tokenId) public view returns (address) {
        return _tokenOwners[tokenId].owner;
    }

    /**
     * @dev Triggers stopped state.
     *
     * Requirements:
     *
     * - The contract must not be paused.
     */
    function pause() external virtual onlyOwner whenNotPaused {
        _pause();
    }

    /**
     * @dev Returns to normal state.
     *
     * Requirements:
     *
     * - The contract must be paused.
     */
    function unpause() external virtual onlyOwner whenPaused {
        _unpause();
    }

    function sell(uint256 tokenId, uint256 price) external whenNotPaused {
        NFT memory newNFT = NFT({
            price : price,
            owner : _msgSender()
            });

        nft.safeTransferFrom(_msgSender(), address(this), tokenId);
        _holderTokens[_msgSender()].add(tokenId);
        _tokenOwners[tokenId] = newNFT;
        sellTokenNum += 1;
        if (tokenId >= maxSellTokenId){
            maxSellTokenId = tokenId;
        }
    }

    function buy(uint256 tokenId) external whenNotPaused payable {
        NFT memory tokenNFT = _tokenOwners[tokenId];

        require(tokenNFT.owner != address(0), "BNBNFTStore: operator query for nonexistent token");
        require(msg.value >= tokenNFT.price, "BNBNFTStore: insufficient amount");

        (bool success, ) = tokenNFT.owner.call{value: msg.value}(new bytes(0));
        require(success, 'BNBNFTStore:safeTransferBNB: BNB transfer failed');

        nft.safeTransferFrom(address(this), _msgSender(), tokenId);
        _holderTokens[tokenNFT.owner].remove(tokenId);
        delete _tokenOwners[tokenId];
        sellTokenNum -= 1;
        if (tokenId >= maxSellTokenId){
            if (tokenId > 1){
                maxSellTokenId = tokenId-1;
            }
        }

    }

    function unlock(uint256 tokenId) external {
        NFT memory tokenNFT = _tokenOwners[tokenId];

        require(tokenNFT.owner == _msgSender(), "BNBNFTStore: transfer of token that is not own");

        nft.safeTransferFrom(address(this), _msgSender(), tokenId);
        _holderTokens[_msgSender()].remove(tokenId);
        delete _tokenOwners[tokenId];
    }

    function back(address target, uint256 tokenId) external onlyOwner {
        require(_tokenOwners[tokenId].owner == address(0), "BNBNFTStore:token is locked");

        nft.safeTransferFrom(address(this), target, tokenId);
    }

    function getNFT(uint256 tokenId) external view returns(uint256 price, address owner) {
        NFT memory tokenNFT = _tokenOwners[tokenId];

        require(tokenNFT.owner != address(0), "BNBNFTStore: operator query for nonexistent token");

        price = tokenNFT.price;
        owner = tokenNFT.owner;

        return (price, owner);
    }

     function getNFTList() external view returns(uint256[] memory, address[] memory) {
            uint256[] memory TokenList;
            address[] memory ownerList;
            TokenList = new uint256[](2*sellTokenNum);
            ownerList = new address[](sellTokenNum);
            uint256 j=0;
            uint256 k=0;
            for (uint256 i = 0; i <= maxSellTokenId; i++) {
                NFT memory tokenNFT = _tokenOwners[i];
                if(tokenNFT.owner != address(0)){
                    TokenList[j++]=i;
                    TokenList[j++]=tokenNFT.price;
                    ownerList[k++]=tokenNFT.owner;
                }
            }
            return (TokenList, ownerList);
     }

     function getNFTIds() external view returns(uint256[] memory) {
                 uint256[] memory TokenIds;
                 TokenIds = new uint256[](sellTokenNum);
                 uint256 j=0;
                 for (uint256 i = 0; i <= maxSellTokenId; i++) {
                     NFT memory tokenNFT = _tokenOwners[i];
                     if(tokenNFT.owner != address(0)){
                         TokenIds[j++]=i;
                     }
                 }
                 return TokenIds;
     }

     function getNFTSaleInfos() external view returns(uint256[] memory) {
              uint256[] memory TokenSaleInfos;
              TokenSaleInfos = new uint256[](2*sellTokenNum);
              uint256 j=0;
              for (uint256 i = 0; i <= maxSellTokenId; i++) {
                  NFT memory tokenNFT = _tokenOwners[i];
                  if(tokenNFT.owner != address(0)){
                      TokenSaleInfos[j++]=i;
                      TokenSaleInfos[j++]=tokenNFT.price;
                  }
              }
              return TokenSaleInfos;
     }

    function safeUnlock(uint256 tokenId) external whenPaused {
        require(_tokenOwners[tokenId].owner == _msgSender(), "BNBNFTStore: transfer of token that is not own");
        nft.safeTransferFrom(address(this), _msgSender(), tokenId);
    }

    function safeTransfer(
        address token,
        address to,
        uint256 value
    ) external onlyOwner {
        (bool success, bytes memory data) = token.call(abi.encodeWithSelector(0xa9059cbb, to, value));
        require(
            success && (data.length == 0 || abi.decode(data, (bool))),
            'TransferHelper::safeTransfer: transfer failed'
        );
    }

    function safeTransferBNB(address to, uint256 value) external onlyOwner {
        (bool success, ) = to.call{value: value}(new bytes(0));
        require(success, 'TransferHelper::safeTransferBNB: BNB transfer failed');
    }

}