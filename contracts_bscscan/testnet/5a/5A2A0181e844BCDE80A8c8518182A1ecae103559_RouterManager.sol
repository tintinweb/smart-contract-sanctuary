pragma solidity ^0.8.0;
import "../../contracts/Router/interfaces/IRouterManager.sol";
import "../../contracts/Rewards/interfaces/IRewards.sol";
import "../../contracts/Token/imported/ERC20.sol";
import "./interfaces/IPancakeFactory.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/proxy/utils/Initializable.sol";

/**
 * @notice Abstract contract, part of OMNIA token contract.
 * @dev Sets which PancakeSwap router to use and create OMNIA-BNB pair.
 */
contract RouterManager is Ownable, Initializable, IRouterManager {
    ERC20 public omnia;
    IRewards public rewards;

    IPancakeRouter02 internal _pancakeswapV2Router;
    IUniswapV2Pair internal _pancakeswapV2Pair;

    function initialize(
        address omnia_,
        address router_,
        address rewards_
    ) public override initializer {
        omnia = ERC20(omnia_);
        rewards = IRewards(rewards_);

        setPancakeSwapRouter(router_);
    }

    function updateRewardsContract(address rewards_) external override {
        require(
            _msgSender() == owner() || _msgSender() == address(omnia),
            "Owner or OMNIA token"
        );
        rewards = IRewards(rewards_);
    }

    /**
     * Requirements:
     * - only the owner or OMNIA contract can update the PCS router.
     *
     * @inheritdoc IRouterManager
     */
    function setPancakeSwapRouter(address routerAddress) public override {
        require(
            _msgSender() == owner() || _msgSender() == address(omnia),
            "Owner or OMNIA token"
        );
        require(routerAddress != address(0), "Router: addr(0)");

        _pancakeswapV2Router = IPancakeRouter02(routerAddress);

        IPancakeFactory factory = IPancakeFactory(
            _pancakeswapV2Router.factory()
        );
        address _pancakeswapV2PairAddress = factory.createPair(
            address(omnia),
            _pancakeswapV2Router.WETH()
        );
        _pancakeswapV2Pair = IUniswapV2Pair(_pancakeswapV2PairAddress);

        rewards.excludeContractFromRewards(routerAddress);
        rewards.excludeContractFromRewards(_pancakeswapV2PairAddress);
    }

    /**
     * @inheritdoc IRouterManager
     */
    function pancakeswapV2Router()
        public
        view
        override
        returns (IPancakeRouter02)
    {
        return _pancakeswapV2Router;
    }

    /**
     * @inheritdoc IRouterManager
     */
    function pancakeswapV2Pair() public view override returns (IUniswapV2Pair) {
        return _pancakeswapV2Pair;
    }

    /**
     * @inheritdoc IRouterManager
     */
    function isSelling(address sender_, address recipient_)
        public
        view
        override
        returns (bool)
    {
        return (address(omnia) != sender_ &&
            address(_pancakeswapV2Pair) == recipient_);
    }

    /**
     * @inheritdoc IRouterManager
     */
    function isNotRouterNorPair(address account_)
        public
        view
        override
        returns (bool)
    {
        return
            account_ != address(_pancakeswapV2Router) &&
            account_ != address(_pancakeswapV2Pair);
    }

    /**
     * @inheritdoc IRouterManager
     */
    function isNotSellingAndNotPurchasing(address sender_, address recipient_)
        public
        view
        override
        returns (bool)
    {
        return
            sender_ != address(_pancakeswapV2Pair) &&
            recipient_ != address(_pancakeswapV2Pair);
    }
}

